            const uint32_t K    = static_cast<uint32_t>(latura * latura);

            output.write(reinterpret_cast<const char*>(&K), 4);
            break;
        }
        case 2:
        {
            uint32_t N;
            input.read(reinterpret_cast<char*>(&N), 4);

            uint32_t latura = 1;
            uint64_t T      = latura;

            while (N > T)
            {
                latura += 2;
                T += latura * latura;
            }

            const uint64_t T2 = static_cast<uint64_t>(static_cast<double>(T) - std::sqrt(static_cast<double>(T))) + 1;
            const uint64_t R  = N - T2;

            const uint64_t pozitie = R % latura;

            const uint64_t i        = pozitie / 2;
            const uint64_t latura2 = latura / 2;

            uint64_t nr = T2 + ((i + 1) * latura2 + latura2 + (pozitie % 2)) * (pozitie + 1);
            nr >>= 1;
            if (pozitie % 2)
            {
                nr = latura * latura - nr + 1;
            }

            output.write(reinterpret_cast<const char*>(&nr), 8);
            break;
        }
        default:
            break;
    }

    return 0;
}
