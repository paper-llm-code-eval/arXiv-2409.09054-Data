    for (int i = 2; i <= n; ++i) {
        dp[i][0] = max(dp[i][0], dp[i - 1][0]);
        dp[i][0] = max(dp[i][0], dp[i - 1][1]);
        if (cubes[i].color != cubes[i - 1].color && cubes[i].size < cubes[i - 1].size) {
            dp[i][1] = max(dp[i][1], dp[i - 1][0] + cubes[i].size);
        }
        dp[i][1] = max(dp[i][1], dp[i - 1][1]);
    }

    if (c == 1) {
        int t = 0, h = 0;
        for (int i = 1; i <= n; ++i) {
            if (dp[i][1] > h) {
                h = dp[i][1];
                t = i;
            }
        }
        fout << t << ' ' << h << '\n';
    } else {
        vector<int> order(n + 1);
        for (int i = 1; i <= n; ++i) {
            order[i] = i;
        }

        sort(order.begin() + 1, order.end(), [](int i, int j) {
            return cubes[i].size < cubes[j].size;
        });

        int mx_idx = 0;
        for (int i = 1; i <= n; ++i) {
            dp[i][0] = max(dp[i][0], dp[i - 1][0]);
            dp[i][0] = max(dp[i][0], dp[i - 1][1]);
            if (cubes[order[i]].color != cubes[order[i - 1]].color) {
                dp[i][1] = max(dp[i][1], dp[i - 1][0] + cubes[order[i]].size);
            }
            dp[i][1] = max(dp[i][1], dp[i - 1][1]);
            if (dp[i][1] > dp[mx_idx][1]) {
                mx_idx = i;
            }
        }
        fout << mx_idx << '\n';
    }

    return 0;
}
