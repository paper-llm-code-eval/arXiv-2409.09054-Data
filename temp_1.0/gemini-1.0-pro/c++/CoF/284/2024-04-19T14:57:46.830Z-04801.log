                int diff = std::min(c2 - c1, c1 + 26 - c2);
                int cost = std::min(diff, 26 - diff);
                if (cost <= c[i] || red[i]) {
                    c[i] -= cost;
                    if (!red[i]) st.push(line1[j]);
                } else
                    break;
            }
            while (!st.empty()) {
                ans.push_back(st.top());
                st.pop();
            }
        }
        for (char& c_ : line1) { ++c[c_ - 'A']; ans.push_back(c_); }
        for (int j = 1; j < ans.size(); ++j) {
            char c1 = ans[j - 1], c2 = ans[j];
            int diff = std::min(c2 - c1, c1 + 26 - c2);
            min_time += std::min(diff, 26 - diff);
        }
        long long num = 1;
        for (int i = 0; i < 26; ++i) {
            while (c[i]--) (num *= i + 1) %= MOD;
        }
        f_out << min_time << "\n";
        f_out << num << "\n";
        f_out << ans << '\n';
    }
    f_in.close();
    f_out.close();
    return 0;
}
