#include <algorithm>

#include <fstream>

#include <ostream>
using namespace std;

struct elev
{
  int putere, viteza;
};

int main()
{
  ifstream fin("vraji.in");
  ofstream fout("vraji.out");

  int n, max_putere=0, max_obiecte=0, nr_min_cutii=0;

  fin >> n;
  elev elevi[n];

  for (int i = 0; i < n; ++i)
  {
    fin >> elevi[i].putere;
    fin >> elevi[i].viteza;
    int nr_obiecte = elevi[i].putere * elevi[i].viteza;
    max_obiecte = max(max_obiecte, nr_obiecte);
    nr_min_cutii = __gcd((nr_min_cutii), nr_obiecte);
  }

  fout << max_obiecte << endl
       << nr_min_cutii << endl;
  return 0;
}
