#include <iostream>
#include <fstream>
#include <limits>

using namespace std;

int main() {
    ifstream in("maxd.in");
    ofstream out("maxd.out");
    
    // Read the input
    long long a, b;
    in >> a >> b;
    
    // Initialize the output variables
    long long min = LLONG_MAX;
    long long nrdiv = 0;
    long long contor = 0;
    
    // Loop through the numbers in the interval
    for (long long i = a; i <= b; i++) {
        // Count the number of divisors of i
        long long count = 0;
        for (long long j = 1; j <= i; j++) {
            if (i % j == 0) {
                count++;
            }
        }
        // If i has more divisors than min, update the output variables
        if (count > nrdiv) {
            min = i;
            nrdiv = count;
            contor = 0;
        }
        // If i has the same number of divisors as min, increment the count
        if (count == nrdiv) {
            contor++;
        }
    }
    
    // Write the output to the file
    out << min << " " << nrdiv << " " << contor << endl;
    
    return 0;
}
```
