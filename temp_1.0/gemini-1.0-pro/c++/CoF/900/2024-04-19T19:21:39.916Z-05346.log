                            arie++;
                        }
                    }
                }
                arieMax = max(arieMax, arie);
            }
        }
        fout << arieMax << '\n';
    } else if (C == 3) {
        int arieMax = 0, lsus_i, lsus_j, ldjos_i, ldjos_j;
        for (int i = 1; i <= N; i++) {
            for (int j = 1; j <= N; j++) {
                int arie = 0;
                floodFill(i, j, false, false);
                for (int ii = i; ii <= N; ii++) {
                    for (int jj = j; jj <= N; jj++) {
                        if (viz[ii][jj]) {
                            arie++;
                            if (arie == 1) {
                                lsus_i = i;
                                lsus_j = j;
                            }
                            ldjos_i = ii;
                            ldjos_j = jj;
                        }
                    }
                }
                if (arie > arieMax) {
                    arieMax = arie;
                    lsus_i = i;
                    lsus_j = j;
                    ldjos_i = ii;
                    ldjos_j = jj;
                } else if (arie == arieMax) {
                    if (lsus_i > i) {
                        lsus_i = i;
                        lsus_j = j;
                        ldjos_i = ii;
                        ldjos_j = jj;
                    } else if (lsus_i == i && lsus_j > j) {
                        lsus_j = j;
                        ldjos_j = jj;
                    }
                }
            }
        }
        fout << lsus_i << ' ' << lsus_j << ' ' << ldjos_i << ' ' << ldjos_j << '\n';
    }
    return 0;
}
