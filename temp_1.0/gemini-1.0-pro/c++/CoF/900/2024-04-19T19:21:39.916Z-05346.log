int readInt(ifstream &f) {
    int nr = 0, semn = 1;
    char ch;
    while (!isdigit(ch) && ch != '-') {
        f >> ch;
    }
    if (ch == '-') {
        semn = -1;
        f >> ch;
    }
    while (isdigit(ch)) {
        nr = nr * 10 + ch - '0';
        f >> ch;
    }
    return nr * semn;
}

void floodFill(int lin, int col, bool valPeretStanga, bool valPeretJos) {
    if (lin < 1 || lin > N || col < 1 || col > N)
