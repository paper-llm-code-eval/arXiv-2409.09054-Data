```c++
#include <fstream>
#include <algorithm>
#include <vector>
#include <iostream>
#include <climits>
using namespace std;

typedef long long intidx;

ifstream in("popas.in");
ofstream out("popas.out");

const intidx MAXN = 100, MAXK = 20;
intidx T, U, N;
vector<intidx> dist[MAXN + 5];
vector<intidx> dp;

intidx min_popas(intidx curr_pos, intidx curr_path)
{
	if (curr_pos >= dist[curr_path].size())
		return 0;
	if (dp[curr_pos][curr_path] != -1)
		return dp[curr_pos][curr_path];
	intidx ans = -1;
	for (intidx next_pos = curr_pos; next_pos < dist[curr_path].size() - 1; next_pos++) {
		if (dist[curr_path][next_pos] - dist[curr_path][curr_pos] > T)
			break;
		ans = max(
			ans,
			1 + min_popas(next_pos + 1, curr_path)
		);
	}
	if (dist[curr_path].back() - dist[curr_path][curr_pos] <= T)
		ans = max(
			ans,
			min_popas(dist[curr_path].size() - 1, curr_path)
		);
	if (dist[curr_path][curr_pos] >= U)
		ans = max(
			ans,
			min_popas(curr_pos + 1, curr_path)
		);
	else
		ans = max(
			ans,
			1 + min_popas(curr_pos + 1, curr_path)
		);
	return dp[curr_pos][curr_path] = ans;
}


int main()
{
	in >> N;
	in >> dist[1];
	in >> T >> U;
	dp.resize(MAXK + 5, vector<intidx>(MAXN + 5, -1));
	intidx ans_count = 0, min_count = LLONG_MAX, last_path = -1;
	for (intidx i = 1; i <= N; i++) {
		int curr_count = min_popas(0, i);
		if (curr_count > 0 && curr_count <= min_count) {
			if (curr_count < min_count) {
				ans_count = 1;
				min_count = curr_count;
				last_path = i;
			} else
				ans_count++;
		}
	}
	out << ans_count << ' ' << last_path << '\n';
	return 0;
}
