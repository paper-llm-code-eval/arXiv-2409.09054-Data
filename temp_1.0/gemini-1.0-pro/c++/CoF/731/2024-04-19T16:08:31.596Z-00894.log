		dist += DISTANCE_NO_WATER;
	}

	for (int i = 0; i < nPaths; i++) {
		sort(paths[i].springs + 1, paths[i].springs + paths[i].nrSpings + 1, compare);

		backtracking(0, distanceToWater[0], 0);
	}

	if (distMin < numeric_limits<double>::max()) {
		out << distMin << " ";
		for (int i = 0; i < nPaths; i++) {
			if (distMin == nrPathMin) {
				out << i + 1;
				return;
			}
		}
	}
	else {
		out << 0 << "\n";
	}
}

int main() {
