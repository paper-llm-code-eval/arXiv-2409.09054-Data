#include <fstream>
#include <cmath>
#include <numeric>

using namespace std;

ifstream in("popas.in");
ofstream out("popas.out");

struct spring {
	double dist;
	double water;
};

struct path {
	int nrSpings;
	spring* springs;
};

const int MAX_SPRINGS = 20;
const double DISTANCE_FOR_THERMOS = 10;
const double DISTANCE_NO_WATER = 5;
const int MAX_PATHS = 100;

path paths[MAX_PATHS];
int nPaths, distanceToWater[MAX_SPRINGS + 1], d, nrPathMin;
double distMin = numeric_limits<double>::max();

bool readInput() {
	in >> nPaths;
	if (!nPaths) return false;

	for (int i = 0; i < nPaths; i++) {
		int id, nrSprings;
		in >> id;
		in >> nrSprings;

		paths[i].nrSpings = nrSprings;
		paths[i].springs = new spring[nrSprings + 1];

		for (int j = 1; j <= nrSprings; j++) {
			in >> paths[i].springs[j].dist;
		}
	}

	in >> d;
	in >> DISTANCE_NO_WATER;

	return true;
}

double getDistRemain(int springIndex) {
	return d - distanceToWater[springIndex];
}

double getDistToSpring(int springIndex) {
	return DISTANCE_NO_WATER - paths[nrPathMin].springs[springIndex].dist;
}

bool isSpringInRange(int springIndex) {
	if (getDistRemain(springIndex) >= DISTANCE_FOR_THERMOS) return false;
	double totalDist = getDistToSpring(springIndex);
	return totalDist <= DISTANCE_NO_WATER;
}


void backtracking(int springIndex, double refillDist, int nrRefills) {
	if (!isSpringInRange(springIndex)) return;
	if (springIndex == paths[nrPathMin].nrSpings) {
		if (nrRefills < distMin) {
			distMin = nrRefills;
		}
		return;
	}

	backtracking(springIndex + 1, paths[nrPathMin].springs[springIndex + 1].dist, nrRefills + 1);

	for (int i = springIndex + 1; i <= paths[nrPathMin].nrSpings; i++) {
		if (!isSpringInRange(i)) break;

		backtracking(i, paths[nrPathMin].springs[i].dist, nrRefills);
	}
}

int compare(spring a, spring b) {
	return a.dist < b.dist;
}


void solve() {
	double dist = 0;
	for (int i = 1; i <= d; i++) {
		distanceToWater[i] = dist;
		dist += DISTANCE_NO_WATER;
	}

	for (int i = 0; i < nPaths; i++) {
		sort(paths[i].springs + 1, paths[i].springs + paths[i].nrSpings + 1, compare); // Fix: sorts the springs by their distance

		backtracking(0, distanceToWater[0], 0);
	}

	if (distMin < numeric_limits<double>::max()) {
		out << distMin << " ";
		for (int i = 0; i < nPaths; i++) {
			if (distMin == nrPathMin) {
				out << i + 1;
				return;
			}
		}
	}
	else {
		out << 0 << "\n";
	}
}

int main() {
