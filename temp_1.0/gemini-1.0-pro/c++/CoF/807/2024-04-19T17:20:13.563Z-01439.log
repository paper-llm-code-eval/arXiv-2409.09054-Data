      magic += ch[i] - 'a' + 'A';
    } else if ('0' <= ch[i] && ch[i] <= '9') {
      int digit = ch[i] - '0';
      if (digit == 0) continue;
      okDigits = true;
      if (numFermecat == -1) {
        numFermecat = digit;
      } else if (digit != numFermecat) {
        numFermecat *= 10;
        numFermecat += digit;
      }
    }
