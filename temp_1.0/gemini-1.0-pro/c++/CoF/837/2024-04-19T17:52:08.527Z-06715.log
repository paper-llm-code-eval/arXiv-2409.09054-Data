#include <fstream>
#include <vector>
#include <algorithm>

std::vector<int> coords_x, coords_y;

int main() {
    std::ifstream in("puncte.in");
    std::ofstream out("puncte.out");

    int n;
    in >> n;

    for (int i = 0; i < n; i++) {
        int x, y;
        in >> x >> y;

        coords_x.push_back(x);
        coords_y.push_back(y);
    }

    std::sort(coords_x.begin(), coords_x.end());
    std::sort(coords_y.begin(), coords_y.end());

    int nrp = 1;  // numărul maxim de puncte pe o dreaptă

    for (int i = 0; i < n; i++) {
        int cnt_x = 1, cnt_y = 1;

        if (i != 0 && coords_x[i] == coords_x[i - 1]) {  // numărăm punctele pe aceeași abscisă
            cnt_x += std::count(coords_x.begin(), coords_x.end(), coords_x[i]);
        }

        if (i != 0 && coords_y[i] == coords_y[i - 1]) {  // numărăm punctele pe aceeași ordonată
            cnt_y += std::count(coords_y.begin(), coords_y.end(), coords_y[i]);
        }

        nrp = std::max(nrp, std::max(cnt_x, cnt_y));
    }

    long long nrd = 0;  // numărul de drepte distincte

    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if (coords_x[i] == coords_y[j] || coords_x[i] + coords_y[j] == 0) {
                nrd += std::count_if(coords_x.begin(), coords_x.end(), [=](int x) {
                    return x == coords_x[i];
                }) - 1;
                nrd += std::count_if(coords_y.begin(), coords_y.end(), [=](int y) {
                    return y == coords_y[j];
                }) - 1;
                break;
            }
        }
    }

    std::vector<char> erased(n, false);  // vector de marcaje pentru puncte erase
    int xp = coords_x[0];               // abscisa punctului rămas

    for (int i = 0; i < n - 1; i++) {  // iterăm prin puncte
        for (int j = 0; j < n; j++) {  // iterăm prin abscisa punctului curent
            if (!erased[j] && coords_x[j] == coords_x[i]) {  // dacă punctul nu e șters și este pe aceeași abscisă
                erased[j] = true;  // marcăm punctul ca fiind șters

                if ((i + 1) % 3 == 0) {  // dacă punctul curent este cel care trebuie numărat
                    if (xp > coords_x[j]) {  // și este mai la stânga decât punctul rămas
                        xp = coords_x[j];  // actualizăm abscisa punctului rămas
                    }
                }
            }
        }
    }

    out << nrp << '\n'
        << nrd << '\n'
        << xp << '\n';

    return 0;
}
