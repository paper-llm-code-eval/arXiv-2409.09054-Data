#include <bits/stdc++.h>

int x[250001], y[250001], dplast[250001][2];
std::pair<int, int> dp[250001][2];

int main() {
  freopen("puncte.in", "r", stdin);
  freopen("puncte.out", "w", stdout);

  int n;
  std::cin >> n;
  for (int i = 0; i < n; i++) {
    std::cin >> x[i] >> y[i];
  }

  int nrdp = 0;
  int x1 = 0, x2 = 0, y1 = 0, y2 = 0;
  for (int i = 0; i < n; i++) {
    if (x[i] == 0 || y[i] == 0) {
      x1 = std::min(x1, x[i]);
      y1 = std::min(y1, y[i]);
      x2 = std::max(x2, x[i]);
      y2 = std::max(y2, y[i]);
      nrdp++;
    }
  }
  x2 -= x1;
  y2 -= y1;
  if (x2 > y2)
    nrdp += x2;
  else
    nrdp += y2;

  int nr = 0, nx = 0, ny = 0;
  dp[n][0] = {0, 0};
  dp[n][1] = {0, 0};
  for (int i = n - 1; i >= 0; i--) {
    nx = dp[i + 1][0].second;
    ny = dp[i + 1][1].second;
    if (x[i] == y[i]) {
      if (dp[i + 1][0].first > dp[i + 1][1].first) {
        dp[i][1] = {dp[i + 1][0].first + 1, x[i]};
        if (dp[i][1].first > nr) {
          nr = dp[i][1].first;
          nx = x[i];
        }
      } else {
        dp[i][1] = {dp[i + 1][1].first + 1, y[i]};
        if (dp[i][1].first > nr) {
          nr = dp[i][1].first;
          nx = y[i];
        }
      }
    } else if (x[i] + y[i] == 0) {
      if (dp[i + 1][0].first > dp[i + 1][1].first) {
        dp[i][0] = {dp[i + 1][0].first + 1, x[i]};
        if (!nx || dp[i][0].first > dplast[nx][0].first) {
          ny = x[i];
          dplast[nx][0] = dp[i][0];
        }
      } else {
        dp[i][0] = {dp[i + 1][1].first + 1, y[i]};
        if (!ny || dp[i][0].first > dplast[ny][0].first) {
          nx = y[i];
          dplast[ny][0] = dp[i][0];
        }
      }
    } else {
      dp[i][0] = dp[i + 1][0];
      dp[i][1] = dp[i + 1][1];
    }
  }

  int nrd = nr;

  nx = 0;
  ny = 0;
  int xp = -1;
  int ord = 1;
  for (int i = 1; i <= n; i += 3) {
    if (xp == -1) {
      xp = x[i - 1];
      continue;
    }
    if (x[i - 1] == xp) {
      if (i - 1 > ord) {
        nx = std::max(nx, ord);
        ny = std::max(ny, i - 1);
      }
      ord += 3;
    } else {
      if (x[i - 1] > xp) {
        if (nx < ny) {
          xp = x[i - 1];
          ord = i;
        } else {
          if (x[ny - 1] > xp) {
            xp = x[i - 1];
            ord = i;
          } else {
            nx = 0;
            ny = 0;
            xp = x[i - 1];
            ord = i;
          }
        }
      } else {
        xp = x[i - 1];
        ord = i;
      }
    }
  }

  std::cout << nrdp << '\n';
  std::cout << nrd << '\n';
  std::cout << xp << '\n';
  return 0;
}
