#include <fstream>
#include <vector>

using namespace std;

ifstream f("fermier.in");
ofstream g("fermier.out");

const int NMAX = 100;

int n, c;
int d[NMAX + 5], q[NMAX + 5];
int sol[NMAX + 5], used[NMAX + 5];

void citire() {
    f >> n >> c;
    for (int i = 0; i <= n; ++i) {
        f >> d[i];
    }
    for (int i = 1; i <= n; ++i) {
        f >> q[i];
    }
}

int main() {
    citire();

    int dist = 0;
    int pos = 1;
    int rem = c - q[pos];
    used[pos] = 1;
    sol[pos] = d[0];
    bool ok = true;
    while (ok) {
        int minDist = 1e9, nextPos = -1;
        ok = false;
        for (int i = pos + 1; i <= n; ++i) {
            if (rem - q[i] >= 0 && !used[i]) {
                int dist1 = sol[pos] + d[pos];
                int dist2 = sol[pos] + d[n] + d[i];
                if (dist1 < dist2 || used[i - 1]) {
                    if (dist1 < minDist) {
                        ok = true;
                        minDist = dist1;
                        nextPos = i;
                    }
                } else {
                    if (dist2 < minDist) {
                        ok = true;
                        minDist = dist2;
                        nextPos = i;
                    }
                }
            }
        }
        if (!ok) {
            for (int i = pos - 1; i >= 1; --i) {
                if (rem - q[i] >= 0 && !used[i]) {
                    int dist1 = sol[pos] + d[pos];
                    int dist2 = sol[pos] + d[0] + d[i];
                    if (dist1 < dist2 || used[i + 1]) {
                        if (dist1 < minDist) {
                            ok = true;
                            minDist = dist1;
                            nextPos = i;
                        }
                    } else {
                        if (dist2 < minDist) {
                            ok = true;
                            minDist = dist2;
                            nextPos = i;
                        }
                    }
                }
            }
        }
        if (!ok) break;
        dist += minDist;
        rem -= q[nextPos];
        pos = nextPos;
        used[pos] = 1;
        sol[pos] = sol[pos - 1] + d[pos - 1];
    }

    if (rem > 0) {
        dist += d[pos] + d[0];
    }

    g << dist << "\n";
    return 0;
}
