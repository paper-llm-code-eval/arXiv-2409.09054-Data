    sol[pos] = d[0];
    bool ok = true;
    while (ok) {
        int minDist = 1e9, nextPos = -1;
        ok = false;
        for (int i = pos + 1; i <= n; ++i) {
            if (rem - q[i] >= 0 && !used[i]) {
                int dist1 = sol[pos] + d[pos];
                int dist2 = sol[pos] + d[n] + d[i];
                if (dist1 < dist2 || used[i - 1]) {
                    if (dist1 < minDist) {
                        ok = true;
                        minDist = dist1;
                        nextPos = i;
                    }
                } else {
                    if (dist2 < minDist) {
                        ok = true;
                        minDist = dist2;
                        nextPos = i;
                    }
                }
            }
        }
        if (!ok) {
            for (int i = pos - 1; i >= 1; --i) {
                if (rem - q[i] >= 0 && !used[i]) {
                    int dist1 = sol[pos] + d[pos];
                    int dist2 = sol[pos] + d[0] + d[i];
                    if (dist1 < dist2 || used[i + 1]) {
                        if (dist1 < minDist) {
                            ok = true;
                            minDist = dist1;
                            nextPos = i;
                        }
                    } else {
                        if (dist2 < minDist) {
                            ok = true;
                            minDist = dist2;
                            nextPos = i;
                        }
                    }
                }
            }
        }

        if (!ok) {
            break;
        }

        dist += minDist;
        rem -= q[nextPos];
        pos = nextPos;
        used[pos] = 1;
        sol[pos] = sol[pos - 1] + d[pos - 1];

        if (rem < 0) {
            rem = c;
            dist += d[pos] + d[0];
        }
    }

    if (rem > 0) {
        dist += d[pos] + d[0];
    }

    g << dist << "\n";
    return 0;
