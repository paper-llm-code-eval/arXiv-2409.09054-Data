#include <fstream>
#include <iostream>
using namespace std;

const int maxn = 110;

struct _ {
  int d, c;
};

int n, c;
int dx[maxn], dq[maxn];
_ ld[maxn][maxn];  // ld = last distance
int nd[maxn][maxn];  // nd = next distance

int st;
int solve(int p, int q, int d) {
  if (p == n)
    return 0;
  if (q <= 0)
    q = dq[p];
  if (ld[p][q] != 0)
    return ld[p][q];
  int ans = 1 << 30;
  if (q > c) {
    ans = min(ans, solve(p + 1, q - c, d + dx[p]));
    for (int i = p + 2; i <= n; i++)
      ans = min(ans, solve(i, q - c, d + nd[p][i]));
  } else if (q <= c) {
    ans = min(ans, solve(p + 1, 0, d + dx[p]));
    for (int i = p + 2; i <= n; i++)
      ans = min(ans, solve(i, 0, d + nd[p][i]));
  }
  ld[p][q] = ans;
  return ans;
}

int main() {
  ifstream cin("fermier.in");
  ofstream cout("fermier.out");
  cin >> n >> c;
  int x;
  cin >> x;
  st = x;
  dx[0] = x;
  for (int i = 1; i < n; i++) {
    cin >> x;
    dx[i] = x;
    nd[i - 1][i] = x;
  }
  cin >> x;
  dx[n] = x;
  nd[n][0] = x;
  for (int i = 0; i < n; i++) {
    cin >> x;
    dq[i] = x;
  }
  for (int i = n - 1; i >= 1; i--) {
    for (int j = i + 1; j <= n; j++) nd[i][j] = nd[j][i];
  }
  int sol = solve(1, c, 0);
  cout << st + sol << endl;
}
```
