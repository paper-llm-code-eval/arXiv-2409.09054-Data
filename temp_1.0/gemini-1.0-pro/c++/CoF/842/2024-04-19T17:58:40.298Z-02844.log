#include <fstream>
#include <vector>
#include <algorithm>

static constexpr size_t MAX_NUM = 100001;

int main() {
  std::ifstream input("martisoare.in", std::ios::binary);
  std::ofstream output("martisoare.out", std::ios::binary);

  size_t n;
  input.read(reinterpret_cast<char*>(&n), sizeof(size_t));
  std::vector<size_t> numbers(n + 1, 0);
  std::vector<size_t> reversed(n + 1, 0);
  for (size_t i = 1; i <= n; i++) {
    input.read(reinterpret_cast<char*>(numbers.data() + i), sizeof(size_t));
    reversed[numbers[i]] = i;
  }
  output.write(reinterpret_cast<const char*>(reversed.data() + 1), n * sizeof(size_t));

#define get_digit(n, i) (((n) / (static_cast<size_t>(10) << i)) % 10)
  for (size_t i = 0; i < 6; i++) {
    size_t a = 0, b = 0;
    for (size_t j = 0; j <= n; j++) {
      const size_t digit_j = get_digit(numbers[j], i);
      for (size_t k = j + 1; k <= n; k++) {
        const size_t digit_k = get_digit(numbers[k], i);
        if (digit_j > digit_k) {
          a = j;
          b = k;
        } else if (digit_j < digit_k) {
          a = 0;
          b = 0;
          break;
        }
      }
      if (a != 0) {
        break;
      }
    }
    if (a != 0) {
      const size_t digit_a = get_digit(numbers[a], i);
      const size_t digit_b = get_digit(numbers[b], i);
      std::swap(reversed[digit_a], reversed[digit_b]);
      std::swap(numbers[reversed[digit_a]], numbers[reversed[digit_b]]);
      output << digit_a << ' ' << digit_b << '\n';
      break;
    }
  }

  std::sort(numbers.begin() + 1, numbers.begin() + 1 + n);
  output << *std::max_element(numbers.begin() + 1, numbers.begin() + 1 + n);
}
