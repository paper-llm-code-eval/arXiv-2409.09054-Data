#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream fin("joc.in");
  ofstream fout("joc.out");
  unsigned nr;
  fin >> nr;
  vector<unsigned> v(nr);

  for (auto&el:v) {
    fin >> el;
  }

  unsigned player1 = 0, player2 = 0, nextTarget = 1;

  for (unsigned currentIndex = 0; currentIndex < nr; ++currentIndex) {
    auto candidate =
        upper_bound(v.begin(), v.end(), nextTarget - player1,
                    [](auto a, auto b) { return a < b; }) -
        v.begin();
    if (candidate < nr) {
      if (currentIndex&1) {
        player2 += v[candidate];
      } else {
        player1 += v[candidate];
      }
      nextTarget++;
    }
  }
  fout << max(player1, player2) << endl;
  return 0;
}
```
