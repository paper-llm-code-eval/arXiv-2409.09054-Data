#include <vector>
#include <fstream>
#include <queue>

using namespace std;

const int inf = 1 << 29;

int N, K;
vector<vector<int>> C;
int dp[105][105][1 << 14];
bool viz[105][105][1 << 14];
int movX[5] = {0, 1, 0, -1, 0};
int movY[5] = {0, 0, 1, 0, -1};
int X[5] = {0, 1, 2, 1, 2};
int Y[5] = {0, 1, 1, 2, 1};

struct State {
    int x, y, mask, sum;

    bool operator<(const State &o) const {
        return sum < o.sum;
    }
};

int compute() {
    priority_queue<State> heap;

    heap.push({1, 1, 0, C[1][1]});
    dp[1][1][0] = C[1][1];
    viz[1][1][0] = true;

    int ans = -inf;
    while (!heap.empty()) {
        State node = heap.top();
        heap.pop();

        if (node.x == N && node.y == N) {
            ans = max(ans, node.sum);
        }

        for (int m = 0; m < 5; ++m) {
            int moves = X[m] + Y[m];
            int newMask = node.mask | (1 << moves);
            if ((1 << moves) <= node.mask) {
                continue;
            }

            if (node.x + movX[m] < 1 || node.x + movX[m] > N ||
                node.y + movY[m] < 1 || node.y + movY[m] > N) {
                continue;
            }
            int &cost = dp[node.x + movX[m]][node.y + movY[m]][newMask];
            if (!viz[node.x + movX[m]][node.y + movY[m]][newMask] ||
                node.sum + C[node.x + movX[m]][node.y + movY[m]] > cost) {
                viz[node.x + movX[m]][node.y + movY[m]][newMask] = true;
                cost = node.sum + C[node.x + movX[m]][node.y + movY[m]];
                heap.push({node.x + movX[m], node.y + movY[m], newMask, cost});
            }
        }
    }
    return ans;
}

int main() {
    ifstream in("sudest.in");
    ofstream out("sudest.out");

    in >> N >> K;
    C.resize(N + 1, vector<int>(N + 1, 0));
    for (int i = 1; i <= N; ++i) {
        for (int j = 1; j <= N; ++j) {
            in >> C[i][j];
        }
    }
    int val;
    while (K--) {
        in >> val;
        for (int i = 0; i < val; ++i) {
            in >> val;
        }
    }
    int res = compute();
    out << res << endl;
    return 0;
}
