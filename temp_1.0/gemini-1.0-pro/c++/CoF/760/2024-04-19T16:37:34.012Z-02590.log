#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream fin("telecabina.in");
  ofstream fout("telecabina.out");

  int n;
  fin >> n;

  vector<pair<int, int>> stations(n);
  for (int i = 0; i < n; i++) {
    fin >> stations[i].first >> stations[i].second;
  }

  int up = 0, down = 0;
  int special = 0;
  int total_up = 0;
  for (int i = 0; i < n - 1; i++) {
    if (stations[i].first < stations[i + 1].first) {
      if (down > 0) {
        down = 0;
        special++;
      }
      up += stations[i].second;
      total_up += stations[i].first * stations[i].second;
    } else {
      if (up > 0) {
        up = 0;
        special++;
      }
      down += up;
      up = stations[i + 1].second;
      total_up += stations[i].first * up;
    }
  }

  fout << up << '\n';
  fout << total_up * 3 + down * 1 << '\n';
  fout << special << '\n';

  fin.close();
  fout.close();

  return 0;
}
