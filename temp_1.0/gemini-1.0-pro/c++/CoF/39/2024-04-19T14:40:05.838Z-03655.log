/* Fișierul de intrare: suma.in
 * Fișierul de ieșire: suma.out
 */
#include <iostream>
#include <fstream>
#include <vector>
#include <limits>

using namespace std;
using std::vector;

const int INF = numeric_limits<int>::max();

struct Drum {
    int numar_camere;
    int cost;
    int next;
};

int n, m;
vector<int> costuri;
vector<Drum> drumuri[63366];

void CitireDate() {
    ifstream in ("suma.in");
    in >> n;
    costuri.resize(n);
    for (auto &comanda : costuri)
        in >> comanda;
    in.close();
}

void ConstruirePiramida() {
    m = 1;
    while (m * m < n)
        m++;
    int nr_cam = 1;
    int nrc = 2;
    while (nrc <= m) {
        for (int i = 1; i <= nrc; i++) {
            for (int j = 1; j <= i; j++) {
                drumuri[nr_cam].push_back({nr_cam + (nrc - j) * nrc, costuri[nr_cam -1], 0});
            }
            nr_cam++;
        }
        nrc++;
    }
}

void Floyd_Warshall() {
    for (int k = 1; k <= n; k++) {
        for (int i = 1; i <= n; i++) {
            for (auto &comanda : drumuri[i]) {
                if (k != i && comanda.numar_camere != i && comanda.cost < INF &&
                    drumuri[i][comanda.numar_camere].numar_camere == 1) {
                    if (drumuri[i][comanda.numar_camere].cost > drumuri[i][k].cost + comanda.cost) {
                        drumuri[i][comanda.numar_camere].cost = drumuri[i][k].cost + comanda.cost;
                        drumuri[i][comanda.numar_camere].next = k;
                    }
                }
            }
        }
    }
}

vector<int> ObtineDrumMinim(int s) {
    vector<int> dr;
    dr.push_back(s);
    while (drumuri[s][1].numar_camere != 1) {
        s = drumuri[s][1].next;
        dr.push_back(s);
    }
    return dr;
}

void AfisareRezultate() {
    ofstream out("suma.out");
    int smin = INF, vstart;
    for (int i = 1; i <= n; i++) {
        if (drumuri[i][1].cost < smin) {
            smin = drumuri[i][1].cost;
            vstart = i;
        }
    }
    out << m << " " << smin << endl;
    auto dr = ObtineDrumMinim(vstart);
    for (auto it : dr)
        out << it << " ";
}

int main() {
    CitireDate();
    ConstruirePiramida();
    Floyd_Warshall();
    AfisareRezultate();
    return 0;
}
