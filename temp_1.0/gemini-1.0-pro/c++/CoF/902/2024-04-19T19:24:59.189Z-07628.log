    vector<int64_t> pre(n);
    pre[0] = h[0];
    for (int64_t i = 1; i < n; i++)
        pre[i] = pre[i - 1] + h[i];
    vector<int64_t> suf(n);
    suf[n - 1] = h[n - 1];
    for (int64_t i = n - 2; i >= 0; i--)
        suf[i] = suf[i + 1] + h[i];
    vector<int64_t> ans(n);
    for (int64_t i = 0; i < n; i++) {
        int64_t left = i - 1, right = i + 1;
        while(left >= 0 && h[left] <= h[i])
            left--;
        while(right < n && h[right] <= h[i])
            right++;
        ans[i] = pre[i - 1] - (left >= 0 ? pre[left] : 0) + suf[i + 1] - (right < n ? suf[right] : 0);
    }
    return sum(ans);
}
int main() {
    int64_t n;
    in >> n;
    vector<int64_t> h(n);
    for (auto& x : h)
        in >> x;
    auto ans = solve(h, n);
    out << ans << "\n";
    h.erase(h.begin());
    ans -= (h.empty() ? 0 : h[0]); 
    for (int64_t i = 1; i < n; i++) { 
        int64_t left = i - 1, right = i + 1;
        while(left >= 0 && h[left] <= h[i])
            left--;
        while(right < n && h[right] <= h[i])
            right++;
        vector<int64_t> pre(n - 1);
        pre[0] = h[0];
        for (int64_t j = 1; j < i; j++)
            pre[j] = pre[j - 1] + h[j];
        for (int64_t j = i + 1; j < n - 1; j++)
            pre[j] = pre[j - 1] + h[j];
        vector<int64_t> suf(n - 1);
        suf[n - 2] = h[n - 2];
        for (int64_t j = n - 3; j > i; j--)
            suf[j] = suf[j + 1] + h[j];
        for (int64_t j = i; j < n - 2; j++)
            suf[j] = suf[j + 1] + h[j];
        ans += pre[i - 1] - (left >= 0 ? pre[left] : 0) + suf[i + 1] - (right < n - 1 ? suf[right] : 0);
        out << ans << "\n";
    }
    return 0;
}
