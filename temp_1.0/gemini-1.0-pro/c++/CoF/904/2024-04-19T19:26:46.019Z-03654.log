    nrmaxmine = max((lmax - 1) * (cmax - 1) - x * (cmax - 1) + minime +
                        y -
                        1,
                    nrmaxmine);
    lmax = max(lmax, x);
    lmin = min(lmin, x);
    cmax = max(cmax, y);
    cmin = min(cmin, y);
  }
  close(fisier);
  fintrare = open("deminare.out", O_WRONLY);
  if (v == 2) write(fintrare, &nrmaxmine, sizeof(nrmaxmine));
  if (v == 1) write(fintrare, &lmax, sizeof(lmax));
  close(fintrare);
  return 0;
}
