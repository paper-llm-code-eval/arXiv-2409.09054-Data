#define min(x, y) ((x) < (y) ? (x) : (y))
#define max(x, y) ((x) > (y) ? (x) : (y))
int m[501][501];
int main() {
  int fisier, fintrare, minime, nrmaxmine, v;
  int i, j, x, y, lmax, lmin, cmax, cmin;
  fisier = open("deminare.in", O_RDONLY);
  int n = read(fisier, &v, sizeof(v));
  n = read(fisier, &lmax, sizeof(lmax));
