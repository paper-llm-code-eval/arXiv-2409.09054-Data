#include <fstream>
#include <vector>

using namespace std;

typedef unsigned int uint;

ofstream fout("grad.out");
ifstream fin("grad.in");

const uint STRLNG = 32001;
vector<uint> get_perm(uint n, const uint skipPos)
{
    vector<bool> present(n, false);
    vector<uint> perm(n);
    perm[0] = STRLNG;
    present[skipPos - 1] = true;
    for (uint i = 1; i < n; ++i)
    {
        for (uint j = i; j < n; ++j)
        {
            if (!present[j])
            {
                perm[i] = j;
                present[j] = true;
                break;
            }
        }
    }
    return perm;
}

int main()
{
    uint n, k;
    fin >> n >> k;
    vector<vector<uint>> permVect(k);
    vector<uint> perm(n);
    for (permVect[0] = perm = get_perm(n, 2);;)
    {
        if(++perm[0] == n - 1)
        {
            if(--k == 0) break;
            perm = get_perm(n, 1);
        }
        permVect[k] = perm;
    }
    vector<uint> sz(n, 1);
    uint lg = 0;
    for (uint i = 1; i < n; ++i)
    {
        uint cnt = 0;
        for (uint j = 0; j < i; ++j) if (perm[j] < perm[i]) ++cnt;
        if(cnt != i)
        {
            sz[i] = sz[i - 1] + 1;
            if(sz[i] > sz[lg]) lg = i;
        }
    }
    fout << sz[lg] << '\n' << perm[lg] + 1 << ' ' << sz[lg];
    return 0;
}
