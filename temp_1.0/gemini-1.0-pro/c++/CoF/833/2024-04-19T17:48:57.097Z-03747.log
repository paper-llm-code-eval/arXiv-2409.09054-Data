int vi[maxn];
int vmax;
int frev[10];
int frecvmax;
int frecnt;
int hmin;
int t;

void ReadData()
{
    int i;
    in >> t;
    for (i = 1; i <= t; i++)
    {
        in >> vi[i];
    }
}
bool Equal(int i, int j)
{
    int a[10] = {0};
    int b[10] = {0};
    int x;
    while (i)
    {
        x = i % 10;
        a[x]++;
        i /= 10;
    }
    while (j)
    {
        x = j % 10;
        b[x]++;
        j /= 10;
    }
    int k;
    for (k = 0; k <= 9; k++)
        if (a[k] != b[k])
            return false;
    return true;
}
void BuildFreq()
{
    int aux;
    int mmax = 0;
    for (int i = 1; i <= t; i++)
    {
        frev[0] = 1;
        aux = vi[i];
        vi[i] = 0;
        while (aux)
        {
            if (aux % 10 > mmax)
                mmax = aux % 10;
            aux /= 10;
        }
        aux = vi[i];
        while (aux)
        {
            if (aux % 10 == vmax)
                frecvmax++;
            frev[aux % 10] *= (aux % 10 + 1);
            aux /= 10;
        }
    }
}
void Solve()
{
    int i;
    int temp;
    BuildFreq();
    for (i = 1; i <= t; i++)
    {
        if (Equal(vi[i], v[i]) == true)
            frecnt++;
    }
    for (i = 1; i <= t; i++)
    {
        temp = 0;
        for (int j = 1; j <= t; j++)
            if (v[j] < vi[i])
                temp += frev[vi[i] % 10];
        if (temp < hmin)
            hmin = temp;
    }
}
int main()
{
    ReadData();
    Solve();
    out << vmax << " " << frecvmax << endl;
    out << frecnt << endl;
    out << hmin << endl;
    in.close();
    out.close();
    return 0;
}
