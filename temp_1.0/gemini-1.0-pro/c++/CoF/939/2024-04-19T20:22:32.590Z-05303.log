#include <cstdio>
using namespace std;
using ll = long long;
const int MAXN = (int)4e6 + 1;
const int MOD = (int)1e9 + 7;
int v[MAXN], p2[MAXN], h[26], f[26];

ll fastpow(ll a, ll b) {
  ll ret = 1;
  while (b) {
    if (b & 1) ret = ret * a % MOD;
    a = a * a % MOD, b >>= 1;
  }
  return ret;
}

int main() {
  freopen("sdistante.in", "r", stdin);
  freopen("sdistante.out", "w", stdout);
  char c;
  while ((c = fgetc(stdin)) != '\n') v[c - 'a']++;
  p2[0] = 1;
  for (int i = 1; i < MAXN; i++) p2[i] = p2[i - 1] * 2 % MOD;
  for (int i = 0; i < 26; i++) {
    h[i] = v[i];
    f[i] = p2[v[i]];
  }
  for (int a = 0; a < 26; a++) {
    int cnt = 0;
    for (int b = a + 1; b < 26; b++) {
      cnt += h[b];
      f[a] = f[a] * fastpow(p2[cnt] * p2[h[b]] % MOD, MOD - 2) % MOD;
    }
    for (int b = a + 1; b < 26; b++) {
      h[a] += v[b], h[b] -= v[b];
    }
  }
  ll ans = 0;
  for (int i = 0; i < 26; i++) ans = (ans + f[i]) % MOD;
  printf("%lld", ans);
  return 0;
}
