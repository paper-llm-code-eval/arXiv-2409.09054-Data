#include<bits/stdc++.h>

using namespace std;

ifstream fin("partitura.in");
ofstream fout("partitura.out");

int n;
short int x[300005];
int y[300005];

short int p[300005][19];
int lg[300005];

#define pb push_back
int d[300005];

vector<int>mu[300005];

void build(){
    lg[1] = 0;
    for(int i=2;i<=n;i++)
        lg[i] = lg[i/2] + 1;
    for(int i=1;i<=n;i++)
        p[i][0] = x[i];
    for(int i=1;i<=lg[n];i++)
        for(int j=1;j<=n;j++){
            if(j + (1 << (i - 1)) - 1 <= n)
                p[j][i] = min (p[j][i - 1], p[j + (1 << (i - 1))][i - 1]);
        }
}

int cer(int st, int dr){

    if(st == dr)
        return p[st][0];

    int k = lg[dr - st + 1];
    return min(p[st][k], p[dr - (1 << k) + 1][k]);
}

void dfs(int nod, int Tata){
    d[nod] = d[Tata] + y[nod];
    mu[d[nod]].pb(nod);
    for(auto it:mu[d[nod] - 1]){
        if(cer(it + 1, nod) == x[it + 1]){
            dfs(it + 1, nod);
        }
    }
}

int main(){
    fin >> n;
    for(int i=1;i<=n;i++){
        fin >> x[i] >> y[i];
    }
    build();
    for(int i=n;i>=1;i--){
        if(x[i] == 1)
            dfs(i, 0);
    }
    long long int ans = 0;
    for(int i=1;i<=300004;i++){
        for(auto it:mu[i]){
            ans += 1LL * y[it] * y[it];
        }
    }
    fout << ans << "\n";
    return 0;
}
