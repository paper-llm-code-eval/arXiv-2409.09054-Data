        int sumt = 0;
        for (int i = 1; i <= n; i++) {
            if (!arr[i].w) continue;
            if (arr[i].w == ri) {
                dsu.unite(1, i);
                dsu.unite(i, n);
                sumt += arr[i].h;
                arr[i].w = -1;
