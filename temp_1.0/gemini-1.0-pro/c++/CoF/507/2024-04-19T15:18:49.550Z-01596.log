    for (int i = 1; i <= n; i++) {
        le = min(le, (int)arr[i].w), ri = max(ri, (int)arr[i].w);
    }
    if (le == ri) {
        ll pres[10001];
        memset(pres, 0, sizeof pres);
        for (int i = 1; i <= n; i++) ++pres[arr[i].h];
