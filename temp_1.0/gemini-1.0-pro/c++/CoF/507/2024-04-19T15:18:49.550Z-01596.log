    int get(int x) { return x == P[x] ? x : P[x] = get(P[x]); }
    void unite(int x, int y) { x = get(x), y = get(y); if (x != y) { if (Sz[x] < Sz[y]) swap(x, y); P[y] = x; Sz[x] += Sz[y]; } }
};

int n;
note arr[MAXN];
DSU dsu(MAXN);

bool cmp_time(note const& a, note const& b) { return a.w < b.w; }
bool cmp_height(note const& a, note const& b) { return a.h < b.h; }

ll calc_a(int L, int R) {
    ll pw = 2, val = 0;
    for (int he = 10000; he; he >>= 1) {
        ll sum = 0;
        for (int i = L; i <= R; i++)
            if (he <= arr[i].h) sum += arr[i].h;
        if ((val | he) > val and 1LL * pw * sum * sum > 1LL * pw * val * val) val |= he, pw <<= 1;
    }
    return val;
}

ll solve() {
