#include <fstream>
#include <vector>
#include <unordered_map>

using namespace std;

int main() {
  ifstream in("litere.in");
  ofstream out("litere.out");

  int n;
  in >> n;
  string s;
  in >> s;
  int m;
  in >> m;
  unordered_map<char, int> cod;
  for (int i = 0; i < m; i++) {
    char c;
    int k;
    in >> c >> k;
    cod[c] = k;
  }
  // numarul de perechi vocale-consoane
  int pairs = 0;
  const char vowels[] = "AEIOU";
  for (int i = 0; i < n - 1; i++) {
    int ok = 1;
    for (int j = 0; j < 5; j++) {
      if (s[i] == vowels[j]) ok = 0;
    }
    int ok2 = 1;
    for (int j = 0; j < 5; j++) {
      if (s[i + 1] == vowels[j]) ok2 = 0;
    }
    pairs += ok * ok2;
  }
  // numarul k1
  int k1 = 0;
  for (int i = 1; i <= n; i++) {
    k1 = k1 * 10 + cod[s[i - 1]];
  }
  // suma numerelor obtinute iterativ
  int sum = 0;
  for (int i = 1; i <= n; i++) {
    int nr = 0;
    for (int j = i; j <= n; j++) {
      nr = nr * 10 + cod[s[j - 1]];
    }
    sum += nr;
  }
  out << pairs << '\n';
  out << k1 << '\n';
  out << sum << '\n';
  in.close();
  out.close();
  return 0;
}
