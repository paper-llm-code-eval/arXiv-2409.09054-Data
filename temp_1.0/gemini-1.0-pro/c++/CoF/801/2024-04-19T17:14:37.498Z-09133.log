#include <fstream>

using namespace std;

ifstream fin("cladiri.in");
ofstream fout("cladiri.out");

const int MAXN = 100001;
struct Point {
    int longi, lati, rezis;
};
Point cladiri[MAXN];
int n,nivmax;
int p[11], Sol[11];

void read() {
    fin >> p[0] >> p[1] >> p[2];
    int lungimeLatitudine, lungimeLongitudine, rezistenta;
    while (fin >> lungimeLatitudine >> lungimeLongitudine >> rezistenta) {
        ++n;
        cladiri[n].longi = lungimeLongitudine;
        cladiri[n].lati = lungimeLatitudine;
        cladiri[n].rezis = rezistenta;
    }
}

void solve() {
    for (int i = 1; i <= n; ++i) {
        int distantaX = abs(cladiri[i].longi - p[0]);
        int distantaY = abs(cladiri[i].lati - p[1]);
        int distanta = max(distantaX, distantaY);
        if (distanta <= p[2])
            ++p[distanta];
    }
    for (int i = 0; i <= p[2]; ++i) {
        if (p[i] > nivmax) {
            nivmax = p[i];
        }
    }
}

void write() {
    int suma = 0;
    for (int i = 0; i <= p[2]; ++i) {
        suma += p[i];
    }
    fout << suma << endl;
    fout << nivmax << endl;
    int cnt = 0;
    for (int i = 0; i <= p[2]; ++i) {
        if (p[i] == nivmax)
            Sol[++cnt] = i;
    }
    for (int i = 1; i <= cnt; ++i) {
        fout << Sol[i] << " ";
    }
}

int main() {
    read();
    solve();
    write();
    return 0;
}
```
