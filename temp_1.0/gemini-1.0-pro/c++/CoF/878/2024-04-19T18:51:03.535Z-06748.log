        if (!lungime) {
            lungime = 2;
            poz_start = i - 1;
            continue;
        }
        ++lungime;
        if (maxim < lungime) {
            maxim = lungime;
            poz_start = i - maxim + 1;
        }
    }
    if (row == 1)
        fout << maxim << '\n';
    else {
        ull sum = 0;
        for (ull i = poz_start; i <= n; ++i) {
            if (v[i] > v[i - 1])
                sum = (sum + v[i - 1] * v[i]) % MOD;
