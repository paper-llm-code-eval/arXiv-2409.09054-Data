        ull nr_permutari = 1;
        for (ull i = 2; i <= n; ++i)
            nr_permutari = i * nr_permutari % MOD;
        nr_permutari = (nr_permutari - 1) % MOD;
        fout << sum << ' ';
        poz_start++;
        for (ull i = 0; i < n; ++i) {
            ull put = v[poz_start + i];
            poz_start = poz_start + 1;
            ull cicluri = i + 1;
            puteri = nr_permutari;
            puteri %= MOD;
            puteri = pow(puteri, MOD - 2);
            puteri %= MOD;
            put = put * puteri % MOD;
            sum = sum + put;
            sum %= MOD;
            nr_permutari = nr_permutari * (i + 1) % MOD;
            nr_permutari = (nr_permutari - 1) % MOD;
            nr_permutari %= MOD;
        }
        fout << sum % MOD << '\n';
    }
    return 0;
}
