#include <fstream>
#include <cstring>

using namespace std;

ifstream fin("maxp.in");
ofstream fout("maxp.out");

const int maxn = 200005;

int n, a[maxn], F[maxn], L[maxn], freq[maxn];

int main() {
    fin >> n;
    for(int i = 1; i <= n; i++) fin >> a[i];
    for(int i = 1; i <= n; i++) {
        F[i] = (i - 1 ? F[i - 1] : 0);
        if(i == 1 || a[i - 1] < a[i])
            F[i]++;
    }
    for(int i = n; i >= 1; i--) {
        L[i] = (i + 1 <= n ? L[i + 1] : 0);
        if(i == n || a[i + 1] < a[i])
            L[i]++;
    }
    int maxf = 0, ans, nr = 0;
    for(int i = 1; i <= n; i++) {
        int curr = F[i - 1] + L[i + 1];
        if(curr > maxf) {
            maxf = curr;
            ans = a[i];
            nr = 1;
        }
        else if(curr == maxf)
            nr++;
        freq[a[i]] = max(freq[a[i]], curr);
    }
    fout << maxf << '\n' << nr << '\n';
    for(int j, i = 1; i <= n; i++)
        if(freq[a[i]] == maxf) {
            if(i == 1 || a[i] > a[i - 1]) {
                for(j = i; j <= n; j++)
                    if(a[j] < a[i])
                        break;
                i = j - 1;
            }
            fout << a[i] << ' ';
        }
    return 0;
}
```
