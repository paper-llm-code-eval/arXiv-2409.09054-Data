uint64_t solve(uint32_t digitCount, uint32_t knownPieces, uint32_t lastPieceIsReverse) {
  if (dp[digitCount][knownPieces][lastPieceIsReverse] == -1U) {
    uint64_t ans = 0;
    if (digitCount == 0)
      return 1;
    uint32_t lastPiece = 0;
    for (uint32_t i = 0; i < (1U << maxDigitCount); ++i)
      if (pieceCount[digitCount][i]) {
        lastPiece = i;
        break;
      }

    for (uint32_t i = 1; i * 2 <= digitCount; ++i) {
      for (uint32_t j =
                0; j * 2 <= (1U << maxDigitCount); ++j) {
        if (pieceCount[digitCount - i][j] &&
            (pieceCount[digitCount - i][j] != 1 ||
             j != lastPiece)) {
          if (lastPiece != j || lastPieceIsReverse)
            ans += pieceCount[digitCount - i][j] *
                   solve(digitCount - 2 * i + 2 * j,
                        knownPieces | j, lastPieceIsReverse);
          else if (i > 1 || j != 0)
            ans += pieceCount[digitCount - i][j] *
                   solve(digitCount - 2 * i + 2 * j,
                        knownPieces | j, !lastPieceIsReverse);
        }
      }
    }
    if (digitCount > 1 || lastPiece != 0 ||
        (digitCount & 1) == 1 || lastPieceIsReverse ||
        (digitCount & 1) == 0 && lastPiece == 0)
      ans += solve(digitCount - 1, knownPieces, lastPieceIsReverse);
    dp[digitCount][knownPieces][lastPieceIsReverse] = ans;
  }
  return dp[digitCount][knownPieces][lastPieceIsReverse];
}

int main() {
  freopen("puzzle.in", "r", stdin);
  freopen("puzzle.out", "w", stdout);
  scanf("%d\n", &n);
  for (int32_t i = 0; i < n; ++i) {
    scanf("%d", &pieces[i]);
    uint32_t piece = pieces[i];
    uint32_t digitCount = 0;
    do {
      pieceCount[digitCount][piece & ((1 << maxDigitCount) - 1)]++;
      digitCount++;
      piece >>= maxDigitCount;
    } while (piece);
  }
  std::fill(dp[0][0], dp[maxDigitCount][1 << maxDigitCount] + 2, -1U);
  printf("%lu\n", solve(maxDigitCount * (maxDigitCount + 1) / 2, 0, 1));
}
