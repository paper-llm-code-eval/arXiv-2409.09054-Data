    vector<vector<int>> data = input_data();
    
    int task = data[0][0],
        tunnels = data[0][0],
        elements = data[0][1],
        entry = data[0][2];
    
    ofstream out("tunel.out");
    
    vector<int> path = { entry };
    vector<int> connections;
    
    for (int i = 1; i < tunnels; i++) {
        connections = data[i];
        if (path.back() != connections[0]) {
            path.push_back(last_element_path(path, connections));
        }
    }
    
    if (task == 1) {
        out << path.back();
    } else {
        if (path.size() > 1) {
            while (last_element_path(path, connections) == path.back()) {
                path.pop_back();
            }
        }
        out << path.size() + elements - 1;
    }
    
    out.close();
    
    return 0;
}
