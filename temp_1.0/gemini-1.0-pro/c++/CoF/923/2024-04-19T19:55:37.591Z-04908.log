```c++
#include <fstream>

using namespace std;

int main()
{
  ifstream in("foto.in");
  ofstream out("foto.out");

  int c, n, m, a, t[101][101];

  in >> c >> n >> m;
  for (int i = 1; i <= n; i++)
  {
    int k = 0;
    for (int j = 1; j <= m; j++)
    {
      in >> a;
      t[i][j] = a;
      if (a == 0) k++; else k = 0;
    }
  }

  int max = 0;
  for (int i = 1; i <= n; i++)
    if (k > max) max = k;

  out << max;

  return 0;
}
