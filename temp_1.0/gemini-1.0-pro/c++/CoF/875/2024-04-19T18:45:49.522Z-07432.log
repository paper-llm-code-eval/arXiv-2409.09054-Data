#include <bits/stdc++.h>

using namespace std;

const int maxn = 70;
const int dir[4][2] = {{1, 0}, {0, 1}, {-1, 0}, {0, -1}};
int n, m, k, s, e, f, p, dist[maxn][maxn], group[maxn], cou[maxn], st[maxn];
bool inter[maxn][maxn];
char mat[maxn][maxn];

int findGroup(int x) {
    return group[x] ? group[x] = findGroup(group[x]) : x;
}

void reuneste(int x, int y) {
    group[x] = findGroup(y);
}

void bfs() {
    queue<pair<int, int>> q;
    q.push({s, e});
    dist[s][e] = 0;
    while (!q.empty()) {
        int x = q.front().first, y = q.front().second;
        q.pop();
        for (int i = 0; i < 4; ++i) {
            int dx = x + dir[i][0], dy = y + dir[i][1];
            if (dx < 1 || dx > n || dy < 1 || dy > m || inter[dx][dy])
                continue;
            if (dist[dx][dy] > dist[x][y] + (mat[dx][dy] == '_' ? 0 : 1)) {
                dist[dx][dy] = dist[x][y] + (mat[dx][dy] == '_' ? 0 : 1);
                q.push({dx, dy});
            }
        }
    }
}

void dfs(int x, int v) {
    st[x] = true;
    cou[v]++;
    for (int i = 0; i < 4; ++i) {
        int dx = x + dir[i][0], dy = x + dir[i][1];
        if (dx < 1 || dx > n || dy < 1 || dy > m || inter[dx][dy])
            continue;
        if ((mat[x][y] != mat[dx][dy]) || st[dx][dy])
            continue;
        dfs(dx, dy);
    }
}

void count(int s, int e) {
    dfs(s, e);
    for (int i = 0; i < 4; ++i) {
        int dx = s + dir[i][0], dy = s + dir[i][1];
        if (dx < 1 || dx > n || dy < 1 || dy > m || inter[dx][dy])
            continue;
        if ((mat[s][e] != mat[dx][dy]) || cou[findGroup(dx)][dy])
            continue;
        count(dx, dy);
    }
}

int main() {
    freopen("miting.in", "r", stdin);
    freopen("miting.out", "w", stdout);

    cin >> p;
    cin >> n >> m;
    cin >> mat[0];
    for (int i = 1; i <= n; ++i) {
        cin >> mat[i];
    }

    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= m; ++j) {
            if (mat[i][j] == '#')
                inter[i][j] = true;
            else if (mat[i][j] >= 'A' && mat[i][j] <= 'Z') {
                k++;
                f++;
                s = i;
                e = j;
                st[i][j] = true;
            }
        }
    }

    bfs();

    if (k == 1) {
        cout << 1 << '\n';
        return 0;
    }

    if (p == 1) {
        int sol = 0;
        for (int i = 1; i <= n; ++i) {
            for (int j = 1; j <= m; ++j) {
                if (!inter[i][j])
                    sol++;
            }
        }
        cout << sol << '\n';
        return 0;
    }

    f = 0;
    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= m; ++j) {
            if (mat[i][j] >= 'A' && mat[i][j] <= 'Z') {
                f++;
            }
        }
    }

    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= m; ++j) {
            if (!st[i][j] && mat[i][j] >= 'A' && mat[i][j] <= 'Z') {
                count(i, j);
            }
        }
    }

    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= m; ++j) {
            if (mat[i][j] == '_') {
                continue;
            }
            int g = findGroup(mat[i][j]);
            if (!cou[g])
                cou[g] = -1;
        }
    }

    int sol = 0;
    for (int i = 1; i <= k; ++i) {
        if (cou[i] >= 0) {
            sol += cou[i] - 1;
        }
    }
    cout << (sol != -1 ? sol : -1) << '\n';

    return 0;
}
