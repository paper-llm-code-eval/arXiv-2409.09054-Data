			masc |= (1 << (grid[i][j] ^ grid[start / 100][start % 100]));
		masc = (masc >> (tlg + 1)) & DIFMASK;
		for(int j = 1; j <= m; ++j)
		{
			int dif = 0;
			dif |= (1 << (grid[i][j - 1] ^ grid[i][j]));
			dif |= (1 << (grid[i + 1][j] ^ grid[i][j]));
			dif |= (1 << (grid[i][j + 1] ^ grid[i][j]));
			dif |= (1 << (grid[i - 1][j] ^ grid[i][j]));
			if((dif & (1 << masc)) == 0)
				grid[i][j] = grid[i][j] ^ 1;
			out << char(grid[i][j] + 'a' - 1);
		}
		out << '\n';
	}
	out.close();
	return 0;
}
