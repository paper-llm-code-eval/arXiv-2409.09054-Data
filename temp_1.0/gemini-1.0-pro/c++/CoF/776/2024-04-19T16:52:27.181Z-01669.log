						dif |= (1 << (grid[x + 1][y] ^ k));
						dif |= (1 << (grid[x][y + 1] ^ k));
						dif |= (1 << (grid[x - 1][y] ^ k));
						int modif = 0;
						for(int p = 0; dif; ++p)
							if((dif & 1) == 1)
								modif = p;
							dif >>= 1;
						mapa_nou[j ^ ((modif >> tlg) & 1)][k].push_back(x * 100 + y);
					}
				}
		for(int i = 0; i < 2; ++i)
			for(int j = 0; j < 2; ++j)
			{
				mapa[i][j].clear();
				swap(mapa[i][j], mapa_nou[i][j]);
			}
		--t;
	}
	ofstream out("culori.out");
	for(int i = 1; i <= n; ++i)
	{
		int start = mapa[0][0][i - 1].back();
		int masc = 0;
