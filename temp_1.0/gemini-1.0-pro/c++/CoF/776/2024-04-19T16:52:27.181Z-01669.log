			int modif = 0;
			for(int k = 0; dif; ++k)
				if((dif & 1) == 1)
					modif = k;
				dif >>= 1;
			mapa[grid[i][j]][modif].push_back(i * 100 + j);
		}
	int tlg = t & 31;
	t >>= 5;
	while(t)
	{
		vector <int> mapa_nou[2][2];
		for(int i = 0; i < 2; ++i)
			for(int j = 0; j < 2; ++j)
