  double ans = 0, v = 0;
  for (int i = 0; i < m - 1; i++) {
    const Point s = hull[i], e = hull[i + 1];
    int dx = max(max(0, e.x - s.x), s.x - e.x);
    int dy = max(max(0, e.y - s.y), s.y - e.y);
    ans += max(dx, dy);
    Point z(s.x, s.y - e.y), w(e.x, e.y - s.y);
    v += labs(z % w);
  }
  v /= 2;
  ans += v;
  fout << fixed << setprecision(4) << ans << '\n';
  return 0;
}
