    return Point(x + p.x, y + p.y);
  }
  Point operator - (const Point& p) const {
    return Point(x - p.x, y - p.y);
  }
  int operator * (const Point& p) const {
    return x * p.x + y * p.y;
  }
  int operator % (const Point& p) const {
    return x * p.y - y * p.x;
  }
};

int ConvexHull(Point *a, int n) {
  int p;
  std::sort(a, a + n, [](const Point& a, const Point& b) {return a.x < b.x || a.x == b.x && a.y < b.y; });
  Point *c = new Point[n], *e = c;
  for (int i = 0; i < n; i++) {
    while (e >= c + 2 && ((e[-1] - e[-2]) % (a[i] - e[-2])) >= 0) e--;
    *e++ = a[i];
  }
  for (int i = e - c - 1, j = e - c; i >= 0; i--) {
    while (e >= c + 2 && ((e[-1] - e[-2]) % (a[i] - e[-2])) >= 0) e--;
    *e++ = a[i];
  }
  p = e - c;
  delete[] c;
  return p;
}

int main() {
  int n;
  fin >> n;
  vector<Point> pts(n);
  vector<int> ds(n);
  for (auto&[pt, d] : ranges::zip(pts, ds)) {
    fin >> pt.x >> pt.y >> d;
  }
  int m = ConvexHull(&pts[0], n);
  vector<Point> hull(m);
  for (int i = 0; i < m; i++) {
    hull[i] = pts[i];
  }
  double ans = 0, v = 0;
  for (int i = 0; i < m - 1; i++) {
    const Point s = hull[i], e = hull[i + 1];
    int dx = max(max(0, e.x - s.x), s.x - e.x);
    int dy = max(max(0, e.y - s.y), s.y - e.y);
    ans += max(dx, dy);
    Point z(s.x, s.y - e.y), w(e.x, e.y - s.y);
    v += labs(z % w);
  }
  v /= 2;
  ans += v;
  fout << fixed << setprecision(4) << ans << '\n';
  return 0;
}
