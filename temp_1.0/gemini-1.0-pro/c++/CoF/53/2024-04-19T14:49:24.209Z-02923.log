    return Point(x + p.x, y + p.y);
  }
  Point operator - (const Point& p) const {
    return Point(x - p.x, y - p.y);
  }
  int operator * (const Point& p) const {
    return x * p.x + y * p.y;
  }
  int operator % (const Point& p) const {
    return x * p.y - y * p.x;
  }
};

int ConvexHull(Point *a, int n) {
  int p;
  std::sort(a, a + n, [](const Point& a, const Point& b) {return a.x < b.x || a.x == b.x && a.y < b.y; });
  Point *c = new Point[n], *e = c;
  for (int i = 0; i < n; i++) {
    while (e >= c + 2 && ((e[-1] - e[-2]) % (a[i] - e[-2])) >= 0) e--;
    *e++ = a[i];
  }
  for (int i = e - c - 1, j = e - c; i >= 0; i--) {
    while (e >= c + 2 && ((e[-1] - e[-2]) % (a[i] - e[-2])) >= 0) e--;
    *e++ = a[i];
  }
  p = e - c;
  delete[] c;
  return p;
}

int main() {
  int n;
  fin >> n;
  vector<Point> pts(n);
  vector<int> ds(n);
  for (auto&[pt, d] : zip(pts, ds)) {
    fin >> pt.x >> pt.y >> d;
  }
  int m = ConvexHull(&pts[0], n);
  vector<Point> hull(m);
  for (int i = 0; i < m; i++) {
