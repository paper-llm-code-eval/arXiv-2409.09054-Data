#include <fstream>

using namespace std;
#define ll long long
const ll mod = 1e9ll;
ifstream f("expozitie.in");
ofstream g("expozitie.out");

ll n, d, k, invk;
ll dp[505][505][505];
ll cum[505];

ll powm(ll x, ll p, ll mod) {
    if (!p)
        return 1;
    if (p & 1)
        return ((x % mod) * (powm(x, p - 1, mod) % mod)) % mod;
    return powm((x % mod) * (x % mod), p / 2, mod);
}

void pre() {
    invk = powm(k, mod - 2, mod);
}

int main() {
    f >> n >> d >> k >> invk;
    dp[0][0][0] = 1;
    for (int i = 1; i <= d; ++i)
        dp[0][i][0] = 1;
    for (int m = 1; m <= n; ++m) {
        for (int i = 1; i <= d; ++i) {
            for (int j = 0; j <= i * k; ++j) {
                cum[i] += dp[m - 1][i - 1][j];
                cum[i] %= mod;
                dp[m][i][j + 1] = (dp[m][i][j + 1] + cum[i]) % mod;
                dp[m][i][j] =
                    (dp[m][i][j] + dp[m - 1][i][j] * (d - i + 1) % mod) % mod;
                dp[m][i][j] =
                    (dp[m][i][j] + dp[m - 1][i - 1][j] * (i * invk) % mod) % mod;
            }
        }
    }
    g << dp[n][d][k] % mod << endl;
    f.close();
    g.close();
    return 0;
}
