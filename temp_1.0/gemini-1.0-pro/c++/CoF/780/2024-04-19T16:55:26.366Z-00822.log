#include <map>
#include <fstream>
#include <algorithm>

using namespace std;

ifstream in("colaj.in");
ofstream out("colaj.out");

const int MAXN = 100;
const int HMAX = 8000;

int n, m, p, L[HMAX], C[HMAX], nnL, nnC;
struct punct { int x, y; };

punct dr[MAXN * MAXN + 1];
bool dr_c[MAXN * MAXN + 1];

punct a, b;

map <int, map<int, int>> zona;

void add(int bl) {
  if(dr[bl].x == a.x && dr[bl].y == a.y)
    return;

  if(zona[a.x][a.y] == 0)
    nnL++, nnC++;

  zona[a.x][a.y]++;
  a = dr[bl];
}

int main() {
  in >> n >> m >> p;

  for(int i = 1; i <= n; i++) {
    in >> a.x >> a.y >> b.x >> b.y;

    for(int x = a.x; x <= b.x; x++)
      L[x] = 1;
    for(int y = a.y; y <= b.y; y++)
      C[y] = 1;
  }

  for(int i = 1; i <= m; i++)
    if(L[i])
      L[++nnL] = i;
  for(int i = 1; i <= p; i++)
    if(C[i])
      C[++nnC] = i;

  for(int i = 1; i <= n; i++) {
    in >> a.x >> a.y >> b.x >> b.y;

    a.x = lower_bound(L + 1, L + nnL + 1, a.x) - L;
    a.y = lower_bound(C + 1, C + nnC + 1, a.y) - C;
    b.x = lower_bound(L + 1, L + nnL + 1, b.x) - L;
    b.y = lower_bound(C + 1, C + nnC + 1, b.y) - C;

    dr[++nnL * nnC] = b;
    dr_c[nnL * nnC] = 1;

    dr[nnL * nnC - 1] = {a.x - 1, a.y - 1};
    dr_c[nnL * nnC - 1] = 0;

    dr[nnL * nnC - 2] = {b.x, a.y - 1};
    dr_c[nnL * nnC - 2] = 0;

    dr[nnL * nnC - 3] = {a.x - 1, b.y};
    dr_c[nnL * nnC - 3] = 0;
  }

  sort(dr + 1, dr + nnL * nnC + 1, [] (const punct &a, const punct &b) {
    return make_pair(a.x, a.y) < make_pair(b.x, b.y);
  });

  a.x = a.y = 0;

  for(int i = 1; i <= nnL * nnC; i++) {
    if(dr[i].x != a.x || dr[i].y != a.y) {
      while(zona[a.x][a.y]) {
        zona[a.x][a.y]--;
        a = dr[zone[a.x][a.y] + 1];
      }

      add(i);
    }

    if(!dr_c[i])
      a = { dr[i].x, dr[i].y + 1 };
  }

  while(zona[a.x][a.y]) {
    zona[a.x][a.y]--;
    a = dr[zone[a.x][a.y] + 1];
  }

  out << nnL * nnC - zona.size() << "\n";

  return 0;
}
