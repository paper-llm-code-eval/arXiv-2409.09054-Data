#include <fstream>
#include <unordered_map>
#include <vector>
using namespace std;

ifstream fin("cuvinte.in");
ofstream fout("cuvinte.out");

class Solution{
    const int MAX_WORDS=25, MAX_LENGTH=10+1;
    const char DELIM=' ', STOP='!';

    bool changed;
    int m, deleted=-1;
    vector<string> words;
    unordered_map<string,int> words_order;

    void readWords();
    string transform(int i);
    void findAndDelete();

public:
    void solve();
