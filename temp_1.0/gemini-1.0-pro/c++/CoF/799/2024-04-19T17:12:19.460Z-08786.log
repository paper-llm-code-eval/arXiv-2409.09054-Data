                words_order[w]=cnt;
                cnt++;
            }
            w.clear();
            if(c==STOP) break;
        }
    }
}

string Solution::transform(int i){
    string r(words[i]);
    char c=r[0];
    for(int k=1; k<r.size(); ++k){
        r[k-1]=r[k];
    }
    r.back()=c;
    return r;
}

void Solution::findAndDelete(){
    int i=0;

    while(i<words.size()-1 && deleted==-1){
        changed=false;
        if(words_order[ transform(i) ] == i+1){
            deleted=i+1;
        }else if(words_order[ words[i+1] ]<i+1){
            words.erase(words.begin()+i+1);
            changed=true;
        }else{
            i++;
        }
    }
}

