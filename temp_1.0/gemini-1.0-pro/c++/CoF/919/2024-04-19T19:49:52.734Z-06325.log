#include <fstream>
#include <vector>

using namespace std;

const int MAXN = 500;

struct Cartonase {
    int stanga, dreapta;
};

int n, c, ans;
vector<Cartonase> cart;
bool viz[MAXN + 1];

int SePotrivesc(int a, int b) {
    return cart[a].dreapta == cart[b].stanga;
}

void Backtracking(int l, int cnt) {
    if (viz[l]) {
        if (cnt > ans) ans = cnt;
        return;
    }
    viz[l] = true;
    for (int r = l + 1; r <= n; r++) {
        if (SePotrivesc(l, r)) Backtracking(r, cnt + 1);
    }
    viz[l] = false;
}

int main() {
    ifstream in("cartonase.in");
    ofstream out("cartonase.out");

    in >> c >> n;
    for (int i = 1; i <= n; i++) {
        Cartonase c;
        in >> c.stanga >> c.dreapta;
        cart.push_back(c);
    }

    int nrPerechi = 0;
    for (int i = 1; i < n; i++) nrPerechi += SePotrivesc(i, i + 1);

    if (c == 1) out << nrPerechi;
    else {
        ans = 0;
        for (int i = 1; i <= n; i++) Backtracking(i, 0);
        out << ((c == 2) ? ans : nrPerechi - ans + 1);
    }

    return 0;
}
