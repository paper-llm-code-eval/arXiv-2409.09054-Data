bool queryValidation(int n) {
  return 0 < n && n <= 100000;
}

bool liquidVolumeValidation(int liquidVolume) {
  return liquidVolume % 2 == 0 && 1 < liquidVolume && liquidVolume <= 100000;
}

bool liquidColorValidation(int liquidColor, int n) {
  return 0 < liquidColor && liquidColor <= n;
}

using read = std::istream & (*)(std::istream &);
bool lineReadValidation(read r, int lineNr, std::istream & f, const std::string & msg) {
  assert(0 < lineNr and lineNr < 4);
  if (lineNr == 1) {
    int n;
    return r(f) >> n && queryValidation(n);
  }
  if (lineNr == 2) {
    for (int i = 0; i < n; ++i) {
      int liquidVolume;
      char direction;
      if (!(r(f) >> liquidVolume >> direction && liquidVolumeValidation(liquidVolume) &&
            (direction == 'D' || direction == 'S'))) {
        return false;
      }
    }
  }
  if (lineNr == 3) {
    int liquidColor;
    return r(f) >> liquidColor >> liquidColorValidation(liquidColor, n);
  }
  std::cerr << msg << std::endl;
  return false;
}

bool inputFileValidation(std::istream & f, const std::string & fileName) {
  std::istream_iterator<std::string> eos;
  std::istream_iterator<std::string> iit(f);
  if (std::distance(iit, eos) != 4) {
    std::cerr << "Invalid number of lines in the input file " << fileName << '\n';
    return false;
  }
  bool queryValidationStatus = lineReadValidation(std::istream_iterator<std::string>(*iit++), 1, f, fileName + " - line 1");
  bool liquidsValidationStatus = lineReadValidation(std::istream_iterator<std::string>(*iit++), 2, f, fileName + " - line 2");
  bool liquidColorValidationStatus = lineReadValidation(std::istream_iterator<std::string>(*iit++), 3, f, fileName + " - line 3");
  return queryValidationStatus && liquidsValidationStatus && liquidColorValidationStatus;
}

bool outputStreamValidation(std::string result, std::string expected) {
  return result == expected;
}

}  // namespace validate

using write = std::ostream & (*)(std::ostream &);
auto printResult = [](write w, std::ostream & os, std::string resultLine, int height,
                     int gradLeft, char sideLeft, int gradRight, char sideRight) {
  os << resultLine << std::endl;
  switch (sideLeft) {
    case 'D':
      os << gradRight + height << ' D' << std::endl;
      os << gradLeft - height << ' S' << std::endl;
      return;
    case 'S':
      os << gradLeft + height << ' S' << std::endl;
      os << gradRight - height << ' D' << std::endl;
      return;
  }
