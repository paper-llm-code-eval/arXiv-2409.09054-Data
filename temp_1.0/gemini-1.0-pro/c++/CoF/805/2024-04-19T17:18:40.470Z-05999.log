  inputFile >> color;
  outputFile << maximHeight << std::endl;
  int index = n;
  int stHeight = 0;
  while (stHeight + liquidsHeights[index] != maximHeight) {
    stHeight += liquidsHeights[index];
    --index;
  }
  outputFile << maximHeight - stHeight << ' ';
  outputFile << (liquidsHeights[index] == -height ? 'S' : 'D') << std::endl;
  return 0;
}
