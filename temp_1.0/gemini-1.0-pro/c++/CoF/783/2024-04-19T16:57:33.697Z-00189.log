    int x;
    while (1) {
      fin >> x;
      if (!x) {
        break;
      }
      bool ok = false;
      if (x < 0) {
        x /= -1;
        for (int j = i - x + 1; j <= i; j++) {
          if (v[j].size() <= m - x || abs(v[j][m - x]) == x) ok = true;
        }
      } else if (v[i].size() <= m - x || abs(v[i][m - x]) == x) ok = true;
      if (ok) {
        if (x > 0)
          for (int j = 0; j < x; j++) v[i].push_back(x);
        else
          for (int j = 0; j < x; j++) { v[i].push_back(x); v[i - j].push_back(x); }
      }
    }
  }
  fin.close();
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) fout << abs(v[i][j]) << ' ';
    fout << '\n';
  }
  fout.close();
  return 0;
}
