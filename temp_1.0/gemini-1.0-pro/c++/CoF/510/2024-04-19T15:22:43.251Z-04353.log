// fisier castel.cpp

#include<fstream>
using namespace std;
ifstream f("castel.in");
ofstream g("castel.out");

struct cub
{int nr,st;};

cub v[5010];
int n,s=0, nr1=0, st, nr, j;

int main()
{
    f>>n;
    for (int i=1;i<=n;++i)
    {
        f>>v[i].nr;
        if (v[i].nr<10) nr1++;
    }
    for (int i=1; i<=n; ++i)
    {
        j++;
        st=i;
        do
        {
            if (v[i].st==0) {v[i].st=j; nr++;}
            if (i==j && v[i].st==j) s+=v[i].nr;
             i=v[i].nr;
        } while (i!=j);
    }
    j--;
    g<<nr1<<endl<<j<<" "<<s;
    f.close();
    g.close();
    return 0;
}
```
