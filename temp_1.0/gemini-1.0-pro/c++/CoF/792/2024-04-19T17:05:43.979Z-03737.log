using namespace std;

void tokenize(const string& str, vector<string>& tokens) {
  tokens.clear();
  string token;
  for (char c : str) {
    if (c == '(' || c == ')' || c == ' ' || c == '\n') {
      if (!token.empty()) tokens.push_back(token);
      token.clear();
      if (c == '(' || c == ')') tokens.push_back(string(1, c));
