  vector<string> tokens;
  tokenize(str, tokens);

  map<string, int> ingredients;
  int total_time = 0;
  vector<pair<int, string>> ingredients_vec;

  bool in_parenthesis = false;
  int time = 0;
  for (const string& token : tokens) {
    if (token == "(") {
