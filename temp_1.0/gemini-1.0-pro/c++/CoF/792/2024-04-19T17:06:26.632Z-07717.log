      stiva_timp.push(timp);
      ss >> ingredient;
    } else { // ingredient
      ss >> cantitate;
      ingrediente[ingredient] += cantitate;
    }
  }

  int total_timp = stiva_timp.top();

  out << total_timp << endl;
  for (auto [ingredient, cantitate] : ingrediente)
    out << ingredient << " " << cantitate << endl;
  return 0;
}
