#include <vector>

using namespace std;
using vi = vector<int64_t>;

namespace io {
    ifstream fin("strips.in");
    ofstream fout("strips.out");

    template<typename T>
    inline T read(T &x) { return fin >> x, x; }

    template<typename T>
    inline void write(T const &x) { fout << x << ' '; }

    template<typename T>
    inline void write_line(T const &x) { fout << x << '\n'; }
}  // namespace io

namespace solver {
    void run() {
        using namespace io;
        const int64_t penalty_points = 1;
        vi belts(2), points(2), length(3);
        read(length[0]), read(belts[0]), read(length[1]);
        length[2] = length[0] - length[1];

        vi pos[2], colored[2];
        for (int64_t player = 0; player < 2; ++player)
            pos[player].reserve(belts[player]);
        for (int64_t i = 0; i < belts[0]; ++i)
            pos[0].push_back(read<int64_t>());
        for (int64_t i = 0; i < belts[1]; ++i)
            pos[1].push_back(read<int64_t>());
        sort(pos[0].begin(), pos[0].end());
        sort(pos[1].begin(), pos[1].end());

        for (int64_t player = 0; player < 2; ++player) {
            colored[player].reserve(belts[player]);
            int64_t backward_pos = 0, cur_len = 0;
            for (const auto &p: pos[player]) {
                int64_t cur_pos = backward_pos + cur_len, n_len = min(p - cur_pos, length[2]);
                if (n_len < 0)
                    ++points[player], --belts[player], continue;
                else if (n_len > 0) {
                    cur_len = n_len + length[player];
                    while (length[player] <= cur_len)
                        colored[player].push_back(backward_pos++), cur_len -= length[player];
                }
            }
            while (length[player] <= cur_len)
                colored[player].push_back(backward_pos++), cur_len -= length[player];
        }
        write(points[0]), write_line(points[1]);
        write(colored[0].empty() ? 0ll : colored[0].back() + length[player]),
                write_line(colored[1].empty() ? 0ll : colored[1].back() + length[player]);
    }
}  // namespace solver

int main() {
    solver::run();
    return 0;
}
