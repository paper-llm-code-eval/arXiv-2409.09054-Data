            cc++;
         }
         if (x[1].v[i]) {
            pc++;
         }
         if (!x[0].v[i] && !x[1].v[i]) {
            if (cc > ma) {
               ma = cc;
            }
            if (pc > mb) {
               mb = pc;
            }
            cc = pc = 0;
         }
      }
      if (cc > ma) {
         ma = cc;
      }
      if (pc > mb) {
         mb = pc;
      }
      fout << ma << " " << mb << endl;
   }
   return 0;
}
