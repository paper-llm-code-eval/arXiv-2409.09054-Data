   for (int i = k - 1; i >= 0; i--) {
      if (n - (a[i] + l - 1) % l) {
         pb[i] = 0;
      } else {
         pb[i] = pb[i + 1] + n - (a[i] + l - 1) % l;
      }
   }
   for (int i = 0; i < k; i++) {
      if (!pa[i]) {
         sa.push_back(a[i]);
      }
      if (!pb[i + 1]) {
         sb.push_back(a[i] + l);
