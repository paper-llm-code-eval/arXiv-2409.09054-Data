#include <iostream>
#include <vector>
using namespace std;

const int MAX_N = 31;
ifstream fin("forus.in");
ofstream fout("forus.out");

int N, C, ans[MAX_N], num;
bool cant_cut[MAX_N];
vector<int> v;

void read_data() {
  fin >> C;
  fin >> N;
