        *jt=g[x];
        jt=std::lower_bound(minsec.rbegin()->rbegin(),minsec.rbegin()->rend(),*it->first);
        *jt=*it->first;
      }
    }
    else
      if(hist[g[x]].empty()){
        hist[g[x]].emplace_back(x);
        auto jt=std::lower_bound(minpic.rbegin()->rbegin(),minpic.rbegin()->rend(),g[x]);
        *jt=g[x];
        jt=std::lower_bound(minsec.rbegin()->rbegin(),minsec.rbegin()->rend(),*it->first);
        *jt=*it->first;
      }
  }
  for(int y:g){
    if(y<g[x])
      for(int z:hist[y])
        if(x!=z && z!=-x && z!=x){
