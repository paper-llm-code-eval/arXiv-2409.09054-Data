#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

struct levelInfo {
    int levelNumber;
    int nodesOnLevel;
    long long sumOfValuesOnLevel;
};

int main()
{
    ifstream in("pic.in");
    ofstream out("pic.out");

    int request;
    in >> request;
    int lastLine;
    in >> lastLine;

    vector<int> capacities(lastLine * (lastLine + 1) / 2, 0);
    for (int i = 0; i < capacities.size(); i++) {
        in >> capacities[i];
    }
    in.close();

    // create a reversed copy of the capacities vector
    int numberOfNodesInReversedVector = capacities.size();
    vector<int> reversedCapacities(numberOfNodesInReversedVector);
    for (int i = 0; i < capacities.size(); i++) {
        reversedCapacities[numberOfNodesInReversedVector - 1 - i] = capacities[i];
    }

    // calculate the cumulative sum of values for reversed vector
    vector<long long> reversedCumulativeSum(numberOfNodesInReversedVector, 0);
    reversedCumulativeSum[0] = reversedCapacities[0];
    for (int i = 1; i < numberOfNodesInReversedVector; i++) {
        reversedCumulativeSum[i] = reversedCapacities[i] + reversedCumulativeSum[i - 1];
    }

    long long minSumOnLevel = LLONG_MAX;
    int levelWithMinSum;
    int numberOfSecondsElapsed;
    long long spilledMilliliters = 0;

    levelInfo lastLevelInfo{-1, lastLine, 0};
    for (int depth = 0; depth < lastLine; depth++) {
        levelInfo currentLevelInfo;
        currentLevelInfo.levelNumber = depth + 1;
        currentLevelInfo.nodesOnLevel = lastLine - depth;

        if (depth != 0) {
            // copy values from previous level
            currentLevelInfo.sumOfValuesOnLevel = lastLevelInfo.sumOfValuesOnLevel;
        }

        // add cumuls from previous reversed vector
        long long addedCapacity = !depth ? 0 : reversedCumulativeSum[depth - 1];
        currentLevelInfo.sumOfValuesOnLevel += addedCapacity;

        // track level with max sum of values
        if (!depth || currentLevelInfo.sumOfValuesOnLevel > minSumOnLevel) {
            minSumOnLevel = currentLevelInfo.sumOfValuesOnLevel;
            levelWithMinSum = currentLevelInfo.levelNumber;
        }

        // iterate over the stream of filtered cumulative values of capacities from the beginning of the vector
        long long sumOfFilteredStream = 0;
        long long previousStream = 0;
        for (long long i = (numberOfNodesInReversedVector - 1) - depth;
             i >= 0;
             i -= currentLevelInfo.nodesOnLevel) {
            long long currentStream = reversedCumulativeSum[i];
            sumOfFilteredStream += currentStream;

            // increase the time counter when the filtered stream is smaller than the previous one
            // and smaller than the sum on the current level
            if (previousStream > currentStream &&
                currentStream < currentLevelInfo.sumOfValuesOnLevel) {
                long long difference = (previousStream - currentStream) / currentLevelInfo.nodesOnLevel;
                numberOfSecondsElapsed += difference;
            }

            // count spilled milliliters when needed
            if (sumOfFilteredStream > currentLevelInfo.sumOfValuesOnLevel) {
                long long spill = (sumOfFilteredStream - currentLevelInfo.sumOfValuesOnLevel) / currentLevelInfo.nodesOnLevel;
                spilledMilliliters += spill;
                sumOfFilteredStream -= spill * currentLevelInfo.nodesOnLevel;
            }

            previousStream = reversedCumulativeSum[i];
        }

        lastLevelInfo = currentLevelInfo;
    }

    // return coresponding values based on request
    if (request == 1) {
        out << levelWithMinSum;
    } else if (request == 2) {
        out << numberOfSecondsElapsed << " " << spilledMilliliters;
    }

    out.close();
    return 0;
}
