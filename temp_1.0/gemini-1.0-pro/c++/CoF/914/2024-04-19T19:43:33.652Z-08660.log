#include <fstream>
#include <algorithm>
#include <vector>
#include <array>

using int64 = long long;

int main() {
    std::ifstream fin("pif.in");
    std::ofstream fout("pif.out");
    static constexpr int64 mod = 1234567;
    static constexpr int64 half = mod/2;

    int64 n, k, zv, zt;
    fin >> n >> k >> zv >> zt;

    auto solve = [&](int64 v, int64 d) {
        int64 num = std::min(n/d, k/2);
        int64 FBun = num * d * k;
        int64 rem = (n - num * d) % d;
        return (FBun % mod + (rem * k) % mod + half) % mod;
    };

    int64 FB = ((solve(zv, zt) % mod + solve(zt, zv) % mod) % mod + half) % mod;

    fout << FB << '\n';
    return 0;
}
```
