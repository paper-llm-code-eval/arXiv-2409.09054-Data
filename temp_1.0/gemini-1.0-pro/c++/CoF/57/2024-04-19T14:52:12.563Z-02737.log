  for (int i = 1; i <= n; ++i) {
    for (auto &j : G[i]) {
      if (Find(i, g) != Find(j.first, g)) {
        g << i << ' ' << j.first << '\n';
        --sol;
        if (!sol)
          break;
      }
    }
    if (!sol)
      break;
  }
  return 0;
}
