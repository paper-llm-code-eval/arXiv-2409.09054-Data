#include <fstream>
#include <iostream>
#include <unordered_map>
#include <utility>
#include <vector>
#define max(a, b) ((a) > (b) ? (a) : (b))

using namespace std;

int n, m, k, sol, g[256], size[256], maxprio;
unordered_map<int, unordered_map<int, int>> G;

int Find(int u, int *g) {
  if (g[u] == 0 || g[u] == u)
    return u;
  return g[u] = Find(g[u], g);
}

void Union(int u, int v, int *g) {
  int x = Find(u, g);
  int y = Find(v, g);
  if (x != y) {
    g[x] = y;
    size[y] += size[x];
  }
}

int dfs(vector<int> &u, int p) {
  for (auto &v : G[p]) {
    if (v.first == u.back())
      continue;
    Union(p, v.first, g);
    if (Find(p, g) != p || size[p] > size[u.back()]) {
      u.push_back(p);
      int t = dfs(u, v.first);
      if (t > 0)
        return t;
      u.pop_back();
      Union(p, v.first, size);
    }
  }
  return 0;
}

int main() {
  ifstream f("urgenta.in");
  ofstream g("urgenta.out");
  f >> n >> m >> k;
  while (m--) {
    int u, v, p;
    f >> u >> v >> p;
    G[u][v] = p;
    G[v][u] = p;
    maxprio = max(maxprio, p);
  }
  vector<int> u(1, 1);
  dfs(u, 1);
  for (int x = 1, i = 1; i <= k; ++x) {
    while (x != g[x])
      x = g[x];
    if (x == i) {
      ++i;
      if (size[x] > sol) {
        sol = size[x];
        memcpy(g, size, sizeof g);
      }
    }
  }
  g << sol << '\n';
  for (int i = 1; i <= n; ++i) {
    for (auto &j : G[i]) {
      if (Find(i, g) != Find(j.first, g)) {
        g << i << ' ' << j.first << '\n';
        --sol;
        if (!sol)
          break;
      }
    }
    if (!sol)
      break;
  }
  return 0;
}
