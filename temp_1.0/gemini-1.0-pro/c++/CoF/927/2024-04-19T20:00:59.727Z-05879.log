#include <iostream>
#include <fstream>
#include <variant>
#include <cmath>
#include <vector>
#include <algorithm>

using namespace std;

struct Query
{
	int v, i, l, r, op;
	bool operator<(const Query &other) const
	{
		return v < other.v;
	}
};
using VQuery = vector<Query>;
using VInt = vector<int>;

pair<int, int> get_first_last_horiz_days(int v, int z)
{
	int h = (z + 180 - v) / 360;
	int last = 360 * h;
	h--;
	int first = 360 * h + (360 - v);
	return make_pair(first, last);
}

int main()
{
	ifstream in("alinieri.in");
	ofstream out("alinieri.out");
	int n, p, z;
	in >> n >> p >> z;
	VInt v(n);
	for (auto &it : v)
	{
		in >> it;
	}
	VQuery q;
	for (int i = 0; i < n; i++)
	{
		int l, r;
		tie(l, r) = get_first_last_horiz_days(v[i], z);
		q.push_back({v[i], i, l + v[i], r, 1});
		q.push_back({v[i], i, l, r, -1});
	}

	sort(q.begin(), q.end());

	int count = 0;
	int last_changed = -1;
	int ans = 0;
	for (const auto &[v, i, l, r, op] : q)
	{
		count += op;
		if (count >= p)
		{
			if (last_changed != i)
			{
				last_changed = i;
				ans++;
			}
			if (r % 360 > l)
			{
				last_changed = -1;
			}
		}
	}
	out << ans << "\n";
	return 0;
}
