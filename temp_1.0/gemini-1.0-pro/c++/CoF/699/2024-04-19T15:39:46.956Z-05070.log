#include <fstream>
#include <iostream>
#include <cstring>
#include <vector>
#include <queue>

using namespace std;

ifstream fin("poarta.in");
ofstream fout("poarta.out");

const int maxn = 251;
const int INF = 0x3f3f3f3f;

int n, D[maxn][maxn], st[maxn][maxn];
vector<pair<int, int>> pos[maxn][maxn];

int BFS(const pair<int, int>& start, const vector<pair<int, int>>& end) {
    memset(D, INF, sizeof D);
    queue<pair<int, int>> q;
    q.push(start);
    D[start.first][start.second] = 0;
    while(!q.empty()) {
        int x = q.front().first;
        int y = q.front().second;
        q.pop();
        if(x < 1 || x > n || y < 1 || y > n) continue;
        for(int i = 0; i < (int)pos[x][y].size(); ++i) {
            int cx = pos[x][y][i].first;
            int cy = pos[x][y][i].second;
            if(D[cx][cy] > D[x][y] + 1) {
                D[cx][cy] = D[x][y] + 1;
                st[cx][cy] = x;
                q.push(pos[x][y][i]);
            }
        }
    }
    int mn = INF;
    for(int i = 0; i < (int)end.size(); ++i) {
        int x = end[i].first;
        int y = end[i].second;
        if(D[x][y] != INF) mn = min(mn, D[x][y]);
    }
    return mn == INF ? -1 : mn;
}

int main() {
    fin >> n;
    for(int i = 1; i <= n; ++i)
        for(int j = 1; j <= n; ++j) {
            int kx, ky;
            fin >> kx >> ky;
            pos[kx][ky].push_back({i, j});
        }
    int total = 0;
    for(int i = 1; i <= n; ++i)
        for(int j = 1; j <= n; ++j) {
            vector<pair<int, int>> end;
            int kx, ky;
            fin >> kx >> ky;
            if(kx == i && ky == j) continue;
            end.push_back({kx, ky});
            pos[i][j].push_back({kx, ky});
            int mn = BFS({i, j}, end);
            total += mn;
            if(mn == -1) {
                fout << -1 << '\n';
                return 0;
            }
        }
    fout << total << '\n';
    return 0;
}
