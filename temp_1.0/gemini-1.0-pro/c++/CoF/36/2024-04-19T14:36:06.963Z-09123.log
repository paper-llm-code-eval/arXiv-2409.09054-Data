#include <fstream>
#include <string>
#include <vector>

using namespace std;

int main()
{
    ifstream fin("subsecvente.in");
    ofstream fout("subsecvente.out");

    int n;
    fin >> n;

    vector<string> strings(n);
    for (string& s : strings)
        fin >> s;

    int maxLength = 0;
    for (int i = 0; i < strings[0].length(); i++)
    {
        for (int j = i; j < strings[0].length(); j++)
        {
            bool found = true;
            for (string& s : strings)
            {
                if (s.find(strings[0].substr(i, j - i + 1)) == string::npos)
                {
                    found = false;
                    break;
                }
            }

            if (found && j - i + 1 > maxLength)
                maxLength = j - i + 1;
        }
    }

    fout << maxLength << '\n';
}
