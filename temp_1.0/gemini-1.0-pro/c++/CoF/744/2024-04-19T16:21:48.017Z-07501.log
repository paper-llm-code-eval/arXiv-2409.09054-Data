#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("volei.in");
    ofstream out("volei.out");

    int n, m;
    in >> n >> m;
    vector<int> fete(n), baieti(m), ans;
    in >> fete[0];
    int tallest = fete[0];
    int sol = 0;
    for (int i = 1; i < n; i++) {
        in >> fete[i];
        tallest = max(tallest, fete[i]);
    }
    for (int i = 0; i < m; i++) {
        in >> baieti[i];
    }
    sort(baieti.rbegin(), baieti.rend());
    for (int i = 0, j = 0; i < n && j < m;) {
        if (baieti[j] > fete[i]) {
            if (baieti[j] > tallest) {
                ans.push_back(baieti[j]);
                j++;
                sol++;
            }
            i++;
            tallest = fete[i];
        } else {
            i++;
        }
    }
    out << sol << '\n';
    for (int i = 0, j = 0; i < n || j < sol;) {
        if (j < sol && ans[j] > fete[i]) {
            out << '(' << ans[j] << ") ";
            j++;
        } else {
            out << fete[i] << ' ';
            i++;
        }
    }
    out << '\n';
    return 0;
}
