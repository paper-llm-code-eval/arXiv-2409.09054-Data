#include <vector>
#include <queue>
#include <limits>

using namespace std;

class Edge {
public:
    int node;
    int weight;
    int power;

    Edge(int node, int weight, int power) : node(node), weight(weight), power(power) {}
};

class Node {
public:
    int distance;
    int lantern;

    Node() : distance(numeric_limits<int>::max()), lantern(0) {}
};

int main() {
    ifstream in("lanterna.in");
    ofstream out("lanterna.out");

    int n, k;
    in >> n >> k;

    vector<bool> bases(n + 1);
    for (int i = 1; i <= n; i++) {
        int base;
        in >> base;
        bases[i] = base;
    }

    int m;
    in >> m;

    vector<vector<Edge>> edges(n + 1);
    for (int i = 0; i < m; i++) {
        int a, b, weight, power;
        in >> a >> b >> weight >> power;
        edges[a].emplace_back(b, weight, power);
        edges[b].emplace_back(a, weight, power);
    }

    priority_queue<pair<int, pair<int, int>>> pq;
    pq.emplace(0, make_pair(1, k));

    vector<Node> shortest_paths(n + 1);

    while (!pq.empty()) {
        int weight = -pq.top().first;
        int node = pq.top().second.first;
        int lantern = pq.top().second.second;
        pq.pop();

        if (node == n) {
            out << weight << ' ' << lantern << '\n';
            return 0;
        }

        if (weight > shortest_paths[node].distance) {
            continue;
        }

        for (auto& edge : edges[node]) {
            int new_weight = weight + edge.weight;
            int new_lantern = lantern;
            if (!bases[edge.node]) {
                new_lantern = max(0, new_lantern - edge.power);
            }

            if (new_weight < shortest_paths[edge.node].distance || new_weight == shortest_paths[edge.node].distance && new_lantern < shortest_paths[edge.node].lantern) {
                shortest_paths[edge.node].distance = new_weight;
                shortest_paths[edge.node].lantern = new_lantern;
                pq.emplace(-new_weight, make_pair(edge.node, new_lantern));
            }
        }
    }

    return 0;
}
