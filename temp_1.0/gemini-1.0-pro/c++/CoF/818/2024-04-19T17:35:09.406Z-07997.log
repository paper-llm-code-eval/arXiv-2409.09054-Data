#include <fstream>
#include <vector>
#include <algorithm>

struct Client {
    int id;
    int rotations;
};

int main() {
    std::ifstream in("roata.in");
    std::ofstream out("roata.out");

    int n, p;
    in >> n >> p;

    std::vector<Client> clients(p);
    for (int i = 0; i < p; i++) {
        int c;
        in >> c;
        clients[i].id = i + 1;
        clients[i].rotations = c;
    }

    int revenue = 0;
    std::vector<bool> occupied(n, false);
    std::vector<int> boardingOrder;
    std::vector<int> boardingPositions;

    std::vector<Client> queue;
    for (auto& client : clients) {
        revenue += 1;
        queue.push_back(client);
    }

    while (!queue.empty()) {
        Client& currClient = queue.back();

        int boardingPos = std::find(occupied.begin(), occupied.end(), false) - occupied.begin();
        occupied[boardingPos] = true;
        boardingOrder.push_back(currClient.id);
        boardingPositions.push_back(boardingPos);

        currClient.rotations -= 1;
        if (currClient.rotations == 0) {
            queue.pop_back();
        }

        occupied[(boardingPos + n - 1) % n] = false;
        rotate(occupied.begin(), occupied.begin() + 1, occupied.end());
    }

    int lastCab = boardingPositions.back();
    out << revenue << std::endl;
    for (auto& id : boardingOrder) {
        out << id << ' ';
    }
    out << std::endl;
    out << lastCab + 1 << std::endl;

    return 0;
}
```
