#include <fstream>
#include <vector>

using namespace std;

ifstream in("patrat.in");
ofstream out("patrat.out");

int main()
{
    int n, m;
    in >> n >> m;
    vector<vector<int>> stars(n, vector<int>(m));
    for (auto &line : stars)
        for (int &star : line) in >> star;

    int bright_stars = 0;
    int squares = 0;
    int max_square_side = 0;
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < m; j++)
        {
            bool bright_star = true;
            for (int di = -1; di <= 1; di++)
                for (int dj = -1; dj <= 1; dj++)
                {
                    int ni = i + di;
                    int nj = j + dj;
                    if (ni >= 0 && ni < n && nj >= 0 && nj < m)
                        bright_star &= stars[i][j] > stars[ni][nj];
                }
            if (bright_star)
            {
                bright_stars++;
                int square_side = 1;
                while (i + square_side < n && j + square_side < m && stars[i][j] > stars[i + square_side][j] && stars[i][j] > stars[i][j + square_side] &&
                       stars[i][j] > stars[i + square_side][j + square_side])
                    square_side++;
                if (square_side > max_square_side)
                {
                    max_square_side = square_side;
                    squares = 1;
                }
                else if (square_side == max_square_side)
                {
                    squares++;
                }
            }
        }
    }
    out << bright_stars << endl;
    out << squares << endl;
    out << max_square_side << endl;
    return 0;
}
