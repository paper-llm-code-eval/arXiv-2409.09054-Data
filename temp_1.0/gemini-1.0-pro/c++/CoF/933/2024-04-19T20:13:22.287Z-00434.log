		__fout.close();
		break;
	case 2:
		__fin.close();
		ofstream __fout2("seism.out");
		__fout2 << cnt << '\n';
		__fout2.close();
		break;
	case 3:
		__fin.close();
		ofstream __fout3("seism.out");
		__fout3 << ans3 << '\n';
		__fout3.close();
		break;
	default:
		break;
	}

#ifdef WIN64
	system("pause");
#else
	auto __perfCounterEnd = std::chrono::high_resolution_clock::now();
	std::cout.imbue(std::locale("en_US.UTF-8"));
	std::cout << "EXECUTION COMPLETE IN " << std::chrono::duration_cast<std::chrono::seconds>(__perfCounterEnd - __perfCounterBegin).count() << '.'
	          << std::setw(6) << std::setfill('0') << std::chrono::duration_cast<std::chrono::microseconds>(__perfCounterEnd - __perfCounterBegin).count() % 1000000 << " ms\n";
#endif
	return 0;
}
