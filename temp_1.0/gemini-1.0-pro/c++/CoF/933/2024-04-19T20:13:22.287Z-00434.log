inline void* _tcsncpy_s(wchar_t* dest, rsize_t sizeInBytes, const wchar_t* src, rsize_t count) { return wcsncpy_s(dest, sizeInBytes / sizeof(wchar_t), src, count); }
inline void* _tcsrchr(const wchar_t* str, wchar_t c) { return wcsrchr(str, c); }

auto main() -> int {
#ifdef WIN64
	using namespace std;
#else
	_set_output_format(_TWO_DIGIT_EXPONENT);
	auto __perfCounterBegin = std::chrono::high_resolution_clock::now();
#endif

	char __c;
	wifstream __fin("seism.in");
	__fin >> __c;
	const int t1 = (__c == '1') + 2 * (__c == '2') + 3 * (__c == '3');
	size_t n;
	__fin >> n >> noskipws;
	vector<char> v(n + 2), &a = v, &p = v + 1, &r = v + 2;
	__fin.read(&a.front(), sizeof(char) * n);
	size_t i, cnt{0}, ans1{0}, ans2{0}, ans3{0};
	for (i = 1; i < n + 1; ++i) {
		int t2 = (p[i - 1] += a[i] == '1' ? 1 : -1);
		*r = r[i - 1] + (t1 == 3 && t2 != 0 && t2 == (i != 1 ? r[i - p[i - 1] - 1] : 0));
		if (p[i - 1] >= 3) {
			if (t1 != 3 || t2 != 0) {
				++cnt;
			} else
				++ans2;
			if (p[i - 1] > p[i - cnt])
				cnt = i - p[i - cnt];
			ans1 = max(ans1, p[i - cnt]);
		}
	}
	if (t1 == 3) {
		for (i = 1; i < n + 2; ++i) {
			if (p[i] != 0 || p[i - 1] != p[i]) {
				ans3 = max(ans3, r[i] - (p[i] ? r[i - p[i] - 1] : 0));
