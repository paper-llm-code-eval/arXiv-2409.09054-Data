#include <bits/stdc++.h>
using namespace std;

const int maxn = 4005;
int t, n, d[maxn], v[maxn], f[maxn][maxn / 4 + 1];

double min_d(double time) {
    double ans = 1e16;
    for (int i = 1; i <= n; i++) ans = min(ans, d[i] - time * v[i]);
    return ans;
}

int main() {
    freopen("startrek.in", "r", stdin);
    freopen("startrek.out", "w", stdout);
    ios_base::sync_with_stdio(false);
    cin >> n >> t;
    for (int i = 1; i <= n; i++) cin >> d[i];
    for (int i = 1; i <= n; i++) cin >> v[i];
    memset(f, -0x3f, sizeof f);
    f[0][0] = 0;
    for (int taken = 0; taken <= n; taken++)
        for (double reloads = 0; reloads <= n / 4; reloads++) {
            f[taken + 1][reloads + 1] = max(f[taken + 1][reloads + 1], f[taken][reloads] + (min_d(t * reloads + t) > 0));
            f[taken + 1][reloads] = max(f[taken + 1][reloads], f[taken][reloads]);
        }
    cout << f[n][n / 4] << "\n";
    return 0;
}
