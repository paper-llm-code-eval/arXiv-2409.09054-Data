#include <fstream>
#include <vector>
#include <algorithm>
#include <cmath>

using namespace std;

struct Proiectil {
    double distanta, viteza;
    
    bool operator<(const Proiectil& other) const {
        return distanta / viteza < other.distanta / other.viteza;
    }
};

int main() {
    ifstream fin("startrek.in");
    ofstream fout("startrek.out");

    int n, t;
