        if (timp_trecut + t < proiectil.distanta / proiectil.viteza) {
            timp_trecut += t;
            numar_proiectile_distruse++;
        } else {
            break;
        }
    }
    fout << numar_proiectile_distruse << "\n";
    
    return 0;
}
