    fib.push_back(unit_x);
    ++p;
    x /= 10;
    if (x == 0) {
      p += 2;
      fib.push_back(1);
      fib.push_back(0);
      break;
    }
  }
  return fib[(n - p - 1) % (p + 2)] % 10;
}

