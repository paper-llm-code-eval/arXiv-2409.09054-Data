inline IN get() {
  g_BuffUsed = g_BuffUsed + 1;
  if (g_BuffUsed >= BUFF_SIZE) {
    g_BuffUsed = 0;
    for (uint64_t i = 0; i < BUFF_SIZE && in.peek() != EOF; i++) { g_Buff[i] = in.get(); }
  }
  return g_Buff[g_BuffUsed];
}

uint32_t read_uint32() {
  uint32_t x = 0;
  char c;
  do {
    c = get<char>();
    x = (x << 3) + (x << 1) + c - '0';
  } while (c >= '0' && c <= '9');
  return x;
}

template <typename T>
inline void write(const T pVal) {
  static char const digits[] = "0123456789";

  int v = pVal;
  if (v < 0) {
    out.put('-');
    v = -v;
  }
  uint64_t tmp;
  do {
    tmp = v % 10;
    v /= 10;
    out.put(digits[tmp]);
  } while (v > 0);
}

int main() {
  uint32_t n = read_uint32();
  uint32_t k = read_uint32();
  vector<uint32_t> lds(n + 1, 0);
  vector<uint32_t> vals(n + 1, 0);
  for (uint32_t i = 1; i <= n; i++) { vals[i] = read_uint32(); }
  uint32_t ds_len = 0;
  uint32_t last = -1;
  for (uint32_t i = 1; i <= n; i++) {
    uint32_t cur = vals[i];
    if (cur == last) { lds[ds_len]++; } else { ds_len++; }
  }
  for (uint32_t i = 0; i < k; i++) {
    uint32_t t = read_uint32();
    write((cds[t] ? 1 : 0) * t);
    out.put(' ');
  }
  out.put('\n');
  return 0;
}
