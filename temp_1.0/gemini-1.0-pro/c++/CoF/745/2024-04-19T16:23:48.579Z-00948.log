#include <fstream>
#include <vector>
#include <limits>

using namespace std;

auto min_traversare(const vector<vector<int>>& v) -> int
{
    const size_t m = v.size(), n = v.front().size();
    vector<vector<int>> d(m + 1, vector<int>(n + 1, numeric_limits<int>::max() / 2));
    d[1][1] = v[0][0];
    for (size_t i = 1; i <= m; ++i)
    {
        for (size_t j = 1; j <= n; ++j)
        {
            for (size_t k = j; k <= n; ++k)
                d[i][k] = min(d[i][k], d[i - 1][j] + v[i - 1][k]);
        }
    }
    int s = d.back()[1];
    for (size_t j = 1; j <= n; ++j)
        s = min(s, d.back()[j]);
    return s;
}

int main() {
    ifstream  fin("lacusta.in");
    ofstream fout("lacusta.out");
    size_t m, n;
    fin >> m >> n;
    vector<vector<int>> v(m);
    for (auto& a : v)
    {
        a.resize(n);
        for (auto& b : a) fin >> b;
    }
    fout << min_traversare(v);
    return 0;
}

