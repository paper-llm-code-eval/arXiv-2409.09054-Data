#include <fstream>
#include <deque>
#include <vector>
using namespace std;

ifstream fin("transport.in");
ofstream fout("transport.out");

using ll = long long; constexpr ll mod = 1e9 + 7;
constexpr ll inv2 = 500000004;
constexpr ll inv3 = (mod + 1) / 3;
inline ll mul(const ll &a, const ll &b) { return (1LL * a * b) % mod; }
inline ll powr(const ll &a, const ll &b) {
  ll ans = 1; while (b) { if (b & 1) ans = mul(ans, a); a = mul(a, a); b >>= 1; }
  return ans;
}

struct info {
  ll suma, cnt;
  info(const ll &sm = 0, const ll &cn = 0) : suma(sm), cnt(cn) {}
  inline info operator+(const info &a) const { return info(suma + a.suma, cnt + a.cnt); }
  inline info operator*(const info &a) const { return info(mul(suma, a.suma), mul(cnt, a.cnt)); }
};

int main() {
  ll t; fin >> t;
  ll n, c; fin >> n >> c; deque<ll> x(n), d(n);
  for (ll i = 0; i < n; ++i) { fin >> x[i] >> d[i]; }
  sort(x.begin(), x.end()); sort(d.begin(), d.end());

  deque<info> s; info sm0(0, 1); s.push_back(sm0);
  for (ll i = 0; i < n; ++i) {
    info sm1(x[i] - d[i], 1); info sm2(d[i] - x[i], d[i] - x[i]);
    s.push_back(s.back() + sm1); s.push_back(s.back() + sm2);
  }

  if (t == 1) {
    ll ans = 0;
    for (ll i = 0; i < s.size(); ++i) { ans = (ans + sm0 + s[i]) * c; ans %= mod; }
    fout << ans;
  } else {
    info smc = sm0 + s.back();
    ll ans = 0;
    for (ll i = 0; i + 1 < s.size(); ++i) { ans += smc * (s[i] * s[i + 1]); ans %= mod; }
    ll ans1 = mul(mul(ans, powr(smc.cnt + 1, mod - 2)), c);
    ll ans2 = smc.suma * smc.cnt; ans2 %= mod; ans2 = mul(ans2, inv2);
    ans2 = mul(ans2, powr(c, mod - 2));
    ll ans3 = smc.suma * smc.suma; ans3 %= mod; ans3 = mul(ans3, inv3);
    ans3 = mul(ans3, powr(c, mod - 2));
    ll ans4 = powr(smc.cnt, 2); ans4 %= mod; ans4 = mul(ans4, powr(c, mod - 2));
    ans = ans1 - ans2 - ans3 - ans4; ans %= mod; ans += mod; ans %= mod;

    ll ans5 = powr(c, mod - 2);
    ans5 = mul(ans5, powr(2, mod - 2));
    ans6 = powr(c - 1, mod - 2);
    ans6 = mul(ans6, powr(2, mod - 2));
    ans = ans + smc.cnt * (ans6 - ans5); ans %= mod;

    fout << ans;
  }
  return 0;
}
