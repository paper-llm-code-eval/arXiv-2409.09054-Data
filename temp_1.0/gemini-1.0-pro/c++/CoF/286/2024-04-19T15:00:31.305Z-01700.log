  cin >> n >> c;
  for (int i = 1; i <= n; ++i) cin >> x[i] >> d[i];
  // dp[i][st] = nr de rute distincte din subintervalul (1,i) care
  // incep din statia i (daca st = 1) sau in care se opreste statia i
  // (daca st = 0)
  dp[0][0] = 1;
  for (int i = 1; i <= n; ++i) {
    for (int st = 0; st <= 1; ++st) {
      // alegem sau nu statia i in ruta
      dp[i][st] = (dp[i - 1][1] + dp[i - 1][0]) % MOD;
      if (st == 0) {
        // daca statia i nu e in ruta, atunci ultima ruta din
        // (1,i-1) care contine statia i-1 poate fi extinsa
        // cu statia i
        dp[i][st] = (dp[i][st] + dp[i - 1][1]) % MOD;
      } else {
        // daca statia i e in ruta, atunci lungimea rutei se
        // modifica doar daca statia i-1 nu era in ruta
        (dp[i][st] += dp[i - 1][0] * (x[i] - x[i - 1])) %= MOD;
        // si nu exista o ruta deja existenta care sa se termine
        // in statia i
        (dp[i][st] += c) %= MOD;
      }
      if (t == 2) {
        // daca se cere ruta de tip Expres, atunci putem sari
        // peste statia i si alegem cele mai bune rute din (1,i-1)
        dp[i][st] = (dp[i][st] + dp[i - 1][st] * d[i]) % MOD;
      }
    }
  }
  cout << dp[n][t == 2] << "\n";
  return 0;
}
