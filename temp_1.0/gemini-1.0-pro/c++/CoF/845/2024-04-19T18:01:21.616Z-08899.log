#include <vector>
#include <iostream>
#include <fstream>
#include <algorithm>
#include <set>

using namespace std;

using sequence = vector<unsigned int>;

#define endl "\n"

int main() {
  ifstream fin("munte.in");
  ofstream fout("munte.out");

  unsigned int n;
  fin >> n;
  vector<unsigned int> a(n);
  set<unsigned int> a_sups;
  for (auto& x : a) {
    fin >> x;
    a_sups.insert(x);
  }

  vector<sequence> munti;
  for (auto it = a_sups.begin(); it != a_sups.end(); ++it) {
    // parcurgem setul elementelor ramase cu iteratorii
    unsigned int left = 0, right = 0;
    unsigned int d = numeric_limits<unsigned int>::max();
    for (const auto& x : a) {
      ++right;
      if (x == *it) {
        if (right - left >= 3) {
          if (!left || (x - *(--it)) == d) {
            sequence munte;
            for (auto it2 = it; it2 != a.cbegin() + right;) {
              munte.push_back(*it2);
              it2++;
            }

            bool ok = true;
            for (unsigned int k = 0; k < munte.size() && ok; ++k) {
              if ((!k || munte[k] > munte[k - 1]) && (k == munte.size() - 1 || munte[k] > munte[k + 1])) {
                ok = true;
              } else {
                ok = false;
              }
            }

            if (ok) {
              munti.push_back(munte);
              for (const auto& p : munte) a_sups.erase(a_sups.find(p));
            }
          }
        }
        left = right;
      }
      d = x == *it ? d : *(--it) - x;
    }
    a.erase(a.begin() + left, a.begin() + right);
    sort(a.begin(), a.end());
    it = a_sups.lower_bound(*a.begin());
  }

  a.clear();
  for (const auto& x : a_sups) a.push_back(x);

  fout << munti.size() << endl;
  fout << munti.size() + a.size() << endl;
  fout << a.size() << endl;
  return 0;
}
