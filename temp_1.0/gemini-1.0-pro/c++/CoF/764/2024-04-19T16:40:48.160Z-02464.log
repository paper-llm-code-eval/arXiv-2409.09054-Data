#include <algorithm>
#include <chrono>
#include <fstream>
#include <map>
#include <string>
#include <vector>

namespace chrono = std::chrono;

class Timer {
public:
    Timer() noexcept { restart(); }

    void restart() noexcept { m_begin = chrono::high_resolution_clock::now(); }

    uint64_t elapsed_milliseconds() const noexcept {
        auto const now = chrono::high_resolution_clock::now();
        auto const duration = now - m_begin;
        return chrono::duration_cast<chrono::milliseconds>(duration).count();
    }

private:
    chrono::time_point<chrono::high_resolution_clock> m_begin;
};

struct Bit {
    bool value;
};

struct Digit {
    std::vector<Bit> bits;
    static Digit create(unsigned value, unsigned num_bits) {
        Digit digit;
        digit.bits.reserve(num_bits);
        for (unsigned i = 0; i < num_bits; ++i) {
            unsigned bit_mask = 1u << i;
            bool bit_value = (value & bit_mask) == bit_mask;
            digit.bits.push_back({bit_value});
        }
        return digit;
    }
};

struct Display {
    std::map<unsigned, Digit> m_digits;
    unsigned num_columns;
    void init() {
        m_digits[0] = Digit::create(0, 2);
        m_digits[1] = Digit::create(1, 2);
        m_digits[2] = Digit::create(2, 2);
        m_digits[3] = Digit::create(3, 3);
        m_digits[4] = Digit::create(4, 3);
        m_digits[5] = Digit::create(5, 3);
        m_digits[6] = Digit::create(6, 4);
        m_digits[7] = Digit::create(7, 4);
        m_digits[8] = Digit::create(8, 4);
        m_digits[9] = Digit::create(9, 4);
        num_columns = 8;
    }
    void update(unsigned value, std::vector<char>& display, unsigned column) {
        unsigned row = 1;
        auto const& digit = m_digits[value];
        for (int i = digit.bits.size() - 1; i >= 0; --i) {
            const bool bit_value = digit.bits[i].value;
            const auto code = bit_value ? 'o' : 'x';
            display[column + row++] = code;
        }
    }
};

int main() {
    Display display;
    display.init();
    Timer timer;
    std::ifstream in("ceas.in");
    std::ofstream out("ceas.out");
    std::vector<char> buf(display.num_columns * 6 + 1);
    unsigned time;
    in >> buf >> buf >> buf >> buf >> time;
    in.close();
    unsigned milliseconds = time * 10;
    if (milliseconds == 0) {
        out << buf << "\n" << buf << "\n" << buf << "\n" << buf << "\n";
        return 0;
    }
    unsigned second = milliseconds / 100;
    milliseconds %= 100;
    std::vector<unsigned> values = {milliseconds, second / 60, second / 3600, second / 86400};
    std::ranges::sort(values, std::greater<>{});
    for (unsigned v : values) {
        if (milliseconds == 0 && second > 0) {
            for (unsigned column = 0; column < display.num_columns; ++column) {
                buf[column * 6 + 3] = ' ';
            }
        }
        unsigned val = second > 0 ? milliseconds : v;
        unsigned column = 0;
        for (int i = 2; i >= 0; --i) {
            unsigned n = val % 10;
            display.update(n, buf, column);
            column += 3;
            val /= 10;
        }
        if (millisecond > 0) {
            buf[2] = buf[5] = 'o';
        }
        out << buf << "\n";
        if (val == 0) {
            break;
        }
        while (val > 0) {
            buf[5] = (buf[5] == 'x') ? 'o' : 'x';
            val--;
        }
    }
    out.close();
    return 0;
}
