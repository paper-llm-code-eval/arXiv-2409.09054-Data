#include <fstream>
int main() {
  std::fstream f("turnuri.in");
  int m, n;
  f >> m >> n;
  int grid[m][n], height[n], finalheight[n], ans = 0, maxi = 0;
  for (int i = 0; i < m; i++) {
    for (int j = 0; j < n; j++) {
      f >> grid[i][j];
    }
  }
  for (int j = 0; j < n; j++) {
    int h = 0;
    for (int i = m - 1; i >= 0; i--) {
      h += grid[i][j];
    }
    height[j] = h;
    maxi = std::max(h, maxi);
  }
  std::fstream g("turnuri.out");
  for (int j = 0; j < n; j++) {
    g << height[j] << " ";
  }
  g << "\n";
  for (int j = 0; j < n; j++) {
    for (int j2 = j + 1; j2 < n; j2++) {
      int h1 = height[j], h2 = height[j2];
      h1 += h2;
      if (h1 <= maxi) {
        finalheight[j] = 0;
        finalheight[j2] = 0;
        height[j] = 0;
        height[j2] = 0;
        ans++;
      }
    }
  }
  g << maxi << "\n";
  g << ans << "\n";
  f.close();
  g.close();
  return 0;
}
```
