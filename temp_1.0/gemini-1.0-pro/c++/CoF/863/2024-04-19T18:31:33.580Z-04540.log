    while(x <= n) {
        while(y <= m) {
            aib[x][y] += val;
            y += y&-y;
        }
        x += x&-x;
        y = 0;
    }
}

int main() {
    fin >> p >> n >> m >> T >> L >> C >> k >> S;
    mask[0] = 1, mask = mask << (S - 1), mask <<= 1, mask += 1;
    for(int i = 1; i <= T; ++ i) {
        fin >> foodX[i] >> foodY[i];
    }
    for(int i = 1; i <= n; ++ i)
        for(int j = 1; j <= m; ++ j)
            fin >> map[i][j];
    if(p == 1) {
        for(int i = 1; i <= n; ++ i) {
            for(int j = 1; j <= m; ++ j) {
                if((i ^ L) || (j ^ C)) {
                    bitset<9> cure;
                    for(int k = 7 - S + 1; k; -- k)
                        cure[k] = map[i][j] & (1 << k), cure[k] = !cure[k];
                    if(!(mask & cure).count())
                        ++ ans1;
                }
            }
        }
        fout << ans1 << "\n";
        return 0;
    }
    update(L, C, 1);
    Q.push(make_pair(Point(L, C), 0));
    while(!Q.empty()) {
        int x = Q.front().first.x, y = Q.front().first.y, d = Q.front().second;
        Q.pop();
        if(ans2 == d) {
            continue;
        }
        ans2 = d;
        for(int i = 0; i < 4; ++ i) {
            int xx = x + dx[i], yy = y + dy[i];
            if(xx < 1 || xx > n || yy < 1 || yy > m)
                continue;
            int curr = map[x][y], add = 0;
            if(map[xx][yy] == curr) {
                add = 1;
            }
            update(xx, yy, add);
            if(add) {
                Q.push(make_pair(Point(xx, yy), d + 1));
            }
        }
        for(int i = 1; i <= T; ++ i) {
            int count = sum(foodX[i], foodY[i]);
            if(count) {
                ans2 = min(ans2, d + 1);
                Q.push(make_pair(Point(foodX[i], foodY[i]), ans2));
            }
        }
    }
    for(int i = 1; i <= T; ++ i) {
        int count = sum(foodX[i], foodY[i]);
        if(count == ans2)
            ++ ans1;
    }
    fout << ans2 << " " << ans1 << "\n";
    return 0;
}
