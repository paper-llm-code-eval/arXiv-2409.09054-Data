#define _CRT_SECURE_NO_WARNINGS
#include <iostream>
#include <fstream>

using namespace std;

int main() {
	int n;
	ifstream f("ucif.in");
	f >> n;
	f.close();
	int s = 0;
	for (int i = 1; i <= n; i++) {
		long long p = i;
		for (int j = 1; j < i; j++) {
			p *= i;
		}
		s = (s + (int)(p % 10)) % 10;
	}
	ofstream g("ucif.out");
	g << s;
	g.close();
}
