#include<fstream>
#include<vector>
using namespace std;
ifstream f("grupe.in");
ofstream g("grupe.out");
struct grupa
{
    int fete=0, baieti=0;
};

vector<grupa>sol;
vector<vector<int>>componenta_grupe;
vector<int>sex,prezent;
short ok=1;
int n,k,nr_elevi;

int main()
{
    f>>n>>k;
    sex.resize(n+1);
    prezent.resize(n+1);
    componenta_grupe.resize(k+1);
    while(n--)
    {
        char s;
        f>>s;
        if(s=='b')
            sex[n+1]=1;
    }
    while(k--)
    {
        sol.push_back(grupa());
        f>>nr_elevi;
        componenta_grupe[k].resize(nr_elevi+1);
        while(nr_elevi--)
        {
            int x;
            f>>x;
            prezent[x]=1;
            componenta_grupe[k][nr_elevi]=x;
            sol[k].baieti+=(sex[x]==0);
            sol[k].fete+=(sex[x]==1);
        }
        g<<sol[k].fete<<" "<<sol[k].baieti<<'\n';
    }
    for(int i=1; i<=n; i++)
        if(!prezent[i])
            ok=0;
    for(int i=0; i<sol.size(); i++)
        for(int j=i+1; j<sol.size(); j++)
            if(abs(sol[i].fete-sol[j].fete)>1 || abs(sol[i].baieti-sol[j].baieti)>1 ||
                 abs((int)componenta_grupe[i].size()-(int)componenta_grupe[j].size())>1)
                ok=0;
    if(ok)
        g<<"DA";
    else
        g<<"NU";
    return 0;
}
