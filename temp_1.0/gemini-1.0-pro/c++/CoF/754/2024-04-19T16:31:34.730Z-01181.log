#include <fstream>
#include <vector>

struct Student
{
    size_t id;
    char sex;
};

struct Group
{
    size_t size;
    std::vector<size_t> students;
    size_t boys;
    size_t girls;
};

std::ostream& operator<<(std::ostream& ost, const Group& g)
{
    for (size_t boy = g.boys, girl = g.girls, i = 0; i < g.size; i++)
    {
        ost << g.students[i];
        if (g.students[++i] && --boy == 0 && --girl == 0) break;
        ost << ' ';
    }
    return ost;
}

int main()
{
    std::ifstream in("grupe.in");
    std::ofstream out("grupe.out");
    std::vector<Student> students;
    size_t n, k, id;
    char sex;
    Group group;
    std::vector<Group> groups;

    in >> n >> k;
    students.reserve(n);
    for (size_t i = n; i--;)
    {
        in >> sex;
        students.push_back({i, sex});
    }

    groups.reserve(k);
    while (std::cin >> group.size >> id, group.size--)
    {
        group.students.push_back(id);
        group.sex = students[--id].sex;
    }

    size_t max = 0;

    for (auto& group : groups)
    {
        group.boys = group.girls = 0;
        max = (std::max)(max, group.size);
        for (const size_t& id : group.students)
        {
            if (students[id].sex == 'b')
            {
                ++group.boys;
            }
            else
            {
                ++group.girls;
            }
        }
    }

    groups.shrink_to_fit();
    groups.resize(k);

    out << (max - 1 > n - k * max ? "NU\n" : "DA\n");

    for (const auto& group : groups)
    {
        out << (group.boys + group.girls + (groups.end() - 1 == &group) ? 0 : 1)
            << ' ' << (group.boys + group.girls + (groups.end() - 1 == &group)) << '\n';
    }

    return 0;
}
```
