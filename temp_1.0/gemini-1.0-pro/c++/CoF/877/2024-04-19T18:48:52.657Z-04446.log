            int ur = lower_bound(note.begin(), note.end(), e.r) - note.begin();
            int uu = lower_bound(note.begin(), note.end(), e.u) - note.begin();
            if (ur - 1 <= sol) {
                decizie[e.a - 1] = 'R';
                sol = max({sol, ur - 1, uu - 1});
            } else if (uu - 1 <= sol) {
                decizie[e.a - 1] = 'U';
                sol = max(sol, uu - 1);
            }
        }
    }
    g << sol << '\n' << decizie;

    return 0;
}
