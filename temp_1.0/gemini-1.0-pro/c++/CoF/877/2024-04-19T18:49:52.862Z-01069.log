#include <fstream>
using namespace std;

ifstream fin("admitere.in");
ofstream fout("admitere.out");
int n, m, v[21000], u[21000], ok[21000], sol[21000], k;
pair<int, int> a[21000], b;

inline int cmp(const void *a, const void *b) { return ((*(int*)a)-(*(int*)b)); }

int main() {
  int V, U;
  fin >> V >> n >> m;
  for (int i = 0; i < m; i++) { fin >> v[i]; }
  for (int i = 0; i < m; i++) { fin >> u[i]; }
  for (int i = 0; i < n; i++) {
    fin >> a[i].first >> a[i].second;
    ok[i] = 1;
  }
  qsort(v, m, sizeof(int), cmp);
  qsort(u, m, sizeof(int), cmp);
  for (int i = 0, c1 = 0, c2 = 0; k < m && i < n; i++) {
    if (c1 < m && c2 < m) {
      if (v[c1] <= u[c2]) {
        a[i].first = v[c1];
        c1++;
      } else {
        a[i].first = u[c2];
        c2++;
      }
    } else if (c1 < m) {
      a[i].first = v[c1];
      c1++;
    } else if (c2 < m) {
      a[i].first = u[c2];
      c2++;
    }
  }
  if (V) {
    qsort(a, n, sizeof(int), cmp);
    V = 0;
    b.first = a[0].first;
    U = 0;
    for (int i = 0; i < n; i++) {
      if (a[i].first > b.first) {
        V = max(V, U);
        U = i;
        b.first = a[i].first;
      } else {
        U = max(U, i);
      }
    }
    V = max(V, U);
    if (V == 0) {
      fout << 0 << '\n';
      for (int i = 0; i < n; i++) fout << 'X';
      fin.close();
      fout.close();
      return 0;
    }
    fout << V + 1 << '\n';
    for (int i = 0; i < n; i++) {
      if (i <= V) sol[i] = 1;
      if (sol[i] && a[i].first == b.first) {
        if (b.second > a[i].second) {
          sol[i]--;
          U--;
          b.second = a[i].second;
        }
      } else if (!sol[i] && a[i].first == b.first) {
        sol[i]++;
        U++;
        b.second = a[i].second;
      }
    }
    fout << b.second << ' ';
    for (int i = 0; i < n; i++) {
      if (sol[i]) {
        ok[i] = 0;
        if (a[i].first != b.second) fout << a[i].first << ' ';
        else fout << b.second << ' ';
      }
    }
    for (int i = 0; i < n; i++) {
      if (ok[i]) {
        if (a[V].first != a[i].first)
          fout << a[i].first << ' ';
        else
          fout << a[V].first << ' ';
      }
    }
  } else {
    qsort(a, n, sizeof(pair<int, int>),
          [](const auto &a, const auto &b) { return a.second > b.second; });
    for (int i = 0; i < n; i++) b.second = max(b.second, a[i].second);
    fout << b.second << '\n';
    for (int i = 0; b.second >= 0; i++) {
      if (a[i].second <= b.second && sol[i] < 2) sol[i]++, b.second -= a[i].first;
    }
    for (int i = 0; i < n; i++) {
      if (sol[i]) {
        ok[i] = 0;
        fout << a[i].first << ' ';
        k++;
      }
    }
    for (int i = 0; i < n; i++) {
      if (ok[i] && k < m) {
        if (a[i].first == sol[k]) k++, fout << a[i].first << ' ';
        else if (a[i].first + a[k] <= sol[k]) k++, fout << a[i].first << ' ';
      }
    }
  }
  fin.close();
  fout.close();
  return 0;
}
