struct sol_t
{
  int nr_rows;
  int arrows_white_rem;
  int arrows_black_rem;
};

sol_t solve(int k_a, int k_b)
{
  vector<int> raws(3);
  raws[0] = 1;
  raws[1] = 1;
  raws[2] = 2;

  int nr_raws = 2;
  while (true)
