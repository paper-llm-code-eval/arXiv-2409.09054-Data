  int k_a, k_b;
  in >> k_a >> k_b;

  sol_t sol = solve(k_a, k_b);

  out << sol.nr_rows << endl;
  out << sol.arrows_white_rem << endl;
  out << sol.arrows_black_rem << endl;

  return 0;
}
