#include <cstdio>
#include <vector>

using namespace std;

typedef long long llong;

const int NMAX = 200010;
const llong MOD = 666013;

llong v[NMAX];
int fr[MOD];

llong fastpow(llong a, int p) {
    llong ans = 1;
    while (p) {
        if (p % 2) {
            ans = ((ans % MOD) * (a % MOD)) % MOD;
        }
        a = ((a % MOD) * (a % MOD)) % MOD;
        p /= 2;
    }
    return ans;
}

int rmq(int x, int y) {
    int ans = 0;
    for (int b = 1; b <= 400000; b *= 2) {
        if ((x & b) && (y & b)) {
            ans |= b;
        }
    }
    return ans;
}

int main() {
    freopen("tort.in", "r", stdin);
    freopen("tort.out", "w", stdout);

    int n;
    scanf("%d", &n);
    for (int i = 1; i <= n; i++) {
        scanf("%lld", &v[i]);
    }
    vector<pair<int, llong>> ord;
    for (int i = 1; i <= n; i++) {
        ord.push_back(make_pair(-v[i], i));
    }
    sort(ord.begin(), ord.end());

    llong sum = 0;
    int lim = 0;
    for (int i = n; i >= 1; i--) {
        sum += ord[i - 1].first;
        lim = max(lim, rmq(lim, -ord[i - 1].first));
        fr[-sum] = ord[i - 1].second;
    }
    llong ps = 0;
    llong ans = 0;
    for (int i = 1; i <= n; i++) {
        ps += v[i];
        llong akt = rmq(lim, ps);
        ans += (fr[akt] > 0);
    }
    printf("%lld\n", ans);
    return 0;
}
