#include <bits/stdc++.h>
#include <fstream>

using namespace std;

ifstream in("text.in");
ofstream out("text.out");

const int MAX_LINE_LENGTH = 1000;

struct word_info {
    int length;
    int start_index;
    char str[MAX_LINE_LENGTH];
};

int num_spaces, num_words, num_characters, max_line_length;

void parse_input() {
    in >> max_line_length;
    in.get();  // Read the newline character after the max_line_length

    num_spaces = 0;
    num_words = 0;
    num_characters = 0;

    string current_word;
    in >> current_word;
    while (current_word != ".") {
        num_words++;
        num_characters += current_word.length();
        num_spaces++;
        out << current_word << " ";
        in >> current_word;
    }
    num_spaces--;
    out << "\n";
}

void align_paragraph() {
    int current_line_length = 0;
    int start_of_line = out.tellp();
    int words_on_current_line = 0;
    int spaces_on_current_line = 0;
    num_words = 0;
    num_characters = 0;

    string current_word;
    in >> current_word;
    while (current_word != ".") {
        num_words++;
        num_characters += current_word.length();
        if (current_line_length + current_word.length() + spaces_on_current_line <= max_line_length) {
            if (words_on_current_line > 0) {
                out << " ";
                spaces_on_current_line++;
            }
            out << current_word;
            current_line_length += current_word.length();
            words_on_current_line++;
        } else {
            int spaces_to_add = max_line_length - current_line_length;
            int spaces_per_word = spaces_to_add / (words_on_current_line - 1);
            if (spaces_to_add % (words_on_current_line - 1) != 0) { spaces_per_word++; }

            out.seekp(start_of_line);
            for (int i = 1; i < words_on_current_line; i++) {
                for (int j = 0; j < spaces_per_word; j++) { out << " "; }
                out << out.rdbuf()[out.tellp() - current_word.length() - spaces_on_current_line - 1];
                current_line_length += spaces_per_word;
                spaces_on_current_line += spaces_per_word;
            }

            out.seekp(0, ios_base::end);
            out << "\n" << current_word;
            current_line_length = current_word.length();
            start_of_line = out.tellp();
            words_on_current_line = 1;
            spaces_on_current_line = 0;
        }
        in >> current_word;
    }

    int spaces_to_add = max_line_length - current_line_length;
    for (int i = 0; i < spaces_to_add; i++) { out << " "; }
}

int main() {
    parse_input();
    align_paragraph();
    return 0;
}
