      yi--;
      while (yi > p && a[yi] != '\n') {
        b[yi] = ' ';
        yi--;
        nlc++;
      }
      p = yi;
      c = (MAXL - nlc) / nlc;
      while (a[xi] != '\n') {
        yi++;
