using std::vector;

const int MAX_LINE = 1000;

int main() {
    std::ifstream in("text.in");
    std::ofstream out("text.out");
    int max_line, max_len = 0;
    in >> max_line;
    vector<vector<std::string>> lines;
    std::string line, buffer;
    while (std::getline(in, line)) {
        max_len = std::max(max_len, (int) line.length());
        lines.push_back({});
        while (!line.empty()) {
            int pos = 0;
            while (pos < line.length() && line[pos] != ' ') ++pos;
            buffer = line.substr(0, pos);
            if (!buffer.empty()) lines.back().push_back(buffer);
            if (pos < line.length()) line.erase(0, pos + 1);
        }
        if (in.peek() == '\n') {
            in.ignore();
            max_len = std::max(max_len, (int) std::string(" ").length());
            lines.push_back({});
        }
    }
    for (const vector<std::string> &line_vec : lines) {
        int len = 0;
        for (const std::string &word : line_vec) len += word.length();
        int spaces = max_line - len;
        int spaces_per_word = spaces / (line_vec.size() - 1);
        int extra_spaces = spaces % (line_vec.size() - 1);
        for (size_t i = 0; i < line_vec.size(); ++i) {
            out << line_vec[i];
            int s;
