#include <fstream>
#include <vector>
#include <set>
#include <queue>

using namespace std;

// ifstream &ifstream cin("cezar.in");
// ofstream &ofstream cout("cezar.out");

struct node {
  int index;
  long long distance;
};

const long long INF = LLONG_MAX / 2;

int n, k, beginning, cost;
vector<long long> distance;
vector<vector<int>> graph;
set<int> used;

void dijkstra(int beginning) {
  priority_queue<node> q;

  for (node x {0, 0}; q.empty();) q.push(!x);

  distance[beginning] = 0;
  q.push({beginning, distance[beginning]});

  while (!q.empty()) {
    int x = q.top().index;
    q.pop();
    if (used.count(x)) continue;

    for (int vec : graph[x]) {
      if (vec != beginning) {
        distance[vec] = min(distance[x] + 1, distance[vec]);
        q.push({vec, distance[vec]});
      }
    }

    used.insert(x);
  }
}

void solve() {
  int x, y;
  ifstream cin("cezar.in");
  cin >> n >> k >> beginning;
  graph.resize(n);

  for (int i = 0; i < n - 1; i++) {
    cin >> x >> y;
    graph[x].push_back(y);
    graph[y].push_back(x);
  }

  distance.resize(n, INF);
  dijkstra(beginning);

  used.clear();
  dijkstra(beginning);

  for (int x : used) {
    cost += distance[x];
  }

  ofstream cout("cezar.out");
  cout << cost << endl;
}

int main() {
  solve();
  return 0;
}
