struct node {
  int index;
  long long distance;

  bool operator<(const node& other) const {
    return distance > other.distance;  // invert comparison for priority_queue
  }
};

const long long INF = numeric_limits<long long>::max() / 2;

int n, k, beginning, cost;
vector<long long> distance;
