#include <fstream>
#include <algorithm>

using namespace std;

const int NMAX = 100001;

int n, x, k;
int a[NMAX], b[NMAX], alb[NMAX];

int f(int x) {
    int ans = 0;
    for(int i = 0; i <= n; i++)
        if(i * (i + 1) / 2 <= x)
            ans = max(ans, i);
    return ans;
}

int c1, c2;

int findpos(int x) {
    int low = 0, high = k - 1, mid;
    while(low <= high) {
        mid = (low + high) / 2;
        if(alb[mid] >= x)
            high = mid - 1;
        else
            low = mid + 1;
    }
    return low - 1;
}

int main() {
    ifstream fin("piramide.in");
    ofstream fout("piramide.out");
    int d;
    fin >> n >> x >> k;
    for(int i = 0; i < k; i++) {
        fin >> d;
        alb[i] = d;
    }
    c1 = 0;
    c2 = 0;
    for(int i = 0; i < k; i++) {
        int aux = k - findpos(alb[i]);
        if(c1 < aux) {
            c1 = aux;
            c2 = alb[i];
        }
    }
    a[0] = 0;
    for(int i = 1; i <= n; i++)
        a[i] = i * (i + 1) / 2;
    b[0] = 0;
    int aux = 0;
    for(int i = 1; i <= n; i++) {
        if(a[i] > a[i - 1] + aux + 1)
            b[++aux] = i;
    }
    fout << f(x) << endl;
    fout << aux << endl;
    fout << n - a[aux] << endl;
    if(c1 > 0)
        fout << f(c2) << endl;
    else
        fout << "0" << endl;
    return 0;
}
```
