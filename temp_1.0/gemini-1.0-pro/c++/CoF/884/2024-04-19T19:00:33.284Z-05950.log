  ofstream fout("joc.out");
  fin >> Kii >> N >> K;

  for (int i = 1; i <= N; ++i) {
    for (int j = 1; j <= N; ++j) {
      fin >> grid[i][j];
      cellx[grid[i][j]] = i;
      celly[grid[i][j]] = j;
    }
  }

  for (int r, c, s, tmp, mask, it = 0; it < K; ++it, wins = 0) {
    memset(dp[it & 1], 0, sizeof(dp[it & 1]));
    for (int i = 1; i <= Kii; ++i) {
      fin >> tmp;
      s = it & 1;
      r = cellx[tmp];
      c = celly[tmp];
      mask = (1 << ((r - 1) * N + (c - 1))) |
             (sumL[r - 1] + sumL[r + N]) | (sumC[c - 1] + sumC[c + N]);
      dp[s][mask] = max(dp[s][mask], dp[s ^ 1][mask]);
      sumL[r] |= 1 << (c - 1);
      sumC[c] |= 1 << (r - 1);
      if (r == c)
        diagP |= 1 << (r - 1);
      if (r + c == N + 1)
        diagS |= 1 << (r - 1);
      sum[0][mask] = diagP;
      sum[1][mask] = diagS;
      sum[2][mask] = sumL[r];
      sum[3][mask] = sumC[c];
      for (int j = 0; j < SZ; ++j) {
        if (dp[s][mask] + 2 > dp[s][(mask | sum[j][mask])]) {
          dp[s][(mask | sum[j][mask])] = dp[s][mask] + 2;
          wins = 1;
        }
      }
    }

    if (wins)
      fout << dp[1][(1 << (N * N)) - 1] << '\n';
    else {
      int full = (1 << (N * N)) - 1;
      if (dp[0][full] == dp[1][full])
        fout << 0 << '\n';
      else
        fout << dp[1][(1 << (N * N)) - 1] << '\n';
    }
  }
  return 0;
}
