                max_puncte = puncte[i] + grade[i];
            } else if (puncte[i] + grade[i] == max_puncte)
                castigatori.push_back(i + 1);
        }
        for (int i = 0; i < castigatori.size(); ++i)
            out << castigatori[i] << " ";
    } else if (tip == 2) {
        int castigator = -1;
        for (int i = 0; i < n; ++i) {
            int puncte_max_alte_echipe = 0;
            for (int j = 0; j < n; ++j) {
                if (i != j)
                    puncte_max_alte_echipe = max(puncte_max_alte_echipe, puncte[j] + grade[j]);
            }
            int puncte_echipa_i = puncte[i] + grade[i];
            if (puncte_echipa_i >= puncte_max_alte_echipe) {
                if (castigator != 0) {
                    castigator = 0;
                    break;
                } else
                    castigator = i + 1;
            }
        }
        if (castigator == 0)
            out << 0;
        else
            out << castigator;
    }
    return 0;
