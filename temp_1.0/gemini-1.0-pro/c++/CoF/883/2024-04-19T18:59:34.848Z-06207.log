#include <fstream>
#include <algorithm>

using namespace std;

const int MAXN = 1000;
const int MAXK = 1000;
const int MAXT = 100000;

int n, k, c;
int l[MAXN + 1], r[MAXN + 1];
int t[MAXT + 1];

int main() {
    ifstream fin("cursuri.in");
    ofstream fout("cursuri.out");

    fin >> c;
    fin >> n >> k;

    for (int i = 1; i <= n; i++) {
        fin >> l[i] >> r[i];
    }

    if (c == 1) {
        int ans = 0;

        for (int i = 1; i <= n; i++) {
            bool ok = true;

            for (int j = l[i]; j < r[i]; j++) {
                if (t[j] == k) {
                    ok = false;
                    break;
                }
            }

            if (ok) {
                ans++;

                for (int j = l[i]; j < r[i]; j++) {
                    t[j]++;
                }
            }
        }

        fout << ans << endl;
    } else {
        int ans = r[1] - l[1];

        for (int i = 2; i <= n; i++) {
            int maxr = 0;

            for (int j = l[i]; j < r[i]; j++) {
                maxr = max(maxr, t[j]);
            }

            ans = max(ans, maxr + r[i] - l[i]);
        }

        fout << ans << endl;
    }

    return 0;
}
