#include <fstream>

struct pair {
  int x, y;
};
pair path[2][500][500];
int prev[2][500][500];
int dp[2][500][500];
int sums[500][500];

int max(int a, int b) { return a > b ? a : b; }
int min(int a, int b) { return a < b ? a : b; }
int a(int x, int y) { return sums[x][y] + sums[x - 1][y - 1] - sums[x - 1][y] - sums[x][y - 1]; }
pair topair(int x, int y) { return {x, y}; }

int main() {
  ifstream fin("cri.in");
  ofstream fout("cri.out");
  int n, m, x, y;
  fin >> n >> m >> x >> y;
  for (int i = 0; i < n; ++i)
    for (int j = 0; j < m; ++j) {
      int t;
      fin >> t;
      sums[i + 1][j + 1] = sums[i][j + 1] + sums[i + 1][j] - sums[i][j] + t;
    }
  int Z = (x == 1) ? 2 : 1;
  dp[Z][x][y] = a(!(Z == 1), x, min(m, y));
  prev[Z][x][y] = x;
  for (int j = min(m, y) - 1; j >= 1; --j)
    dp[Z][x][j] = a(!(Z == 1), x, j) + dp[Z][x][j + 1];
  dp[3 - Z][x][y] = a(max(x - 1, 1), !Z ? 1 : max(1, y), m) + dp[Z][x][y];
  for (int i = min(n, x) - 1; i > 1 || !Z; --i)
    dp[3 - Z][i][y] = a(max(i - 1, 1), max(1, y) - !Z, m) + dp[3 - Z][i + 1][y];
  for (int i = max(x - 1, 1); i >= 1; --i) {
    dp[3 - Z][i][y] = max(dp[3 - Z][i][y], a(i, !Z ? 1 : max(1, y), m) + dp[3 - Z][i + 1][y]);
    dp[3 - Z][i][y] = max(dp[3 - Z][i][y], a(i, 1, max(y - 1, 1)) + dp[3 - Z][i][y + 1]);
  }
  dp[3 - Z][n][y] = a(n, !Z ? 1 : max(1, y), m) + dp[3 - Z][n][y];
  for (int j = min(m, y) - 1; j >= 1; --j)
    dp[3 - Z][n][j] = max({a(n, !Z ? 1 : max(1, j), m) + dp[3 - Z][n][j + 1], dp[3 - Z][n][j]});
  int T = dp[3 - Z][x][y];
  fout << Z << ' ' << T << ' ';
  int K = 1;
  for (int x = (Z == 2) ? max(1, x - 1) : x, y = (Z == 2) ? min(m, y) : y; x != prev[Z][x][y] || y != (m + 1 - Z); K++) {
    if (dp[Z][x][y] == dp[Z][x][y - 1] + a(!(Z == 1), x, y - 1)) {
      path[0][K] = topair(x, y);
      y--;
    } else
      path[0][K] = topair(x, y), x++;
  }
  X = path[0][100];
  for (y = max(1, X.y - 1); y != (m + 1 - Z); K++) {
    if (dp[3 - Z][X.x][y] == dp[3 - Z][X.x][y - 1] + a(max(X.x - 1, 1), max(1, y - 1), m)) {
      path[1][K] = topair(X.x, y);
      y--;
    } else
      path[1][K] = topair(X.x, y), X.x++;
  }
  fout << K << '\n';
}
