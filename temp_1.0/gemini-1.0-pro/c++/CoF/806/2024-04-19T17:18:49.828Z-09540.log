    if (z == 2 && (ii < x || ii > n || jj < 1 || jj > y)) continue;
    if (z == 3 && (ii < x || ii > n || jj < y || jj > m)) continue;

    int dzz = ((mask >> 1) & (((1 << dir) | (1 << (dir ^ 2))) & mask) != 0);

    if (dzz && dz) res = max(res, dfs(z, i, j, mask));
    int nmask = (mask << 1) & (L - 1);
    if (dir == 3) nmask ^= 1;
    nmask |= dzz;
    res = max(res, grid[i][j] + dfs(z, ii, jj, nmask));
  }

  return res;
}

int main() {
  seen = 0;
  n = read();
  m = read();
  x = read();
  y = read();

