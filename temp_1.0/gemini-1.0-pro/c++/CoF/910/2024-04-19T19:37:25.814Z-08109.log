#include <fstream>
#include <vector>
#include <cstdio>
#include <limits.h>

class Notiuni_elementare {
  // Instructiuni de la compilare:
  //  g++ -O3 poarta.cpp -o poarta
  //  ./poarta

 public:
  using ll = unsigned long long;

  static ll compute_minimum_squares_needed_to_get_to_destination(
      ll N, ll P, ll& numar_sarituri) {
    if (P == 1) {
      numar_sarituri = 1;
      return 0;
    }
    Notiuni_elementare aplicare_notiuni;
    aplicare_notiuni.N = N;
    vector<ll> t_dp = aplicare_notiuni.populeaza_tabel_dinamic(P);

    numar_sarituri = aplicare_notiuni.compute_minimum_jumps(P, t_dp);
    return t_dp[P];
  }

  ll N;
  vector<ll> t_dp;

 private:
  ll populeaza_tabel_dinamic(ll P) {
    t_dp.resize(P + 1, numeric_limits<ll>::max());
    vector<ll> f2(P + 1);

    t_dp[1] = 0;
    if (P % N == 0) t_dp[P] = 2;
    if (N % 2 == 0) {
      for (ll putere = 2; putere < P + 1; putere++) {
        f2[putere] = (N * putere / 2) % N;
      }
    }
    // Populeaza tabelul dinamic pana la P
    for (ll i = 2; i < P + 1; i++) {
      ll f1_i =
          t_dp[i - 1] == numeric_limits<ll>::max() ? LLONG_MAX : t_dp[i - 1] + 1;
      // Caz optim:
      //  cand nu se consuna din poțiune, iar i este multul lui N (i = 2N, 3N,
      //  4N, ...)
      ll f2_i =
          t_dp[f2[i]] == numeric_limits<ll>::max() ? LLONG_MAX
                                                      : t_dp[f2[i]] + 2;

      if (f1_i <= f2_i)
        t_dp[i] = f1_i;
      else
        t_dp[i] = f2_i;
    }

    return t_dp[P];
  }

  // Returneaza numarul minim de salturi
  ll compute_minimum_jumps(ll P, vector<ll>& t_dp) {
    ll numar_sarituri = 0;
    ll i = P;
    while (i >= 1) {
      if (t_dp[i] == t_dp[i - 1] + 1) {
        t_dp[i - 1] = LLONG_MAX;
        i = i - 1;
      } else {
        t_dp[i / 2] = LLONG_MAX;
        i = i / 2;
      }
      numar_sarituri++;
    }
    return numar_sarituri;
  }
};

int main() {
  ifstream f("poarta.in");
  ofstream g("poarta.out");
  ll C, N, P, numar_sarituri;
  f >> C;
  f >> N >> P;

  Notiuni_elementare aplicare_notiuni;
  if (C == 1) {
    g << aplicare_notiuni.compute_minimum_jumps(P, aplicare_notiuni.t_dp)
      << "\n";
  } else {
    aplicare_notiuni.compute_minimum_squares_needed_to_get_to_destination(
        N, P, numar_sarituri);
    g << aplicare_notiuni.t_dp[P] << "\n";
  }

  f.close();
  g.close();
  return 0;
}
