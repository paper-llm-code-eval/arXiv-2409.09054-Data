#include <fstream>
#include <map>
#include <vector>
using namespace std;
ifstream f("pagini.in");
ofstream g("pagini.out");
int n, v[100010], vf[100010], ans;
pair<int, int> sol = { -1, -1 };
int main() {
  f >> n;
  for (int i = 0; i < n; i++) {
    f >> v[i];
    if (!vf[v[i]])
      ans++;
    vf[v[i]] = 1;
  }
  map<int, vector<int>> mp;
  for (int i = 0; i < n; i++)
    mp[v[i]].push_back(i);
  for (const auto &x : mp) {
    vector<int> pages = x.second;
    int l = 0;
    while (l < pages.size()) {
      int r = l;
      int actual_pages = 0;
      while (r < pages.size() &&
             pages[r] - pages[l] <= pages.size() - r) {
        actual_pages = r - l + 1;
        r++;
      }
      if (actual_pages > sol.second)
        sol = { x.first, actual_pages };
      l = r;
    }
  }
  g << ans << '\n' << sol.second;
  return 0;
}
