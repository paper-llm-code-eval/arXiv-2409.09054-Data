#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream f("pagini.in");
ofstream g("pagini.out");

const int MAXN = 1e5 + 3;

int n, a[MAXN], fr[MAXN], poz[MAXN], cel_mai_mare, nr_agrafe, maxi;

int main() {
  f >> n;
  for (int i = 1; i <= n; i++) {
    f >> a[i];
    fr[a[i]]++;
  }

  for (int i = 1; i <= n; i++) {
    if (!fr[i]) {
      g << -1;
      return 0;
    }
    poz[a[i]] = i;
  }

  int i = 1;
  while (i <= n) {
    if (!fr[i]) {
      i++;
      continue;
    }
    int j = i;
    cel_mai_mare = 0;
    while (fr[j]) {
      cel_mai_mare = max(cel_mai_mare, poz[j]);
      fr[j]--;
      j++;
    }

    maxi = max(maxi, cel_mai_mare - i + 1);
    nr_agrafe++;
    i = j;
  }

  g << nr_agrafe << "\n" << maxi;
}
```
