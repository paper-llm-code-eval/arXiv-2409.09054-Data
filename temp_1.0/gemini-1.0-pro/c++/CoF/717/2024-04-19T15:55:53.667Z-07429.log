#include <fstream>

int main() {
    std::ifstream in("sir.in");
    std::ofstream out("sir.out");
    unsigned long long n;
    in >> n;
    unsigned long long b = 1ull << n;
    b -= (1ull << 1);
    b += (1ull << ((n % 2 == 1) ? 2 : 3));
    while (b % 2 == 0)
        b >>= 1;
    std::string s = (n == 1) ? "a" : (b & 1) ? "ba" : "b";
    while (b >>= 2) {
        if (b & 1)
            s = s + 'a' + ((s.length() + 1) >> 1) + ((b >> 1) & 1);
        else
            s = s + 'a' + ((s.length() + 1) >> 2) + ((b >> 1) & 1);
    }
    std::reverse(s.begin(), s.end());
    out << s;
    return 0;
}
