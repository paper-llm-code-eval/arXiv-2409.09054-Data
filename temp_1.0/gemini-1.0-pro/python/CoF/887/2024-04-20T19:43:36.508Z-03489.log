def find_letter(k, s, n):
    ans = ''
    cnt = 0

    if n <= 2 * k:
        if n <= k:
            ans = s[n - 1]
            cnt = n - 1
        else:
            ans = s[k - (n - k) - 1]
            cnt = k - (n - k)
        return ans, cnt

    sk = s + s.lower() + s.upper()
    m = len(sk)
    num = n // m
    rem = n % m
    ans = sk[(rem - 1)]
    if ans.islower():
        cnt += num * k + (rem - 1)
    else:
        cnt += num * k + (k - (rem - 1))
    return ans, cnt


