with open('alee.in', 'r') as f:
    n, m = map(int, f.readline().split())
    trees = {tuple(map(int, f.readline().split())): True for _ in range(m)}

with open('alee.out', 'w') as f:
    queue = {(n, 1): (0, 1), (1, n): (1, 1)}  # (pos, direction)
    distance = dict()  # pos: (dist, direction)
    while queue:
        pos, (dist, direction) = queue.popitem()
        if pos in trees:
            continue
        if distance.get(pos, (float('inf'), 0)) > (dist, direction):
            distance[pos] = (dist, direction)
            
            next_pos = (pos[0], pos[1]-1)  # left
            if direction != 2 and not (next_pos in trees or next_pos in queue):
                queue[next_pos] = (dist+1, 0)
            
            next_pos = (pos[0]-1, pos[1])  # up
            if direction != 3 and not (next_pos in trees or next_pos in queue):
                queue[next_pos] = (dist+1, 1)
            
            next_pos = (pos[0], pos[1]+1)  # right
            if direction != 0 and not (next_pos in trees or next_pos in queue):
                queue[next_pos] = (dist+1, 2)
            
            next_pos = (pos[0]+1, pos[1])  # down
            if direction != 1 and not (next_pos in trees or next_pos in queue):
                queue[next_pos] = (dist+1, 3)
            
    end_pos = (n, 8) if direction[0] == 0 else (8, n)
    f.write(str(distance[end_pos][0]))
