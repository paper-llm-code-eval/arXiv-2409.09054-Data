import sys

sys.setrecursionlimit(100020)

MAX_N = 100040

mod = 10**9 + 9
fact = [1] * (MAX_N + 2)

def solve(l: list[int], n: int) -> int:
    if n == 2:
        return l[1] - l[0] + 1
    m1 = l[n - 1] - l[n - 2] + 1
    m2 = l[n - 2] - l[n - 3] + 1
    ct = (m1 + m2) % mod
    return (ct - 1 + mod) % mod

def binpow(base: int, p: int) -> int:
    if p == 0:
        return 1
    if p == 1:
        return base
    res = binpow(base, int(p / 2))
    res = (res * res) % mod
    if p % 2:
        res = (res * base) % mod
    return res % mod

def comb(n: int, k: int) -> int:
    if n < k:
        return 0
    return ((fact[n] * binpow(fact[k] * fact[n - k], mod - 2)) % mod) % mod


with open("fotbal.in", "r") as fin, open("fotbal.out", "w") as fout:
    n, k = map(int, fin.readline().split())
    if k == 2:
        fout.write(str(solve([0] + list(sorted(list(set(map(int, fin.readline().split()) for _ in range(n))))), n + 1)) % mod)
        exit(0)

    st = [0]
    dr = [0]
    for _ in range(n):
        a, b, p = map(int, fin.readline().split())
        if not p:
            st.append(a); st.append(b)
        else:
            dr.append(a); dr.append(b)
    st.sort(); dr.sort()
    st = list(set(st))
    dr = list(set(dr))

    fout.write(str(comb(len(st) + len(dr) - 1, len(st) - 1)) % mod)
