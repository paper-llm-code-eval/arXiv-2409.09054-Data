                    nm[i][b - 1 + (s - nr_clusteri_libere_pe_pista[i])] = 1 if nm[i][b - 1 + (s - nr_clusteri_libere_pe_pista[i])] == -1 else nm[i][b - 1 + (s - nr_clusteri_libere_pe_pista[i])] + 1

            for i in range(p):
                print(lst[nm[i]])
                f_out.write(str(nm[i][0]) + ' ')
