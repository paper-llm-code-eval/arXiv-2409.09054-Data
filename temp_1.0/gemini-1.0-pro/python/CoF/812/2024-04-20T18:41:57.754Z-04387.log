with open('litere.in', 'r') as fin, open('litere.out', 'w') as fout:
    n = int(fin.readline())
    word = fin.readline().strip()
    m = int(fin.readline())
    codes = list(map(int, fin.readline().split()))

    vowels = set('AEIOU')
    consonants = set('BCDFGHJKLMNPQRSTVWXYZ')
    count_voc_cons = 0
    vocals = []
    i = 0
    for letter in word:
        i += 1
        score = codes[ord(letter) - ord('A')]
        if letter in vowels:
            vocals.append(i)
        else:
            for v in vocals:
                if 1 <= i - v <= 2:
                    count_voc_cons += 1
        
    result = codes.copy()
    for i in range(1, n):
        j = i
        while j <= n:
            result[j - 1] = result[j]
            j += 1
        result[n] = 0
    
    k1 = 0
    for i in range(n):
        k1 += result[i]

    suma = k1
    for i in range(1, n):
        result2 = codes.copy()
        j = i + 1
        while j <= n:
            result2[j - 1] = result2[j]
            j += 1
        result2[n] = 0
        
        k = 0
        for i in range(n):
            k += result2[i]
        suma += k
    
    fout.write(f'{count_voc_cons}\n{k1}\n{suma}\n')
