with open('startrek.in') as fin, open('startrek.out', 'w') as fout:
    n, t = map(int, fin.readline().split())
    d = list(map(int, fin.readline().split()))
    v = list(map(int, fin.readline().split()))
    
    d_v = [(d[i] / v[i], i) for i in range(n)]
    
    # sortează proiectilele dupa timpul pana ajung la nava
    d_v.sort()
    
    # contorizează cate proiectile pot fi distruse
    ans = 0
    timp = 0
    
    for timp_ajungere, i in d_v:
        # verifica daca nava laser poate fi reincarcata la timp
        if timp + t <= timp_ajungere:
            timp = timp_ajungere + t
            ans += 1
    
    fout.write(str(ans) + '\n')
```
