import sys

with open('deminare.in', 'r') as f_in, open('deminare.out', 'w') as f_out:
    v = f_in.readline().strip()
    l, c = map(int, f_in.readline().split())
    m = int(f_in.readline())

    lins = c * [0]
    cols = l * [0]
    mine = []

    for _ in range(m):
        x, y = map(int, f_in.readline().split())
        mine.append((x, y))
        lins[x-1] += 1
        cols[y-1] += 1

    if v == '1':
        f_out.write(' '.join(str(i+1) for i in range(len(lins)) if lins[i] == max(lins)))
    elif v == '2':
        mine.sort()

        last_in_line = dict()
        min_lines = sys.maxsize
        min_cols = sys.maxsize
        first_line = -1

        for el in mine:
            x, y = el

            if y not in last_in_line:
                last_in_line[y] = set()
            if x not in last_in_line[y]:
                last_in_line[y].add(x)

            last_x = last_in_line[y]

            min_lines = min(min_lines, max(last_x) - min(last_x) + 1)
            if x == min(last_x) or y == min(last_in_line):
                if first_line == -1:
                    first_line = x
                min_cols = min(min_cols, x - first_line + 1)

        if min_lines * min_cols != m:
            f_out.write('-1')
        else:
            f_out.write(str(m - min_lines * min_cols))
