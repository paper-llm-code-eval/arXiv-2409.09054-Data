def read_data():
    with open('magic.in') as f:
        n = int(f.readline())
        characters = f.readline().strip().split()
    return n, characters


def find_magic_word(characters):
    magic_word = ''
    for character in characters:
        if character.isalpha():
            magic_word += character.upper()
    return magic_word


def find_magic_number(characters):
    digits = []
    for character in characters:
        if character.isdigit():
            digits.append(character)
    digits = list(set(digits))
    digits.sort()
    magic_number = int(''.join(digits))
    return magic_number


def write_output(magic_word, magic_number):
    with open('magic.out', 'w') as f:
        f.write(magic_word + '\n')
        f.write(str(magic_number) + '\n')


if __name__ == '__main__':
    n, characters = read_data()
    magic_word = find_magic_word(characters)
    magic_number = find_magic_number(characters)
    write_output(magic_word, magic_number)
```
