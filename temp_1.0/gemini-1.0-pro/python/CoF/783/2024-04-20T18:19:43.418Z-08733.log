# Se citesc dimensiunile matricei
with open('placare.in', 'r') as f:
    n, m = map(int, f.readline().split())
    matrice = []  # Matricea care va stoca codul placării

# Se citesc codurile de pe fiecare linie
for _ in range(n):
    linie = list(map(int, f.readline().split()))
    matrice.append(linie)

# Se creează matricea de imaginea placării
imagine = [[0] * m for _ in range(n)]

# Se parcurge matricea de coduri pentru a genera imaginea placării
for i in range(n):
    for j in range(m):
        cod = matrice[i][j]
        if cod > 0:
            # Este o placă orizontală
            for k in range(j, j + cod):
                imagine[i][k] = cod
        else:
            # Este o placă verticală
            for k in range(i - abs(cod) + 1, i + 1):
                imagine[k][j] = abs(cod)

# Se scrie matricea de imagine în fișierul de ieșire
with open('placare.out', 'w') as f:
    for linie in imagine:
        for element in linie:
            f.write(f'{element} ')
        f.write('\n')
```
