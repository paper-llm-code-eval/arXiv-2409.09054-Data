def read_data(file):
    with open(file, 'r') as f:
        n, m = map(int, f.readline().split())
        board = [list(map(int, line.split())) for line in f.readlines()]
    return n, m, board


def dfs(i, j, board, n, m, visited, cnt):
    if i < 0 or i >= n or j < 0 or j >= m or visited[i][j] == 1:
        return cnt

    if board[i][j] == cnt % 2:
        visited[i][j] = 1
        cnt += 1
        cnt = max(dfs(i + 1, j, board, n, m, visited, cnt), dfs(i - 1, j, board, n, m, visited, cnt),
                  dfs(i, j + 1, board, n, m, visited, cnt), dfs(i, j - 1, board, n, m, visited, cnt))
        visited[i][j] = 0

    return cnt


if __name__ == '__main__':
    n, m, board = read_data('fadema.in')
    visited = [[0] * m for _ in range(n)]

    max_cnt = 0
    for i in range(n):
        for j in range(m):
            max_cnt = max(max_cnt, dfs(i, j, board, n, m, visited, 0))

    if max_cnt == 0:
        max_cnt = 1

    with open('fadema.out', 'w') as f:
        f.write(str(max_cnt))
```
