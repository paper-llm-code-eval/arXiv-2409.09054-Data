n, m = map(int, open("fadema.in").readline().split())
mat = [list(map(int, i)) for i in open("fadema.in").read().splitlines()]

def valid(i, j):
    return i >= 0 and i < n and j >= 0 and j < m and mat[i][j] != -1

mx = 1
for i in range(n):
    for j in range(m):
        if mat[i][j] != -1:
            q = [(i, j)]
            mat[i][j] = -1
            cnt = 1
            while q:
                x, y = q.pop()
                for dx, dy in ((-1, 0), (1, 0), (0, -1), (0, 1)):
                    nx, ny = x + dx, y + dy
                    if valid(nx, ny) and mat[nx][ny] != -1 and mat[x][y] != mat[nx][ny]:
                        mat[nx][ny] = -1
                        cnt += 1
                        q.append((nx, ny))
            mx = max(mx, cnt)

with open("fadema.out", "w") as f:
    f.write(str(mx))
```
