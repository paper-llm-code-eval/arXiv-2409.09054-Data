def read_input(file_name):
    with open(file_name, 'r') as file:
        p = int(file.readline())
        s = file.readline().strip()
        s = list(s)
    return p, s

def write_output(file_name, output):
    with open(file_name, 'w') as file:
        for line in output:
            file.write(line + '\n')

def solve_part1(s):
    max_len = 0
    current_len = 0
    for i in range(1, len(s)):
        if s[i] != s[i - 1]:
            current_len += 1
        else:
            current_len = 0
        max_len = max(max_len, current_len)
    return max_len

def solve_part2(s):
    # initialize f array
    f = [i for i in range(1, 27)]

    dp = []
    for i in range(len(s)):
        dp.append([0] * 2)

    dp[0][0] = f[ord(s[0]) - ord('a')]
    dp[0][1] = 0

    for i in range(1, len(s)):
        dp[i][0] = max(dp[i-1][0], dp[i-1][1] + f[ord(s[i]) - ord('a')])
        dp[i][1] = max(dp[i-1][1], dp[i-1][0] - f[ord(s[i]) - ord('a')])

    ans = dp[len(s) - 1][0]

    # reconstruct the sequence
    t = []
    i = len(s) - 1
    while i >= 0:
        if dp[i][0] > dp[i][1]:
            t.append(s[i])
            i -= 1
        else:
            i -= 2
    t = ''.join(t[::-1])

    return t, ans

if __name__ == '__main__':
    p, s = read_input('charlie.in')
    if p == 1:
        output = [str(solve_part1(s))]
    else:
        t, ans = solve_part2(s)
        output = [t, str(ans)]
    write_output('charlie.out', output)
