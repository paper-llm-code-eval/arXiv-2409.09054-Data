f = open('triunghiuri.in')
p, n = map(int, f.readline().split())
x = [0] * (1000 + 5)
y = [[] for _ in range(1000 + 5)]
mxX = 0
mxY = 0
for i in range(n):
    xi, yi = map(int, f.readline().split())
    x[xi] += 1
    y[yi].append(xi)
    mxX = max(mxX, x[xi])
    mxY = max(mxY, yi)
f.close()

if p == 1:
    print(mxX)
elif p == 2:
