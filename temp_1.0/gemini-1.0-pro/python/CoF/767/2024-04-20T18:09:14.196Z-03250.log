from collections import defaultdict
N, C = map(int, open('dreptc.in').readline().split())
x = [0] * N
y = [0] * N
c = [0] * N
for i in range(N):
    x[i], y[i], c[i] = map(int, open('dreptc.in').readline().split())
ans = 0
for col in range(C):
    cntx = defaultdict(int)
    cnty = defaultdict(int)
    for i in range(N):
        if c[i] == col:
            cntx[x[i]] += 1
            cnty[y[i]] += 1
    for XC in cntx:
        for YC in cnty:
            ans += cntx[XC] * cnty[YC] - 1
print(ans, file=open('dreptc.out', 'w'))
```
