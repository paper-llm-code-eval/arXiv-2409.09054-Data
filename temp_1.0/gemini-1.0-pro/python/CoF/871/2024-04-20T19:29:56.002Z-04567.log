                    if (nx, ny) in graf:
                        graf[(x, y)].add((nx, ny))
                        graf[(nx, ny)].add((x, y))

        # Calculeaza dimensiunea celei mai mari componente conexe din graf
        viz = set()

        def dfs(x, y):
            if (x, y) in viz:
                return
            viz.add((x, y))
            for vecin in graf[(x, y)]:
                dfs(vecin[0], vecin[1])

        celule_max = 0
        for nod in graf:
            if nod not in viz:
                dfs(nod[0], nod[1])
                celule = len(viz)
                celule_max = max(celule_max, celule)

        return celule_max

    if cerinta == 1:
        celule, celule_galerie = max_celule_o_cartita()
    else:
        celule_max = max_celule_o_galerie()

    if cerinta == 1:
