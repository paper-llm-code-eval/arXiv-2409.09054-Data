                    cuvinte_sterse.append(nr_cuv2)

    fout.write(f'{cuvinte_sterse[0] if cuvinte_sterse else 0}\n')
    for cuv, nr_cuv in cuvinte:
        if nr_cuv not in cuvinte_sterse:
            fout.write(f'{nr_cuv} ')
