import sys

input_path = 'vecine.in'
output_path = 'vecine.out'

def num_consecutive(L):
    ans = 0
    for i in range(len(L) - 1):
        if L[i] + 1 == L[i+1]:
            ans += 1
    return ans

def max_consecutive(L):
    ans = [0]
    idx = 0
    for i in range(len(L) - 1):
        if L[i] + 1 == L[i+1]:
            if ans[idx] < L[i]:
                ans[idx] = L[i]
            idx += 1
            ans.append(0)
        ans[idx] = max(ans[idx], L[i])
    ans.sort(reverse=True)
    return ans[0]

with open(input_path, 'r') as f:
