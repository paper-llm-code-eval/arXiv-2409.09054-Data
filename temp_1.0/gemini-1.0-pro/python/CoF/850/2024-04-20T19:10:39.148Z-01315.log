    q = deque([(x, y)])
    vis[x][y] = True
    area = 1

    while q:
        x, y = q.popleft()

        for dx, dy in [(0, 1), (1, 0), (-1, 0), (0, -1)]:
            nx, ny = x + dx, y + dy
            if 0 <= nx < m and 0 <= ny < n and not vis[nx][ny] and field[nx][ny] == color:
                q.append((nx, ny))
                vis[nx][ny] = True
                area += 1

