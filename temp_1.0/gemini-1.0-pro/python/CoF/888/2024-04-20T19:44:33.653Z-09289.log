            if matrix[x][y + 1] < G:
                dfs(x, y + 1, cost + 1)
            else:
                dfs(x, y + 1, cost)

            if matrix[x + 1][y] < G:
                dfs(x + 1, y, cost + 1)
            else:
                dfs(x + 1, y, cost)

            if matrix[x][y - 1] < G:
                dfs(x, y - 1, cost + 1)
            else:
                dfs(x, y - 1, cost)

            visited.remove((x, y))

