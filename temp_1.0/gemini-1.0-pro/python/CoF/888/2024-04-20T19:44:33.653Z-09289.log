        N, G = map(int, f.readline().split())
        matrix = [[int(num) for num in line.split()] for line in f]

        min_cost = float('inf')
        visited = set()

        def dfs(x, y, cost):
            nonlocal min_cost
            if (x, y) == (N, N):
