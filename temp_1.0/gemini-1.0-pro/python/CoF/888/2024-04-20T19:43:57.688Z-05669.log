import sys

fin = open('rover.in', 'r')
fout = open('rover.out', 'w')
V = int(fin.readline())
if V == 1:
    N, G = map(int, fin.readline().split())
    m = [[int(x) for x in line.split()] for line in fin.readlines()]
    for i in range(N):
        for j in range(N):
            if m[i][j] < G:
                m[i][j] = -1
    dp = [[(0, N**2) for _ in range(N)] for _ in range(N)]  # cost to get to (i, j), number of dangers passed.
    dp[0][0] = 0, 0  # we always start with no dangers passed.
    directions = ((-1, 0), (0, 1), (1, 0), (0, -1))
    for i in range(N):
        for j in range(N):
            for di, dj in directions:
                if 0 <= i + di < N and 0 <= j + dj < N:  # Check if in bounds.
                    cost, dangers = dp[i][j]
                    c, d = dp[i + di][j + dj]
                    if m[i + di][j + dj] != -1 or (i + di, j + dj) == (N-1, N-1):  # if not dangerous or goal.
                        if cost + 1 < c:  # improve.
                            dp[i + di][j + dj] = cost + 1, dangers
                    else:  # if dangerous.
                        if cost + 1 < c:  # improve.
                            dp[i + di][j + dj] = cost + 1, dangers + 1
    res, _ = dp[N - 1][N - 1]
    fout.write(str(res) + '\n')
else:
    N = int(fin.readline())
    m = [[int(x) for x in line.split()] for line in fin.readlines()]
    max_weight = N*N*10000 + 1  # impossible.
    for i in range(N):
        for j in range(N):
            max_weight = min(max_weight, m[i][j])  # find the minimum stability.
    fout.write(str(max_weight) + '\n')

fin.close()
fout.close()
