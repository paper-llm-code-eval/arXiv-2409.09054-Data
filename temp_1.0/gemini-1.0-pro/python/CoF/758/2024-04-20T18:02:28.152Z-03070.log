        f.write('DA\n')
        # Decode the message and write it to the output file
        for i in range(0, len(data), 8):
            byte = data[i:i+8]
            byte = byte[:7] + str(parities[i])
            char = chr(int(byte, 2))
            if char == '\n':
                char = '\\n'
            f.write(char)
    else:
        f.write('NU\n')
        # Write the indices of the characters with incorrect parity to the output file
        for i in range(len(parities)):
            if parities[i] != int(data[i+8]):
                f.write(str(i+1) + ' ')
