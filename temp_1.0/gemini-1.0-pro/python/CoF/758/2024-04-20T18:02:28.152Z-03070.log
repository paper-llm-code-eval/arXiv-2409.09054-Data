# Read the input
with open('paritate.in', 'r') as f:
    data = f.read()

# Check if the number of bits is a multiple of 8
if len(data) % 8 != 0:
    with open('paritate.out', 'w') as f:
        f.write('NU')
        exit()

# Calculate the parity of each byte
parities = []
for i in range(0, len(data), 8):
    byte = data[i:i+8]
    parity = 0
    for bit in byte:
        parity += int(bit)
    parities.append(parity % 2)

# Check if the parity of each byte is correct
correct = True
for i in range(len(parities)):
    if parities[i] != int(data[i+8]):
        correct = False

# Write the output
