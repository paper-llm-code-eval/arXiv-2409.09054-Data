with open("joc.in") as f:
    C = int(f.readline())
    N, K = map(int, f.readline().split())
    runda = list()
    for _ in range(K):
        runda.append([int(x) for x in f.readline().split()])
tp, sb = 0, 0
maxm = -1
for t in runda:
    dpH = [[0 for _ in range(N)] for _ in range(N)]
    dpV = [[0 for _ in range(N)] for _ in range(N)]
    dpD = [[0 for _ in range(N)] for _ in range(N)]
    dpDD = [[0 for _ in range(N)] for _ in range(N)]
    for px, x in enumerate(t):
        y = (x-1) // N
        if not dpH[y][px]:
            tpH[y][px] = 1
            for i in range(px+1, N):
                if t[i] == x:
                    tpH[y][i] = 1
            if 0 <= y-1 < N:
                for i in range(0, px):
                    tpH[y-1][i] += tpH[y][px]
        if not dpV[px][y]:
            dpV[px][y] = 1
            for i in range(y+1, N):
                if t[i*N + px] == x:
                    dpV[i][px] = 1
            if 0 <= px-1 < N:
                for i in range(0, y):
                    dpV[i][px-1] += dpV[px][y]
        if not dpD[y][px]:
            dpD[y][px] = 1
            if 0<= y-1 < N and 0 <= px-1 < N:
                for i, j in zip(range(px+1, N), range(y+1, N)):
                    if t[i*N + j] == x:
                        dpD[i][j] = 1
            if px == y and 0 <= y-1 < N:
                for i in range(px+1, N):
                    dpD[i][i] = 1
        if not dpDD[y][px]:
            dpDD[y][px] = 1
            if 0 <= y-1< N and px+1 < N:
                for i, j in zip(range(px, 0, -1), range(y+1, N)):
                    if t[i*N + j] == x:
                        dpDD[i][j] = 1
            if y+px == N-1 and 0 <= y+1< N:
                dpDD[y+1][y+1] = 1
        if x % 2 == 0:
            sb += dpD[y][px] + dpDD[y][px] + dpH[y][px] + dpV[px][y]
        else:
            tp += dpD[y][px] + dpDD[y][px] + dpH[y][px] + dpV[px][y]
    maxm = max(maxm, max([x for f in [dpH, dpV, dpD, dpDD] for x in f for g in x]))
with open("joc.out", "w") as f:
    if C == 1:
        f.write(f"{tp} {sb}\n")
    elif C == 2:
        f.write(f"{maxm}\n")
