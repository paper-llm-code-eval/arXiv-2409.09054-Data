with open('poarta.in', 'r') as f:
    nr_echipaje = int(f.readline().strip())
    echipaje = []
    for _ in range(nr_echipaje):
        r1, c1, r2, c2 = map(int, f.readline().strip().split())
        echipaje.append((r1 - 1, c1 - 1, r2 - 1, c2 - 1))

matrice = [[False for _ in range(250)] for _ in range(250)]
for r1, c1, r2, c2 in echipaje:
    matrice[r1][c1] = True
    matrice[r2][c2] = True

def bfs(r_start, c_start):
    vizitate = set()
    coada = [(r_start, c_start, 0)]
    while coada:
        r, c, distanta = coada.pop(0)
        if (r, c) in vizitate:
            continue
        vizitate.add((r, c))
        if matrice[r][c]:
            return distanta
        for x, y in [(r - 1, c), (r + 1, c), (r, c + 1), (r, c - 1)]:
            if 0 <= x < 250 and 0 <= y < 250 and (x, y) not in vizitate:
                coada.append((x, y, distanta + 1))
    return -1

distanta_minima = 0x3f3f3f3f
for r1, c1, r2, c2 in echipaje:
    distanta_minima = min(distanta_minima, bfs(r1, c1) + bfs(r2, c2))

with open('poarta.out', 'w') as f:
    f.write(str(distanta_minima))
```
