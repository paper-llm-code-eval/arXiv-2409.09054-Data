import sys

K = 30
dp = [[0 for i in range(K + 1)] for j in range(100001)]
maks = 0
L = []
with open("blis.in") as f:
    k = int(f.readline())
    sir = list(map(int, list(f.readline().strip())))
for j in range(len(sir) - 1, -1, -1):
    for i in range(k, -1, -1):
        dp[j][i] = max(dp[j + 1][i], sir[j] + dp[j + 1][i - 1])
        if dp[j][i] == dp[j + 1][i]:
            L.append((0, j))
        else:
            L.append((1, j))
    maks = max(maks, dp[j][0])

with open("blis.out", "w") as f:
    f.write(str(maks) + "\n")
    f.write(str(len(L) - L.count((0, -1))) + "\n")
```
