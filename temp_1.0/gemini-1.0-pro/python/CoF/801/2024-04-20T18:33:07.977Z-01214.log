import sys

with open("cladiri.in", "r") as f_in:
    centru_x, centru_y, intensitate = map(int, f_in.readline().split())
    cladiri = []
    for linie in f_in.readlines():
        x, y, rez = map(int, linie.split())
        cladiri.append((x, y, rez))

afectate = 0
max_nivel = 0
celule = [0] * intensitate
for cx, cy, rez in cladiri:
    dist_x = abs(centru_x - cx)
    dist_y = abs(centru_y - cy)
    nivel = max(dist_x, dist_y)
    if rez <= intensitate - nivel:
        afectate += 1
        celule[nivel] += 1
        max_nivel = max(max_nivel, nivel)

with open("cladiri.out", "w") as f_out:
    f_out.write(str(afectate) + "\n")
    f_out.write(str(max(celule)) + "\n")
    nivele = []
    for i in range(intensitate):
        if celule[i] == max(celule):
            nivele.append(i)
    f_out.write(" ".join(map(str, nivele)))
```
