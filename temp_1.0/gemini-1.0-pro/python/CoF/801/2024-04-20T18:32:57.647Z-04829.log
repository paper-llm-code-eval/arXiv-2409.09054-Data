    if numar_cladiri_afectate == max_cladiri:
        niveluri.append(nivel)

with open("cladiri.out", "w") as f:
    f.write(str(numar_cladiri) + "\n")
    f.write(str(max_cladiri) + "\n")
    f.write(" ".join(map(str, niveluri)))
