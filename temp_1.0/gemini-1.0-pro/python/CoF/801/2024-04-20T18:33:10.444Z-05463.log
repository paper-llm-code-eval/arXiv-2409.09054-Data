with open('cladiri.in') as f:
    sc = f.readline().strip().split()
    c = [int(i) for i in sc]
    cv = [(int(i), *map(int, f.readline().strip().split())) for i in range(int(f.readline().strip()))]

lvls = []
vl = 1
m = 0
mx = 0
sc = 0
for v in cv[:]:
    l, lon, lat, rez = v
    d = abs(c[0] - lon) + abs(c[1] - lat)
    if d <= c[2] and rez <= c[2] - d:
        lv = d
        cv.remove(v)
        if vl == lv:
            sc += 1
        else:
            vl = lv
            lvls += [[sc, lv]]
            sc = 1
        if sc > m:
            m = sc
            mx = lv
if sc:
    lvls += [[sc, lv]]
if not m:
    m = 1
    mx = 0
sort_lvls = [lvl for lvl in lvls if lvl[0] == m]
sort_lvls.sort(key=lambda x: x[1])
with open('cladiri.out', 'w') as f:
    f.write(f'{len(cv)}\n')
    f.write(f'{m}\n')
    for lvl in sort_lvls:
        f.write(f'{lvl[1]} ')
```
