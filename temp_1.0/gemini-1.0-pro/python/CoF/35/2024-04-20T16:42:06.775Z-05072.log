import bisect
import sys

nrmutari = 0
n = None
pp = None
bk = None
v = None

def back(p, v1, v2):
    global nrmutari, pp, bk
    if p == n + 1:
        nrmutari += 1
        if pp != -1 and v1[1:] == v[pp + 1:] and v2[1:] == v[pp - n: pp]:
            bk = v2
    else:
        f1 = bisect.bisect_left(v2, v1[p])
        for i in v2[f1:]:
            if i > v1[p]:
                break
            v1[p], v2[f1] = v2[f1], v1[p]
            back(p + 1, v1, v2)
            v1[p], v2[f1] = v2[f1], v1[p]

def read_input():
    global n, pp, bk, v
    with open("biperm.in", "r") as f:
        n = int(f.readline())
        v = [0 for i in range(2 * n + 1)]
        for i in range(2):
            v[1:] = list(map(int, f.readline().split()))
            p = v[1:].index(1) + 1
            if i == 0:
                pp = p
        bk = [0 for i in range(2 * n + 1)]

def write_output():
    with open("biperm.out", "w") as f:
        f.write(f'{nrmutari} {n - 1}\n')
        f.write(f"{' '.join(list(map(str, v[1:n + 1])))}\n")
        f.write(f"{' '.join(list(map(str, bk[1:n + 1])))}\n")

if __name__ == "__main__":
    read_input()
    back(1, v[1:], v[1:])
    write_output()
