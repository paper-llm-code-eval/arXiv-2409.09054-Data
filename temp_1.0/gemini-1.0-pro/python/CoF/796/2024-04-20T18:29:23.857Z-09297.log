with open('tren.in', 'r') as t:
    lines = t.readlines()

T = int(lines[0])
arrival_times = []
for i in range(1, T+1):
    line, H, M, S = map(int, lines[i].split())
    arrival_times.append((line, H*60 + M, H*60 + M + S))

arrival_times.sort(key=lambda x: x[1])
current_time = arrival_times[0][1]
current_line = arrival_times[0][0]
Z = 1
X = 1
Y = 0
max_wait = 0

for i in range(1, T):
    line, H, M = arrival_times[i][0], arrival_times[i][1], arrival_times[i][2]
    if line == current_line:
        Z += 1
    elif line != current_line and H != current_time:
        max_wait = max(max_wait, H - current_time)
        X += 1
    current_time = M
    current_line = line

print(Z, X, max_wait, file=open('tren.out', 'w'))
```
