with open('tren.in', 'r') as fin, open('tren.out', 'w') as fout:
    T = int(fin.readline())
    trenuri = []
    plecari = {}
    for _ in range(T):
        linie, ora, minut, stagiune = [int(x) for x in fin.readline().split()]
        trenuri.append((linie, 60 * ora + minut, stagiune))
        if linie not in plecari: plecari[linie] = []
        plecari[linie].append(60 * ora + minut + stagiune)
        
    trenuri.sort(key=lambda x : (x[1], -x[0]))
    plecari[1].sort()
    plecari[2].sort()
    
    zmax = xmax = ydurmax = 0
    zdur = xdur = ydur = 0
    for i in range(T):
        linie, sos, s = trenuri[i]
        lin_compl = 3 - linie
        if linie == 1 and plecari[lin_compl]:
            yaux = plecari[lin_compl][0] - sos
            if yaux > ydurmax: ydurmax = yaux
            while plecari[lin_compl][0] < sos:
                plecari[lin_compl].pop(0)
        if not ydur: ydur -= (sos - trenuri[i-1][1] if i > 0 else 0)
        elif ydur > 0: ydur -= 1
        
        zdur += 1
        if ydur <= 0 and linie == 1 and plecari[lin_compl]:
            zdur += 1 + len(plecari[lin_compl])
        if zdur > zmax: zmax = zdur
        
        if ydur <= 0:
            xdur = 1
        elif s + sos == plecari[linie][0]:
            if xdur > 0: xdur = 0
            xdur += 1 + len([(x,y) for x,y in zip(plecari[linie][1:], plecari[linie][:-1]) if x - y == 1])
            plecari[linie].pop(0)
        elif s + sos < plecari[linie][0]:
            if xdur > 0: xdur = 0
            xdur += 1            
        if xdur > xmax: xmax = xdur
    
    fout.write('{} {} {}'.format(zmax, xmax, ydurmax))
