def pavare(n):
    """
    Returneaza numarul minim de dale necesare pentru a pava un drum de lungime n,
    iar daca numarul de dale este minim, returneaza cea mai mica posibilitate
    de pavare din punct de vedere lexicografic.
    """

    dale = [1, 2]
    
    while dale[-1] < n:
        dale.append(dale[-1] + 1)
    
    numarul_min_de_dale = 0
    pavare_min = []

    for i in range(len(dale) - 1, -1, -1):
        while n >= dale[i]:
            n -= dale[i]
            numarul_min_de_dale += 1
            pavare_min.append(dale[i])
    
    return numarul_min_de_dale, pavare_min


with open("pavare.in", "r") as fin:
    v = int(fin.readline().strip())
    n = int(fin.readline().strip())

with open("pavare.out", "w") as fout:
    numar_min_de_dale, pavare_min = pavare(n)

    if v == 1:
        fout.write(str(numar_min_de_dale) + '\n')
    elif v == 2:
        fout.write(' '.join(map(str, pavare_min)) + '\n')
