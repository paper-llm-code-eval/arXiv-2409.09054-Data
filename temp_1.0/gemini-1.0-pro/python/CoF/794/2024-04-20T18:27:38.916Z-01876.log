if __name__ == '__main__':
    with open('numar.in', 'r') as fin, open('numar.out', 'w') as fout:
        ni, nz = map(int, fin.readline().split())
        if not nz:
            fout.write('1\n{}'.format(0))
            fout.write('1\n{}'.format(1))
            exit()
        a = sum(map(lambda i: int(i) * (10**i), range(ni))) * (10**nz)
        b = int(fin.readline().replace(' ', ''))

        modulo_diff = []
        for i in range(1, 100000):
            c = (a + b*i) // b
            modulo_diff.append(c-i)

        best = min(modulo_diff)
        candidates = [c for c in modulo_diff if c == best]
        c = candidates[0]

        nA = len(str(a+b*c))
        fout.write('{}\n{}'.format(nA, a+b*c))

        nB = len(str(b))
        fout.write('{}\n{}'.format(nB, b))
```
