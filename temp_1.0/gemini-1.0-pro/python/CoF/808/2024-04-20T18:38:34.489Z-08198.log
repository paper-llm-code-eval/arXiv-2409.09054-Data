if k % 2 == 0:
    num_list = [start - i for i in range(start)]
    num_list.reverse()
    num_list.extend([start + i for i in range(1, start + 1)])
else:
    num_list = [start + i for i in range(start)]
    num_list.extend([start - i for i in range(1, start + 1)])

with open("numerus.out", "w") as f:
    print(' '.join(str(i) for i in num_list), file=f)
