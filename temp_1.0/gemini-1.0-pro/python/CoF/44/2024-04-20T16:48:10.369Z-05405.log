import sys
from queue import Queue


def citire(fisier_in):
    n = int(fisier_in.readline())
    timp_faze = [int(i) for i in fisier_in.readline().split()]
    graf = [[] for i in range(n + 1)]
    grad_interior = [0 for i in range(n + 1)]
    for i in range(1, n + 1):
        m_i, line = int(fisier_in.readline()), [int(j) for j in fisier_in.readline().split()]
        for j in range(1, m_i + 1):
            graf[line[j]].append(i)
            grad_interior[i] += 1
    return n, timp_faze, graf, grad_interior


def sortare_topologica(grad_interior, graf):
    q = Queue()
    for i in range(1, len(grad_interior)):
        if grad_interior[i] == 0:
            q.put(i)
    ordonare = []
    while not q.empty():
        u = q.get()
        ordonare.append(u)
        for v in graf[u]:
            grad_interior[v] -= 1
            if grad_interior[v] == 0:
                q.put(v)
    return ordonare


def pm(n, timp_faze, graf, grad_interior):
    ordonare = sortare_topologica(grad_interior, graf)
    timp_inceput = [0 for i in range(n + 1)]
    timp_final = [0 for i in range(n + 1)]
    timp_final[0] = 0
    for u in ordonare:
        for v in graf[u]:
            timp_inceput[v] = max(timp_inceput[v], timp_final[u])
        timp_final[u] = timp_inceput[u] + timp_faze[u - 1]
    timp_cel_mai_devreme = [timp_inceput[u] for u in ordonare]
    timp_cel_mai_tarziu = [timp_final[u] - timp_faze[u - 1] for u in ordonare]
    return timp_final[n], timp_cel_mai_devreme, timp_cel_mai_tarziu


def main():
    with open("pm.in", "r") as fisier_in, open("pm.out", "w") as fisier_out:
        n, timp_faze, graf, grad_interior = citire(fisier_in)
        timp_final, timp_cel_mai_devreme, timp_cel_mai_tarziu = pm(n, timp_faze, graf, grad_interior)
        fisier_out.write(str(timp_final) + "\n")
        for i in range(1, n + 1):
            fisier_out.write(f"{timp_cel_mai_devreme[i - 1]} {timp_cel_mai_tarziu[i - 1]}\n")


if __name__ == "__main__":
    main()
