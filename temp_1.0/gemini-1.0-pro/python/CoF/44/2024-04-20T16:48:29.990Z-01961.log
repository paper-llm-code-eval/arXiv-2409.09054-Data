import sys

input = sys.stdin.readlines()

with open("pm.out", "w") as f:
    n = int(input[0])
    t = [int(i) for i in input[1].split()]
    g = [[] for i in range(n+1)]
    ig = [[] for i in range(n+1)]
    tin = [None for i in range(n+1)]
    tout = [None for i in range(n+1)]
    dep = [0 for i in range(n+1)]
    dp = [[None] * 2 for i in range(n+1)]
    for i in range(2, n+2):
        l = list(map(int, input[i-1].split()))
        u = l[1:]
        for j in u:
            g[i].append(j)
            ig[j].append(i)
            dep[i] += 1

    def dfs1(u):
        tin[u] = len(tin)
        for v in g[u]:
            if tin[v] is None:
                dfs1(v)
        tout[u] = len(tin)

    dfs1(1)

    def dfs2(u):
        dp[u][0] = 0
        dp[u][1] = t[u-1]
        for v in ig[u]:
            dfs2(v)
            dp[u][0] += max(dp[v])
            dp[u][1] += min(dp[v])
        return max(dp[u])

    dfs2(1)

    f.write(str(max(dp[1])) + "\n")
    for i in range(2, n+2):
        f.write("{} {} {}\n".format(i, tin[i]-dp[i][0], tout[i]-min(dp[i])))
