import sys
from io import BufferedReader

def read_int() -> int:
    return int(readline())

def readline() -> str:
    return reader.readline().decode().strip()

def write(value: int):
    writer.write(str(value).encode())
    writer.write(b'\n')

reader = BufferedReader(sys.stdin.buffer)
writer = sys.stdout.buffer

def main():
    n = read_int()
    notes = []
    notes_inverse = []
    for i in range(n):
        x, y = map(int, readline().split())
        notes.append((x, y))
        notes_inverse.append((y, x))
    notes.sort()
    notes_inverse.sort()
    score = 0
    i = 0
    j = n - 1
    while i < j:
        x1, y1 = notes[i]
        x2, y2 = notes_inverse[j]
        if x1 + x2 == 3:
            score += y1 * y1 + y2 * y2
            i += 1
            j -= 1
        elif x1 + x2 < 3:
            i += 1
        else:
            j -= 1
    write(score)

main()
```
