import sys
sys.setrecursionlimit(1000000)  # Set the recursion limit for the third level algorithm

# Read the input from the file
with open("puncte.in") as f:
    n = int(f.readline())
    points = []
    for i in range(n):
        x, y = map(int, f.readline().split())
        points.append((x, y))

# Find the maximum number of points on a line
line_points = {}
for x, y in points:
    if not x in line_points:
        line_points[x] = set()
    line_points[x].add(y)
for y in line_points.values():
    line_points[max(y, key=lambda x: len(y))] = y
nrp = max(map(len, line_points.values()))

# Find the number of distinct lines passing through points on the diagonals
diag_points = {}
for x, y in points:
    if not x - y in diag_points:
        diag_points[x - y] = set()
    diag_points[x - y].add(y)
for y in diag_points.values():
    diag_points[max(y, key=lambda x: len(y))] = y
nrd = sum(len(p) for p in diag_points.values()) // 2

# Execute the third level algorithm
def remove_every_third(points, start):
    if len(points) <= 1:
        return points[0]
    del points[start::3]
    return remove_every_third(points, start)

xp = remove_every_third(sorted(points), 0)[0]

# Write the output to the file
with open("puncte.out", "w") as f:
    f.write(f"{nrp}\n")
    f.write(f"{nrd}\n")
    f.write(f"{xp}\n")
```
