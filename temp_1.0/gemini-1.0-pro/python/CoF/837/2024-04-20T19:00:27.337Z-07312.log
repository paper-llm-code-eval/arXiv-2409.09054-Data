import sys
from collections import defaultdict

# Read input data
with open("puncte.in", "r") as f:
    n = int(f.readline())

    puncte = []
    for _ in range(n):
        x, y = map(int, f.readline().split())
        puncte.append((x, y))

# Create a dictionary to store the number of points on each line
lines = defaultdict(int)
for x, y in puncte:
    lines[(x, 0)] += 1  # Line parallel to x-axis at y = 0
    lines[(0, y)] += 1  # Line parallel to y-axis at x = 0
    lines[(x, y)] += 1  # Line with equation y = x
    lines[(-x, y)] += 1  # Line with equation y = -x

# Find the line with the maximum number of points
nrp = max(lines.values())

# Create a dictionary to store the number of pairs of points on each line
pairs = {}
for x, y in puncte:
    x += y
    if x not in pairs:
        pairs[x] = set()
    pairs[x].add(y)

# Find the number of lines with at least two points
nrd = len({x for x, y in pairs.items() if len(y) > 1})

# Sort the points in ascending order of their x-coordinates and then their y-coordinates
puncte.sort(key=lambda p: (p[0], p[1]))

# Remove points from the list in groups of three
xp = None
while puncte:
    xp = puncte[0][0]
    for i in range(0, len(puncte), 3):
        puncte = puncte[i + 3:]

# Write output data
with open("puncte.out", "w") as f:
    f.write(f"{nrp}\n")
    f.write(f"{nrd}\n")
    f.write(f"{xp}\n")
```
