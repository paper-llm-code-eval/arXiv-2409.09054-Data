import sys

N = int(sys.stdin.readline())
a = list(map(int, sys.stdin.readline().split()))

pref = [0] * (N + 1)  # prefix sums of the sequence

for i in range(N):
    pref[i + 1] = pref[i] + a[i]

sum_ = sum(a)  # total sum of the sequence
d = {}  # number of sequences with a given prefix sum

for i in range(N + 1):
    if pref[i] not in d:
        d[pref[i]] = 0
    d[pref[i]] += 1

ans = 0  # number of ways to divide the sequence

for p in d:
    # if the prefix sum is half of the total sum
    # then the remaining part of the sequence must have the same prefix sum
    if p == sum_ // 2:
        ans += d[p] * (d[p] - 1) // 2
    # otherwise, the remaining part must have a different prefix sum
    else:
        ans += d[p] * d.get(sum_ - p, 0)

sys.stdout.write(str(ans))
```
