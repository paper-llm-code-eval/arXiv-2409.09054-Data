import sys

sys.setrecursionlimit(100000)

N, K = map(int, open('iepuri.in').readline().split())

morcovi = [[0] * K for _ in range(N + 1)]
tata = [0] * (N + 1)
fii = [[] for _ in range(N + 1)]

for _ in range(N - 1):
    a, b = map(int, open('iepuri.in').readline().split())
    tata[b] = a
    fii[a].append(b)

l = [[-1] * (2 << K) for _ in range(N + 2)]
mod = 30011

def calc(nod, masca):
    if l[nod][masca] != -1:
        return l[nod][masca]

    ans = 1
    ok = (masca & (1 << (K - 1))) == 0
    if nod != 1:
        if ok and (masca & (1 << (K - 2))) == 0:
            ans = 0
        elif ok:
            for x in fii[nod]:
                ans = (ans * calc(x, masca >> 1)) % mod

    for x in range(1, K):
        if not ok and (masca & (1 << (x - 1))) == 0:
            break
        urm = masca >> x
        if not (masca & (1 << (x - 1))):
            ans = (ans + calc(nod, urm)) % mod
        else:
            ans = (ans + calc(nod, urm | (1 << x))) % mod

    l[nod][masca] = ans
    return ans

suma = calc(1, 0)
print(suma)
