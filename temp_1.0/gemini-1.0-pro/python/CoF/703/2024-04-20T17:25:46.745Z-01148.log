with open('valori-panta.in') as f:
    n = int(f.readline())
    v = list(map(int, f.readline().split()))

cresc = 0
poz_max = []
poz_min = []
max = 0
min = 1000000000
for i in range(n):
    nr = v[i]
    cif1 = nr % 10
    nr //= 10
    cres = False
    desc = False
    while nr:
        cif2 = nr % 10
        nr //= 10
        if cif1 <= cif2 and not cres:
            cres = True
        if cif1 >= cif2 and not desc:
            desc = True
        cif1 = cif2
    if cres and not desc:
        cresc += 1
        if v[i] > max:
            max = v[i]
            poz_max = [i + 1]
        elif v[i] == max:
            poz_max.append(i + 1)
        if v[i] < min:
            min = v[i]
            poz_min = [i + 1]
        elif v[i] == min:
            poz_min.append(i + 1)

with open('valori-panta.out', 'w') as g:
    g.write(str(cresc) + '\n')
    if cresc != 0:
        g.write(str(max) + ' ')
        for i in poz_max:
            g.write(str(i) + ' ')
        g.write('\n')
        g.write(str(min) + ' ')
        for i in poz_min:
            g.write(str(i) + ' ')
    else:
        g.write('NU EXISTA')
```
