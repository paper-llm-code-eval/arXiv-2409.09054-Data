                permutation = p[i:] + p[:i]
                sum = 0
                for j in range(n - 1):
                    sum += permutation[j] * permutation[j + 1]
                if sum > max_sum:
                    max_sum = sum
                    max_permutation = permutation
            f_out.write(" ".join(map(str, max_permutation)) + "\n")

if __name__ == "__main__":
