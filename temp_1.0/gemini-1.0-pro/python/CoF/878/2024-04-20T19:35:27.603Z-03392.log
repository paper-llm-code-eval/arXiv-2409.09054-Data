with open('roboti.in', 'r') as f_in, open('roboti.out', 'w') as f_out:
    v, n = map(int, f_in.readline().split())
    p = list(map(int, f_in.readline().split()))
    c = 0
    a = []
    x = p[0]
    for i in p[1:] + [x]:
        if x <= i:
            c += 1
        a.append(i)
        x = i
    if v == 1:
        f_out.write(str(c) + '\n')
    else:
        while c > 0:
            j = a.index(min(a))
            a = a[j:] + a[:j]
            c -= 1
        p_a = []
        for i in range(n):
            p_a.append(a[(i - 1 + n) % n] * a[i])
        f_out.write(' '.join(map(str, a)) + '\n')
