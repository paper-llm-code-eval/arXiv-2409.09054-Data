def citire_drum(fisier):
    linie = fisier.readline().strip().split()
    lungime = int(linie[0])
    drum = [int(x) for x in fisier.readline().strip().split()]
    return lungime, drum

def cerinta1(lungime, drum):
    nr = 1
    for dir in drum:
        nr += dir
    return nr

def cerinta2(K):
    suma_max = 0
    nr = 1
    st = 0
    dr = 0
    while nr <= K:
        suma = nr * (nr + 1) // 2
        if suma > suma_max and nr <= K:
            suma_max = suma
            st = nr
            dr = nr + nr % 2
        nr += dr + 1
    return suma_max, st, dr

def main():
    fisier_in = open("numere.in", "r")
    cerinta = int(fisier_in.readline())
    if cerinta == 1:
        lungime, drum = citire_drum(fisier_in)
        fisier_out = open("numere.out", "w")
        fisier_out.write(str(cerinta1(lungime, drum)))
        fisier_out.close()
    else:
        K = int(fisier_in.readline())
        suma_max, st, dr = cerinta2(K)
        fisier_out = open("numere.out", "w")
        fisier_out.write(str(suma_max))
        fisier_out.close()

main()
