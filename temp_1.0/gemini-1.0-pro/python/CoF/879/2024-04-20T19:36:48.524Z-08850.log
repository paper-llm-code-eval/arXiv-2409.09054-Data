import sys 

MAX_NUMBER = 100000
transitions = [[0] * (MAX_NUMBER+1) for _ in range(MAX_NUMBER+1)]
sum_numbers_to = [0] * (MAX_NUMBER+1)

def construct_graph():
    node = 1
    for i in range(1, MAX_NUMBER+1):
        sum_numbers_to[i] = sum_numbers_to[i-1] + i
        transitions[node][i] = node + 1
        if node % 2 == 0:
            transitions[node][i] += 1
        node += 1
        transitions[node][i] = node + 1
        if node % 2 == 0:
            transitions[node][i] += 1
        node += 1

def read_distances(text_file):
    line = text_file.readline()
    distances = list(map(int, line.split()))
    return distances

def get_destination(distance):
    node = 1
    for d in distance:
        node = transitions[node][d]
    return node

def get_maximum_sum(destination):
    max_sum = 0
    while destination != 1:
        if destination % 2 == 0:
            destination -= 1
        else:
            destination //= 2
        max_sum += destination
    return max_sum

with open('numere.in') as text_file:
    choice = int(text_file.readline())
    if choice == 1:
        distance = read_distances(text_file)
        destination = get_destination(distance)
        with open('numere.out', 'w') as out_file:
            out_file.write(str(destination) + '\n')
    else:
        k = int(text_file.readline())
        construct_graph()
        max_sum = get_maximum_sum(k)
        with open('numere.out', 'w') as out_file:
            out_file.write(str(max_sum) + '\n')
