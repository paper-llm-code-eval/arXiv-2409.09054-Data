def build_vector(x):
    y = [None] * len(x)  # Initialize the output vector with None values
    order = sorted(range(len(x)), key=lambda i: x[i])  # Sort the indices of x in ascending order of x values
    for i, index in enumerate(order):
        y[index] = i + 1  # Assign the rank to the corresponding index in the output vector
    return y


with open('siruri.in', 'r') as fin:
    n = int(fin.readline())
    x = list(map(int, fin.readline().split()))

with open('siruri.out', 'w') as fout:
    fout.write(' '.join(map(str, build_vector(x))))
