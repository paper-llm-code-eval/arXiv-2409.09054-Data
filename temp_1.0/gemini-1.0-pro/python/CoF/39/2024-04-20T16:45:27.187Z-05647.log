with open("suma.in", 'r') as fin:
    n = int(fin.readline())
    costuri = list(map(int, fin.readline().split()))
    d = dict()
    costuri[0] = 0
    for i in range(n):
        d[i] = {(i + 1): costuri[i]} # camera curentă poate ajunge la cea de deasupra ei
        if i >= int((n ** 0.5)):  # nu mai poți urca
            continue
        j = 2*i + 1
        if j < n: # camera curentă poate ajunge la camera de sub cea de deasupra ei din stânga
            d[i][j] = costuri[i] + costuri[j]
        j += 1
        if j < n: # camera curentă poate ajunge la camera de sub cea de deasupra ei din dreapta
            d[i][j] = costuri[i] + costuri[j]
        j += 2*i + 1 - n
        if j >= 0 and j < n: # camera curentă poate ajunge la camera de sub cea de deasupra ei din mijloc
            d[i][j] = costuri[i] + costuri[j]

with open("suma.out", 'w') as fout:
    cams = {0: [1]}
    minSum = float('inf')
    for i in range(1, int(n**0.5)):
        nou = {}
        for cam in cams:
            for nextCam, cost in d[cam].items():
                if nextCam not in nou or cost + cams[cam][0] < nou[nextCam]:
                    nou[nextCam] = cost + cams[cam][0]
                    cams[nextCam] = cams[cam] + [nextCam]
        cams = nou
        sumCamere = cams[n - 1][0]
        if sumCamere < minSum:
            minSum = sumCamere
    fout.write(' '.join(map(str, [len(cams[n-1]) - 1, minSum])))
    fout.write('\n')
    fout.write(' '.join(map(str, cams[n-1])))
