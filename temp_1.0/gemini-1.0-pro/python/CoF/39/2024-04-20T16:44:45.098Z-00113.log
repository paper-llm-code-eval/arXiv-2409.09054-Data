            elif level + 1 <= nrLevels and prevRow + direction % 2 - 1 < nrLevels and prevCol + (direction - 1) % 2 <= nrLevels and prevRow == prevCol and direction == 4:
                nrRow = level + 1
                nrCol = prevCol + (direction - 1) % 2 + (nrRow % 2 == 0)
                if dp[nr][nrRow][nrCol] == -1 or dp[nr][nrRow][nrCol] > cost + costs[nr]:
                    visited.append(nr + 1)
                    dp[nr][nrRow][nrCol] = go(nr + 1, nrRow, nrRow, nrCol, cost + costs[nr])
                    visited.pop()

