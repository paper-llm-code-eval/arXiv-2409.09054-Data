    graph[a].add(b)
    graph[b].add(a)
    cost[(a, b)] = 1
    cost[(b, a)] = 1

dist = [0] * (n + 1)
dijkstra(graph, 1, cost, dist)
dist_max = max(dist)
max_node = dist.index(dist_max)

dist = [0] * (n + 1)
dijkstra(graph, max_node, cost, dist)
dist_new_max = max(dist)
total_cost = dist_new_max

if k > 0:
    dist = [0] * (n + 1)
    dijkstra(graph, dist_new_max, cost, dist)
    dist_sec_max = max(dist)
    second_max_node = dist.index(dist_sec_max)
    total_cost -= dist[second_max_node]
    k -= 1

if k > 0:
    while k > 0:
        max_edge = 0
        for a in graph:
            for b in graph[a]:
                if (a, b) in cost and cost[(a, b)] > max_edge:
                    max_edge = cost[(a, b)]
                    edge = (a, b)
        total_cost -= max_edge
        graph[edge[0]].remove(edge[1])
        graph[edge[1]].remove(edge[0])
        cost.pop((edge[0], edge[1]))
        cost.pop((edge[1], edge[0]))
        k -= 1

print(total_cost)
