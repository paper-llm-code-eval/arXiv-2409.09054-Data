import sys
sys.setrecursionlimit(24000)

input_file = open("cezar.in")
line1 = input_file.readline().split()
N = int(line1[0])
K = int(line1[1])
edges = []
for line in input_file:
    a, b = map(int, line.split())
    edges.append((a, b))
input_file.close()
    
adj = [[] for _ in range(N)]
for a, b in edges:
    adj[a-1].append(b-1)
    adj[b-1].append(a-1)

visited = [False] * N
path = [-1] * N

mn = 1e9

def dfs(node, cnt, cost):
    global mn
    global K
    
    visited[node] = True
    path[cnt] = node
    if cnt == K and node == path[0]:
        mn = min(mn, cost)
        return
    
    for u in adj[node]:
        if not visited[u]:
            dfs(u, cnt + 1, cost + 1)
            
    visited[node] = False

dfs(0, 0, 0)
output_file = open("cezar.out", "w")
output_file.write(str(mn) + '\n')
output_file.close()
```
