with open('arrows.in', 'r') as inf, open('arrows.out', 'w') as outf:
    cr, n, m = map(int, inf.readline().split())
    arrows = [[int(c) for c in inf.readline().split()] for _ in range(n)]
    xr, yc = map(int, inf.readline().split())

    vis = [[False] * m for _ in range(n)]
    directions = [(-1, 0), (0, 1), (1, 0), (0, -1)]  # up, right, down, left
    score = 0
    positions = [(xr - 1, yc - 1)]
    while positions:
        next_positions = []
        for (x, y) in positions:
            vis[x][y] = True
            if cr == 1: score += 1000 if all(vis[a][b] for a, b in directions) else 1
            if cr == 2: score += 1 if vis[x][y] else 0
            if cr == 3: score = max(score, 1 if vis[x][y] else 0)
            next_positions.append((
                (x + directions[arrows[x][y] - 1][0]) % n,
                (y + directions[arrows[x][y] - 1][1]) % m
            ))
        positions = next_positions

    outf.write(str(score) + '\n')
```
