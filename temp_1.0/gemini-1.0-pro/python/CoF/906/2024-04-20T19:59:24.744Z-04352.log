def count_digits(n):
  return len(str(n))

def get_kth_digit(n, k):
  return str(n)[k - 1]

def get_biggest_p_digits(n, p):
  # Convert the number to a string and remove leading zeros
  s = str(n).lstrip('0')

  # Prepend zeros if necessary
  if len(s) < p:
    s = '0' * (p - len(s)) + s

  # Find the biggest p-digit number in the string
  j = 0  # Last character of current max
  for i in range(p, len(s) + 1):
    if int(s[i - 1:i]) > int(s[j:j + p]):
      j = i - p

  return s[j:j + p]

# Read the input data
