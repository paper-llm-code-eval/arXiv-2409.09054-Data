n = int(input())
if c == 1:
  print(sum(count_digits(int(x)) for x in input().split()))
elif c == 2:
  k = int(input().split()[1])
  print(get_kth_digit(n, k))
elif c == 3:
  p = int(input().split()[1])
  print(get_biggest_p_digits(n, p))
