            if max_passes_per_section != 0:
                f_out.write(str(max_passes_per_section))
            else:
                f_out.write("-1")
    else:
        for x in X[1:]:
            current_sections = []
            for start, end, cost in taxed_sections:
                if start <= x <= end:
                    current_sections.append((cost, x - start))
            if current_sections:
                current_portions.append(current_sections)
            max_passes_per_section = 0
            for p in current_sections:
                last_pass_dist = p[1]
                last_pass_cost = p[0] * last_pass_dist
                total_cost += last_pass_cost
                if max_passes_per_section < last_pass_dist:
                    max_passes_per_section = last_pass_dist
            last_portions = current_sections

        f_out.write(str(total_cost))
