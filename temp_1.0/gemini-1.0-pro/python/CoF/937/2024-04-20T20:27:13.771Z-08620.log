with open("tunel.in", "r") as f:
    cerinta = int(f.readline().strip())
    n, m, x = map(int, f.readline().strip().split())
    tuneluri = [[] for _ in range(n + 1)]
    for i in range(n - 1):
        p = int(f.readline().strip())
        for j in range(p):
            poz = int(f.readline().strip())
            tuneluri[i + 1].append(poz)
            tuneluri[i + 2].append(poz + 1)

viz = [False for _ in range(m + 1)]
dist = [0 for _ in range(m + 1)]
coada = [1]
while coada:
    u = coada.pop(0)
    if u == m:
        break
    if not viz[u]:
        viz[u] = True
        for v in tuneluri[x]:
            if v <= u or viz[v]:
                continue
            dist[v] = dist[u] + 1
            coada.append(v)

if cerinta == 1:
    for i in range(n - 1, 0, -1):
        if x in tuneluri[i]:
            x = i
    with open("tunel.out", "w") as f:
        f.write(str(x) + "\n")
elif cerinta == 2:
    with open("tunel.out", "w") as f:
        f.write(str(dist[m]) + "\n")
