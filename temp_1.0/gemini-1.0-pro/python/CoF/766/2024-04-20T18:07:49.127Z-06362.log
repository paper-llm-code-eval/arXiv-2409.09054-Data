                dr -= 1
                L -= d
        else:
            break
    return ans, v[dr] + L if ans else 0


if __name__ == "__main__":
    L, n, k, v = citire()
    ans, l = solve(L, n, k, v)
    with open("afise.out", "w") as f:
        f.write(f"{l} {ans}")
