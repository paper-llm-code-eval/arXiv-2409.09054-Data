with open("turnuri.in") as f:
    m, n = map(int, f.readline().split())
    mat = [[int(x) for x in line.split()] for line in f]

def merge_towers(tower1, tower2):
    new_height = tower1 + tower2
    if new_height <= m:
        return new_height
    return 0  # avoid towers that would exceed the matrix's height

towers = []  # store the initial tower heights
heights = []  # store new tower heights after potential merges

for j in range(n):
    height = 0
    for i in range(m - 1, -1, -1):
        height += mat[i][j]
    towers.append(height)

heights = towers[:]  # copy the initial tower heights

for j in range(n - 1):
    new_height = merge_towers(heights[j], heights[j + 1])
    if new_height > 0:
        heights[j] = new_height
        heights[j + 1] = 0

max_height = max(heights)
max_towers = heights.count(max_height)

with open("turnuri.out", "w") as f:
    f.write(' '.join(str(x) for x in towers))
    f.write('\n')
    f.write(str(max_height))
    f.write('\n')
    f.write(str(max_towers))
```
