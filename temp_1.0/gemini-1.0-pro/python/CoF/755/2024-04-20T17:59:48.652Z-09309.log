from math import ceil

with open("elfi.in") as f_in, open("elfi.out", "w") as f_out:
    limit = 105
    n = int(f_in.readline())
    c = []
    k = []
    for _ in range(n):
        x, y = map(int, f_in.readline().split())
        c.append(x)
        k.append(y)
    p = [[0 for _ in range(limit)] for _ in range(n)]
    r = [0 for _ in range(limit)]
    
    def binary_search(l, r, target):
        low = l
        high = r
        while low <= high:
            mid = (low + high) // 2
            if p[0][mid] <= target <= p[n - 1][mid]:
                low = mid + 1
            else:
                high = mid - 1
        return low - 1

    for i in range(n):
        for j in range(c[i]):
            x = binary_search(0, limit - 1, j)
            p[i][x] += 1
    for i in range(limit):
        for j in range(n):
            r[i] += p[j][i]

    ans = 0
    for i in range(limit):
        ans = max(ans, ceil(r[i] / 100))

    f_out.write(str(ans) + "\n")
