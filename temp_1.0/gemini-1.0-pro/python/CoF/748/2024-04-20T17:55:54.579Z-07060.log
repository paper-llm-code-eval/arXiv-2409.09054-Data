with open('pluton.in', 'r') as fin:
    n = int(fin.readline())
    soldiers = set()
    for _ in range(n):
        soldiers.add(int(fin.readline()))

regiments = []
for s in soldiers:
    r = []
    d = {}
    ss = str(s)
    m = len(ss)
    for i in range(m):
        if ss[i] not in d:
            d[ss[i]] = set()
        d[ss[i]].add(s)
    for v in d.values():
        for x in v:
            r.append(x)
    regiments.append(set(r))

regiments_len = [len(r) for r in regiments]
max_len = max(regiments_len)
max_regiments_len = regiments_len.count(max_len)
max_regiments = [r for r in regiments if len(r) == max_len]

with open('pluton.out', 'w') as fout:
    fout.write(f'{len(regiments)}\n')
    fout.write(f'{max_len}\n')
    fout.write(f'{max_regiments_len}\n')
    fout.write(' '.join(map(str, max_regiments[0])))
```
