import math
import sys

input_file_name = 'expresie.in'
output_file_name = 'expresie.out'

def is_perfect_m_power(n, m):
    root = int(n ** (1 / m))
    return root ** m == n

def prime_factorization(n):
    factors = []
    divisor = 2
    while divisor * divisor <= n:
        if n % divisor == 0:
            exp = 0
            while n % divisor == 0:
                n //= divisor
                exp += 1
            factors.append((divisor, exp))
        divisor += 1
    if n > 1:
        factors.append((n, 1))
    return factors

with open(input_file_name, 'r') as input_file, open(output_file_name, 'w') as output_file:
    m = int(input_file.readline())
    n = int(input_file.readline())
    product = 1
    for x in input_file.readline().split():
        product *= int(x)

    if is_perfect_m_power(product, m):
        output_file.write('1\n')
        prime_factors = prime_factorization(int(product ** (1 / m)))
        for factor in prime_factors:
            output_file.write(f'{factor[0]} {factor[1]}\n')
