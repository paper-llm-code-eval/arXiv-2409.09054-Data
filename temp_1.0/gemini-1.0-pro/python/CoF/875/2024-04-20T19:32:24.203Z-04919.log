                                    cnt += 1
                            if cnt == k:
                                mini = min(mini, 2)

    fout.write(str(mini))

if p == 2:
    if k == 1:
        fout.write('0')
    elif dist[word[-1]] == 0:
        fout.write('-1')
    else:
        fout.write(str(dist[word[-1]] - 1))

fin.close()
fout.close()
