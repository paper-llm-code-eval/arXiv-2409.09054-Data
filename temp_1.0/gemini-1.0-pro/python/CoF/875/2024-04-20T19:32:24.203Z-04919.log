    for i in range(n):
        for j in range(m):
            if board[i][j] != '#':
                cnt = 0
                for letter in word:
                    if (i, j) in graph[letter]:
                        cnt += 1
                if cnt == k:
                    mini = min(mini, 1)
                else:
                    for d in range(4):
