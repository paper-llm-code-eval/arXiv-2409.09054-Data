        return False

    for i in range(2, int(num**0.5) + 1):
        if num % i == 0:
            return False

    return True

with open('control.in', 'r') as fin, open('control.out', 'w') as fout:
    n = int(fin.readline())
    unique_weights = []
    counts = []
