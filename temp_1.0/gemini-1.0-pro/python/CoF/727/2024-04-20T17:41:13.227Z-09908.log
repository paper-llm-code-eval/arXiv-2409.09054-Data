        if all(i % 2 == 0 for i in counts[i:i+3]) or all(i % 2 != 0 for i in counts[i:i+3]):
            control_num += 10
    control_num += counts[i:i+3].count(0)

    fout.write(f'{control_num}\n')
    fout.write('1' if is_prime(control_num) else '0')
