with open('grupe.in') as fin, open('grupe.out', 'w') as fout:
    n, k = map(int, fin.readline().split())
    gen = list(fin.readline())
    f_count = []
    b_count = []
    lst = []
    for i in range(k):
        x, *y = map(int, fin.readline().split())
        f = 0
        b = 0
        for c in y:
            if gen[c - 1] == 'f': f += 1
            else: b += 1
        f_count.append(f)
        b_count.append(b)
        lst.extend(y)
    lst.sort()
    if len(lst) != n:
        fout.write('NU')
        exit()
    for i in range(k - 1):
        if max(f_count) - min(f_count) > 1 or max(b_count) - min(b_count) > 1:
            fout.write('NU')
            exit()
        if abs(f_count[i] - f_count[i + 1]) > 1 or abs(b_count[i] - b_count[i + 1]) > 1:
            fout.write('NU')
            exit()
    for i in range(k):
        fout.write(str(f_count[i]) + ' ' + str(b_count[i]) + '\n')
    fout.write('DA')
```
