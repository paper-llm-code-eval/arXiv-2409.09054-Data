import sys

def citire():
    with open('concurs.in') as f:
        c, n = map(int, f.readline().split())
        punctaj = {}
        club = {}
        for i in range(n):
            x, y = map(int, f.readline().split())
            punctaj.setdefault(i + 1, []).append(y)
            club.setdefault(x, []).append(i + 1)
    return c, n, punctaj, club

def maxScor(punctaj):
    maxi = 0
    for x in punctaj:
        if maxi < sum(punctaj[x]):
            maxi = sum(punctaj[x])
            nr = x
    return maxi, nr

def concursCastigat(club, punctaj):
    maxi = 0
    for x in club:
        if maxi < sum(y for y in punctaj[club[x]] if y):
            maxi = sum([x for x in punctaj[club[x]] if x])
            x_w = [x]
    if maxi == 0:
        return 'FARA CAMPION'
    x_w.sort()
    for x in club:
        if maxi == sum([y for y in punctaj[club[x]] if y]):
            x_w.append(x)
    x_w.append(maxi)
    return x_w

c, n, punctaj, club = citire()
if c == 1:
    with open('concurs.out', 'w') as f:
        maxi, nr = maxScor(punctaj)
        f.write(f'{maxi} {nr}')
else:
    with open('concurs.out', 'w') as f:
        win = concursCastigat(club, punctaj)
        f.write(' '.join(map(str, win)))
