with open("palindrom.in", "r") as f:
    c = int(f.readline())
    n = int(f.readline())
    numbers = [int(x) for x in f.readline().split()]

def is_palindrome(n):
    s = str(n)
    return s == s[::-1]

def is_almost_palindrome(n):
    s = str(n)
    return sum([s[i] != s[len(s) - 1 - i] for i in range(len(s) // 2)]) == 1

def associated_palindrome(n):
    p = n + 1
    while not is_palindrome(p):
        p += 1
    return p

if c == 1:
    print(sum(is_palindrome(i) for i in numbers))
elif c == 2:
    print(sum(is_almost_palindrome(i) for i in numbers))
elif c == 3:
    print(*[associated_palindrome(i) for i in numbers])
```
