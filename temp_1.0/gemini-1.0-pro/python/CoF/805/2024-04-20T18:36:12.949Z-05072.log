with open('vase.in') as f:
    n = int(f.readline())
    v = [0 for _ in range(n+1)]
    for i in range(n):
        vi, ri = map(str, f.readline().split())
        v[int(vi)] += 1
        if ri == 'D':
            v[int(vi)]  |= 1
    x = int(f.readline())

mx = 0
for el in v:
    if el != 0:
        mx += el/2
mx = int(mx)

xl, xr = (0, 0)
for i in range(n+1):
    if xr == 0 and (v[i] & 1):
        xr = i
    elif xl == 0 and v[i] != 0 and not (v[i] & 1):
        xl = i
    elif xl != 0 and xr != 0:
        break
    v[i] = 0 

if xl > xr:
    xl, xr = xr, xl

with open('vase.out', 'w') as f:
    f.write(f'{mx}\n')
    f.write(f'{xr-v[xr]} {'D' if v[xr] & 1 else 'S'}\n')
    f.write(f'{xl+v[xl]} {'S' if v[xl] & 1 else 'D'}\n')
