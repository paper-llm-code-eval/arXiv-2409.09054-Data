def main():
    with open("pic.in", "r") as fin, open("pic.out", "w") as fout:
        v = int(fin.readline())
        n = int(fin.readline())
        capacities = list(map(int, fin.readline().split()))
        capacities = capacities[::-1]
        capacities.insert(0, 0)

        if v == 1:
            max_sum = 0
            min_level = -1

            sum = 0
            level = 1
            for i in range(1, n + 2):
                if i > 1:
                    sum -= capacities[i - 1]
                sum += capacities[i]
                if sum > max_sum:
                    max_sum = sum
                    min_level = level
                level += 1

            fout.write(f"{min_level}\n")

        elif v == 2:
            seconds = 0
            overflow = 0

            while capacities[1] > 0:
                seconds += 1
                for i in range(2, n + 2):
                    capacities[i] -= 1
                    if capacities[i] < 0:
                        overflow += -capacities[i]
                        capacities[i] = 0
                    else:
                        capacities[i + 1] += 1

            fout.write(f"{seconds} {overflow}\n")


if __name__ == "__main__":
