def main():
    with open('turnuri.in', 'r') as f:
        n = int(f.readline())
        h = [int(x) for x in f.readline().split()]

    MAX = max(h)
    st = [0] * MAX
    dr = [0] * MAX
    s = 0
    for i in range(n):
        s += h[i]
        st[h[i]] += 1
    coeficient = []

    # Calculam coeficientul de frumusete pentru fiecare turn
    for i in range(n):
        coeficient.append(s - h[i] + st[h[i]] - 2)

        # Stergem turnul curent
        s -= h[i]
        st[h[i]] -= 1

        # Adaugam turnul din dreapta
        if i + 1 < n:
            dr[h[i]] -= 1
            s += h[i + 1]
            st[h[i + 1]] += 1

        # Adaugam turnul din stanga
        if i - 1 >= 0:
            dr[h[i]] += 1
            s += h[i - 1]
            st[h[i - 1]] -= 1

        # Adaugam inapoi turnul curent, ca sa-l folosim la urmatoarea pozitie
        s += h[i]
        st[h[i]] += 1
        dr[h[i]] += 1

    # Scriem coeficientul de frumusete in fisier
    with open('turnuri.out', 'w') as f:
        for x in coeficient:
            f.write(str(x) + '\n')

if __name__ == "__main__":
    main()
