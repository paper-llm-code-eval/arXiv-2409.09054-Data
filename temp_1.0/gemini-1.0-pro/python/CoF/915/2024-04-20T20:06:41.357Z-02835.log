def read_matrix(file_name):
    with open(file_name, 'r') as f:
        n, m = map(int, f.readline().split())
        matrix = [list(map(int, line.split())) for line in f.readlines()]
    return n, m, matrix

def is_valid(x, y, n, m, matrix):
    return 0 <= x < n and 0 <= y < m and matrix[x][y] != 0

def dfs(x, y, n, m, matrix, visited, length):
    visited[x][y] = True
    maximum_length = length
    if x + 1 < n and is_valid(x + 1, y, n, m, matrix) and not visited[x + 1][y]:
        maximum_length = max(maximum_length, dfs(x + 1, y, n, m, matrix, visited, length + 1))
    if y + 1 < m and is_valid(x, y + 1, n, m, matrix) and not visited[x][y + 1]:
        maximum_length = max(maximum_length, dfs(x, y + 1, n, m, matrix, visited, length + 1))
    visited[x][y] = False
    return maximum_length

def main():
    n, m, matrix = read_matrix('traseu.in')
    visited = [[False for _ in range(m)] for _ in range(n)]
    maximum_length = 0

    for i in range(n):
        for j in range(m):
            if matrix[i][j] == 1:
                maximum_length = max(maximum_length, dfs(i, j, n, m, matrix, visited, 0))

    if maximum_length == 0:
        print(0, file=open('traseu.out', 'w'))
    else:
        print(maximum_length + 1, file=open('traseu.out', 'w'))

if __name__ == '__main__':
