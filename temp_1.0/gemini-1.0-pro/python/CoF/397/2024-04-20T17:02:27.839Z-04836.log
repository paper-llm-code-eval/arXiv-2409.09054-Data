with open("ecuatii.in") as f_in, open("ecuatii.out", "w") as f_out:
    n = int(f_in.readline())
    for linie in f_in:
        l_expr = []  # list of (sign? (+1 or -1), coeff)
        cur_num = ""
        cur_sign = +1
        if linie[0] == '+':
            cur_sign = +1
        elif linie[0] == '-':
            cur_sign = -1
        else:
            cur_num += linie[0]
        for c in linie[1:]:
            if c == '+' or c == '-':
                if cur_num:
                    l_expr.append((cur_sign, int(cur_num)))
                cur_sign = +1 if c == '+' else -1
                cur_num = ""
            else:
                cur_num += c
        l_expr.append((cur_sign, int(cur_num) if cur_num else 1))
        l_expr = [termen for termen in l_expr if termen[1]]
        stanga, dreapta = zip(*l_expr)
        num = sum(stanga)
        denom = sum(dreapta)
        if num != denom anddenom:
            f_out.write(f"{num / (denom):.4f}\n")
        elif not denom:
            f_out.write("infinit\n")
