        n = int(f_in.readline())
        for _ in range(n):
            eq = f_in.readline().split("=")
            l = eq[0].split("-")
            r = eq[1].split("-")
            a, b = 0, 0
            for x in l:
                if "x" in x:
                    a += int(x[:-1]) if x[:-1] else 1
                else:
                    b += int(x)
            for x in r:
                if "x" in x:
                    a -= int(x[:-1]) if x[:-1] else 1
                else:
                    b -= int(x)
            if a == 0:
                f_out.write("imposibil\n")
            elif b % a == 0:
                f_out.write(f"{b/a:.4f}\n")
            else:
                f_out.write("infinit\n")
