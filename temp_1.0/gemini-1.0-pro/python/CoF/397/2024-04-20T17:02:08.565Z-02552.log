with open("ecuatii.in","r") as fin:
    with open("ecuatii.out","w") as fout:
        nr = int(fin.readline())
        sign = {'+':1, '-':-1}
        eqs = [fin.readline().strip() for _ in range (nr)]

        for eq in eqs:
            eq = eq.replace("+-","-")
            eq = eq.replace("--","+").split("=")
            lhs,rhs= eq
            vN, vX = 0,0
            for i in lhs.split("+"):
                if "x" in i:
                    i = i.replace('x','')
                    if i:
                        vX += int(i) * sign.get(i[0],1)
                    else:
                        vX += sign.get(i[0],1)
                else:
                    vN += int(i)
            for i in rhs.split("+"):
                if 'x' in i:
                    i = i.replace('x','')
                    if i:
                        vX -= int(i) * sign.get(i[0],1)
                    else:
                        vX -= sign.get(i[0],1)
                else:
                    vN -= int(i)
            if vX:
                if vN % vX == 0:
                    fout.write(f"{vN/vX:>0.4f}\n")
                else:
                    fout.write("imposibil\n")
            else:
                if vN:
                    fout.write("imposibil\n")
                else:
                    fout.write("infinit\n")
