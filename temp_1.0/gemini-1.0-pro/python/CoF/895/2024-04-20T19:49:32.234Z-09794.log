with open('turnuri.in', 'r') as fin, open('turnuri.out', 'w') as fout:
    cerinta = int(fin.readline().strip())
    n = int(fin.readline().strip())
    cuburi = []
    for i in range(n):
        latura, culoare = fin.readline().strip().split()
        cuburi.append((int(latura), culoare))
    cuburi.sort()

    if cerinta == 1:
        inaltime = 0
        turnuri = 1
        anterior = None
        for latura, culoare in cuburi:
            if culoare != anterior:
                turnuri += 1
                anterior = culoare
            inaltime = max(inaltime, latura)

        fout.write(f'{turnuri} {inaltime}\n')

    if cerinta == 2:
        lg = []
        ag = []
        for latura, culoare in cuburi:
            if culoare == 'g':
                lg.append(latura)
            else:
                ag.append(latura)

        lg.sort()
        ag.sort()
        lg.reverse()  # cuburile galbene sunt asezate in turnuri cat mai inalte, deci se ia intai cea cu latura cea mai mare

        nmax = 0
        while lg and ag:
            if lg[0] > ag[0]:
                nmax += 1
                lg.pop(0)
                ag.pop(0)
            else:
                ag.pop(0)

        fout.write(f'{nmax}\n')
```
