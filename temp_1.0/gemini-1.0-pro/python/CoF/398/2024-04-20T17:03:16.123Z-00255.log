                    if 0 <= nx < n and 0 <= ny < m and grid[nx][ny] == grid[x][y] and (nx, ny) not in components:
                        components[(nx, ny)] = component_id
                        queue.append((nx, ny))
            component_id += 1

r_islands, g_islands, b_islands = 0, 0, 0
for component in components.values():
    if component in (1, 3, 5, 7, 9):
        r_islands += 1
    elif component in (2, 4, 6, 8, 10):
        g_islands += 1
    else:
        b_islands += 1

min_bridge_length = float('inf')
for i in range(n):
    for j in range(m):
        if grid[i][j] == 0:
            ji = j
            while ji < m and grid[i][ji] == 0:
                ji += 1
            bridge_length = ji - j
            if bridge_length > 1:
                for k in range(ji, m):
                    if grid[i][k] == 1 or grid[i][k] == 2:
                        nj = k
                        while nj < m and grid[i][nj] != 0:
                            nj += 1
                        min_bridge_length = min(min_bridge_length, bridge_length + nj - k)
        ji = j
        while ji < m and grid[i][ji] == 0:
            ji += 1
        if ji == m:
            ji -= 1
        if grid[i][ji] != 0:
            for k in range(ji, -1, -1):
                if grid[i][k] == 1 or grid[i][k] == 2:
                    nk = k
                    while nk >= 0 and grid[i][nk] != 0:
                        nk -= 1
                    min_bridge_length = min(min_bridge_length, bridge_length + k - nk)

for j in range(m):
    for i in range(n):
        if grid[i][j] == 0:
            ij = i
            while ij < n and grid[ij][j] == 0:
                ij += 1
            bridge_length = ij - i
            if bridge_length > 1:
                for k in range(ij, n):
                    if grid[k][j] == 1 or grid[k][j] == 2:
                        nk = k
                        while nk < n and grid[nk][j] != 0:
                            nk += 1
                        min_bridge_length = min(min_bridge_length, bridge_length + nk - k)
        ij = i
        while ij < n and grid[ij][j] == 0:
            ij += 1
        if ij == n:
            ij -= 1
        if grid[ij][j] != 0:
            for k in range(ij, -1, -1):
                if grid[k][j] == 1 or grid[k][j] == 2:
                    kk = k
                    while kk >= 0 and grid[kk][j] != 0:
                        kk -= 1
                    min_bridge_length = min(min_bridge_length, bridge_length + i - kk)


with open('insule.out', 'w') as f:
    f.write(f"{r_islands} {g_islands} {b_islands} {min_bridge_length}\n")
