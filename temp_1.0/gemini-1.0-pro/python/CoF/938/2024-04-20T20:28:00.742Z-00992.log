with open("labirint.in") as fin, open("labirint.out", "w") as fout:
    N, M = map(int, fin.readline().split())
    mat = [[int(x) for x in fin.readline().strip()] for _ in range(N)]
    
    dp = [[10**9]*M for _ in range(N)]
    dp[0][0] = 1
    min_dist = 10**9

    for x in range(N):
        for y in range(M):
            if not mat[x][y]:
                for dx, dy in (1, 0), (-1, 0), (0, 1), (0, -1):
                    if 0 <= x+dx < N and 0 <= y+dy < M:
                        dp[x][y] = min(dp[x][y], dp[x+dx][y+dy] + 1)
            else:
                dp[x][y] = 10**9

    min_dist = dp[-1][-1]

    for x in range(N):
        for y in range(M):
            if not mat[x][y]: print("0" if dp[x][y] >= min_dist else "1", end='', file=fout)
        print(file=fout)
```
