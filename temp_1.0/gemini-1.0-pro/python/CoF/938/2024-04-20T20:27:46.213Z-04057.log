def main():
    with open('labirint.in') as f:
        n, m = map(int, f.readline().strip().split())
        labirint = [list(map(int, line.strip())) for line in f]

    labirint[0][0] = labirint[n - 1][m - 1] = 0
    d = [[(n + m - 2) for _ in range(m)] for _ in range(n)]

    q = [(0, 0, 0)]
    while q:
        i, j, k = q.pop(0)
        if i == n - 1 and j == m - 1:
            d_0 = k
            break
        for ni, nj in ((i + 1, j), (i - 1, j), (i, j + 1), (i, j - 1)):
            if 0 <= ni < n and 0 <= nj < m and labirint[ni][nj] == 0 and d[ni][nj] > k + 1:
                q.append((ni, nj, k + 1))
                d[ni][nj] = k + 1

    with open('labirint.out', 'w') as f:
        for line in d:
            f.write(''.join(map(str, map(lambda x: x < d_0, line))))

if __name__ == '__main__':
    main()
