import sys

if __name__ == "__main__":
    f = open("cochilie.in")
    c = int(f.readline())
    f.close()
    if c == 1:
        n = int(input())
        nrlinii = n*2 - 1
        nrcoloane = n*4 - 3
        with open('cochilie.out', 'w') as f:
            f.write(str(nrlinii) + ' ' + str(nrcoloane))
    else:
        n, p = map(int, input().split())
        D = [[-1]*100 for i in range(100)]
        latura = 1
        nrcoloane = 1
        elem = 1
        i = 0
        j = 0
        dir = 0
        while n > 0:
            while True:
                D[i][j] = elem
                if i == n*2 - n - 1 and j == n*4 - n - 3:
                    n -= 1
                    latura += 2
                    nrcoloane += 2
                    elem += 1
                if dir == 0: # dreapta
                    j += 1
                    if j == latura + n*4 - n - 3:
                        dir = 1
                        j -= 1
                        i += 1
                elif dir == 1: # jos
                    i += 1
                    if i == latura + n*2 - n - 1:
                        dir = 2
                        i -= 1
                        j -= 1
                elif dir == 2: # stanga
                    j -= 1
                    if j == n*4 - n - 4:
                        dir = 3
                        j += 1
                        i -= 1
                elif dir == 3: # sus
                    i -= 1
                    if i == n*2 - n - 2:
                        dir = 0
                        i += 1
                        j += 1
                if i == p - 1:
                    break
            i += 1
            j = 0
            dir = 0
        result = []
        for j in range(nrcoloane):
            result.append(D[p-1][j])
        with open('cochilie.out', 'w') as f:
            f.write(' '.join(map(str, result)))
