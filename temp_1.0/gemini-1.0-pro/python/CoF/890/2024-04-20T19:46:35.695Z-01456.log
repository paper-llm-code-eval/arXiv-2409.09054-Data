import sys

def solve1(a, k):
  """
  Finds the magic value of a given number.

  Args:
    a (int): The given number.
    k (int): The value of k.

  Returns:
    int: The magic value.
  """

  divs = []
  i = 2
  while i * i <= a:
    if a % i == 0:
      divs.append(i)
      while a % i == 0:
        a //= i
    i += 1
  if a > 1:
    divs.append(a)
  return divs[k - 1]

def solve2(arr):
  """
  Finds the cipher of the chest.

  Args:
    arr (list): A list of tuples representing the numbers and values of k.

  Returns:
    str: The cipher.
  """

  sum = 0
  for a, k in arr:
    sum += solve1(a, k)
  return ''.join(str(x) for x in sorted(map(int, str(sum))))

if __name__ == "__main__":
  p, n = map(int, sys.stdin.readline().split())
  if p == 1:
    a, k = map(int, sys.stdin.readline().split())
    sys.stdout.write(str(solve1(a, k)))
  elif p == 2:
    arr = []
    for _ in range(n):
      a, k = map(int, sys.stdin.readline().split())
      arr.append((a, k))
    sys.stdout.write(solve2(arr))
