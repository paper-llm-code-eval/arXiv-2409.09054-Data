class SuffixArray:
    def __init__(self, t):
        self.t = t + chr(0)
        n = len(self.t)
        self.p = [0] * n
        self.c = [0] * n
        self.lcp = [0] * (n - 1)
        self.build()
                    
    def build(self):
        n = len(self.t)
        self.p = [i for i in range(n)]
        self.c = [0] * n
        
        def cmp(i, j):
            if self.c[i] != self.c[j]: return self.c[i] < self.c[j]
            i += 1
            j += 1
            if i == n or j == n: return i > j
            return self.c[i] < self.c[j]
        
        K = 0
        for i in range(n):
            self.c[i] = ord(self.t[i]) if i != n - 1 else -1
        self.p.sort(key=cmp)
        for i in range(1, n):
            if cmp(self.p[i - 1], self.p[i]): K += 1
            self.c[self.p[i]] = K
            
        h, cnt = 0, 0
        for i in range(n):
            if self.c[i] == K: continue
            j = self.p[self.c[i] - 1]
            while i + h < n and j + h < n and self.t[i + h] == self.t[j + h]:
                h += 1
            self.lcp[cnt] = h
            cnt += 1; h -= 1
            if cnt > 1: self.lcp[cnt - 2] = min(h, self.lcp[cnt - 2])
            
            
