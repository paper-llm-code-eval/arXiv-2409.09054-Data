with open("patrat.in") as fin:
    n, m = map(int, fin.readline().split())
    grid = [list(map(int, line.split())) for line in fin]

def is_brighter(row, col):
    return grid[row][col] > max(
        grid[row - 1][col - 1:col + 2],
        grid[row][col - 1:col + 2],
        grid[row + 1][col - 1:col + 2],
    )

def count_stars():
    count = 0
    for row in range(1, n - 1):
        for col in range(1, m - 1):
            if is_brighter(row, col):
                count += 1
    return count

def count_squares():
    max_side = 1
    for row in range(n - 2):
        for col in range(m - 2):
            if (
                is_brighter(row, col)
                and is_brighter(row + 1, col)
                and is_brighter(row, col + 1)
                and is_brighter(row + 1, col + 1)
            ):
                max_side = max(max_side, 2)
    return max_side

def count_max_square():
    max_side = 0
    for row in range(n - 4):
        for col in range(m - 4):
            for side in range(2, min(n - row, m - col) + 1):
                if all(
                    is_brighter(r, c) for r in range(row, row + side) for c in range(col, col + side)
                ):
                    max_side = max(max_side, side)
    return max_side

with open("patrat.out", "w") as fout:
    fout.write(f"{count_stars()}\n")
    fout.write(f"{count_squares()}\n")
    fout.write(f"{count_max_square()}\n")
