import sys

with open('patrat.in') as f:
    n, m = map(int, f.readline().split())
    table = [list(map(int, line.split())) for line in f.readlines()]

def is_shining(i, j):
    return table[i][j] > max(table[i - 1][j - 1: j + 2], table[i][j - 1: j + 2], table[i + 1][j - 1: j + 2])

shining_stars = 0
for i in range(1, n - 1):
    for j in range(1, m - 1):
        if is_shining(i, j):
            shining_stars += 1

max_square_side = 0
for side in range(min(n, m) - 1, 0, -1):
    for i in range(side, n - side):
        for j in range(side, m - side):
            if is_shining(i, j) and is_shining(i + side - 1, j) and is_shining(i, j + side - 1) and is_shining(i + side - 1, j + side - 1):
                max_square_side = side
    if max_square_side > 0:
        break

square_constellations = 0
for side in range(1, max_square_side + 1):
    for i in range(side - 1, n - side + 1):
        for j in range(side - 1, m - side + 1):
            if is_shining(i, j) and is_shining(i + side - 1, j) and is_shining(i, j + side - 1) and is_shining(i + side - 1, j + side - 1):
                square_constellations += 1

with open('patrat.out', 'w') as f:
    f.write(str(shining_stars) + '\n')
    f.write(str(square_constellations) + '\n')
    f.write(str(max_square_side) + '\n')
```
