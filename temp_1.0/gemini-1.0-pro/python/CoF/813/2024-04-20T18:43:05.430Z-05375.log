with open("adunscad.in") as in_file:
    N, M = map(int, in_file.readline().split())
    a = list(map(int, in_file.readline().split()))

if M * 9 < N or (N > 0 and not any(i > 9 - N for i in a)) or (N < 0 and not any(i < -(9 - N) for i in a)):
    with open("adunscad.out", "w") as out_file:
        out_file.write("0")
else:
    def dfs(i, sum):
        if i == M:
            return sum == N
        if dfs(i + 1, sum + a[i]):
            return True
        if sum + a[i] <= N and dfs(i + 1, sum + a[i]):
            return True
        if sum - a[i] >= N and dfs(i + 1, sum - a[i]):
            return True
        return False

    ans = []
    dfs(0, 0)

    for i in range(M):
        if dfs(0, 0):
            ans += [str(a[i])]
            a[i] = -a[i]
        else:
            ans += ["+" + str(a[i])]

    with open("adunscad.out", "w") as out_file:
        out_file.write("".join(ans))
