        return time, subgroups
    else:
        info, amount = group.split(" ", 1)
        return int(amount), [info]

def compute_time(recipe):
    time, subgroups = recipe
    return time + max([compute_time(subgroup) for subgroup in subgroups])

def compute_ingredients(recipe):
