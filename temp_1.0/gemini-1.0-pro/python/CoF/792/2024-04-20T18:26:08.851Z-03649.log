time = compute_time(recipe)
ingredients = compute_ingredients(recipe)

with open("reteta.out", "w") as fout:
    fout.write(f"{time}\n")
    for ingredient, quantity in sorted(ingredients.items()):
        fout.write(f"{ingredient} {quantity}\n")
