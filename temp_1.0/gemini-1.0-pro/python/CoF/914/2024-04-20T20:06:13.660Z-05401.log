def rounded_divide(numerator, denominator):
    val = (numerator + denominator // 2) // denominator
    return val


if __name__ == "__main__":
    with open("pif.in") as f:
        n = int(f.readline())
        k = int(f.readline())
        zv, zt = map(int, f.readline().split())

    Total_Actions = k + 1
    days_to_add = zv - zt
    if days_to_add == 0:
        interval_actions = Total_Actions // 4
        new_old_actions = Total_Actions % 4
    else:
        if days_to_add > 0:
            days_after_actions = Total_Actions // 2
            interval_actions = n // days_after_actions
            if n % days_after_actions != 0:
                interval_actions += 1
            new_old_actions = rounded_divide((n - interval_actions) * zt, zv)
        else:
            days_before_actions = Total_Actions // 2
            interval_actions = n // days_before_actions
            old_new_actions = rounded_divide((n - interval_actions) * zv, zt)
            new_old_actions = interval_actions - old_new_actions - 1

    old_actions = min(Total_Actions // 2, interval_actions * Total_Actions // 4)
    Tot = old_actions * zt
    Tot -= (new_old_actions + old_actions) * zv
    Tot += new_old_actions * zv * zt + new_old_actions
    Tot %= 1234567
    with open("pif.out", "w") as f:
        f.write(str(Tot) + "\n")
