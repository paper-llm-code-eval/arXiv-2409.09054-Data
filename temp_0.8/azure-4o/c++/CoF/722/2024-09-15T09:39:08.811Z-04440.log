#include <iostream>
#include <fstream>
#include <vector>
#include <queue>
#include <climits>

using namespace std;

struct Cell {
    int x, y;
    int cost;
    Cell(int _x, int _y, int _cost) : x(_x), y(_y), cost(_cost) {}
    bool operator>(const Cell& other) const {
        return cost > other.cost;
    }
};

int main() {
    ifstream inFile("taxe.in");
    ofstream outFile("taxe.out");

    if (!inFile || !outFile) {
        cerr << "Error opening file" << endl;
        return 1;
    }

    int S, n;
    inFile >> S >> n;

    vector<vector<int>> tax(n, vector<int>(n));
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < n; ++j) {
            inFile >> tax[i][j];
        }
    }

    vector<vector<int>> minCost(n, vector<int>(n, INT_MAX));
    priority_queue<Cell, vector<Cell>, greater<Cell>> pq;

    minCost[0][0] = tax[0][0];
    pq.push(Cell(0, 0, tax[0][0]));

    int dx[] = {0, 1, 0, -1};
    int dy[] = {1, 0, -1, 0};

    while (!pq.empty()) {
        Cell current = pq.top();
        pq.pop();

        if (current.cost > minCost[current.x][current.y]) continue;

        for (int i = 0; i < 4; ++i) {
            int newX = current.x + dx[i];
            int newY = current.y + dy[i];
            if (newX >= 0 && newX < n && newY >= 0 && newY < n) {
                int newCost = current.cost + tax[newX][newY];
                if (newCost < minCost[newX][newY]) {
                    minCost[newX][newY] = newCost;
                    pq.push(Cell(newX, newY, newCost));
                }
            }
        }
    }

    int totalCost = minCost[n-1][n-1];
    if (totalCost <= S) {
        outFile << S - totalCost << endl;
    } else {
        outFile << -1 << endl;
    }

    inFile.close();
    outFile.close();
    
    return 0;
}
