#include <fstream>
#include <vector>
#include <set>
#include <algorithm>

using namespace std;

void solve(vector<int>& v) {
    set<int> result;
    int n = v.size();
    sort(v.begin(), v.end());

    // A recursive helper function to simulate the negotiation process
    function<void(vector<int>&)> negotiate = [&](vector<int>& current) {
        if (current.size() == 1) {
            result.insert(current[0]);
            return;
        }

        for (int i = 0; i < current.size() - 1; ++i) {
            vector<int> next = current;
            if (current[i] < current[i + 1]) {
                next[i] = 0;
                next[i + 1] -= current[i];
            } else if (current[i] > current[i + 1]) {
                next[i] -= current[i + 1];
                next[i + 1] = 0;
            } else if (current[i] == current[i + 1]) {
                next[i] = 0;
                next[i + 1] *= 2;
            }

            next.erase(remove(next.begin(), next.end(), 0), next.end());
            if (next != current) {
                negotiate(next);
            }
        }
    };

    negotiate(v);

    ofstream fout("nunta.out");
    fout << result.size() << "\n";
    for (const int& num : result) {
        fout << num << " ";
    }
    fout.close();
}

int main() {
    ifstream fin("nunta.in");
    int n;
    fin >> n;
    vector<int> v(n);
    for (int i = 0; i < n; ++i) {
        fin >> v[i];
    }
    fin.close();

    solve(v);
    return 0;
}
