#include <bits/stdc++.h>
using namespace std;

const int MAXN = 100000;
const int MAXVAL = 1000000;

vector<int> primes;
bool is_prime[MAXVAL + 1];
int smallest_prime[MAXVAL + 1];
int largest_prime[MAXVAL + 1];

void sieve() {
    fill(is_prime, is_prime + MAXVAL + 1, true);
    is_prime[0] = is_prime[1] = false;

    for (int i = 2; i <= MAXVAL; ++i) {
        if (is_prime[i]) {
            primes.push_back(i);
            smallest_prime[i] = largest_prime[i] = i;
            for (int j = 2 * i; j <= MAXVAL; j += i) {
                is_prime[j] = false;
                if (smallest_prime[j] == 0) smallest_prime[j] = i;
                largest_prime[j] = i;
            }
        }
    }
}

ifstream fin("tema.in");
ofstream fout("tema.out");

void solve1(int N, long long K, vector<int>& A) {
    long long sum_prime = 0, sum_comp = 0;
    int max_len = 0, l = 0;
    for (int r = 0; r < N; ++r) {
        if (is_prime[A[r]]) sum_prime += A[r];
        else if (A[r] != 1) sum_comp += A[r];

        while (sum_prime * sum_comp > K && l <= r) {
            if (is_prime[A[l]]) sum_prime -= A[l];
            else if (A[l] != 1) sum_comp -= A[l];
            ++l;
        }
        if (l <= r) max_len = max(max_len, r - l + 1);
    }
    fout << max_len << "\n";
}

void solve2(int N, vector<int>& A) {
    vector<int> B(N);
    for (int i = 0; i < N; ++i) {
        if (is_prime[A[i]] || A[i] == 1) B[i] = A[i];
        else B[i] = smallest_prime[A[i]] * largest_prime[A[i]];
    }

    pair<int, int> best(-1, -1);
    int max_len = 0;

    for (int i = 0; i < N; ++i) {
        int g = B[i];
        for (int j = i; j < N; ++j) {
            g = __gcd(g, B[j]);
            if (g == 1) break;
            if (j - i + 1 > max_len) {
                max_len = j - i + 1;
                best = {i + 1, j + 1};
            } else if (j - i + 1 == max_len) {
                if (i + 1 > best.first) best = {i + 1, j + 1};
            }
        }
    }

    fout << best.first << " " << best.second << "\n";
}

int main() {
    sieve();
    int C, N;
    long long K;
    fin >> C >> N >> K;

    vector<int> A(N);
    for (int i = 0; i < N; ++i) fin >> A[i];

    if (C == 1) solve1(N, K, A);
    else if (C == 2) solve2(N, A);

    return 0;
}
```

### Explanation:

1. **Sieve of Eratosthenes**:
   - Calculate prime numbers up to `MAXVAL`.
   - For each number, find the smallest and largest prime factors.

2. **Reading Input**:
   - Use `ifstream` to read from `tema.in`.
   - Use `ofstream` to write to `tema.out`.

3. **Solve Function for C=1**:
   - Sliding window technique to find the longest subarray with cost less than or equal to `K`.

4. **Solve Function for C=2**:
   - Construct a new array `B`.
   - Iterate through all possible subarrays to find the one with the largest gcd > 1.

5. **Main Function**:
   - Call the appropriate solve function based on the given `C` value.
