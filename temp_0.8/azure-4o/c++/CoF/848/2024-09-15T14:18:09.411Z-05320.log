#include <iostream>
#include <fstream>
#include <vector>
#include <queue>

using namespace std;

enum Arrow { RIGHT = 1, UP, LEFT, DOWN };

struct Position {
    int x, y;
};

const int directions[5][2] = {
    {0, 0},
    {0, 1},  // right
    {-1, 0}, // up
    {0, -1}, // left
    {1, 0}   // down
};

int task, N, M, start_x, start_y;
vector<vector<int>> board;
vector<vector<bool>> visited;

bool isInsideBoard(int x, int y) {
    return x >= 0 && x < N && y >= 0 && y < M;
}

int calculateScore(int x, int y) {
    int score = 0;
    while (isInsideBoard(x, y) && !visited[x][y]) {
        visited[x][y] = true;
        score++;
        int dir = board[x][y];
        x += directions[dir][0];
        y += directions[dir][1];
    }
    if (!isInsideBoard(x, y)) {
        return score;
    }
    return -1; // indicates a favorable position
}

int countFavorablePositions() {
    int favorableCount = 0;
    for (int i = 0; i < N; ++i) {
        for (int j = 0; j < M; ++j) {
            fill(visited.begin(), visited.end(), vector<bool>(M, false));
            if (calculateScore(i, j) == -1) {
                favorableCount++;
            }
        }
    }
    return favorableCount;
}

int findMaxScore() {
    int maxScore = 0;
    for (int i = 0; i < N; ++i) {
        for (int j = 0; j < M; ++j) {
            fill(visited.begin(), visited.end(), vector<bool>(M, false));
            int score = calculateScore(i, j);
            if (score == -1) {
                score = 1000 * count(visited.begin(), visited.end(), true);
            }
