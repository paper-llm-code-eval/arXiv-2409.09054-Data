#include <fstream>
#include <string>
#include <vector>
#include <iostream>
#include <algorithm>
#include <cmath>

// Function to check if a number is prime
bool isPrime(int num) {
    if (num <= 1) return false;
    if (num <= 3) return true;
    if (num % 2 == 0 || num % 3 == 0) return false;
    for (int i = 5; i * i <= num; i += 6) {
        if (num % i == 0 || num % (i + 2) == 0) return false;
    }
    return true;
}

// Function to get the maximum prime from a list of numbers
int getMaxPrime(const std::vector<int>& numbers) {
    int maxPrime = 0;
    for (int num : numbers) {
        if (isPrime(num) && num > maxPrime) {
            maxPrime = num;
        }
    }
    return maxPrime;
}

// Function to generate numbers by splitting a number into two parts
std::vector<int> splitIntoTwo(const std::string& numStr) {
    std::vector<int> results;
    int len = numStr.length();
    for (int i = 1; i < len; ++i) {
        std::string part1 = numStr.substr(0, i);
        std::string part2 = numStr.substr(i);
        results.push_back(std::stoi(part1));
        results.push_back(std::stoi(part2));
    }
    return results;
}

// Function to generate numbers by splitting a number into three parts
std::vector<int> splitIntoThree(const std::string& numStr) {
    std::vector<int> results;
    int len = numStr.length();
    for (int i = 1; i < len - 1; ++i) {
        for (int j = i + 1; j < len; ++j) {
            std::string part1 = numStr.substr(0, i);
            std::string part2 = numStr.substr(i, j - i);
            std::string part3 = numStr.substr(j);
            results.push_back(std::stoi(part1));
            results.push_back(std::stoi(part2));
            results.push_back(std::stoi(part3));
        }
    }
    return results;
}

int main() {
    std::ifstream inputFile("tai.in");
    std::ofstream outputFile("tai.out");

    int C, N;
    inputFile >> C >> N;

    std::vector<int> numbers(N);
    for (int i = 0; i < N; ++i) {
        inputFile >> numbers[i];
    }

    int result = 0;

    if (C == 1) {
        result = getMaxPrime(numbers);
    } else if (C == 2) {
        std::vector<int> splitNumbers;
        for (int num : numbers) {
            std::vector<int> parts = splitIntoTwo(std::to_string(num));
            splitNumbers.insert(splitNumbers.end(), parts.begin(), parts.end());
        }
        result = getMaxPrime(splitNumbers);
    } else if (C == 3) {
        std::vector<int> splitNumbers;
        for (int num : numbers) {
            std::vector<int> parts = splitIntoThree(std::to_string(num));
            splitNumbers.insert(splitNumbers.end(), parts.begin(), parts.end());
        }
        result = getMaxPrime(splitNumbers);
    }

    outputFile << result << std::endl;
    inputFile.close();
    outputFile.close();
    
    return 0;
}
