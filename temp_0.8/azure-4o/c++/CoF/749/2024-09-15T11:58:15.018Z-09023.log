#include <iostream>
#include <fstream>
#include <vector>
#include <string>

using namespace std;

int main() {
    ifstream fin("case.in");
    ofstream fout("case.out");

    int n;
    fin >> n;

    vector<int> degreeCount(3, 0);
    int men = 0, women = 0, children = 0;

    for (int i = 0; i < n; ++i) {
        string house;
        fin >> house;
        
        int degree = house[0] - '0';
        int numMen = house[1] - '0';
        int numWomen = house[2] - '0';
        int numChildren = house[3] - '0';

        degreeCount[degree - 1]++;
        men += numMen;
        women += numWomen;
        children += numChildren;
    }

    fout << degreeCount[0] << endl;
    fout << degreeCount[1] << endl;
    fout << degreeCount[2] << endl;
    fout << men << endl;
    fout << women << endl;
    fout << children << endl;

    fin.close();
    fout.close();

    return 0;
}
```

