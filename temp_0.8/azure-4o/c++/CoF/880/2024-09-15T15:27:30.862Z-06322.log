        getline(inFile, program);
        solveC1(program, outFile);
    } else if (C == 2) {
        string program;
        getline(inFile, program);
        solveC2(program, outFile);
    } else if (C == 3) {
        int N;
        inFile >> N;
        solveC3(N, outFile);
    }

    inFile.close();
    outFile.close();
    return 0;
}
