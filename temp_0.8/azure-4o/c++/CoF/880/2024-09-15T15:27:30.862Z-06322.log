#include <fstream>
#include <string>
#include <vector>
#include <cmath>

using namespace std;

void solveC1(const string& program, ofstream& outFile) {
    int count = 0;
    for (char ch : program) {
        if (ch == 'A') {
            ++count;
        }
    }
    outFile << count << endl;
}

void solveC2(const string& program, ofstream& outFile) {
    vector<char> screen;
    int position = 0;
    for (char ch : program) {
        if (ch == 'D') {
            position = (position + (ch - '0')) % 10;
        } else if (ch == 'S') {
            position = (position - (ch - '0') + 10) % 10;
        } else if (ch == 'A') {
            screen.push_back('0' + position);
        }
    }
    for (char digit : screen) {
        outFile << digit;
    }
    outFile << endl;
}

void solveC3(int N, ofstream& outFile) {
    string number = to_string(N);
    int length = number.length();
    int current_position = 0;
    string program = "";

    for (int i = 0; i < length; ++i) {
        int target_digit = number[i] - '0';
        int move_right = (target_digit - current_position + 10) % 10;
        int move_left = (current_position - target_digit + 10) % 10;

        if (move_right <= move_left) {
            if (move_right > 0) {
                program += "D" + to_string(move_right);
            }
        } else {
            if (move_left > 0) {
                program += "S" + to_string(move_left);
            }
        }
        program += "A";
        current_position = target_digit;
    }
    program += "T";
    outFile << program << endl;
}

int main() {
    ifstream inFile("robot.in");
    ofstream outFile("robot.out");

    int C;
    inFile >> C;
    inFile.ignore(); // Ignore the newline after the integer

