#include <fstream>
#include <vector>
#include <string>
#include <algorithm>

using namespace std;

// Function to check if str1 is a subsequence of str2
bool isSubsequence(const string& str1, const string& str2) {
    int m = str1.size(), n = str2.size();
    int j = 0;
    for (int i = 0; i < n && j < m; i++) {
        if (str1[j] == str2[i]) {
            j++;
        }
    }
    return (j == m);
}

int main() {
    ifstream fin("interesant.in");
    ofstream fout("interesant.out");

    int p, N;
    fin >> p >> N;
    vector<string> strings(N);
    for (int i = 0; i < N; i++) {
        fin >> strings[i];
    }

    if (p == 1) {
        // Requirement 1: Find the longest string (lexicographically smallest if multiple)
        string longest = "";
        for (const auto& str : strings) {
            if (str.size() > longest.size() || (str.size() == longest.size() && str < longest)) {
                longest = str;
            }
        }
        fout << longest << endl;
    } else if (p == 2) {
        // Requirement 2: Find all interesting strings
        vector<string> interesting;
        for (int i = 0; i < N; i++) {
            bool isInteresting = true;
            for (int j = 0; j < N; j++) {
                if (i != j && isSubsequence(strings[i], strings[j])) {
                    isInteresting = false;
                    break;
                }
            }
            if (isInteresting) {
                interesting.push_back(strings[i]);
            }
        }
        fout << interesting.size() << endl;
        for (const auto& str : interesting) {
            fout << str << endl;
        }
    }

    return 0;
}
