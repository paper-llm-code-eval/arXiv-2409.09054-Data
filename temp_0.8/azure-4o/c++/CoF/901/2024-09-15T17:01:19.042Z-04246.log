#include <iostream>
#include <fstream>
#include <string>
#include <unordered_map>
#include <vector>
#include <algorithm>
#include <tuple>

void parse_expression(const std::string &expr, std::unordered_map<char, int> &coefficients, int &constant) {
    int n = expr.size();
    int i = 0;
    while (i < n) {
        int sign = 1;
        if (expr[i] == '+' || expr[i] == '-') {
            if (expr[i] == '-') sign = -1;
            ++i;
        }
        
        int coef = 0;
        bool has_number = false;
        while (i < n && isdigit(expr[i])) {
            coef = coef * 10 + (expr[i] - '0');
            ++i;
            has_number = true;
        }
        if (!has_number) coef = 1;
        coef *= sign;
        
        if (i < n && isalpha(expr[i])) {
            coefficients[expr[i]] += coef;
            ++i;
        } else {
            constant += coef;
        }
    }
}

int value_expression(const std::unordered_map<char, int> &coefficients, int constant) {
    int result = constant;
    for (const auto &kv : coefficients) {
        result += kv.second;
    }
    return result;
}

int count_quartets(const std::unordered_map<char, int> &coefficients, int constant, int a, int b, long long E) {
    int count = 0;
    for (int x = a; x <= b; ++x) {
        for (int y = a; y <= b; ++y) {
            for (int z = a; z <= b; ++z) {
                for (int t = a; t <= b; ++t) {
                    long long value = constant;
                    for (const auto &kv : coefficients) {
                        if (kv.first == 'x') value += kv.second * x;
                        else if (kv.first == 'y') value += kv.second * y;
                        else if (kv.first == 'z') value += kv.second * z;
                        else if (kv.first == 't') value += kv.second * t;
                    }
                    if (value == E) {
                        ++count;
                    }
                }
            }
        }
    }
    return count;
}

int main() {
    std::ifstream infile("eq4.in");
    std::ofstream outfile("eq4.out");

    int C;
    std::string expression;
    int a, b;
    long long E;

    infile >> C;
    infile >> expression;
    infile >> a >> b >> E;

    std::unordered_map<char, int> coefficients;
    int constant = 0;
    parse_expression(expression, coefficients, constant);

    if (C == 1) {
        int result = value_expression(coefficients, constant);
        outfile << result << std::endl;
    } else if (C == 2) {
        int count = count_quartets(coefficients, constant, a, b, E);
        outfile << count << std::endl;
    }

    infile.close();
    outfile.close();

    return 0;
}
```

### Explanation:
1. **Parsing the Expression**:
   - The `parse_expression` function reads through the expression string and extracts coefficients for each variable as well as the constant term.
   - It uses an unordered map to keep track of the coefficients of `x`, `y`, `z`, and `t`.

2. **Value of the Expression**:
   - The `value_expression` function computes the value of the expression when `x=y=z=t=1`.

3. **Counting Quartets**:
   - The `count_quartets` function iterates over all possible values of `x`, `y`, `z`, and `t` in the given range `[a, b]` and counts the number of combinations that fulfill the condition.

4. **Main Function**:
   - Reads input from `eq4.in`.
   - Depending on the value of `C`, it either computes the value of the expression for `x=y=z=t=1` or counts the number of quartets that evaluate to `E`.
   - Outputs the result to `eq4.out`.

### A Note on Performance:
The nested loops in `count_quartets` have a complexity of `O((b-a+1)^4)`, which ensures that the algorithm is performant given the constraints.
