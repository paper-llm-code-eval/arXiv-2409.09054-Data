#include <iostream>
#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

bool isPrime(int num) {
    if (num <= 1) return false;
    if (num <= 3) return true;
    if (num % 2 == 0 || num % 3 == 0) return false;
    for (int i = 5; i * i <= num; i += 6) {
        if (num % i == 0 || num % (i + 2) == 0) return false;
    }
    return true;
}

int sumOfDigits(int num) {
    int sum = 0;
    while (num) {
        sum += num % 10;
        num /= 10;
    }
    return sum;
}

pair<int, int> calculateDuration(int H1, int M1, int H2, int M2) {
    int startMinutes = H1 * 60 + M1;
    int endMinutes = H2 * 60 + M2;
    if (endMinutes < startMinutes) {
        endMinutes += 24 * 60;
    }
    int durationMinutes = endMinutes - startMinutes;
    return {durationMinutes / 60, durationMinutes % 60};
}

int main() {
    ifstream fin("aeriana.in");
    ofstream fout("aeriana.out");

    int C, N;
    fin >> C >> N;

    int maxDurationBefore = 0;
    int maxDurationAfter = 0;

    for (int i = 0; i < N; ++i) {
        int A1, A2, H1, M1, H2, M2;
        fin >> A1 >> A2 >> H1 >> M1 >> H2 >> M2;

        auto durationBefore = calculateDuration(H1, M1, H2, M2);
        int totalMinutesBefore = durationBefore.first * 60 + durationBefore.second;
        maxDurationBefore = max(maxDurationBefore, totalMinutesBefore);

        bool isSpecial = isPrime(A1) && (A2 % sumOfDigits(A1) == 0);
        if (isSpecial) {
            auto durationAfter = calculateDuration(H2, M2, H1, M1);
            int totalMinutesAfter = durationAfter.first * 60 + durationAfter.second;
            maxDurationAfter = max(maxDurationAfter, totalMinutesAfter);
        } else {
            maxDurationAfter = max(maxDurationAfter, totalMinutesBefore);
        }
    }

    if (C == 1) {
        fout << maxDurationBefore / 60 << " " << maxDurationBefore % 60 << endl;
    } else if (C == 2) {
        fout << maxDurationAfter / 60 << " " << maxDurationAfter % 60 << endl;
    }

    fin.close();
    fout.close();

    return 0;
}
```

### Explanation:
1. **Functions**: 
    - `isPrime(int num)`: Checks if a number is prime.
    - `sumOfDigits(int num)`: Computes the sum of the digits of a number.
    - `calculateDuration(int H1, int M1, int H2, int M2)`: Computes the duration between two times, considering crossing midnight if necessary.
2. **Main Logic**:
    - Read `C` (condition) and `N` (number of flights).
    - For each flight, compute its duration before and after the virus's potential intervention.
    - Check if the flight is special and update the maximum durations accordingly.
3. **Output**:
    - Depending on the value of `C`, output the relevant maximum duration.
