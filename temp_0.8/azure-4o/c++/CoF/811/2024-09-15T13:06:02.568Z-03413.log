// Function to calculate the number of divisors for a given number
int countDivisors(int num) {
    int count = 0;
    for (int i = 1; i <= sqrt(num); ++i) {
        if (num % i == 0) {
            if (num / i == i)
                count++;
            else
                count += 2;
        }
    }
    return count;
}

