#include <iostream>
#include <fstream>
#include <vector>
#include <cmath>
#include <algorithm>

using namespace std;

// Function to calculate the k-th prime divisor of a number
int kth_prime_divisor(int num, int k) {
    int count = 0;
    for (int i = 2; i <= sqrt(num); ++i) {
        while (num % i == 0) {
            num /= i;
            if (++count == k)
                return i;
        }
    }
    if (num > 1 && ++count == k)
        return num;
    return -1; // This case should never be hit due to problem constraints
}

int main() {
    // Initialize file streams
    ifstream fin("cufar.in");
    ofstream fout("cufar.out");

    int p, n;
    fin >> p >> n;

    if (p == 1) {
        int number, k;
        fin >> number >> k;
        fout << kth_prime_divisor(number, k) << "\n";
    } else if (p == 2) {
        long long magic_sum = 0;
        for (int i = 0; i < n; ++i) {
            int number, k;
            fin >> number >> k;
            magic_sum += kth_prime_divisor(number, k);
        }
        fout << magic_sum << "\n";
    }

    fin.close();
    fout.close();
    return 0;
}
```

