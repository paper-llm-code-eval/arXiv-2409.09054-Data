import sys

def abs(x):
    return x if x >= 0 else -x

def main():
    n = int(sys.stdin.readline().strip())
    xs = []
    ys = []
    for i in range(n):
        x, y = map(int, sys.stdin.readline().strip().split())
        xs.append(x)
        ys.append(y)

    max_points = 0
    for x in xs:
        max_points = max(max_points, xs.count(x))
    for y in ys:
        max_points = max(max_points, ys.count(y))

    lines = []
    for i in range(n):
        for j in range(i+1, n):
            if xs[i] == xs[j]:
                continue
            if abs(xs[i] - xs[j]) == abs(ys[i] - ys[j]):
                lines.append((xs[i], xs[j]))
            elif xs[i] + ys[i] == xs[j] + ys[j]:
                lines.append((xs[i], xs[j]))

    unique_lines = set(lines)
    num_lines = len(unique_lines)

    i = 0
    while i < n:
        remove = []
        for j in range(i+3, n):
            if xs[j] >= xs[i]:
                break
        for j in range(i+3, n):
            if xs[j] == xs[i]:
                remove.append(j)
        for j in remove:
            xs.pop(j)
            ys.pop(j)
            n -= 1
        i += 3

    if len(xs) == 1:
        x_point = xs[0]
    else:
        x_point = xs[1]

    with open('puncte.out', 'w') as f:
        f.write(str(max_points) + '\n')
        f.write(str(num_lines) + '\n')
        f.write(str(x_point) + '\n')

if __name__ == '__main__':
    main()
