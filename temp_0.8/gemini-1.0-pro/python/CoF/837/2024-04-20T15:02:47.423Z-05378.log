with open("puncte.in") as fin, open("puncte.out", "w") as fout:
    n = int(fin.readline())
    x = [0] * n
    y = [0] * n
    for i in range(n):
        a, b = map(int, fin.readline().split())
        x[i] = a
        y[i] = b

    # Level 1
    nrp = 0
    for i in range(n):
        cntx = 1
        cnty = 1
        for j in range(n):
            if x[i] == x[j]:
                cntx += 1
            if y[i] == y[j]:
                cnty += 1
        if cntx > nrp:
            nrp = cntx
        if cnty > nrp:
            nrp = cnty

    # Level 2
    nrd = 0
    diag1 = {}
    diag2 = {}
    for i in range(n):
        if x[i] == y[i]:
            if x[i] not in diag1:
                diag1[x[i]] = 0
            diag1[x[i]] += 1
        if x[i] + y[i] == 0:
            if y[i] not in diag2:
                diag2[y[i]] = 0
            diag2[y[i]] += 1
    for i in diag1:
        nrd += diag1[i] * (diag1[i] - 1) // 2
    for i in diag2:
        nrd += diag2[i] * (diag2[i] - 1) // 2

    # Level 3
    ind = 0
    while n > 1:
        step = 3
        while ind + step - 1 < n and x[ind + step - 1] == x[ind]:
            step += 3
        for i in range(ind, ind + step - 1):
            x[i] = 1000000
            y[i] = 1000000
        ind += step
        while ind < n and x[ind] == 1000000:
            ind += 1
        n -= step
    xp = x[ind]
    fout.write(str(nrp) + "\n")
    fout.write(str(nrd) + "\n")
    fout.write(str(xp) + "\n")
```
