import sys

with open('puncte.in', 'r') as f:
    n = int(f.readline())
    
    # Initializam variabilele de iesire
    nrp = 0  # numarul maxim de puncte pe o dreapta
    nrd = 0  # numarul de drepte distincte
    xp = 0  # abscisa punctului ramas
        
    # Initializam dictionare pentru a stoca punctele pe fiecare dreapta
    orizontala = {}
    verticala = {}
    diagonala1 = {}
    diagonala2 = {}
    
    # Citim punctele si le stocam in dictionare
    for i in range(n):
        x, y = map(int, f.readline().split())
        
        # Actualizam numarul maxim de puncte pe o dreapta
        nrp = max(nrp, orizontala.get(y, 0) + 1)
        nrp = max(nrp, verticala.get(x, 0) + 1)
        nrp = max(nrp, diagonala1.get(x - y, 0) + 1)
        nrp = max(nrp, diagonala2.get(x + y, 0) + 1)
        
        # Adaugam punctul in dictionarul corespunzator
        orizontala[y] = orizontala.get(y, 0) + 1
        verticala[x] = verticala.get(x, 0) + 1
        diagonala1[x - y] = diagonala1.get(x - y, 0) + 1
        diagonala2[x + y] = diagonala2.get(x + y, 0) + 1
        
        # Actualizam numarul de drepte distincte
        if x == y or x + y == 0:
            nrd += 1
    
    # Stergem punctele din 3 in 3
    while n > 1:
        for i in range(n):
            if i % 3 == 0:
                xp = x
        
        # Stergem punctele din dictionare
        orizontala[y] -= 1
        verticala[x] -= 1
        diagonala1[x - y] -= 1
        diagonala2[x + y] -= 1
        
        n -= 1
        x, y = xp, y + 3
    
    # Scriem rezultatele in fisierul de iesire
    with open('puncte.out', 'w') as f:
        f.write(str(nrp) + '\n')
        f.write(str(nrd) + '\n')
        f.write(str(xp) + '\n')
