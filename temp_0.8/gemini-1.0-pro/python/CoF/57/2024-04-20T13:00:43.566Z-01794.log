import sys

def citire(fisin):
    n, m, k = map(int, fisin.readline().split())
    muchii = []
    for i in range(m):
        a, b, c = map(int, fisin.readline().split())
        muchii.append((a - 1, b - 1, c))
    return n, k, muchii

def kruskal(n, k, muchii):
    p = list(range(n))
    rank = [0] * n

    def find(nod):
        if p[nod] != nod:
            p[nod] = find(p[nod])
        return p[nod]

    def union(a, b):
        ra = find(a)
        rb = find(b)
        if ra != rb:
