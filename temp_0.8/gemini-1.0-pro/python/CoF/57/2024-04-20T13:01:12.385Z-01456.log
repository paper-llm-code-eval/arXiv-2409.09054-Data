import sys

fin = open("urgenta.in", "r")
fout = open("urgenta.out", "w")

n = int(fin.readline())
m = int(fin.readline())
k = int(fin.readline())

adj = [[] for i in range(n + 1)]
for i in range(m):
    a, b, w = map(int, fin.readline().split())
    adj[a].append((b, w))
    adj[b].append((a, w))

vis = [False for i in range(n + 1)]
comp = [[] for i in range(n + 1)]

def dfs(node, c):
    vis[node] = True
    comp[c].append(node)
    for u, w in adj[node]:
        if not vis[u]:
            dfs(u, c)

def remove_edges(comp1, comp2):
    for node1 in comp1:
        for node2 in comp2:
            for i in range(len(adj[node1])):
                if adj[node1][i][0] == node2:
                    adj[node1].pop(i)
                    break

max_score = 0
max_comp = []
for i in range(1, n + 1):
    if not vis[i]:
        dfs(i, len(comp))
        if len(comp[len(comp) - 1]) > max_score:
            max_score = len(comp[len(comp) - 1])
            max_comp = comp[len(comp) - 1]
            max_comp.sort()

comp.pop(len(comp) - 1)
for i in range(len(comp) - k + 1):
    remove_edges(max_comp, comp[i])

score = 0
for i in range(1, n + 1):
    for j in range(len(adj[i])):
        score += adj[i][j][1]

fout.write(str(score) + "\n")
fout.write(str(len(adj) - 1) + "\n")
for i in range(1, n + 1):
    for j in range(len(adj[i])):
        if adj[i][j][0] > i:
            fout.write(str(i) + " " + str(adj[i][j][0]) + "\n")

fin.close()
fout.close()
