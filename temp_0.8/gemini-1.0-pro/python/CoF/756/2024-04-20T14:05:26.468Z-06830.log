def is_palindrome(n: int) -> bool:
    return str(n) == str(n)[::-1]

with open("mare.in", "r") as f_in, open("mare.out", "w") as f_out:
    n = int(f_in.readline())
    numbers = [int(x) for x in f_in.readline().split()]

    palindromes = []

    for i in range(n):
        for j in range(i + 1, n + 1):
            num = int(''.join(map(str, numbers[i:j])))
            if is_palindrome(num):
                palindromes.append(num)

    f_out.write(str(max(palindromes)))
```
