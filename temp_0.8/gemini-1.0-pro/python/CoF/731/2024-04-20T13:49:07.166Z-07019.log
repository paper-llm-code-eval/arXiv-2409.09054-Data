import sys

# Read input
k = int(sys.stdin.readline())
trase = []
for i in range(k):
    line = sys.stdin.readline().strip()
    traseu = [int(x) for x in line.split()]
    traseu[0] -= 1  # index 0-based
    trase.append(traseu)
t, u = map(int, sys.stdin.readline().strip().split())

# Calculate minimum number of popasuri for each traseu
min_popasuri = []
for traseu in trase:
    popasuri = 0
    dist = 0
    for izvor in traseu[2:]:
        if dist + izvor > t:
            popasuri += 1
            dist = 0
        dist += izvor
    if dist > u:
