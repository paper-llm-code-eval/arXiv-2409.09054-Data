if min_popasuri:
    min_popasuri.sort()
    min_popasuri = min_popasuri[0]
    traseu_min = [i for i, x in enumerate(min_popasuri) if x == min_popasuri][0]
else:
    traseu_min = -1

# Write output
sys.stdout.write(f"{min_popasuri} {traseu_min + 1}")
