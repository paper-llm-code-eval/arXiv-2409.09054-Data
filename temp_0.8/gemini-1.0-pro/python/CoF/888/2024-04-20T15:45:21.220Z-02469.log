import sys

def read_input():
    if not sys.stdin.isatty():
        v = int(sys.stdin.readline().strip())
        if v == 1:
            n, g = map(int, sys.stdin.readline().strip().split())
            matrix = [list(map(int, sys.stdin.readline().strip().split())) for _ in range(n)]
            return v, n, g, matrix
        else:
            n = int(sys.stdin.readline().strip())
            matrix = [list(map(int, sys.stdin.readline().strip().split())) for _ in range(n)]
            return v, n, matrix
    else:
        print("Empty input file", file=sys.stderr)
        sys.exit(1)

def min_dangerous_zones(n, g, matrix):
    visited = [[False for _ in range(n)] for _ in range(n)]
    queue = [(1, 1, 0)]
    min_dangerous = n ** 2
    while queue:
        x, y, dangerous = queue.pop(0)
        if x == y == n:
            min_dangerous = min(min_dangerous, dangerous)
            continue
        for dx, dy in [(1, 0), (-1, 0), (0, 1), (0, -1)]:
            nx, ny = x + dx, y + dy
            if 1 <= nx <= n and 1 <= ny <= n and not visited[nx - 1][ny - 1] and (matrix[nx - 1][ny - 1] >= g or dangerous + 1 < min_dangerous):
                visited[nx - 1][ny - 1] = True
                queue.append((nx, ny, dangerous + (matrix[nx - 1][ny - 1] < g)))
    return min_dangerous

def max_safe_weight(n, matrix):
    visited = [[False for _ in range(n)] for _ in range(n)]
    queue = [(1, 1, 10000)]
    max_safe_weight = 0
    while queue:
        x, y, weight = queue.pop(0)
        if x == y == n:
            max_safe_weight = max(max_safe_weight, weight)
            continue
        for dx, dy in [(1, 0), (-1, 0), (0, 1), (0, -1)]:
            nx, ny = x + dx, y + dy
            if 1 <= nx <= n and 1 <= ny <= n and not visited[nx - 1][ny - 1] and matrix[nx - 1][ny - 1] >= weight:
                visited[nx - 1][ny - 1] = True
                queue.append((nx, ny, min(weight, matrix[nx - 1][ny - 1])))
    return max_safe_weight

def main():
    if not sys.stdin.isatty():
        input_data = read_input()
        if input_data is None:
            sys.exit(1)
        v, n, *args = input_data
        if v == 1:
            g, matrix = args
            result = min_dangerous_zones(n, g, matrix)
            sys.stdout.write(str(result) + "\n")
        else:
            matrix = args[0]
            result = max_safe_weight(n, matrix)
            sys.stdout.write(str(result) + "\n")
    else:
        print("Empty input file", file=sys.stderr)
        sys.exit(1)

if __name__ == "__main__":
    main()
