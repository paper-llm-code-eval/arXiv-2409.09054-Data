with open("cool.in", "r") as f_in, open("cool.out", "w") as f_out:
    p = int(f_in.readline())
    n, k = map(int, f_in.readline().split())
    a = list(map(int, f_in.readline().split()))

    if p == 1:
        ok = False
        dist = 0
        for i in range(k):
            if a[i] not in a[i+1:]:
                dist += 1
            else:
                ok = True
        if ok:
            f_out.write(f"{max(a[:k])}\n")
        else:
            f_out.write(f"{dist}\n")

    if p == 2:
        max_len = 0
        cnt = 0
        l = 0
        r = 0
        freq = {}
        for i in range(n):
            freq[a[i]] = freq.get(a[i], 0) + 1
            while r < n and len(freq) <= k:
                r += 1
                if r < n:
                    freq[a[r]] = freq.get(a[r], 0) + 1
            if len(freq) == k:
                max_len = max(max_len, r - l + 1)
                if r - l + 1 == max_len:
                    cnt += 1
            while len(freq) > k:
                freq[a[l]] -= 1
                if freq[a[l]] == 0:
                    del freq[a[l]]
                l += 1
        f_out.write(f"{max_len}\n")
        f_out.write(f"{cnt}\n")
