from sys import stdin, stdout

def is_cool(seq):
    return len(set(seq)) == len(seq)

def max_cool_len(arr, k):
    max_len = 0
    cur_len = 0
    last = -1
    count = 0
    for i in range(len(arr)):
        if arr[i] != last:
            count = 1
        else:
            count += 1
        if count <= k:
            cur_len += 1
            if cur_len > max_len:
                max_len = cur_len
        else:
            cur_len = count
        last = arr[i]
    return max_len

def num_cool_len(arr, k):
    cnt = 0
    last = -1
    count = 0
    for i in range(len(arr)):
        if arr[i] != last:
            count = 1
        else:
            count += 1
        if count <= k:
            if count == k:
                cnt += 1
        else:
            count = 0
        last = arr[i]
    return cnt

def main():
    p, n, k = map(int, stdin.readline().split())
    arr = list(map(int, stdin.readline().split()))
    if p == 1:
        if is_cool(arr[:k]):
            stdout.write(str(max(arr[:k])) + '\n')
        else:
            stdout.write(str(len(set(arr[:k]))) + '\n')
    elif p == 2:
        max_len = max_cool_len(arr, k)
        stdout.write(str(max_len) + '\n')
        stdout.write(str(num_cool_len(arr, k)) + '\n')

if __name__ == "__main__":
    main()
