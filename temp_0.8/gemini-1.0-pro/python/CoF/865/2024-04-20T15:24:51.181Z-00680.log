import io

class Cup:
    def __init__(self, capacity):
        self.capacity = capacity
        self.filled = 0

    def pour(self, amount):
        self.filled += amount
        if self.filled > self.capacity:
            return self.filled - self.capacity
        else:
            return 0

class Stack:
    def __init__(self, n):
        self.cups = [Cup(capacity) for capacity in n]

    def pour(self):
        overflow = self.cups[0].pour(1)
        for i in range(1, len(self.cups)):
            overflow += self.cups[i].pour(overflow)
        return overflow

def read_input(file):
    v = int(file.readline())
    n = int(file.readline())
    capacities = list(map(int, file.readline().split()))
    return v, n, capacities

def solve_part1(n, capacities):
    stack = Stack(capacities)
    level = 1
    max_sum = -1
    while True:
        sum = 0
        for i in range(n ** level - 1, n ** (level + 1) - 1):
            sum += stack.cups[i].capacity
        if sum > max_sum:
            max_sum = sum
            result = level
        if stack.pour() == 0:
            break
        level += 1
    return result

def solve_part2(n, capacities):
    stack = Stack(capacities)
    seconds = 0
    overflow = 0
    while True:
        seconds += 1
        overflow += stack.pour()
        if overflow == 0:
            break
    return seconds, overflow

def write_output(file, result):
    if result[0] == 1:
        file.write(f'{result[1]}\n')
    else:
        file.write(f'{result[1]} {result[2]}\n')

def main():
    with io.open('pic.in', 'r') as input_file, io.open('pic.out', 'w') as output_file:
        v, n, capacities = read_input(input_file)
        if v == 1:
            result = solve_part1(n, capacities)
        else:
            result = solve_part2(n, capacities)
        write_output(output_file, result)

if __name__ == '__main__':
    main()
