                if matrice[i][j] <= matrice[i][j - 1] and not vizitat[i][j]:
                    nr_santuri += 1
                    for k in range(j, n):
                        if matrice[i][k] >= matrice[i][j]:
                            vizitat[i][k] = True
                        else:
                            break
        return nr_santuri

def scriere(rezultat):
    with open("lasere.out", "w") as f:
        f.write(str(rezultat))

if __name__ == "__main__":
    c, n, m, matrice, lasere = citire()
    tragere(matrice, lasere)
    rezultat = numar_gropi_sau_santuri(matrice, c)
    scriere(rezultat)
