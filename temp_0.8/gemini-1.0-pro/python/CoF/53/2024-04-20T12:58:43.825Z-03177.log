import math

with open("mosia.in") as f:
    n = int(f.readline())
    stakes = [(int(x), int(y), int(d)) for x, y, d in (line.split() for line in f)]

def area(stakes):
    """Calculate the area of the polygon defined by the stakes."""
    area = 0
    for i in range(n):
        x1, y1, d1 = stakes[i]
        x2, y2, d2 = stakes[(i + 1) % n]
        area += (x1 * y2 - x2 * y1)
    return abs(area) / 2

def max_area(stakes):
    """Calculate the maximum area that can be enclosed by the stakes."""
    max_area = area(stakes)
    for i in range(n):
        x1, y1, d1 = stakes[i]
        for j in range(i + 1, n):
            x2, y2, d2 = stakes[j]
            dx, dy = x2 - x1, y2 - y1
            dist = math.hypot(dx, dy)
            if dist <= d1 + d2:
                new_stakes = stakes.copy()
                new_stakes[i] = (x1 + dx * d1 / dist, y1 + dy * d1 / dist, d1)
                new_stakes[j] = (x2 - dx * d2 / dist, y2 - dy * d2 / dist, d2)
                max_area = max(max_area, max_area(new_stakes))
    return max_area

with open("mosia.out", "w") as f:
    f.write(f"{max_area:.4f}")
