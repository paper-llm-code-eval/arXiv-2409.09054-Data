import sys
from math import inf

MAX_N = 200

# distanța euclidiană
def euclidian_distance(x1, y1, x2, y2):
    return ((x1 - x2) ** 2 + (y1 - y2) ** 2) ** 0.5

# orientarea a trei puncte
# 0: coliniare, 1: în sens trigonometric, 2: în sens invers trigonometric
def orientation(x1, y1, x2, y2, x3, y3):
    val = (y2 - y1) * (x3 - x2) - (x2 - x1) * (y3 - y2)
    if val == 0:
        return 0
    if val > 0:
        return 1
    return 2

# verifică dacă punctul (x, y) se află în convex hull-ul dat de punctele (p[0], p[1], ..., p[n-1])
# p[0] coincide cu p[n-1]
def is_inside(x, y, p):
    n = len(p)
    for i in range(n-1):
        if orientation(p[i][0], p[i][1], p[i+1][0], p[i+1][1], x, y) == 2:
            return False
    return True

# verifică dacă se poate muta parul i în afara convex hull-ului determinat de punctele (p[0], p[1], ..., p[n-1])
# p[0] coincide cu p[n-1]
def can_move_outside(i, d, p):
    n = len(p)
    x, y = p[i]
    for j in range(n-1):
        x1, y1 = p[j]
        x2, y2 = p[j+1]
        if orientation(x1, y1, x2, y2, x, y) == 2:
            dist = euclidian_distance(x1, y1, x2, y2)
            if dist <= d:
                return True
    return False

# actualizează convex hull-ul după ce parul i a fost mutat
# p[0] coincide cu p[n-1]
def update_convex_hull(i, d, p):
    n = len(p)
    x, y = p[i]
    for j in range(n-1):
        x1, y1 = p[j]
        x2, y2 = p[j+1]
        if orientation(x1, y1, x2, y2, x, y) == 2:
            dist = euclidian_distance(x1, y1, x2, y2)
            if dist <= d:
                p.pop(j+1)
    return p

# citește datele de intrare
with open('mosia.in', 'r') as fin:
    n = int(fin.readline())
    p = []
    for i in range(n):
        x, y, d = map(int, fin.readline().split())
        p.append((x, y, d))

# memorează distanța minimă de la fiecare par până la convex hull
min_dist = [inf for _ in range(n)]

# convex hull-ul inițial
ch = [p[0]]

# actualizează convex hull-ul și calculează distanțele minime
for i in range(1, n):
    ch = update_convex_hull(i, p[i][2], ch)
    min_dist[i] = min(min_dist[i], euclidian_distance(ch[0][0], ch[0][1], p[i][0], p[i][1]))
    if len(ch) > 1:
        min_dist[i] = min(min_dist[i], euclidian_distance(ch[-1][0], ch[-1][1], p[i][0], p[i][1]))

# calculează aria poligonului convex
area = 0
for i in range(len(ch)-1):
    x1, y1 = ch[i]
    x2, y2 = ch[i+1]
    area += (x1 * y2 - x2 * y1)
area = abs(area) / 2

# extinde aria poligonului cu aria triunghiurilor formate de fiecare par mutat
for i in range(1, n):
    if not is_inside(p[i][0], p[i][1], ch):
        if can_move_outside(i, min_dist[i], ch):
            area += min_dist[i] * min_dist[i] / 2

# scrie aria în fișierul de ieșire
with open('mosia.out', 'w') as fout:
    fout.write('{:.4f}'.format(area))
