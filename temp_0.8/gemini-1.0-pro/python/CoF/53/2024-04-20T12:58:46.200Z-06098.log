import sys

# Read input
with open("mosia.in", "r") as f:
    n = int(f.readline())
    points = []
    for line in f:
        x, y, d = map(int, line.split())
        points.append((x, y, d))

# Compute convex hull
hull = []
for point in points:
    while len(hull) >= 2 and (
        (point[1] - hull[-2][1]) * (hull[-1][0] - hull[-2][0])
        - (point[0] - hull[-2][0]) * (hull[-1][1] - hull[-2][1])
    ) <= 0:
        hull.pop()
    hull.append(point)

# Compute area
area = 0
for i in range(len(hull)):
    area += hull[i][0] * hull[(i + 1) % len(hull)][1] - hull[i][1] * hull[(i + 1) % len(hull)][0]
area = abs(area) / 2

# Extend area
for point in points:
    x, y, d = point
    for i in range(len(hull)):
        x1, y1 = hull[i]
        x2, y2 = hull[(i + 1) % len(hull)]
        if (
            (x1 - x) * (y2 - y) - (x2 - x) * (y1 - y)
        ) * ((x1 - x) * (y2 - y1) - (x2 - x) * (y1 - y)) >= 0:
            continue
        if (
            (x - x1) ** 2 + (y - y1) ** 2 <= d ** 2
            or (x - x2) ** 2 + (y - y2) ** 2 <= d ** 2
        ):
            area += abs(
                (x1 - hull[(i - 1 + len(hull)) % len(hull)][0])
                * (y2 - hull[(i - 1 + len(hull)) % len(hull)][1])
                - (x2 - hull[(i - 1 + len(hull)) % len(hull)][0])
                * (y1 - hull[(i - 1 + len(hull)) % len(hull)][1])
            ) / 2
            break

# Write output
with open("mosia.out", "w") as f:
    f.write("{:.4f}".format(area))
