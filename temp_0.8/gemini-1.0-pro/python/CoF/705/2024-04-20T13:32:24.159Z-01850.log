with open("panglica.in", "r") as f:
    n, c = map(int, f.readline().split())
    a = [int(i) for i in f.readlines()]

f = [0] * (c + 1)
s = [0] * (c + 1)
for i in range(n - 1, -1, -1):
    s[a[i]] += 1
for i in range(1, c + 1):
    s[i] += s[i - 1]

for i in range(n):
    f[a[i]] += 1
    if f[a[i]] * 2 > s[a[i]]:
        a.pop(i)
        n -= 1
        i -= 1

cnt = 0
l = 0
r = n - 1
while a[l] == a[r]:
    cnt += 1
    l += 1
    r -= 1
print(n, a[l], cnt // 2, (n - cnt) // 2, file=open("panglica.out", "w"))
```
