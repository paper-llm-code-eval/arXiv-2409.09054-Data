import sys

H, N, M, p = map(int, sys.stdin.readline().split())
INF = float('inf')

# Initialize the dp table with infinity
# dp[i][j] represents the minimum effort to reach the j-th step
# using a maximum of i steps at once
dp = [[INF] * (N + 1) for _ in range(M + 1)]
dp[0][0] = 0.0  # Base case: no steps taken

# Iterate over the number of steps taken at once
for i in range(1, M + 1):
    # Iterate over the current step
    for j in range(N + 1):
        # Try all possible step combinations
        for k in range(1, min(j + 1, i + 1)):
            # Calculate the effort for this combination
            effort = (k * dp[i - k][j - k] + p + sum(range(k))) / k
            # Update the dp table if this combination is better
            dp[i][j] = min(dp[i][j], effort)

# Output the minimum effort and the first possible step combination
print(dp[M][N])
for i in range(M, 0, -1):
    for j in range(N, -1, -1):
        if dp[i][j] + 1e-9 == dp[M][N]:
            print(*range(j, j - i, -1), sep=' ')
            break
```
