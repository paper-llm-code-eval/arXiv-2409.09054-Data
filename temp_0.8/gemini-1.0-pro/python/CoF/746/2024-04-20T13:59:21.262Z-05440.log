import sys

H, N, M, p = map(int, sys.stdin.readline().split())

def calc_effort(heights):
    effort = 0
    for i in range(0, len(heights), M):
        effort += sum(heights[i : i + M]) / min(M, len(heights) - i) + p
    return effort

def find_min_effort(heights):
    min_effort = float('inf')
    min_heights = []
    for i in range(1, H + 1):
        for j in range(i + 1, H + 1):
            for k in range(j + 1, H + 1):
                if i + j + k <= H:
                    effort = calc_effort([i, j, k])
                    if effort < min_effort:
                        min_effort = effort
                        min_heights = [i, j, k]
    return min_effort, min_heights

min_effort, min_heights = find_min_effort([0] * N)

sys.stdout.write(f"{min_effort:.2f}\n")
sys.stdout.write(' '.join(map(str, min_heights)))
```
