from queue import Queue, SimpleQueue
import sys

sys.setrecursionlimit(2000000)

# global variables
N = -1
P = -1
start_x = -1
start_y = -1
end_x = -1
end_y = -1
galaxy_map = []
item_map = []
pulsars = []
time_step = 0
MAX_TIME = 1000000
MAX_RADIUS = 6

# custom datatype to represent items
class Item:
    def __init__(self, x, y, time, steps_to_end):
        self.x = x
        self.y = y
        self.time = time
        self.steps_to_end = steps_to_end

# bfs using a queue
def bfs(start_x, start_y):
    q = SimpleQueue()
    visited = [[0 for i in range(N)] for j in range(N)]
    q.put(Item(start_x, start_y, 0, -1))
    visited[start_y][start_x] = 1
    while not q.empty():
        current_item = q.get()
        current_x = current_item.x
        current_y = current_item.y
        current_time = current_item.time
        if current_x == end_x and current_y == end_y:
            return current_item.steps_to_end
        # check the neighbors
        for i in range(-1, 2):
            for j in range(-1, 2):
                if (not(i == 0 and j == 0)) and current_y + j >= 0 and current_x + i >= 0 and current_y + j < N and current_x + i < N:
                    new_x = current_x + i
                    new_y = current_y + j
                    # check if the new position is not dangerous
                    is_dangerous = False
                    for p in range(P):
                        if item_map[current_time][new_y][new_x] == -1:
                            is_dangerous = True
                            break
                    if not is_dangerous:
                        q.put(Item(new_x, new_y, current_time + 1, current_item.steps_to_end + 1))
                        visited[new_y][new_x] = 1
    return -1


# dfs for subtask 1
def dfs(x, y, time, space):
    global time_step
    time_step = time
    item_map[time][y][x] = space
    for i in range(-1, 2):
        for j in range(-1, 2):
            if (not(i == 0 and j == 0)) and y + j >= 0 and x + i >= 0 and y + j < N and x + i < N:
                new_x = x + i
                new_y = y + j
                # check if the new position is not dangerous
                is_dangerous = False
                for p in range(P):
                    dist = abs(pulsars[p][0] - new_x) + abs(pulsars[p][1] - new_y)
                    if dist <= pulsars[p][2] and pulsars[p][3] == time % pulsars[p][4]:
                        is_dangerous = True
                        break
                if not is_dangerous:
                    dfs(new_x, new_y, time + 1, space)


def main():
    global N, P, start_x, start_y, end_x, end_y, galaxy_map, item_map, pulsars
    with open('pulsar.in', 'r') as f:
        C = int(f.readline().split()[0])
        N = int(f.readline().split()[1])
        P = int(f.readline().split()[2])
        pulsars = []
        for i in range(P):
            x, y, r, t = map(int, f.readline().split())
            pulsars.append([x - 1, y - 1, r, t, MAX_TIME if r > MAX_RADIUS else r])
        start_x, start_y = map(int, f.readline().split())
        start_x -= 1
        start_y -= 1
        end_x, end_y = map(int, f.readline().split())
        end_x -= 1
        end_y -= 1
    if C == 1:
        item_map = [[-1] * N for i in range(MAX_TIME)]
        dfs(start_x, start_y, 0, 0)
        print(time_step)
    else:
        print(bfs(start_x, start_y))


if __name__ == '__main__':
    main()
