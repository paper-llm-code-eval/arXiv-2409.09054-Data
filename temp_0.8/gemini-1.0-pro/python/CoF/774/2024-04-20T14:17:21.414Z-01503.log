with open("turnuri.in", "r") as f:
    m, n = [int(x) for x in f.readline().split()]
    mat = [[int(x) for x in line.split()] for line in f]

turnuri = [0] * n
for j in range(n):
    for i in range(m -1, -1, -1):
        if mat[i][j] == 1:
            turnuri[j] = i + 1
            break

# stergem coloanele cu turnuri de inaltime 0
turnuri_nonzero = [x for x in turnuri if x != 0]
n = len(turnuri_nonzero)
mat = [x for x in mat if x[turnuri.index(x[0])] != 0]

# sortam coloanele in functie de inaltimea turnului
mat.sort(key=lambda x: x[0], reverse=True)
# copiem inaltimile turnurilor inapoi in vectorul turnuri
for i in range(n):
    turnuri[i] = mat[i][0]

# operatia de reconfigurare
max_h = 0
max_towers = 0
for i in range(n -1):
    if turnuri[i] == 0 or turnuri[i +1] == 0:
        continue
    new_h = turnuri[i] + turnuri[i +1]
    if new_h > max_h:
        max_h = new_h
        max_towers = 1
    elif new_h == max_h:
        max_towers += 1
    turnuri[i] = 0
    turnuri[i +1] = 0

# afisam rezultatele
with open("turnuri.out", "w") as f:
    f.write(" ".join([str(x) for x in turnuri]) + "\n")
    f.write(str(max_h) + "\n")
    f.write(str(max_towers) + "\n")
```
