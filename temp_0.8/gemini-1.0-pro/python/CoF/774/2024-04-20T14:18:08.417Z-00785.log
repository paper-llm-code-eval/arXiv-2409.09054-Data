with open("turnuri.in", "r") as f:
    m, n = map(int, f.readline().split())
    grid = [list(map(int, f.readline().split())) for _ in range(m)]

# Calculate tower heights
heights = [0] * n
for c in range(n):
    for r in range(m - 1, -1, -1):
        if grid[r][c]:
            heights[c] = m - r
            break

# Perform tower merging operation
max_height = 0
num_max_height = 0
used = [False] * n
for c in range(n - 1):
    if not used[c] and not used[c + 1] and heights[c] + heights[c + 1] <= m:
        max_height = max(max_height, heights[c] + heights[c + 1])
        used[c] = used[c + 1] = True
        num_max_height += 1

# Output results
with open("turnuri.out", "w") as f:
    f.write(" ".join(map(str, heights)) + "\n")
    f.write(str(max_height) + "\n")
    f.write(str(num_max_height))
```
