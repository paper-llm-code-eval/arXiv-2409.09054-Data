    for i in range(1, 2 * 10 ** 8 + 1):
        if n in gen(i):
            fout.write(str(i) + '\n')
            for j in range(6):
                if numbers[j] == n:
                    fout.write('ABCDEF'[j] + ' ')
            fout.write('\n')
            break
