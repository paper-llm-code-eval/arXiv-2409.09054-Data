    visited[start[0]][start[1]] = True
    dfs(map_, n, m, (start[0] + 1, start[1]), visited, islands)
    dfs(map_, n, m, (start[0] - 1, start[1]), visited, islands)
    dfs(map_, n, m, (start[0], start[1] + 1), visited, islands)
    dfs(map_, n, m, (start[0], start[1] - 1), visited, islands)
    islands.add(map_[start[0]][start[1]])


def find_islands(map_, n, m):
    visited = [[False for _ in range(m)] for _ in range(n)]
    islands = set()
    for i in range(n):
        for j in range(m):
            if not visited[i][j] and map_[i][j] != 0:
                dfs(map_, n, m, (i, j), visited, islands)
    return islands


def find_shortest_bridge(map_, n, m):
    start_point = None
    end_point = None
    for i in range(n):
        for j in range(m):
            if map_[i][j] == 1:
                start_point = (i, j)
            elif map_[i][j] == 2:
                end_point = (i, j)
    if not start_point or not end_point:
        return -1
    queue = [(start_point, 0)]
    visited = [[False for _ in range(m)] for _ in range(n)]
    while queue:
        current, distance = queue.pop(0)
        if current == end_point:
            return distance - 1
        if visited[current[0]][current[1]]:
            continue
        visited[current[0]][current[1]] = True
        neighbors = [(current[0] + 1, current[1]), (current[0] - 1, current[1]),
                     (current[0], current[1] + 1), (current[0], current[1] - 1)]
        for neighbor in neighbors:
            if 0 <= neighbor[0] < n and 0 <= neighbor[1] < m and map_[neighbor[0]][neighbor[1]] == 0:
                queue.append((neighbor, distance + 1))
    return -1


if __name__ == '__main__':
