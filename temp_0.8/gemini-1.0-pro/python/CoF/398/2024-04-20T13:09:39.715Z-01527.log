def flood_fill(arhipelag, x, y, x_dim, y_dim, country):
    if x < 0 or y < 0 or x >= x_dim or y >= y_dim:
        return
    if arhipelag[x][y] != 0:
        return
    arhipelag[x][y] = country
    flood_fill(arhipelag, x + 1, y, x_dim, y_dim, country)
    flood_fill(arhipelag, x - 1, y, x_dim, y_dim, country)
    flood_fill(arhipelag, x, y + 1, x_dim, y_dim, country)
    flood_fill(arhipelag, x, y - 1, x_dim, y_dim, country)


def find_islands(arhipelag, x_dim, y_dim):
    num_islands = [0, 0, 0]
    for i in range(x_dim):
        for j in range(y_dim):
            if arhipelag[i][j] != 0:
                continue
            flood_fill(arhipelag, i, j, x_dim, y_dim, num_islands.index(0) + 1)
            num_islands[num_islands.index(0)] += 1
    return num_islands


def find_bridge_length(arhipelag, x_dim, y_dim):
