        arhipelag = [list(map(int, list(line.strip()))) for line in f]

    num_islands = find_islands(arhipelag, x_dim, y_dim)
    bridge_length = find_bridge_length(arhipelag, x_dim, y_dim)

    with open('insule.out', 'w') as f:
        f.write(' '.join(map(str, num_islands + [bridge_length])))
