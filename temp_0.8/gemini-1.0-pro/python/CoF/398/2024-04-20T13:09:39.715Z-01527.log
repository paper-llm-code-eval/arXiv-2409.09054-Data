    flood_fill(arhipelag, x, y - 1, x_dim, y_dim, country)


def find_islands(arhipelag, x_dim, y_dim):
    num_islands = [1, 1, 1]  # Initialize with ones to avoid the error
    for i in range(x_dim):
        for j in range(y_dim):
            if arhipelag[i][j] != 0:
                continue
            flood_fill(arhipelag, i, j, x_dim, y_dim, num_islands.index(1) + 1)
            num_islands[num_islands.index(1)] += 1
    return num_islands


def find_bridge_length(arhipelag, x_dim, y_dim):
    min_length = float('inf')
    for i in range(x_dim):
        if arhipelag[i][0] == 0:
            for j in range(y_dim):
                if arhipelag[i][j] == 1:
                    for k in range(j + 1, y_dim):
                        if arhipelag[i][k] == 2:
                            min_length = min(min_length, k - j + 1)
        if arhipelag[0][i] == 0:
            for j in range(x_dim):
                if arhipelag[j][i] == 1:
                    for k in range(j + 1, x_dim):
                        if arhipelag[k][i] == 2:
                            min_length = min(min_length, k - j + 1)
    return min_length


if __name__ == '__main__':
    with open('insule.in', 'r') as f:
        x_dim, y_dim = map(int, f.readline().split())
        arhipelag = [list(map(int, list(line.strip()))) for line in f]

    num_islands = find_islands(arhipelag, x_dim, y_dim)
    bridge_length = find_bridge_length(arhipelag, x_dim, y_dim)

    with open('insule.out', 'w') as f:
        f.write(' '.join(map(str, num_islands + [bridge_length])))
