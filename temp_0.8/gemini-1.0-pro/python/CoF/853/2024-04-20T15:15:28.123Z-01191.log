import sys

def main():
    with open("defrag.in") as f:
        V = int(f.readline())
        P, S = [int(x) for x in f.readline().split()]
        C = int(f.readline())
        clusters = [(int(x) - 1, int(y) - 1) for x, y in [f.readline().split() for _ in range(C)]]

    if V == 1:
        print(P - len({x for x, _ in clusters}))
    else:
        print(" ".join([str(min(j - i for i, j in zip([0] + [y for x, y in clusters], clusters + [(P, S)]))) for _ in range(P)]))

if __name__ == "__main__":
    main()
