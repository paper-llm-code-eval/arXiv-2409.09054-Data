import sys

MOD = 1000000007

def is_prime(n):
    if n < 2:
        return False
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            return False
    return True


def primes(n):
    return [i for i in range(n + 1) if is_prime(i)]


def min_cost(n, arr):
    if arr[n] != -1:
        return arr[n]
    if n == 0:
        return 0
    if is_prime(n):
        arr[n] = 0
        return 0
    arr[n] = min(abs(n - i) for i in primes)
    return arr[n]


def update_arr(arr, i, x):
    arr[i] = min_cost(x, arr)


def query(arr, i, p):
    return sum(arr[i:i + p]) % MOD


def main():
    with open("primprim.in", "r") as f_in, open("primprim.out", "w") as f_out:
        c = int(f_in.readline())
        n = int(f_in.readline())
        arr = [-1] * (n + 1)
        for i, x in enumerate(map(int, f_in.readline().split())):
            update_arr(arr, i + 1, x)
        if c == 1:
            f_out.write(str(sum(arr[1:]) % MOD))
        else:
            q = int(f_in.readline())
            for _ in range(q):
                i, x, p = map(int, f_in.readline().split())
                update_arr(arr, i, x)
                f_out.write(str(query(arr, i, p)) + "\n")


if __name__ == "__main__":
    main()
