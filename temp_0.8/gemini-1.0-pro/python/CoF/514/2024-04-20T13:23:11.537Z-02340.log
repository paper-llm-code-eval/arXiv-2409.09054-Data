import math

def next_prime(n):
    if n < 2:
        return 2
    if n % 2 == 0:
        n += 1
    while True:
        for i in range(3, int(math.sqrt(n)) + 1, 2):
            if n % i == 0:
                break
        else:
            return n
        n += 2

def main(input_file, output_file):
    with open(input_file) as f_in, open(output_file, 'w') as f_out:
        C = int(f_in.readline().strip())
        n = int(f_in.readline().strip())
        a = [int(x) for x in f_in.readline().strip().split()]
        b = [next_prime(x) for x in a]
        cost = [abs(x - y) for x, y in zip(a, b)]
        if C == 1:
            f_out.write(str(sum(cost)) + '\n')
        else:
            q = int(f_in.readline().strip())
            for _ in range(q):
                i, x, p = map(int, f_in.readline().strip().split())
                a[i - 1] = x
                b[i - 1] = next_prime(x)
                cost[i - 1] = abs(x - b[i - 1])
                selected = sorted([cost[i - 1] for i in range(n) if i % p == 0])
                f_out.write(str(sum(selected[:p])) + '\n')

if __name__ == '__main__':
    main('primprim.in', 'primprim.out')
```
