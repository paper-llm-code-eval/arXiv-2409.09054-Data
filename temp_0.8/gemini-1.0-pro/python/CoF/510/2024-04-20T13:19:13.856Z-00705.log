def Castel(C, N, Galben):
    Albastru = []
    Rand = [0] * 6000
    Linie = 1
    Cifra = [0] * 10
    for x in Galben:
        while x > 0:
            Cifra[x%10] += 1
            x //= 10

    while Galben:
        for x in Galben:
            if Rand[Linie] == 0:
                Rand[Linie] = 1
                Albastru.append(0)
            Albastru[Linie-1] += x
            Linie += 1
            if Galben:
                Galben.pop(0)
        Linie -= 1
        while Galben and Rand[Linie] > 0:
            Linie -= 1
        Linie += 1
        Rand[Linie] = 1

    if C == 1:
        return sum(x > 0 for x in Cifra)
    if C == 2:
        return Linie, Albastru[-1]
    S = 0
    for x in Albastru:
        S += x
    return len(Albastru), S


