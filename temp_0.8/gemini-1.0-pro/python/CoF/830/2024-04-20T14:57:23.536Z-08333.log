import sys

# citeste datele de intrare
with open('clepsidru.in', 'r') as f:
    n, b = [int(x) for x in f.readline().split()]
    k = int(f.readline())
    stari = [[int(x) for x in f.readline().split()] for _ in range(k)]

# calculeaza timpul pentru primul experiment
timp1 = (n - 1) * (b - 1) + 1

# calculeaza cantitatile de nisip pentru al doilea experiment
stari_dict = {p: s for p, s in stari}
nisip = [0 for _ in range(n)]
nisip[0] = b
for sec in range(1, timp1 + 1):
    if sec in stari_dict:
        p = stari_dict[sec]
    for i in range(p - 1, n):
        if nisip[i] > 0:
            nisip[i] -= 1
            nisip[i + 1] += 1

# scrie rezultatele
with open('clepsidru.out', 'w') as f:
    f.write(str(timp1) + '\n')
    for i in range(n):
        f.write(' '.join([str(nisip[i]), str(b - nisip[i])]) + '\n')
```
