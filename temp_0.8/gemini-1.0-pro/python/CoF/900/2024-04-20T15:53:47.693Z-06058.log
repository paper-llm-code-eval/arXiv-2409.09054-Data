def read_input():
    with open("castel.in", "r") as f:
        c = int(f.readline())
        n = int(f.readline())
        h = [list(map(int, line.split())) for line in f]
    return c, n, h

def dfs(i, j, h, visited, rooms, room_areas):
    if i < 0 or i >= n or j < 0 or j >= n or h[i][j] == 0 or visited[i][j]:
        return
    visited[i][j] = True
    room_areas[rooms] += 1
    if (i == 0 or h[i - 1][j] == 0) and (j == 0 or h[i][j - 1] == 0):
        rooms += 1
    dfs(i + 1, j, h, visited, rooms, room_areas)
    dfs(i, j + 1, h, visited, rooms, room_areas)
    dfs(i - 1, j, h, visited, rooms, room_areas)
    dfs(i, j - 1, h, visited, rooms, room_areas)

def find_rooms_and_areas(h):
    visited = [[False] * n for _ in range(n)]
    rooms = 0
    room_areas = []
    for i in range(n):
        for j in range(n):
            if h[i][j] != 0 and not visited[i][j]:
                dfs(i, j, h, visited, rooms, room_areas)
    return rooms, room_areas

def find_max_room(h, rooms, room_areas):
    max_area = max(room_areas)
    max_rooms = [room for room in rooms if room_areas[room] == max_area]
    min_top_left = min(max_rooms, key=lambda room: (room // n, room % n))
    max_bottom_right = max(max_rooms, key=lambda room: (room // n + max_area - 1, room % n + max_area - 1))
    return min_top_left, max_bottom_right

def write_output(c, rooms, max_area, min_top_left, max_bottom_right):
    with open("castel.out", "w") as f:
        if c == 1:
            f.write(str(rooms) + "\n")
        elif c == 2:
            f.write(str(max_area) + "\n")
        else:
            f.write(" ".join(map(str, [min_top_left, max_bottom_right])) + "\n")

if __name__ == "__main__":
    c, n, h = read_input()
    rooms, room_areas = find_rooms_and_areas(h)
    if c == 1:
        write_output(c, rooms, 0, 0, 0)
    elif c == 2:
        write_output(c, rooms, max(room_areas), 0, 0)
    else:
        min_top_left, max_bottom_right = find_max_room(h, rooms, room_areas)
        write_output(c, rooms, max(room_areas), min_top_left, max_bottom_right)
