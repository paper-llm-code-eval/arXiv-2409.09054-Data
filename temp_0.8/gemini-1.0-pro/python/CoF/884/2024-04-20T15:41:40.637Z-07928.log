import sys

def checkWin(board, symbol):
    for i in range(N):
        if all(board[i][j] == symbol for j in range(N)):
            return True
        if all(board[j][i] == symbol for j in range(N)):
            return True
    if all(board[i][i] == symbol for i in range(N)):
        return True
    if all(board[i][N-i-1] == symbol for i in range(N)):
        return True
    return False

def main():
    with open('joc.in', 'r') as fin, open('joc.out', 'w') as fout:
        C = int(fin.readline())
        N, K = map(int, fin.readline().split())
        board = [[0] * N for _ in range(N)]
        TeodoraWins = 0
        StefanWins = 0
        MaxMoves = 0
        for _ in range(K):
            moves = list(map(int, fin.readline().split()))
            for move in moves:
                if C == 1:
                    if move not in board[0]:
                        board[move//N][move%N] = 1
                        if checkWin(board, 1):
                            TeodoraWins += 1
                            break
                    elif move not in board[N-1]:
                        board[move//N][move%N] = -1
                        if checkWin(board, -1):
                            StefanWins += 1
                            break
                elif C == 2:
                    if move not in board[0] and move not in board[N-1]:
                        board[move//N][move%N] = 1
                    if checkWin(board, 1):
                        MaxMoves = max(MaxMoves, moves.index(move) + 1)
                        break
            if C == 1:
                for i in range(N):
                    board[i] = [0] * N
        if C == 1:
            fout.write(f'{TeodoraWins} {StefanWins}\n')
        elif C == 2:
            fout.write(f'{MaxMoves}\n')

if __name__ == '__main__':
    main()
