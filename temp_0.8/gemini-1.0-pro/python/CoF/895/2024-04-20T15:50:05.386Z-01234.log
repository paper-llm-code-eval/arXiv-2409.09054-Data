def turnuri(fin, fout):
    """
    Functia rezolva problema turnurilor de cuburi.

    :param fin: fisierul de intrare
    :param fout: fisierul de iesire
    """
    # Citirea cerintei
    cerinta = int(fin.readline())

    # Citirea numarului de cuburi
    n = int(fin.readline())

    # Crearea listelor de laturi si culori
    laturi = []
    culori = []
    for _ in range(n):
        latura, culoare = fin.readline().split()
        laturi.append(int(latura))
        culori.append(culoare)

    # Sortarea cuburilor descrescator dupa latura
    cuburi = sorted(zip(laturi, culori), reverse=True)

    # Initializarea variabilelor
    t = 0  # numarul de turnuri
    h = 0  # inaltimea celui mai inalt turn
    topuri_turnuri = []  # lista cu topurile turnurilor
    Nmax = 0  # numarul maxim de cuburi ce pot forma un turn

    if cerinta == 1:
        # Parcurgerea cuburilor
        for latura, culoare in cuburi:
            # Verificarea daca latura este mai mica decat topul ultimului turn
            if len(topuri_turnuri) == 0 or latura < topuri_turnuri[-1]:
                # Crearea unui nou turn
                topuri_turnuri.append(latura)
                t += 1
            else:
                # Inlocuirea topului ultimului turn cu latura cubului curent
                topuri_turnuri[-1] = latura

            # Actualizarea inaltimii celui mai inalt turn
            h = max(h, latura)

        # Scrierea rezultatelor in fisierul de iesire
        fout.write(f"{t} {h}\n")

    else:
        # Parcurgerea cuburilor
        for latura, culoare in cuburi:
            # Verificarea daca latura poate fi asezata pe un alt cub
            if len(topuri_turnuri) == 0 or latura < topuri_turnuri[-1]:
                # Asezarea cubului pe un alt cub
                topuri_turnuri.append(latura)
            else:
                # Inlocuirea topului ultimului turn cu latura cubului curent
                topuri_turnuri[-1] = latura

            # Actualizarea numarului maxim de cuburi ce pot forma un turn
            Nmax = max(Nmax, len(topuri_turnuri))

        # Scrierea rezultatelor in fisierul de iesire
        fout.write(f"{Nmax}\n")


if __name__ == "__main__":
    with open("turnuri.in", "r") as fin,\
         open("turnuri.out", "w") as fout:
        turnuri(fin, fout)
