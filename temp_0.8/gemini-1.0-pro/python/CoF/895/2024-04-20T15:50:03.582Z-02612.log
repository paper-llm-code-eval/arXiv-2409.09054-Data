with open('turnuri.in', 'r') as fin, open('turnuri.out', 'w') as fout:
    cerinta = int(fin.readline())
    if cerinta == 1:
        n = int(fin.readline())
        cuburi = []
        for _ in range(n):
            latura, culoare = fin.readline().strip().split()
            cuburi.append((int(latura), culoare))
        turnuri = 0
        hmax = 0
        for cub in cuburi:
            if not turnuri or cub[1] != turnuri[-1][1] and cub[0] < turnuri[-1][0]:
                turnuri += 1
                hmax += cub[0]
            turnuri[-1] = cub
        fout.write(f'{turnuri} {hmax}\n')
    elif cerinta == 2:
        n = int(fin.readline())
        cuburi = []
        for _ in range(n):
            latura, culoare = fin.readline().strip().split()
            cuburi.append((int(latura), culoare))
        cuburi.sort(key=lambda x: x[0])
        galbene, albastre = [], []
        for cub in cuburi:
            if cub[1] == 'g':
                galbene.append(cub[0])
            else:
                albastre.append(cub[0])
        m = len(galbene)
        n = len(albastre)
        i, j, k = 0, 0, 0
        max_turnuri = 0
        while i < m and j < n:
            if galbene[i] < albastre[j]:
                k += 1
                i += 1
            else:
                j += 1
            max_turnuri = max(max_turnuri, k)
        fout.write(f'{max_turnuri}\n')
