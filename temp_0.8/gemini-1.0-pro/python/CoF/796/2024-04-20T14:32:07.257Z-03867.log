with open("tren.in", "r") as fin, open("tren.out", "w") as fout:
    T = int(fin.readline())
    L = [0] * T
    H = [0] * T
    M = [0] * T
    S = [0] * T
    for i in range(T):
        L[i], H[i], M[i], S[i] = map(int, fin.readline().split())
    
    max_trains = 0
    for i in range(T):
        for j in range(i+1, T):
            if L[i] == L[j] and (H[j]*60+M[j]) >= (H[i]*60+M[i]+S[i]):
                max_trains = max(max_trains, j-i+1)
    
    visible_trains = 0
    last_end = -1
    for i in range(T):
        if (H[i]*60+M[i]) >= last_end:
            visible_trains += 1
            last_end = (H[i]*60+M[i]+S[i])
    
    max_gap = 0
    current_gap = 0
    for i in range(T):
        if (H[i]*60+M[i]) >= last_end:
            current_gap = 0
            last_end = (H[i]*60+M[i]+S[i])
        else:
            current_gap += 1
            max_gap = max(max_gap, current_gap)
    
    fout.write(f"{max_trains} {visible_trains} {max_gap}")
```
