with open('ferma.in', 'r') as f:
    v = int(f.readline())
    m, n = map(int, f.readline().split())
    grid = [list(f.readline().strip()) for _ in range(m)]

# Initialize variables for both variants
max_area = 0
max_row = 0
max_col = 0
new_color = ''

# Variant 1: Find the maximum area of a plot in the initial plan
if v == 1:
    for i in range(m):
        for j in range(n):
            area = 1
            color = grid[i][j]
            queue = [(i, j)]
            while queue:
                x, y = queue.pop(0)
                for dx, dy in [(1, 0), (-1, 0), (0, 1), (0, -1)]:
                    nx, ny = x + dx, y + dy
                    if 0 <= nx < m and 0 <= ny < n and grid[nx][ny] == color:
                        queue.append((nx, ny))
                        area += 1
            if area > max_area:
                max_area = area

# Variant 2: Find the cell to change to obtain the maximum area plot
elif v == 2:
    for i in range(m):
        for j in range(n):
            for c in set(grid[i][j] for i in range(m) for j in range(n)):
                if c != grid[i][j]:
                    area = 1
                    grid[i][j] = c
                    color = grid[i][j]
                    queue = [(i, j)]
                    while queue:
                        x, y = queue.pop(0)
                        for dx, dy in [(1, 0), (-1, 0), (0, 1), (0, -1)]:
                            nx, ny = x + dx, y + dy
                            if 0 <= nx < m and 0 <= ny < n and grid[nx][ny] == color:
                                queue.append((nx, ny))
                                area += 1
                    grid[i][j] = color
                    if area > max_area:
                        max_area = area
                        max_row = i + 1
                        max_col = j + 1
                        new_color = c

# Output the results
with open('ferma.out', 'w') as f:
    if v == 1:
        f.write(str(max_area) + '\n')
    elif v == 2:
        f.write(f'{max_row} {max_col}\n')
        f.write(new_color)
```
