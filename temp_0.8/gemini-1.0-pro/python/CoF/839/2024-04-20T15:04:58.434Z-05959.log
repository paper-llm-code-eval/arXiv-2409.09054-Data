with open('zona.in', 'r') as f:
    x, y, n, l = map(int, f.readline().split())
    traseu = list(map(int, f.readline().split()))

def count_steps(x, y, traseu):
    prev_x, prev_y = x, y
    steps = 0
    for directie in traseu:
        if directie == 1:
            x -= 1
        elif directie == 2:
            y += 1
        elif directie == 3:
            x += 1
        elif directie == 4:
            y -= 1

        if x == prev_x and y == prev_y:
            return steps
        else:
            steps += 1
            prev_x, prev_y = x, y

def count_area(x, y, traseu):
    area = 0
    visited = [[False] * n for _ in range(n)]
    visited[x][y] = True

    prev_x, prev_y = x, y
    for directie in traseu:
        if directie == 1:
            x -= 1
        elif directie == 2:
            y += 1
        elif directie == 3:
            x += 1
        elif directie == 4:
            y -= 1

        if visited[x][y]:
            break
        else:
            area += 1
            visited[x][y] = True
            prev_x, prev_y = x, y

    return area

steps = count_steps(x, y, traseu)
area = count_area(x, y, traseu)
with open('zona.out', 'w') as f:
    f.write(f'{steps}\n{area}')
```
