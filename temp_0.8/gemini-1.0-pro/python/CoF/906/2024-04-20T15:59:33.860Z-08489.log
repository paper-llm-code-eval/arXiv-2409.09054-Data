with open('aur.in') as f:
    ci, n = list(map(int, f.readline().split()))
    if ci == 1:
        nr_cifre = 0
        for a in f.readline().split():
            nr_cifre += len(a)
        with open('aur.out', 'w') as g:
            g.write(str(nr_cifre))
    else:
        k = int(f.readline().split()[1])
        ta = ''.join(f.readline().split())
        with open('aur.out', 'w') as g:
            g.write(ta[k - 1])
    if ci == 3:
        p = int(f.readline().split()[1])
        ta = ''.join(f.readline().split())
        x = []
        for i in range(len(ta)):
            for j in range(min(len(ta), i + p), i, -1):
                x.append(int(ta[i:j]))
        with open('aur.out', 'w') as g:
            g.write(str(max(x)))
