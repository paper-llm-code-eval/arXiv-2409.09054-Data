    current_player = 0
    nr_moves_made = 0

    while nr_moves_made < nr_moves:
        nr_moves_made += 1
        nr = (a * nr_0 + b) % nr_exchange_offices + 1
        nr_0 = nr
        current_player = (current_player + nr - 1) % nr_players
        exchange_office = exchange_offices[current_player]

        if exchange_office[0] == "A":
            continue
        elif exchange_office[0] == "R":
            players[current_player]["pass"] = True
        elif exchange_office[0] == "G":
            cost = int(exchange_office[2])
            if players[current_player]["lei"] < cost:
                if not players[current_player]["pass"]:
                    players[current_player] = None
            else:
                players[current_player]["lei"] -= cost
                players[current_player]["euro"] += cost
        elif exchange_office[0] == "V":
            cost = int(exchange_office[2])
            if players[current_player]["euro"] < cost:
                if not players[current_player]["pass"]:
                    players[current_player] = None
            else:
                players[current_player]["euro"] -= cost
                players[current_player]["lei"] += cost

    return players

def count_players(players):
    return len([player for player in players if player is not None])

def find_winner(players):
    max_euro = -1
    winner = None
    for player in players:
        if player is not None and player["euro"] > max_euro:
            max_euro = player["euro"]
            winner = player

    return winner

def write_output(players):
    if nr_moves == 0:
        print(nr_players)
    else:
        winner = find_winner(players)
        print(winner["token"])

def main():
    read_input()
    players = simulate_game()
    write_output(players)

if __name__ == "__main__":
    main()
