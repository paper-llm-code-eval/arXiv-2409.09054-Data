        nr_exchange_offices, nr_players, initial_lei, initial_euro, nr_moves = map(int, f.readline().split())
        a, b, nr_0 = map(int, f.readline().split())
        exchange_offices = [list(map(str, f.readline().split())) for _ in range(nr_exchange_offices)]

def simulate_game():
