    global nr_lei
    global nr_euro
    global nr_jucatori
    global pas_ramase
    global case_schimb_valutar

    nr = calculeaza_nr(nr_mutare)
    culoare, cost_lei, cost_euro = case_schimb_valutar[nr - 1]

    for jucator in range(1, M + 1):
        if not nr_jucatori[jucator]:
            continue
        if culoare == 'A':
            continue
        elif culoare == 'G':
            if nr_lei[jucator] < nr * cost_lei:
                if pas_ramase[jucator]:
                    pas_ramase[jucator] = False
                else:
                    nr_jucatori[jucator] = False
        elif culoare == 'V':
            if nr_euro[jucator] < nr * cost_euro:
                if pas_ramase[jucator]:
                    pas_ramase[jucator] = False
                else:
                    nr_jucatori[jucator] = False
        elif culoare == 'R':
            pas_ramase[jucator] = True
        nr_mutaii += 1

    if nr_mutaii >= X:
        return


def rezolva_cerinta1():
    for i in range(X):
        executa_mutare(i + 1)
    numar_jucatori = sum(nr_jucatori)
    f = open('valutar.out', 'w')
    f.write(str(numar_jucatori))
    f.close()


def rezolva_cerinta2():
    for i in range(X):
        executa_mutare(i + 1)
    max_euro = max(nr_euro)
    jucator_castigator = 0
    for i in range(1, M + 1):
        if nr_jucatori[i] and nr_euro[i] == max_euro:
            jucator_castigator = i

    f = open('valutar.out', 'w')
    f.write(str(jucator_castigator))
    f.close()


if tip_cerinta == 1:
    rezolva_cerinta1()
else:
    rezolva_cerinta2()
