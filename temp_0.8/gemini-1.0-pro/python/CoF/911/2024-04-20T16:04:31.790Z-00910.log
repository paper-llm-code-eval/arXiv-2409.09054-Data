f.close()

nr_jucatori = [True] * (M + 1)
nr_euro = [E] * (M + 1)
nr_lei = [L] * (M + 1)
pas_ramase = [False] * (M + 1)
nr_mutaii = 0


def calculeaza_nr(nr_mutare):
