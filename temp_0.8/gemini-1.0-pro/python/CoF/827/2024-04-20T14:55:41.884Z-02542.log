def decompresie(sir):
    """
    Decompreseaza un sir de caractere obtinut prin comprimarea unei matrice de caractere.

    Args:
        sir (str): Sirul de caractere comprimat.

    Returns:
        tuple: Un tuplu care contine:
            - numarul de impartiri necesare pentru decompresie
            - matricea de caractere decomprimata
    """

    def impartire(sir, x1, y1, x2, y2):
        """
        Imparte matricea in patru submatrice.

        Args:
            sir (str): Sirul de caractere comprimat.
            x1 (int): Coordonata x a colțului stânga sus al submatricei.
            y1 (int): Coordonata y a colțului stânga sus al submatricei.
            x2 (int): Coordonata x a colțului dreapta jos al submatricei.
            y2 (int): Coordonata y a colțului dreapta jos al submatricei.

        Returns:
            list: O listă cu cele patru submatrice.
        """
        mij_x = (x1 + x2) // 2
        mij_y = (y1 + y2) // 2
        return [
            sir[x1:mij_x + 1, y1:mij_y + 1],
            sir[x1:mij_x + 1, mij_y + 1 : y2 + 1],
            sir[mij_x + 1 : x2 + 1, y1:mij_y + 1],
            sir[mij_x + 1 : x2 + 1, mij_y + 1 : y2 + 1],
        ]

    def decompresie_aux(sir, x1, y1, x2, y2):
        """
        Decompreseaza o matrice de caractere.

        Args:
            sir (str): Sirul de caractere comprimat.
            x1 (int): Coordonata x a colțului stânga sus al submatricei.
            y1 (int): Coordonata y a colțului stânga sus al submatricei.
            x2 (int): Coordonata x a colțului dreapta jos al submatricei.
            y2 (int): Coordonata y a colțului dreapta jos al submatricei.

        Returns:
            tuple: Un tuplu care contine:
                - numarul de impartiri necesare pentru decompresie
                - matricea de caractere decomprimata
        """
        if x2 - x1 == 0 and y2 - y1 == 0:
            return 0, sir[x1][y1]
        if x2 - x1 == 0 or y2 - y1 == 0:
            return 0, sir[x1:x2 + 1, y1:y2 + 1]
        if len(set(sir[x1:x2 + 1, y1:y2 + 1])) == 1:
            return 0, sir[x1][y1] * (x2 - x1 + 1) * (y2 - y1 + 1)
        nr_impartiri = 1
        submatrice = impartire(sir, x1, y1, x2, y2)
        for submatrice_aux in submatrice:
            nr_impartiri_aux, matrice_aux = decompresie_aux(
                submatrice_aux, 0, 0, len(submatrice_aux) - 1, len(submatrice_aux[0]) - 1
            )
            nr_impartiri += nr_impartiri_aux
        return nr_impartiri, "*" + "".join(["*".join(matrice_aux.split("\n")) for matrice_aux in submatrice])

    matrice_decomprimata = decompresie_aux(sir, 0, 0, len(sir) - 1, len(sir[0]) - 1)
    return matrice_decomprimata

