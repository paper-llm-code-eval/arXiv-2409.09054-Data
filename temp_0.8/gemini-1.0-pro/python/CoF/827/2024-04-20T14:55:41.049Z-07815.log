    s3 = decompress(string[1 + len(s1) + len(s2):])
    s4 = decompress(string[1 + len(s1) + len(s2) + len(s3):])
    return s1 + s2 + s3 + s4

def decompress_matrix(string, n):
    num = 0
    for c in string:
        if c.isdigit():
            num *= 10
            num += int(c)
    if num > 0:
        return [[chr(ord('a') + num - 1)] * n for _ in range(n)]
    if len(string) == 1:
