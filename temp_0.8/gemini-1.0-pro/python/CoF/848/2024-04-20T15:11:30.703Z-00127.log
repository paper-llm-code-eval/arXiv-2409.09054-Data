                            if x < 0 or x >= n or y < 0 or y >= m or vis[x][y]:
                                break
                            vis[x][y] = True
                            score += 1
                        if vis[x][y]:
                            count += 1
            fout.write(str(count) + '\n')
        else:
            maxscore = 0
            for x in range(n):
                for y in range(m):
                    vis = [[False] * m for _ in range(n)]
                    vis[x][y] = True
                    score = 0
                    while True:
                        if arrows[x][y] == 1:
                            y += 1
                        elif arrows[x][y] == 2:
                            x -= 1
                        elif arrows[x][y] == 3:
                            y -= 1
                        elif arrows[x][y] == 4:
                            x += 1
                        if x < 0 or x >= n or y < 0 or y >= m or vis[x][y]:
                            break
                        vis[x][y] = True
                        score += 1
                    if vis[x][y]:
                        score *= 1000
                    maxscore = max(maxscore, score)
            fout.write(str(maxscore) + '\n')
