with open('arrows.in', 'r') as f_in, open('arrows.out', 'w') as f_out:
    cerinta, n, m = map(int, f_in.readline().split())
    tabla = [list(map(int, line.split())) for line in f_in.readlines()]
    lin, col = map(int, f_in.readline().split())
    lin -= 1
    col -= 1
    vizitate = [[False] * m for _ in range(n)]

    def punctaj_pozitie(lin, col):
        if vizitate[lin][col]:
            return -1
        vizitate[lin][col] = True
        if cerinta == 1:
            return 1
        elif cerinta == 2:
            return 0
        elif cerinta == 3:
            return 1000

    def punctaj_celula(lin, col):
        if 0 <= lin < n and 0 <= col < m and not vizitate[lin][col]:
            directie = tabla[lin][col] - 1
            if directie == 0:
                return punctaj_pozitie(lin, col + 1)
            elif directie == 1:
                return punctaj_pozitie(lin - 1, col)
            elif directie == 2:
                return punctaj_pozitie(lin, col - 1)
            else:
                return punctaj_pozitie(lin + 1, col)
        return 0

    punctaj = punctaj_celula(lin, col)
    if punctaj == -1:
        f_out.write("0\n")
    elif cerinta == 1:
        f_out.write("{}\n".format(punctaj))
    elif cerinta == 2:
        f_out.write("{}\n".format(sum(punctaj_celula(i, j) for i in range(n) for j in range(m))))
    else:
        f_out.write("{}\n".format(punctaj * 1000))
```
