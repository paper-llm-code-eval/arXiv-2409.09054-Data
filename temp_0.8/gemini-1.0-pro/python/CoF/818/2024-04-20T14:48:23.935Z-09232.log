with open("roata.in", "r") as f:
    n = int(f.readline())
    p = int(f.readline())
    c = list(map(int, f.readline().split()))

# Calcularea sumei totale incasate
suma = 0
for i in c:
    suma += i

# Simularea urcarii si coborarii clientilor
cabina = 1
coborari = []
while True:
    # Urcarea clientului cu numarul cel mai mic din rand
    for i in range(len(c)):
        if c[i] > 0:
            c[i] -= 1
            cabina += 1
            if cabina > n:
                cabina = 1
            break

    # Coborarea clientului din cabina curenta
    if c == [0] * p:
        break
    else:
        coborari.append(i + 1)
        cabina += 1
        if cabina > n:
            cabina = 1

# Scrierea datelor de iesire
with open("roata.out", "w") as f:
    f.write(str(suma) + "\n")
    f.write(" ".join(map(str, coborari)) + "\n")
    f.write(str(cabina) + "\n")
```
