import sys

def read_input():
    v = int(sys.stdin.readline())
    n = int(sys.stdin.readline())
    p = [int(x) for x in sys.stdin.readline().split()]
    return v, n, p

def solve(v, n, p):
    if v == 1:
        max_len = 0
        for i in range(n):
            j = i
            while j < n and p[j] > p[j - 1]:
                j += 1
            max_len = max(max_len, j - i)
        return max_len
    else:
        p.sort()
        max_sum = -1
        min_arr = []
        for i in range(n):
            sum = 0
            arr = []
            for j in range(n):
                k = (j + i) % n
                sum += p[j] * p[k]
                arr.append(p[k])
            if sum > max_sum or (sum == max_sum and arr < min_arr):
                max_sum = sum
                min_arr = arr
        return min_arr

if __name__ == "__main__":
    v, n, p = read_input()
    result = solve(v, n, p)
    with open('roboti.out', 'w') as f:
        if v == 1:
            f.write(str(result))
        else:
            f.write(' '.join(map(str, result)))
