import sys

def main():
    with open("roboti.in") as f_in, open("roboti.out", "w") as f_out:
        v = int(f_in.readline().strip())
        n = int(f_in.readline().strip())
        p = list(map(int, f_in.readline().strip().split()))

        if v == 1:
            max_len = 0
            start = -1
            for i in range(n):
                if start == -1 or p[i] > p[start]:
                    start = i
                    max_len = max(max_len, 1)
                else:
                    start = i
                    max_len = max(max_len, start - i)
            f_out.write(str(max_len) + "\n")

        elif v == 2:
            p.sort()

            max_sum = 0
            for i in range(n):
                j = (i + 1) % n
                max_sum += p[i]*p[j]

            min_string = ""
            for i in range(n):
                j = (i + 1) % n
                if p[i]*p[j] >= p[j]*p[(j+1)%n]:
                    min_string += str(p[i]) + " "
                else:
                    min_string += str(p[j]) + " "

            f_out.write(min_string)


if __name__ == "__main__":
    main()
```
