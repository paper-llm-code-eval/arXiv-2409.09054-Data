import re

with open('reteta.in', 'r') as f_in:
    line = f_in.readline().strip()

# Preprocess the input string
line = re.sub(r'\s+', ' ', line)
line = line.replace('(', ' ( ').replace(')', ' ) ')

# Split the recipe into ingredients and times
ingredients = []
times = []
for item in line.split():
    if item[-1].isdigit():
        times.append(int(item[:-1]))
    else:
        ingredients.append(item)

# Calculate the total time
total_time = sum(times)

# Count the quantities of each ingredient
ingredient_counts = {}
for ingredient in ingredients:
    if ingredient not in ingredient_counts:
        ingredient_counts[ingredient] = 0
    ingredient_counts[ingredient] += 1

# Sort the ingredients lexicographically
sorted_ingredients = sorted(ingredient_counts.keys())

# Write the output
with open('reteta.out', 'w') as f_out:
    f_out.write(f'{total_time}\n')
    for ingredient in sorted_ingredients:
        f_out.write(f'{ingredient} {ingredient_counts[ingredient]}\n')
