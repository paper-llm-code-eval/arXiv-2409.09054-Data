import sys

def is_palindrome(n):
    s = str(n)
    return s == s[::-1]

def add_digits(n):
    s = str(n)
    while not is_palindrome(n):
        s += s[-1]
        n = int(s)
    return n

def main():
    with open('palindrom.in', 'r') as f_in, open('palindrom.out', 'w') as f_out:
        C = int(f_in.readline())
        n = int(f_in.readline())
        a = [int(f_in.readline()) for _ in range(n)]
        if C == 1:
            f_out.write(str(sum(len(str(add_digits(x))) - len(str(x)) for x in a)))
        else:
            S = int(f_in.readline())
            ans = 0
            cur = 0
            for x in a:
                if cur + len(str(add_digits(x))) - len(str(x)) <= S:
                    cur += len(str(add_digits(x))) - len(str(x))
                    ans += 1
                else:
                    break
            f_out.write(str(ans))

if __name__ == '__main__':
    main()
