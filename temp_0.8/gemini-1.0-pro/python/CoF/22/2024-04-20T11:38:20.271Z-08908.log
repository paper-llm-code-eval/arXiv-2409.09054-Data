N = 0
D = 0
edges = []

with open('tairos.in', 'r') as fin:
    N = int(fin.readline())
    D = int(fin.readline())
    edges = [list(map(int, line.split())) for line in fin.readlines()]

adj = [[] for i in range(N + 1)]
for u, v in edges:
    adj[u].append(v)
    adj[v].append(u)

dist = [-1] * (N + 1)
dist[1] = 0

queue = [1]
while queue:
    u = queue.pop(0)
    for v in adj[u]:
        if dist[v] == -1:
            dist[v] = dist[u] + 1
            queue.append(v)

cnt = 0
for i in range(1, N + 1):
    if dist[i] == D:
        cnt += 1

with open('tairos.out', 'w') as fout:
    fout.write(str(cnt % (10 ** 9 + 7)))
