def find_magic_word(chars):
    """
    Finds the magic word from the given characters.

    Args:
        chars (str): The characters to find the magic word from.

    Returns:
        str: The magic word.
    """
    magic_word = ""
    for char in chars:
        if char.isalpha():
            magic_word += char.upper()
    return magic_word

def find_magic_number(chars):
    """
    Finds the magic number from the given characters.

    Args:
        chars (str): The characters to find the magic number from.

    Returns:
        int: The magic number.
    """
    digits = []
    for char in chars:
        if char.isdigit():
            digits.append(int(char))
    digits = sorted(set(digits))
    magic_number = 0
    for digit in digits:
        magic_number *= 10
        magic_number += digit
    return magic_number

with open("magic.in", "r") as f_in:
    n = int(f_in.readline())
    chars = f_in.readline().split()

with open("magic.out", "w") as f_out:
    f_out.write(find_magic_word(chars) + "\n")
    f_out.write(str(find_magic_number(chars)) + "\n")
