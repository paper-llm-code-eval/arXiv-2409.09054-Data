            x //= 10

        if digits == sorted(digits) or digits == sorted(digits, reverse=True):
            vp.append((v[i], i + 1))

    fout.write(f'{len(vp)}\n')

    if len(vp) > 0:
        mx = max(vp)
        mn = min(vp)
        fout.write(f'{mx[0]} {" ".join(map(str, [pos for val, pos in vp if val == mx[0]]))}\n')
