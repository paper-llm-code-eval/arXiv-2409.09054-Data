with open("circular.in", "r") as f:
    c = int(f.readline())
    albastre = f.readline().strip()
    rosii = f.readline().strip()

def cerinta1(albastre):
    timp = 0
    indicator = "A"
    for albastru in albastre:
        stanga = (ord(albastru) - ord(indicator) + 26) % 26
        dreapta = (ord(indicator) - ord(albastru) + 26) % 26
        timp += min(stanga, dreapta)
        indicator = albastru
    return timp

def cerinta2(albastre, rosii):
    n = len(albastre)
    m = len(rosii)
    cost = [[0] * (m + 1) for _ in range(n + 1)]
    dp = [[""] * (m + 1) for _ in range(n + 1)]

    for i in range(n - 1, -1, -1):
        for j in range(m):
            stanga = (ord(albastre[i]) - ord(rosii[j]) + 26) % 26
            dreapta = (ord(rosii[j]) - ord(albastre[i]) + 26) % 26
            if stanga < dreapta:
                cost[i][j] = cost[i + 1][j] + stanga
                dp[i][j] = dp[i + 1][j] + albastre[i]
            else:
                cost[i][j] = cost[i][j + 1] + dreapta
                dp[i][j] = dp[i][j + 1] + rosii[j] + albastre[i]

    timp = cost[0][0]
    nr_sir = 0
    sir_min = ""

    for i in range(n + 1):
        for j in range(m + 1):
            if cost[i][j] == timp:
                nr_sir += 1
                if dp[i][j] < sir_min or sir_min == "":
                    sir_min = dp[i][j]

    return timp, nr_sir % 666013, sir_min

with open("circular.out", "w") as f:
    if c == 1:
        f.write(str(cerinta1(albastre)))
    else:
        timp, nr_sir, sir_min = cerinta2(albastre, rosii)
        f.write(str(timp) + "\n")
        f.write(str(nr_sir) + "\n")
        f.write(sir_min)
