import sys

# Read input
C = int(sys.stdin.readline())
n, k = map(int, sys.stdin.readline().split())
a = list(map(int, sys.stdin.readline().split()))

# Find the largest number with exactly k digits
max_k_digits = 0
for i in range(n):
    num_digits = 0
    number = a[i]
    while number > 0:
        number //= 10
        num_digits += 1
    if num_digits == k:
        max_k_digits = max(max_k_digits, a[i])

# Find the largest number obtained by unifying two adjacent numbers
max_adjacent = 0
for i in range(n - 1):
    num1 = a[i]
    num2 = a[i + 1]
    unified = set()
    while num1 > 0:
        unified.add(num1 % 10)
        num1 //= 10
    while num2 > 0:
        unified.add(num2 % 10)
        num2 //= 10
    unified_number = 0
    for digit in sorted(unified, reverse=True):
        unified_number = unified_number * 10 + digit
    max_adjacent = max(max_adjacent, unified_number)

# Find the largest number obtained by unifying k consecutive numbers
max_consecutive = 0
for i in range(n - k + 1):
    unified = set()
    for j in range(i, i + k):
        num = a[j]
        while num > 0:
            unified.add(num % 10)
            num //= 10
    unified_number = 0
    for digit in sorted(unified, reverse=True):
        unified_number = unified_number * 10 + digit
    max_consecutive = max(max_consecutive, unified_number)

# Write output
if C == 1:
    print(max_k_digits)
elif C == 2:
    print(max_adjacent)
elif C == 3:
    print(max_consecutive)
