with open("unificare.in") as fin, open("unificare.out", "w") as fout:
    C = int(fin.readline())
    n, k = map(int, fin.readline().split())
    arr = [int(x) for x in fin.readline().split()]

    maxx = 0
    for el in arr:
        mx = 0
        vis = [False for i in range(10)]
        while el > 0:
            if not vis[el % 10]:
                vis[el % 10] = True
                mx += 1
            el //= 10
        if mx == k:
            maxx = max(maxx, el)

    if C == 1:
        fout.write(str(maxx))

    else:
        if C == 2:
            mx = 0
            for i in range(n - 1):
                a = arr[i]
                b = arr[i + 1]
                vis = [False for i in range(10)]
                while a > 0 or b > 0:
                    if a > 0:
                        if not vis[a % 10]:
                            vis[a % 10] = True
                            mx += 1
                        a //= 10
                    if b > 0:
                        if not vis[b % 10]:
                            vis[b % 10] = True
                            mx += 1
                        b //= 10
            fout.write(str(mx))

        else:
            mx = 0
            for i in range(n - k + 1):
                a = arr[i]
                vis = [False for i in range(10)]
                for j in range(k):
                    a = arr[i + j]
                    while a > 0:
                        if not vis[a % 10]:
                            vis[a % 10] = True
                            mx += 1
                        a //= 10
            fout.write(str(mx))
