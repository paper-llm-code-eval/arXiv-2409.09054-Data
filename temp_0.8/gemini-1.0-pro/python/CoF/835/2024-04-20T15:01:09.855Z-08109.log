import sys

def unific(A, B):
    C = []
    for i in range(max(len(A), len(B))):
        a = ord(A[i]) - ord('0') if i < len(A) else 0
        b = ord(B[i]) - ord('0') if i < len(B) else 0
        if a and b:
            C.append(str(min(a, b)))
    return ''.join(C), len(C) != 0

def unific_max(A):
    B = []
    while A:
        if len(A) == 1:
            B.append(A[0])
            break
        A2 = []
        i = 0
        while i < len(A) - 1:
            U, ok = unific(A[i], A[i + 1])
            if ok:
                A2.append(U)
                i += 2
            else:
                A2.append(A[i])
                i += 1
        if len(A2) == len(A):
            B.extend(A)
            break
        A = A2
    return B

f = open('unific.in')
n = int(f.readline())
A = [f.readline().strip() for i in range(n)]
f.close()

d = dict()
for x in ''.join(A):
    if not x.isdigit():
        continue
    if x in d:
        d[x] += 1
    else:
        d[x] = 1
c = sorted(d.items(), key=lambda x: (x[1], x[0]))[0][0]

B = unific_max(A)
f = open('unific.out', 'w')
f.write(str(c) + '\n')
f.write(str(len(B)) + '\n')
f.write(' '.join(B))
f.close()
```
