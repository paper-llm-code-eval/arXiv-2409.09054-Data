with open("strips.in", "r") as f_in, open("strips.out", "w") as f_out:
    cerinta, N, Nr, L = map(int, f_in.readline().split())
    L -= 1
    penalties = [0, 0]
    board = [0] * N
    for poz in f_in:
        poz = int(poz)
        if poz + L < N and not board[poz:poz + L + 1]:
            board[poz:poz + L + 1] = [1 if cerinta == 1 else -1] * (L + 1)
        else:
            penalties[cerinta == 2] += 1
            Nr -= 1
        if not Nr:
            break
    if cerinta == 2:
        max_len = [0, 0]
        curr_len = [0, 0]
        for color in board:
            if color:
                curr_len[color] += 1
            else:
                max_len[abs(color)] = max(max_len[abs(color)], curr_len[abs(color)])
                curr_len[abs(color)] = 0
        max_len[abs(color)] = max(max_len[abs(color)], curr_len[abs(color)])
        f_out.write(f"{max_len[1]} {max_len[2]}")
    else:
        f_out.write(f"{penalties[0]} {penalties[1]}")
```
