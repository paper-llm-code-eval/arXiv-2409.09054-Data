    N, Nr, L = (int(x) for x in f.readline().strip().split())
    P = [0, 0]  # penalty
    L_max = [0, 0]  # longest color segment
    strip = [0] * N  # colored strips on board

    for pos in f:
        pos = int(pos.strip())
        for player in range(2):
            if 0 <= pos + L - 1 < N and strip[pos:pos + L] == [0] * L:
                strip[pos:pos + L] = [1 + player] * L
                L_max[player] = max(L_max[player], pos + L)
                break
            else:
                P[player] += 1
                Nr -= 1

    if C == 1:
        print(*P)
    else:
        print(*L_max)
