import array
f = open('vecini.in')
m, n, k = map(int, f.readline().split())
A = array.array('i', [0] * n * m)
for _ in range(k):
    i, j = map(int, f.readline().split())
    A[(i - 1) * n + j - 1] = 1
f.close()

dx = [-1, -1, 0, 1, 1, 1, 0, -1]
dy = [0, 1, 1, 1, 0, -1, -1, -1]
ans = 0
st = 0
for i in range(m):
    for j in range(n):
        cur = 0
        if A[i * n + j]:
            for d in range(8):
                x = i + dx[d]
                y = j + dy[d]
                if 0 <= x < m and 0 <= y < n and A[x * n + y]:
                    cur += 1
        if cur > ans:
            ans = cur
            st = 1
        elif cur == ans:
            st += 1

with open('vecini.out', 'w') as f:
    f.write(str(ans) + ' ' + str(st))
```
