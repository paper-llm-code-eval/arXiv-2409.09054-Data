with open('triunghiuri.in', 'r') as f, open('triunghiuri.out', 'w') as g:
    p, n = list(map(int, f.readline().split()))
    x_count = {}
    points = []
    for i in range(n):
        x, y = list(map(int, f.readline().split()))
        points.append((x, y))
        x_count[x] = x_count.get(x, 0) + 1
    if p == 1:
        g.write(f'{max(x_count.values())}\n')
    else:
        points.sort()
        cnt = 0
        for i in range(n):
            l, r = i, i
            while l >= 0 and r < n and points[l][1] == points[r][1]:
                l -= 1
                r += 1
            cnt += (r - l - 1) * (r - l) // 2
        g.write(f'{cnt % 1000003}\n')
```
