MOD = int(1e9) + 7

def fastpow(base, p):
    if p == 0:
        return 1
    if p == 1:
        return base
    val = fastpow(base, int(p / 2))
    val = (val * val) % MOD
    if p % 2 != 0:
        val = (val * base) % MOD
    return val

def solve(arr, n):
    """
    Returns the number of ways to select consecutive elements from the array
    such that the tallest element is in the first half of the selection.
    """

    dp = [0] * (n + 1)
    dp[0] = 1
    dp[1] = 1
    for i in range(2, n + 1):
        left = i // 2
        right = i - 1 - left
        if arr[i - 1] >= arr[left]:
            dp[i] = (dp[i - 1] + dp[left]) % MOD
        else:
            dp[i] = (dp[i - 1] + dp[right]) % MOD
    return dp[n]

n = int(open('leftmax.in').readline().strip())
arr = list(map(int, open('leftmax.in').readline().strip().split()))
result = solve(arr, n)
open('leftmax.out', 'w').write(str(result))
