with open('text.in') as f:
    text = f.readlines()

text = [x.strip() for x in text]
text = ' '.join(text).split()

# DP: [length of remaining substring][last character]
dp = {}
for i in range(len(text) - 1, -1, -1):
    dp[i, text[i][-1]] = 1  # base case

for i in range(len(text) - 1):
    for j in range(26):
        c = chr(ord('a') + j)
        if not c in dp:
            dp[i, c] = len(text)  # initialize with infinity
        dp[i, c] = min(dp[i, c], 1 + dp[i + 1, text[i][0]])

min_removals = dp[0, text[0][-1]]
num_words = len(text)
output = []
i = 0
last_char = text[0][-1]
while i < len(text):
    if dp[i, last_char] == 1:
        output.append(text[i])
        last_char = text[i][0]
        i += 1
    else:
        i += 1

with open('text.out', 'w') as f:
    f.write(str(min_removals) + '\n')
    f.write(str(num_words) + '\n')
    for w in output:
        f.write(w + '\n')
```
