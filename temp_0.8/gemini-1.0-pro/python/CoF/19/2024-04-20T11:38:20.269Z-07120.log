import math
import sys
input = sys.stdin.readline
with open('recyclebin.out', 'w') as fout:
    n = int(input())
    a = list(map(int, input().split()))
    a.sort()
    positive = False
    cost = 0
    for x in a:
        if x > 0:
            positive = True
    for i in range(n-1, -1, -1):
        if a[i] > 0:
            cost += a[i]
            cnt = 1
            while cnt * cnt <= n - i:
                cost -= a[i - cnt * cnt]
                cnt += 1
    if positive:
        fout.write(str(cost))
    else:
        fout.write('0')
