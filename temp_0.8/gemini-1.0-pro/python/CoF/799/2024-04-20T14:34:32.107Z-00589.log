def main():
    with open('cuvinte.in', 'r') as f:
        cuvinte = f.readline().rstrip('!')
        cuvinte_separate = cuvinte.split('  ')

    cuvinte_transformate = set()

    for cuvant in cuvinte_separate:
        cuvant_transformat = cuvant[1:] + cuvant[0]
        cuvinte_transformate.add(cuvant_transformat)

    i = 0
    cuvinte_ramase = []

    while i < len(cuvinte_separate):
        if cuvinte_separate[i] in cuvinte_transformate:
            cuvinte_ramase.append(cuvinte_separate[i - 1])
            del cuvinte_separate[i - 1]
            i -= 1
        i += 1

    cuvinte_ramase.sort()

    with open('cuvinte.out', 'w') as f:
        if len(cuvinte_separate) == len(cuvinte_ramase):
            f.write('0\n')
        else:
            f.write(f'{cuvinte_ramase[0]}\n')
        f.write(' '.join(map(str, cuvinte_ramase[1:])))


if __name__ == '__main__':
    main()
