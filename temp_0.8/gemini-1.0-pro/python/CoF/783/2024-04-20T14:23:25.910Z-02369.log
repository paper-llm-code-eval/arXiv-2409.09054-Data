def codifica_placare(linii, N, M):
  matrice = [[-1] * M for _ in range(N)]
  for i in range(N):
    linie = linii[i].split()
    for j in range(M):
      if linie[j] != '0':
        valoare = int(linie[j])
        if valoare > 0:
          for k in range(valoare):
            matrice[i][j + k] = valoare
        else:
          for k in range(abs(valoare)):
            matrice[i + k][j] = abs(valoare)
  return matrice


def scrie_matrice(matrice, N, M):
  with open('placare.out', 'w') as f:
    for i in range(N):
      for j in range(M):
        f.write(str(abs(matrice[i][j])) + ' ')
      f.write('\n')

with open('placare.in', 'r') as f:
  N, M = [int(x) for x in f.readline().split()]
  linii = f.read().splitlines()

matrice = codifica_placare(linii, N, M)
scrie_matrice(matrice, N, M)
