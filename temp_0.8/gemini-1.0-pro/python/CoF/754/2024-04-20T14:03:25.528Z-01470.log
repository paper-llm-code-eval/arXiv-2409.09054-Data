def main():
    with open("grupe.in", "r") as f_in, open("grupe.out", "w") as f_out:
        n, k = map(int, f_in.readline().split())
        sex = f_in.readline()
        grupe = []
        for _ in range(k):
            l, *grupa = map(int, f_in.readline().split())
            grupe.append(grupa)

        f_count = [0] * k
        b_count = [0] * k
        for grupa in grupe:
            for elev in grupa:
                if sex[elev - 1] == "f":
                    f_count[grupe.index(grupa)] += 1
                else:
                    b_count[grupe.index(grupa)] += 1

        if max(f_count) - min(f_count) > 1 or max(b_count) - min(b_count) > 1 or max(map(len, grupe)) - min(map(len, grupe)) > 1:
            f_out.write("NU")
        else:
            f_out.write("DA\n")
            for i in range(k):
                f_out.write(f"{b_count[i]} {f_count[i]}\n")


if __name__ == "__main__":
    main()
