with open('grupe.in') as f_in, open('grupe.out', 'w') as f_out:
    n, k = map(int, f_in.readline().strip().split())
    sexes = f_in.readline().strip()
    groups = [list(map(int, f_in.readline().strip().split())) for _ in range(k)]

    cnt_sex = [0, 0]  # [boys, girls]
    cnt_groups = [0] * k
    used = set()
    for group in groups:
        for student in group[1:]:
            if student in used:
                f_out.write('NU')
                exit()
            used.add(student)
        cnt_groups[group[0] - 1] += 1
        cnt_sex[sexes[group[1] - 1] == 'b'] += len(group) - 1

    is_valid = True
    for i in range(k):
        is_valid &= abs(cnt_groups[i] - cnt_groups[(i + 1) % k]) <= 1
        is_valid &= abs(cnt_sex[0] - cnt_sex[1]) <= 1
    f_out.write('DA' if is_valid else 'NU')
    for group in groups:
        f_out.write(f'\n{" ".join(map(str, cnt_sex))}')
```
