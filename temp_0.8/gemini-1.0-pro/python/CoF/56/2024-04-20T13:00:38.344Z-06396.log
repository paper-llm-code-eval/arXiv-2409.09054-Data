import sys

MAX_N = 500
MAX_P = 200
MAX_K = 30000
MAX_D = 10
MAX_C = 10

n, p, k = map(int, sys.stdin.readline().split())
d = [0] * (p + 1)
c = [0] * (p + 1)
adj = [[] for _ in range(p + 1)]
for i in range(1, p + 1):
    d[i], c[i] = map(int, sys.stdin.readline().split())
for i in range(k):
    p1, p2 = map(int, sys.stdin.readline().split())
    adj[p1].append(p2)

dp = [[[-1] * (MAX_C + 1) for _ in range(MAX_P + 1)] for _ in range(MAX_N + 1)]

def solve(i, j, heads):
    if i == p + 1:
        if heads >= 1:
            return 0
        else:
            return -1
    if dp[i][j][heads] != -1:
        return dp[i][j][heads]
    ans = -1
    for k in range(j, p + 1):
        if k not in adj[j]:
            new_heads = heads - c[k]
            if new_heads >= 0:
                cur = solve(i + 1, k + 1, new_heads)
                if cur != -1:
                    if ans == -1:
                        ans = cur + d[k]
                    else:
                        ans = min(ans, cur + d[k])
    dp[i][j][heads] = ans
    return ans

ans = solve(1, 1, n)
if ans == -1:
    sys.stdout.write('-1')
else:
    sys.stdout.write(str(ans))
