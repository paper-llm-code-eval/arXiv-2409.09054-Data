import sys

f = open("zmeu.in", "r")
g = open("zmeu.out", "w")

n, p, k = map(int, f.readline().split())
durate = [0] + [[] for _ in range(p+1)]
nr_capete = [0] * (p+1)

for i in range(1, p+1):
    d, c = map(int, f.readline().split())
    durate[i].append(d)
    nr_capete[i] = c

for _ in range(k):
    a, b = map(int, f.readline().split())
    durate[a].append(p+b)

dp = [[[-1] * (n+1) for _ in range(p+1)] for _ in range(2)]

dp[0][1][n] = 0
for i in range(1, p+1):
    for j in range(max(0, n-nr_capete[i]), n+1):
        if dp[0][i][j] == -1:
            continue
        # continui povestea curenta
        dp[1][i][j] = max(dp[1][i][j], dp[0][i][j] + durate[i][0])
        # trec la urmatoarea poveste
        for next_story in durate[i][1:]:
            if next_story <= i or dp[0][next_story][j-nr_capete[next_story]] == -1:
                continue
            dp[1][i][j] = max(dp[1][i][j], dp[0][next_story][j-nr_capete[next_story]] + durate[i][0])
    dp[0] = dp[1]

ans = -1
for i in range(1, n+1):
    if dp[0][p][i] != -1:
        ans = max(ans, dp[0][p][i])

g.write(str(ans) + "\n")

f.close()
g.close()
```
