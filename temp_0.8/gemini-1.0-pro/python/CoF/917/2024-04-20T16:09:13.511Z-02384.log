with open('cetate.in', 'r') as fin, open('cetate.out', 'w') as fout:
    c, n, m, k = map(int, fin.readline().split())
    mat = []
    for _ in range(n):
        mat.append(list(map(int, fin.readline().split())))
    dp_max = [[[[-1] * k for _ in range(m - k + 1)] for _ in range(n - k + 1)] for _ in range(k)]
    dp_min = [[[[-1] * k for _ in range(m - k + 1)] for _ in range(n - k + 1)] for _ in range(k)]

    def get_max(i, j, k):
        if i == n - k + 1 or j == m - k + 1:
            return 0
        if dp_max[k][i][j][0] != -1:
            return dp_max[k][i][j][0]
        dp_max[k][i][j][0] = max(
            get_max(i + 1, j, k) + sum(mat[i + k - 1][j: j + k]),
            get_max(i, j + 1, k) + sum(x[j: j + k] for x in mat[i: i + k]),
            get_max(i + 1, j + 1, k),
        )
        return dp_max[k][i][j][0]

    def get_min(i, j, k):
        if i == n - k + 1 or j == m - k + 1:
            return 0
        if dp_min[k][i][j][0] != -1:
            return dp_min[k][i][j][0]
        dp_min[k][i][j][0] = min(
            get_min(i + 1, j, k) + sum(mat[i + k - 1][j: j + k]),
            get_min(i, j + 1, k) + sum(x[j: j + k] for x in mat[i: i + k]),
            get_min(i + 1, j + 1, k),
        )
        return dp_min[k][i][j][0]

    if c == 1:
        fout.write(f'{get_max(0, 0, k)}\n')
        i, j = 0, 0
        for p in range(k):
            next_i = i + 1
            next_j = j + 1
            if get_max(next_i, j, k) > get_max(i, next_j, k):
                next_i += k - 1
            if get_max(next_i, j, k) > get_max(i, next_j, k):
                next_j += k - 1
            i, j = next_i, next_j
        fout.write(f'{i + 1} {j + 1} {i + k} {j + k}')
    else:
        fout.write(f'{get_min(0, 0, k)}\n')
        i, j = 0, 0
        for p in range(k):
            next_i = i + 1
            next_j = j + 1
            if get_min(next_i, j, k) < get_min(i, next_j, k):
                next_i += k - 1
            if get_min(next_i, j, k) < get_min(i, next_j, k):
                next_j += k - 1
            i, j = next_i, next_j
        fout.write(f'{i + 1} {j + 1} {i + k} {j + k}')
