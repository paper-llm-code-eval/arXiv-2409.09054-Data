def solve(n: int, nums: list[int]) -> tuple[int, int, int]:
    """
    Returns a tuple of (max_height, num_max_height, num_equal).
    """
    max_height = 0
    num_max_height = 0
    num_equal = 0
    for num in nums:
        height = 0
        while num > 0:
            height += 1
            num //= 10
        if height > max_height:
            max_height = height
            num_max_height = 1
        elif height == max_height:
            num_max_height += 1

    for i in range(n):
        num = nums[i]
        j = n - i - 1
        num_rev = nums[j]
        height = 0
        height_rev = 0
        while num > 0:
            height += 1
            num //= 10
        while num_rev > 0:
            height_rev += 1
            num_rev //= 10
        if height == height_rev:
            num_equal += 1

    num_diff = max_height - height_rev
    if num_diff % 2 == 0 and height_rev != 0:
        num_diff = num_diff // 2
    else:
        num_diff = num_diff // 2 + 1

    num_cubes = num_equal * num_diff

    return (max_height, num_max_height, num_cubes)


