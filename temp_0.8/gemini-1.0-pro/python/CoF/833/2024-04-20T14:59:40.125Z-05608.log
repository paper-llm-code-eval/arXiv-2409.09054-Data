            nr_identic += 1

    nr_turnuri = 0
    for i in range(n):
        nr_turnuri += len(crina[i])

    if nr_turnuri % 2 == 0:
        print(nr_turnuri // 2, file=out)
    else:
        print(nr_turnuri // 2 + 1, file=out)

    print(hmax, nr_hmax, nr_identic, file=out)
