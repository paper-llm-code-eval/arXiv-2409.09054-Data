with open("galerie.in", "r") as fin, open("galerie.out", "w") as fout:
    cerinta = int(fin.readline())
    n, m, t = map(int, fin.readline().split())
    init = list(map(int, fin.readline().split()))
    
    # Initializare teren
    teren = [[0 for j in range(m)] for i in range(n)]
    for i in range(t):
        x, y = (init[i]-1) // m, (init[i]-1) % m
        teren[x][y] = 1
    
    # Directiile de deplasare
    dx = [0, 1, 0, -1]
    dy = [1, 0, -1, 0]
    
    max_celule_sapa_o_cartita = 0
    max_celule_galerie = 0
    
    # BFS
    while True:
        # Initializare coada
        q = []
        for i in range(n):
            for j in range(m):
                if teren[i][j] == 1:
                    q.append((i, j, 1))
                    teren[i][j] = 2
        
        if not q:
            break
        
        celule_galerie = 0
        while q:
            x, y, celule = q.pop(0)
            celule_galerie = max(celule_galerie, celule)
            max_celule_sapa_o_cartita = max(max_celule_sapa_o_cartita, celule)
            
            for d in range(4):
                nx, ny = x + dx[d], y + dy[d]
                if 0 <= nx < n and 0 <= ny < m and teren[nx][ny] == 0:
                    q.append((nx, ny, celule + 1))
                    teren[nx][ny] = 2
        
        max_celule_galerie = max(max_celule_galerie, celule_galerie)
    
    if cerinta == 1:
        fout.write(f"{max_celule_sapa_o_cartita}\n")
    else:
        fout.write(f"{max_celule_galerie}\n")
