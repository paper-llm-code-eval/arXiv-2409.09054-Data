with open("betasah.in") as f:
    n, d, k = f.readline().split()
    n, d, k = int(n), int(d), int(k)
    queens = [tuple(map(int, f.readline().split())) for _ in range(d)]
    greys = [tuple(map(int, f.readline().split())) for _ in range(k)]

board = [[0] * i for i in range(1, n + 1)]

for r, c in queens:
    board[r][c] = 1

for r, c in greys:
    board[r][c] = -1

# task 1
m = 0
for i in board:
    m = max(m, max(i) * 2 + 1)

# task 2
p = 0
for r, row in enumerate(board, 1):
    for c, cell in enumerate(row, 1):
        if cell == 1:
            for dr, dc in [(0, 1), (0, -1), (1, 0), (-1, 0)]:
                nr, nc = r, c
                while True:
                    nr, nc = nr + dr, nc + dc
                    if not (1 <= nr <= n and 1 <= nc <= nr):
                        break
                    if board[nr][nc] == -1 or board[nr][nc] == 1:
                        break
                    board[nr][nc] = 2
            p += 1

for r, row in enumerate(board, 1):
    for c, cell in enumerate(row, 1):
        if cell == 2:
            p += 1

with open("betasah.out", "w") as f:
    f.write(f"{m}\n{p}")
