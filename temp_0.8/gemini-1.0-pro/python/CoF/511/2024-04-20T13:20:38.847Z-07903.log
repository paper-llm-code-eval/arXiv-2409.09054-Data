with open('ciocolata.in', 'r') as f:
    c, n = map(int, f.readline().split())
    choco = list(map(int, f.readline().split()))

if c == 1:
    freq = {}
    for x in choco:
        if x not in freq: freq[x] = 0
        freq[x] += 1
    max_freq = max(freq.values())
    most_freq = []
    for x in freq:
        if freq[x] == max_freq: most_freq.append(x)
    print(min(most_freq))

elif c == 2:
    choco.sort(reverse=True)
    irina = 0
    mihaela = 0
    for i in range(n):
        if irina <= mihaela:
            irina += choco[i]
        else:
            mihaela += choco[n - i - 1]
    print(abs(irina - mihaela))
```
