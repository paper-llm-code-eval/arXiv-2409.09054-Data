with open("cladiri.in", "r") as fin, open("cladiri.out", "w") as fout:
    cx, cy, i = map(int, fin.readline().split())
    buildings = []
    r = 0
    while True:
        line = fin.readline()
        if not line:
            break
        x, y, v = map(int, line.split())
        buildings.append((x, y, v))
        r = max(r, abs(x - cx) + abs(y - cy))
    
    affected = 0
    max_affected = 0
    affected_levels = []
    for level in range(r + 1):
        current_affected = 0
        for x, y, v in buildings:
            if i - abs(x - cx) - abs(y - cy) >= v:
                affected += 1
                current_affected += 1
        if current_affected > 0:
            affected_levels.append(level)
        max_affected = max(max_affected, current_affected)
    
    fout.write(str(affected) + "\n")
    fout.write(str(max_affected) + "\n")
    fout.write(" ".join(map(str, affected_levels)))
```
