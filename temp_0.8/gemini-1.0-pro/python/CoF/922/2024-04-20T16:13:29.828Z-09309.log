with open("furnica.in",'r') as f:
    c = int(f.readline())
    n, *d = map(int,f.read().split())

t = 0
for i in range(n-1):
    t += d[i][0]*3
    t += d[i][1]*5
    t += d[i][1]*2
    d[i+1][1] += d[i][1]
if c == 1:
    with open("furnica.out",'w') as g:
        g.write(f"{t}")
elif c == 2:
    l = 0
    for i in range(n-1):
        if d[i][1] < d[i+1][1]:
            l = 0
        l += d[i][0]
    with open("furnica.out",'w') as g:
        g.write(f"{l}")
else:
    for i in range(n):
        if t - d[i][1]*2 >= 0:
            t -= d[i][1]*2
        else:
            break
    with open("furnica.out",'w') as g:
        g.write(f"{i+1}")
```
