with open("joc.in") as f:
    n = int(f.readline().strip())
    values = [int(x) for x in f.readline().split()]
    throws = int(f.readline().strip())
    zar = [int(x) for x in f.readline().split()]

def play(values, zar):
    pos = [1, 1]
    points = [0, 0]

    for i in range(throws):
        for player in range(2):
            pos[player] = (pos[player] + zar[i] - 1) % n + 1
            if values[pos[player] - 1] == 0:
                points[player] = 0
                pos[player] = 1
            elif values[pos[player] - 1] == 10:
                points[player] += 10
            elif values[pos[player] - 1] == 1:
                points[player] += 1
            if pos[0] == pos[1] and pos[0] != 1:
                points[1] = 0
                pos[1] = 1

    winner = 1 if points[0] > points[1] or (points[0] == points[1] and pos[0] > pos[1]) else 2
    return winner, pos, points

winner, pos, points = play(values, zar)

with open("joc.out", "w") as f:
    f.write(str(winner) + "\n")
    f.write(f"{pos[0]} {points[0]}\n")
    f.write(f"{pos[1]} {points[1]}\n")
```
