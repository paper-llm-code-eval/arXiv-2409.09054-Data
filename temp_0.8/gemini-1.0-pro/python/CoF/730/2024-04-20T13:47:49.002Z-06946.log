import sys
sys.stdin = open("joc.in", "r")
sys.stdout = open("joc.out", "w")

n = int(input())
values = list(map(int, input().split()))
throws = int(input())
rolls = list(map(int, input().split()))
score = [0, 0]
pos = [1, 1]

def play(player):
    global score, pos, rolls
    roll = rolls.pop(0)
    pos[player] = (pos[player] + roll - 1) % n + 1
    score[player] += values[pos[player] - 1]
    if values[pos[player] - 1] == 0:
        score[player] = 0
        pos[player] = 1
    elif values[pos[player] - 1] == 10:
        score[player] += 10
    elif pos[player] == pos[player ^ 1] and pos[player] != 1:
        score[player ^ 1] = 0
        pos[player ^ 1] = 1

winner = None
while rolls:
    play(0)
    if pos[0] == 1 and score[0] > 0:
        winner = 1
        break
    play(1)
    if pos[1] == 1 and score[1] > 0:
        winner = 2
        break

if not winner:
    if score[0] > score[1]:
        winner = 1
    elif score[1] > score[0]:
        winner = 2
    else:
        if pos[0] > pos[1]:
            winner = 1
        else:
            winner = 2

print(winner)
print(pos[0], score[0])
print(pos[1], score[1])
