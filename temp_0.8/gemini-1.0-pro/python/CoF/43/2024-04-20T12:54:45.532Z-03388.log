import sys

with open("cerc.in", "r") as f:
    n = int(f.readline())
    cercuri = [(int(x), int(y), int(r)) for x, y, r in [z.split() for z in f]]

# Cerinta a)
drepte = set()
for i in range(n):
    x1, y1, r1 = cercuri[i]
    for j in range(i + 1, n):
        x2, y2, r2 = cercuri[j]
        if (x1 - x2) ** 2 + (y1 - y2) ** 2 == (r1 + r2) ** 2:
            drepte.add((x1, y1, x2, y2))
        elif (x1 - x2) ** 2 + (y1 - y2) ** 2 == (r1 - r2) ** 2:
            drepte.add((x1, y1, x2, y2))
m = len(drepte)

# Cerinta b)
q = 0
for i in range(n):
    x1, y1, r1 = cercuri[i]
    for x2, y2, r2 in cercuri[i + 1:]:
        if (x1 - x2) ** 2 + (y1 - y2) ** 2 == (r1 + r2) ** 2:
            q = max(q, 2)
        elif (x1 - x2) ** 2 + (y1 - y2) ** 2 == (r1 - r2) ** 2:
            q = max(q, 2)

# Cerinta c)
p = 0
for dreapta in drepte:
    x1, y1, x2, y2 = dreapta
    cnt = 0
    for x, y, r in cercuri:
        if (x - x1) * (y2 - y1) == (x2 - x1) * (y - y1) and (x2 - x1) ** 2 + (y2 - y1) ** 2 == (x - x1) ** 2 + (y - y1) ** 2 + r ** 2:
            cnt += 1
    p = max(p, cnt)

with open("cerc.out", "w") as f:
    f.write(f"{m} {q} {p}")
