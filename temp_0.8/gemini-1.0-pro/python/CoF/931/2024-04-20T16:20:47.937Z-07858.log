import sys

def read_input():
    with open('sir.in', 'r') as f:
        c = int(f.readline())
        n = int(f.readline())
        a = list(map(int, f.readline().split()))
    return c, n, a

def solve_type1(n, a):
    for i in range(n - 1, 0, -1):
        if a[i] > a[i - 1]:
            return i
    return -1

def solve_type2(n, a):
    ans = []
    for i in range(1, n):
        if a[i] > a[i - 1]:
            ans.append(i)
    return ans

def solve_type3(n, a):
    max_val = max(a)
    max_pos = a.index(max_val)
    sum = 0
    for i in range(max_pos):
        if a[i] < max_val:
            sum += max_val - a[i]
    for i in range(max_pos + 1, n):
        if a[i] < max_val:
            sum += max_val - a[i]
    return sum

def write_output(ans):
    with open('sir.out', 'w') as f:
        f.write(str(ans))

def main():
    c, n, a = read_input()
    if c == 1:
        ans = solve_type1(n, a)
    elif c == 2:
        ans = solve_type2(n, a)
    elif c == 3:
        ans = solve_type3(n, a)
    else:
        print('Invalid question type!')
        return
    
    write_output(ans)

if __name__ == '__main__':
    main()
