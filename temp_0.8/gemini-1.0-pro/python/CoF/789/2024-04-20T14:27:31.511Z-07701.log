import sys

def main():
    with open('startrek.in', 'r') as fin:
        n, t = map(int, fin.readline().split())
        d = [int(x) for x in fin.readline().split()]
        v = [int(x) for x in fin.readline().split()]

    d_sorted = sorted(enumerate(d), key=lambda x: x[1])
    v_sorted = sorted(enumerate(v), key=lambda x: x[1])

    max_destroyed = 0
    time = 0
    for i, vi in v_sorted:
        for j, dj in d_sorted:
            if dj <= vi * time:
                continue
            if dj <= vi * (time + t):
                max_destroyed += 1
                time += t
                break
            else:
                break
    
    with open('startrek.out', 'w') as fout:
        fout.write(str(max_destroyed))

if __name__ == '__main__':
    main()
