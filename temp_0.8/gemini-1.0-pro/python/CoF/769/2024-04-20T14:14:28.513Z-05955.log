import sys

def main():
    with open('dir.in', 'r') as f_in:
        dir_str = f_in.readline().strip()
    files = []
    stack = []
    current_dir = []
    for char in dir_str:
        if char == '(':
            stack.append(current_dir)
            current_dir = []
        elif char == ')':
            current_dir = stack.pop()
        elif char == ',' or char == ')':
            continue
        else:
            if char.islower():
                files.append('/'.join(current_dir + [char]))
            else:
                current_dir.append(char)
    with open('dir.out', 'w') as f_out:
        f_out.write(str(len(files)) + '\n')
        for file in sorted(files):
            f_out.write(file + '\n')

if __name__ == '__main__':
