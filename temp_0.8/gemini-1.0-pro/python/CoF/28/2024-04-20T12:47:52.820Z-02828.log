import sys
from io import StringIO

def read_input():
    with open("permutare.in", "r") as f:
        n, q = map(int, f.readline().split())
        queries = []
        for _ in range(q):
            line = f.readline().split()
            query_type = int(line[0])
            if query_type == 1:
                p = int(line[2])
                queries.append((query_type, n, p))
            else:
                a = list(map(int, line[1:]))
                queries.append((query_type, n, a))
    return n, queries

def write_output(answers):
    with open("permutare.out", "w") as f:
        for answer in answers:
            if isinstance(answer, list):
                f.write(' '.join(map(str, answer)) + '\n')
            else:
                f.write(str(answer) + '\n')

def solve(n, queries):
    def gen_permutations():
        p = [i for i in range(1, n + 1)]
        yield p
        while True:
            i = n - 1
            while i >= 0 and p[i] >= p[i + 1]:
                i -= 1
            if i == -1:
                return
            j = n - 1
            while p[j] <= p[i]:
                j -= 1
            p[i], p[j] = p[j], p[i]
            p[i + 1:] = reversed(p[i + 1:])
            yield p

    def get_permutation(n, p):
        permutations = gen_permutations()
        for i in range(p - 1):
            next(permutations)
        return next(permutations)

    def get_position(n, a):
        permutations = gen_permutations()
        position = 1
        for p in permutations:
            if p == a:
                return position
            position += 1
        return -1

    answers = []
    for query_type, n, arg in queries:
        if query_type == 1:
            answers.append(get_permutation(n, arg))
        else:
            answers.append(get_position(n, arg))
    return answers

if __name__ == "__main__":
    n, queries = read_input()
    answers = solve(n, queries)
    write_output(answers)
