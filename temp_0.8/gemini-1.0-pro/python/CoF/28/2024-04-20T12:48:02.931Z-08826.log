import sys

f_in = open('permutare.in', 'r')
f_out = open('permutare.out', 'w')

def fact(n):
	if n == 0:
		return 1
	ans = 1
	for i in range(1, n + 1):
		ans *= i
	return ans

def get_permutation(n, p):
	permut = []
	nums = [i for i in range(1, 2 * n + 1)]
	p -= 1
	for i in range(n - 1, -1, -1):
		idx = p // fact(i)
		permut.append(nums[idx])
		del nums[idx]
		p %= fact(i)
	permut.extend(nums)
	return permut

def get_position(n, permut):
	pos = 1
	nums = [i for i in range(1, 2 * n + 1)]
	for i in range(2 * n):
		idx = nums.index(permut[i])
		pos += idx * fact(2 * n - i - 1)
		del nums[idx]
	return pos

def main():
	queries = []
	for line in f_in.readlines():
		query = [int(x) for x in line.split()]
		queries.append(query)
	f_in.close()

	for query in queries:
		if query[0] == 1:
			permut = get_permutation(query[1], query[2])
			f_out.write(' '.join(map(str, permut)) + '\n')
		else:
			pos = get_position(query[1], query[2:])
			f_out.write(str(pos) + '\n')
	f_out.close()

if __name__ == '__main__':
	main()
```
