with open('arhitect.in', 'r') as fin, open('arhitect.out', 'w') as fout:
    N = int(fin.readline())
    segments = [tuple(map(int, fin.readline().split())) for _ in range(N)]

    # For each vertical line passing through any segment's x-coordinate,
    # compute the number of segments parallel to that line after rotation.
    max_aligned = 0
    for x in set(x for segment in segments for x in segment[:2]):
        aligned = 0
        for segment in segments:
            x1, y1, x2, y2 = segment
            if (x1 <= x <= x2 or x2 <= x <= x1) and (y1 - y2) % (x1 - x2) == 0:
                aligned += 1
        max_aligned = max(max_aligned, aligned)

    fout.write(str(max_aligned))
