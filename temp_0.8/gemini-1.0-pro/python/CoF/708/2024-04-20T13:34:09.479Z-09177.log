def permutari(word):
    """
    Genereaza toate permutarile posibile ale unui cuvant.

    :param word: Cuvantul pentru care se genereaza permutarile.
    :return: O lista cu toate permutarile posibile ale cuvantului.
    """
    if len(word) == 0:
        return [""]

    result = []
    for i in range(len(word)):
        for permutation in permutari(word[:i] + word[i + 1:]):
            result.append(word[i] + permutation)
    return result


def main():
    with open("anagrame.in", "r") as f:
        word = f.readline().strip()

    permutations = permutari(word)
    permutations.sort()

    with open("anagrame.out", "w") as f:
        for permutation in permutations:
            f.write(permutation + "\n")


if __name__ == "__main__":
    main()
