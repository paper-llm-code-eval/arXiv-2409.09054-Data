import sys

def main():
    with open('iepuri.in') as f:
        n, k = map(int, f.readline().split())
        graph = [[] for _ in range(n + 1)]
        for _ in range(n - 1):
            a, b = map(int, f.readline().split())
            graph[a].append(b)

    dp = [[[-1] * (k + 1) for _ in range(2)] for _ in range(n + 1)]
    def dfs(node, can_be_boss, carrots_eaten):
        if dp[node][can_be_boss][carrots_eaten] != -1:
            return dp[node][can_be_boss][carrots_eaten]

        ways = 0
        if can_be_boss:
            for i in range(carrots_eaten + 1, k + 1):
                ways += dfs(node, False, i)
        else:
            for i in range(carrots_eaten + 1, k + 1):
                for child in graph[node]:
                    ways += dfs(child, True, i)

        dp[node][can_be_boss][carrots_eaten] = ways % 30011
        return dp[node][can_be_boss][carrots_eaten]

    print(dfs(1, True, 0))

if __name__ == '__main__':
    main()
