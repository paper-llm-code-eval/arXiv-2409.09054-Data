        eq1 = eq1.split('+')
        eq2 = eq2.split('+')

        v1 = [int(ch[:-1]) * 10 ** ord(ch[-1]) for ch in eq1]
        v2 = [int(ch[:-1]) * 10 ** ord(ch[-1]) for ch in eq2]

        less_cnt += int(op == '<' and sum(v1) < sum(v2))
        f_out.write('1' if sum(v1) > sum(v2) else '0')
        f_out.write('\n')
    f_out.write(f'{less_cnt}\n')
