            for dx, dy in (-1, 0), (1, 0), (0, -1), (0, 1):
                if 1 <= x + 2 * dx <= n and 1 <= y + 2 * dy <= m:
                    if (x + dx, y + dy) in pos:
                        pos.remove((x + dx, y + dy))
                        f.write(f'{x} {y} {x + 2 * dx} {y + 2 * dy}\n')
                        break

solve(20, 20, 15)
