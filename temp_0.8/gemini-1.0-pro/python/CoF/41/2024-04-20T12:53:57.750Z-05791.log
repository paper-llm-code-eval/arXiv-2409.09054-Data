def main():
    f = open("immortal.in", "r")
    n, m, I = map(int, f.readline().split())
    M = [[0 for i in range(m + 2)] for j in range(n + 2)]
    for i in range(1, n + 1):
        for j in range(1, m + 1):
            M[i][j] = " "
    for i in range(I):
        x, y = map(int, f.readline().split())
        M[x][y] = "#"
    f.close()

    moves = [(0, -2), (0, 2), (-2, 0), (2, 0)]

    def move(x1, y1, x2, y2):
        if x1 == x2 and y1 == y2:
            return
        M[x2][y2] = M[x1][y1]
        M[x1][y1] = " "
        move(x2, y2, x2 + x2 - x1, y2 + y2 - y1)

    f = open("immortal.out", "w")
    while True:
        ok = False
        for i in range(1, n + 1):
            for j in range(1, m + 1):
                if M[i][j] == "#":
                    for dx, dy in moves:
                        x = i + dx
                        y = j + dy
                        if M[x][y] == "#" and M[x + dx][y + dy] == " ":
                            move(i, j, x + dx, y + dy)
                            f.write(f"{i} {j} {x + dx} {y + dy}\n")
                            ok = True
        if not ok:
            break
    f.close()


if __name__ == "__main__":
    main()
