with open('alee.in') as f, open('alee.out', 'w') as o:
    n, m = map(int, f.readline().split())
    grid = [[0] * (n + 2) for i in range(n + 2)]  # pad with 0 to simplify indexing
    for i in range(m):
        x, y = map(int, f.readline().split())
        grid[x][y] = -1  # mark trees with -1
    x1, y1, x2, y2 = map(int, f.readline().split())

    def valid(x, y):
        return 0 <= x < n + 2 and 0 <= y < n + 2 and grid[x][y] >= 0

    def bfs():
        q = [(x1, y1, 0)]
        while q:
            x, y, d = q.pop(0)
            if (x, y) == (x2, y2):
                return d
            for dx, dy in [(0, 1), (0, -1), (1, 0), (-1, 0)]:
                nx, ny = x + dx, y + dy
                if valid(nx, ny) and grid[nx][ny] == 0:
                    grid[nx][ny] = d + 1
                    q.append((nx, ny, d + 1))
        return -1

    result = bfs()
    if result == -1:
        result = -1
    else:
        result -= 1
    o.write(str(result))
