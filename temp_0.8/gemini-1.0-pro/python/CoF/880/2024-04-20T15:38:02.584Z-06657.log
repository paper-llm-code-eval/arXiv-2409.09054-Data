with open("robot.in", "r") as f_in, open("robot.out", "w") as f_out:
    C = int(f_in.readline())
    if C == 1:
        program = f_in.readline().strip()
        count = 0
        for i in range(len(program)):
            if program[i] == "A":
                count += 1
        f_out.write(str(count))
    elif C == 2:
        program = f_in.readline().strip()
        screen = ""
        position = 0
        for i in range(len(program)):
            if program[i] == "D":
                position = (position + int(program[i + 1])) % 10
            elif program[i] == "S":
                position = (position - int(program[i + 1])) % 10
            else:
                screen += str(position)
        f_out.write(screen)
    else:
        N = int(f_in.readline())
        program = ""
        current = 0
        while current != N:
            if current < N:
                program += "D" + str((N - current) % 10)
                current += (N - current) % 10
            elif current > N:
                program += "S" + str(current - N)
                current -= current - N
            program += "A"
        f_out.write(program + "T")
```
