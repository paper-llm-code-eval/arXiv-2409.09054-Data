with open('pm.in') as f, open('pm.out', 'w') as g:
    n = int(f.readline())
    times = [int(i) for i in f.readline().split()]
    graph = [[] for i in range(n)]
    indegree = [0] * n
    for i in range(n):
        m, a = f.readline().split()
        m = int(m)
        for j in a.split():
            graph[int(j) - 1].append(i)
            indegree[i] += 1

    # Bellman Ford - Earliest Start Time
    dist = [float('inf')] * n
    dist[0] = 0
    for i in range(n - 1):
        for u in range(n):
            for v in graph[u]:
                if dist[u] + times[u] < dist[v]:
                    dist[v] = dist[u] + times[u]

    # Critical Path - Latest Start Time
    critical = [0] * n
    critical[n - 1] = dist[n - 1]
    for i in range(n - 2, -1, -1):
        for v in graph[i]:
            if critical[v] - times[v] > critical[i]:
                critical[i] = critical[v] - times[v]

    g.write(str(critical[n - 1]) + '\n')
    for i in range(n):
        g.write(str(dist[i]) + ' ' + str(critical[i]) + '\n')
