import queue
import sys

# Read input data
n = int(sys.stdin.readline())
times = [int(x) for x in sys.stdin.readline().split()]
dependencies = [[] for _ in range(n+1)]
for i in range(1, n+1):
    m = int(sys.stdin.readline())
    dependencies[i].extend([int(x) for x in sys.stdin.readline().split()])

# Initialize data structures
indegree = [0 for _ in range(n+1)]
for i in range(1, n+1):
    for j in dependencies[i]:
        indegree[j] += 1
q = queue.Queue()
for i in range(1, n+1):
    if indegree[i] == 0:
        q.put(i)

# Perform topological sort
t = 0
start = [0 for _ in range(n+1)]
end = [0 for _ in range(n+1)]
while not q.empty():
    u = q.get()
    start[u] = t
    t += times[u-1]
    end[u] = t
    for v in dependencies[u]:
        indegree[v] -= 1
        if indegree[v] == 0:
            q.put(v)

# Write output data
sys.stdout.write(str(t) + '\n')
for i in range(1, n+1):
    sys.stdout.write(str(start[i]) + ' ' + str(end[i]) + '\n')
```
