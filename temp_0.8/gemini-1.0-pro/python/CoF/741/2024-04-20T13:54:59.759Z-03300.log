import sys

with open("ocr.in", "r") as f:
    n, m = [int(x) for x in f.readline().split()]
    arr = [[float(x) for x in line.split()] for line in f]

# Calculate cumulative sum
for i in range(1, n + 1):
    for j in range(1, m + 1):
        arr[i][j] += arr[i - 1][j] + arr[i][j - 1] - arr[i - 1][j - 1]

# Find the center of gravity
min_diff_row = sys.maxsize
min_diff_col = sys.maxsize
ans_row = 0
ans_col = 0

for i in range(1, n + 1):
    for j in range(1, m + 1):
        sum_above = arr[i - 1][m]
        sum_below = arr[n][m] - arr[i][m]
        diff_row = abs(sum_above - sum_below)
        if diff_row < min_diff_row or (diff_row == min_diff_row and i > ans_row):
            min_diff_row = diff_row
            ans_row = i

for j in range(1, m + 1):
    for i in range(1, n + 1):
        sum_left = arr[n][j - 1]
        sum_right = arr[n][m] - arr[n][j - 1]
        diff_col = abs(sum_left - sum_right)
        if diff_col < min_diff_col or (diff_col == min_diff_col and j > ans_col):
            min_diff_col = diff_col
            ans_col = j

with open("ocr.out", "w") as f:
    f.write(f"{ans_row} {ans_col}")
```
