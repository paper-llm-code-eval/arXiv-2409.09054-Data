with open('cartonase.in', 'r') as f:
    cerinta = int(f.readline())
    n = int(f.readline())
    v = []
    for _ in range(n):
        s, d = map(int, f.readline().split())
        v.append((s, d))

if cerinta == 1:
    nr_p = 0
    for i in range(n-1):
        if v[i][1] == v[i+1][0]:
            nr_p += 1
    with open('cartonase.out', 'w') as g:
        g.write(str(nr_p))

elif cerinta == 2:
    max_lung = 0
    for i in range(n):
        j = i
        lung = 1
        while j+1 < n and v[j][1] == v[j+1][0]:
            j += 1
            lung += 1
        max_lung = max(max_lung, lung)
    with open('cartonase.out', 'w') as g:
        g.write(str(max_lung))

elif cerinta == 3:
    nr_max = 0
    for i in range(n):
        j = i
        lung = 1
        while j+1 < n and v[j][1] == v[j+1][0]:
            j += 1
            lung += 1
        if lung == max_lung:
            nr_max += 1
    with open('cartonase.out', 'w') as g:
        g.write(str(nr_max))
