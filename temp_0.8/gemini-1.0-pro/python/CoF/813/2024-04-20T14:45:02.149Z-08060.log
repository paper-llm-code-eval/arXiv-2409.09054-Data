with open('adunscad.in') as f:
    n, m = map(int, f.readline().split())
    data = []
    for i in range(m):
        data.append(int(f.readline().split()[i]))

for i in range(1<<m):
    val = data[0]
    expr = []
    expr.append(str(val))
    for j in range(1, m):
        if (i & (1<<j)):
            val += data[j]
            expr.append('+')
        else:
            val -= data[j]
            expr.append('-')
        expr.append(str(data[j]))
    if val == n:
        with open('adunscad.out', 'w') as f2:
            f2.write(''.join(expr))
        break
else:
    with open('adunscad.out', 'w') as f2:
        f2.write('0')
