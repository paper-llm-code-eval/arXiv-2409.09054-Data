def read_data():
    with open('adunscad.in', 'r') as f:
        n, m = map(int, f.readline().split())
        digits = list(map(int, f.readline().split()))
    return n, m, digits


def solve(n, m, digits):
    if n == 0:
        return '0'
    if m == 1:
        if digits[0] == n:
            return str(n)
        else:
            return '0'
    for i in range(m):
        for j in range(i + 1, m):
            if digits[i] + digits[j] == n:
                return str(digits[i]) + '+' + str(digits[j])
            if digits[i] - digits[j] == n:
                return str(digits[i]) + '-' + str(digits[j])
            for k in range(j + 1, m):
                if digits[i] + digits[j] - digits[k] == n:
                    return str(digits[i]) + '+' + str(digits[j]) + '-' + str(digits[k])
                if digits[i] - digits[j] + digits[k] == n:
                    return str(digits[i]) + '-' + str(digits[j]) + '+' + str(digits[k])
                if digits[i] - digits[j] - digits[k] == n:
                    return str(digits[i]) + '-' + str(digits[j]) + '-' + str(digits[k])
    return '0'


if __name__ == '__main__':
    n, m, digits = read_data()
    result = solve(n, m, digits)
    with open('adunscad.out', 'w') as f:
        f.write(result)
