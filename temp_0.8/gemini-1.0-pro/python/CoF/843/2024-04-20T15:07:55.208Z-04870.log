    with open("piramide.in", "r") as f:
        n, x, k = map(int, f.readline().split())
        white = set(map(int, f.readline().split()))
        pyramids = n // 2
        p = 0
        m = 0
        c = 0
        a = 0
        alb = 0
        max_alb = 0
        for i in range(1, n + 1):
