def main():
    with open("rj.in") as f:
        n, m = map(int, f.readline().split())
        matrix = [list(f.readline().strip()) for _ in range(n)]

    distances = [[[-1] * 3 for _ in range(m)] for _ in range(n)]

    startR, startJ = (-1, -1), (-1, -1)
    for i in range(n):
        for j in range(m):
            if matrix[i][j] == "R":
                startR = (i, j)
            elif matrix[i][j] == "J":
                startJ = (i, j)

    def dfs(x, y, t, visited, type):
        if x < 0 or x >= n or y < 0 or y >= m or matrix[x][y] == "X":
            return
        if visited[x][y][type] != -1 and visited[x][y][type] <= t:
            return
        if type == 0:
            distances[x][y][type] = t
        elif type == 1:
            if distances[x][y][0] != -1 and distances[x][y][0] <= t:
                return
            distances[x][y][type] = t
        visited[x][y][type] = t

        dfs(x + 1, y, t + 1, visited, type)
        dfs(x - 1, y, t + 1, visited, type)
        dfs(x, y + 1, t + 1, visited, type)
        dfs(x, y - 1, t + 1, visited, type)
        dfs(x + 1, y + 1, t + 1, visited, type)
        dfs(x - 1, y - 1, t + 1, visited, type)
        dfs(x + 1, y - 1, t + 1, visited, type)
        dfs(x - 1, y + 1, t + 1, visited, type)

    dfs(startR[0], startR[1], 0, distances, 0)
    dfs(startJ[0], startJ[1], 0, distances, 1)

    minDist = float("inf")
    for i in range(n):
        for j in range(m):
            if distances[i][j][0] != -1 and distances[i][j][1] != -1:
                minDist = min(minDist, distances[i][j][0] + distances[i][j][1])

    for i in range(n):
        for j in range(m):
            if distances[i][j][0] != -1 and distances[i][j][1] != -1 and distances[i][j][0] + distances[i][j][1] == minDist:
                with open("rj.out", "w") as f:
                    f.write(f"{i + 1} {j + 1} {minDist}")
                return

    with open("rj.out", "w") as f:
        f.write("-1")


main()
