def main():
    with open('harry.in') as f:
        text = f.readline().rstrip()

    max_word = ''
    for i in range(len(text)):
        for j in range(i + 1, len(text) + 1):
            word = text[i:j]
            if word > max_word:
                max_word = word

    with open('harry.out', 'w') as f:
        f.write(max_word)


if __name__ == '__main__':
    main()
