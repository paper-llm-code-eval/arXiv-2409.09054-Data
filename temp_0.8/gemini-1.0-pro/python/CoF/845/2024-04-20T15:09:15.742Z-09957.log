import sys

def munte(v):
    global nr_mv
    global nr_ms
    global nr_mf
    nr_mv += 1
    #print(v)
    if len(v) < 3: return
    for i in range(len(v)-2):
        if v[i] < v[i+1] and v[i+1] > v[i+2]:
            nr_ms += 1
            munte(v[0:i+1] + v[i+2:])
            munte(v[i+1:])

with open('munte.in') as f:
    n = int(f.readline())
    v = list(map(int, f.readline().split()))
with open('munte.out', 'w') as g:
    nr_mv = 0
    nr_ms = 0
    nr_mf = 0
    munte(v)
    g.write(str(nr_mv) + '\n')
    g.write(str(nr_ms) + '\n')
    g.write(str(n - nr_ms) + '\n')
```
