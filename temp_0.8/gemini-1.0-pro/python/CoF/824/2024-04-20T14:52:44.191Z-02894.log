    x,y = map(int, open("triunghi.in").readline().split())
    p[i],v[i] = x,y
for i in range(n-1,0,-1):
    v[p[i]] -= v[p[i]+1]
for i in range(n-1,0,-1):
    v[p[i]] -= v[p[i]-1]
open("triunghi.out","w").write(" ".join(map(str,v[1:n+1])))
