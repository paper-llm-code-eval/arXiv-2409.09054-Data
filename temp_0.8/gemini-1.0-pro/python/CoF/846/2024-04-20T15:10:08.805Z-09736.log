    matr = [list(map(int, f.readline().split())) for _ in range(n)]

stele_str = 0
constelatii = 0
max_latura = 0
for i in range(n):
    for j in range(m):
        if (i == 0 or matr[i][j] > matr[i - 1][j]) and \
                (j == 0 or matr[i][j] > matr[i][j - 1]) and \
                (i == n - 1 or matr[i][j] > matr[i + 1][j]) and \
                (j == m - 1 or matr[i][j] > matr[i][j + 1]):
            stele_str += 1

            # verificam orizontala
            latura = 0
            while j + latura < m and matr[i][j + latura] > matr[i][j + latura + 1]:
                latura += 1
            while j - latura >= 0 and matr[i][j - latura] > matr[i][j - latura - 1]:
                latura += 1
            latura -= 1  # se scade 1 pt stea
            constelatii += latura > 0
            max_latura = max(max_latura, latura)

            # verificam verticala
            latura = 0
            while i + latura < n and matr[i + latura][j] > matr[i + latura + 1][j]:
                latura += 1
            while i - latura >= 0 and matr[i - latura][j] > matr[i - latura - 1][j]:
                latura += 1
            latura -= 1  # se scade 1 pt stea
            constelatii += latura > 0
            max_latura = max(max_latura, latura)

with open('patrat.out', 'w') as f:
    f.write(f'{stele_str}\n{constelatii}\n{max_latura}')
