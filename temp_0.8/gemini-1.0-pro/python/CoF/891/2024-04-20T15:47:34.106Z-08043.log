import sys

sys.stdin = open('fadema.in', 'r')
sys.stdout = open('fadema.out', 'w')


def read_int():
    return int(sys.stdin.readline())


def read_ints():
    return list(map(int, sys.stdin.readline().split()))


n, m = read_ints()
grid = [read_ints() for _ in range(n)]

dp = [[0] * m for _ in range(n)]

for i in range(n):
    for j in range(m):
        if i == 0 or j == 0:
            dp[i][j] = 1
        else:
            if (i + j) % 2 == grid[i][j]:
                dp[i][j] = min(dp[i - 1][j], dp[i][j - 1]) + 1

max_size = 0

for i in range(n):
    for j in range(m):
        max_size = max(max_size, dp[i][j])

if max_size == 1:
    max_size = 0

print(max_size)
```
