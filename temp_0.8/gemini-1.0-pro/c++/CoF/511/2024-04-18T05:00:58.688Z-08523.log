                g.at(k) = 0;
            }
            if (f.at(k) > fmax) {
                fmax = f.at(k);
            }
            if (g.at(k) > gmax) {
                gmax = g.at(k);
            }
            if (f.at(k) < fmin) {
                fmin = f.at(k);
            }
            if (g.at(k) < gmin) {
                gmin = g.at(k);
            }
        }
        for (j = 0; j < n; j++) {
            f.at(j) += f.at(k);
            g.at(j) += g.at(k);
            if (f.at(j) < 0) {
                f.at(j) = 0;
            }
            if (g.at(j) < 0) {
                g.at(j) = 0;
            }
            if (f.at(j) > fmax) {
                fmax = f.at(j);
            }
            if (g.at(j) > gmax) {
                gmax = g.at(j);
            }
            if (f.at(j) < fmin) {
                fmin = f.at(j);
            }
            if (g.at(j) < gmin) {
                gmin = g.at(j);
            }
        }
        k++;
    }
    for (i = 0; i < n; i++) {
        if (fmin == fmax) {
            fmij = fmin;
        } else {
            fmij = fmin + 1;
        }
        if (gmin == gmax) {
            gmij = gmin;
        } else {
            gmij = gmin + 1;
        }
        if (f.at(i) == fmij) {
            if (fcnt == 0) {
                p = i;
            }
            fcnt++;
        }
        if (g.at(i) == gmij) {
            if (gcnt == 0) {
                p = n - 1 - i;
            }
            gcnt++;
        }
    }
    if (c == 1) {
        g << p << "\n";
    } else if (c == 2) {
        g << max(fmax - fmin, gmax - gmin) << "\n";
    }
    f.close();
    g.close();
    return 0;
}
