#include <fstream>
#include <vector>
#include <queue>

using namespace std;

vector<vector<int>> graph;
vector<int> inDegree;
vector<int> time;
vector<int> earlyStart;
vector<int> latestStart;

struct Node {
  int node;
  int distance;

  Node(int node, int distance) : node(node), distance(distance) {}
};

bool operator<(const Node& a, const Node& b) {
  return a.distance > b.distance;
}

void topologicalSort(int n) {
  priority_queue<Node> q;
  for (int i = 0; i < n; i++) {
    if (inDegree[i] == 0) {
      q.push(Node(i, 0));
    }
  }

  while (!q.empty()) {
    Node node = q.top();
    q.pop();

    earlyStart[node.node] = node.distance;
    latestStart[node.node] = node.distance;

    for (int u : graph[node.node]) {
      inDegree[u]--;
      if (inDegree[u] == 0) {
        q.push(Node(u, node.distance + time[u]));
      }

      latestStart[u] = max(latestStart[u], node.distance + time[u]);
    }
  }
}

int main() {
  ifstream in("pm.in");
  ofstream out("pm.out");

  int n;
  in >> n;

  time.resize(n);
  graph.resize(n);
  inDegree.resize(n);
  earlyStart.resize(n);
  latestStart.resize(n);

  for (int i = 0; i < n; i++) {
    in >> time[i];
  }

  for (int i = 0; i < n; i++) {
    int m;
    in >> m;

    for (int j = 0; j < m; j++) {
      int u;
      in >> u;
      u--;

      graph[u].push_back(i);
      inDegree[i]++;
    }
  }

  topologicalSort(n);

  int maxTime = 0;
  for (int i = 0; i < n; i++) {
    maxTime = max(maxTime, latestStart[i]);
  }

  out << maxTime << endl;

  for (int i = 0; i < n; i++) {
    out << earlyStart[i] << " " << latestStart[i] << endl;
  }

  return 0;
}
