#include <fstream>

using namespace std;

const int MAXN = 100;
ifstream fin("elicop.in");
ofstream fout("elicop.out");

int m, n, k, a[MAXN + 1][MAXN + 1], fr[MAXN + 1],
    b[MAXN * MAXN], nr, v[MAXN + 1];

void citire() {
  fin >> m >> n;
  for (int i = 1; i <= m; i++)
    for (int j = 1; j <= n; j++)
      fin >> a[i][j];
  fin >> k;
  for (int i = 1; i <= k; i++) {
    int x1, y1, x2, y2, p;
    fin >> x1 >> y1 >> x2 >> y2 >> p;
    int dx = x2 - x1;
    int dy = y2 - y1;
    if (dx == 0) {
      if (p == 1)
        for (int j = min(y1, y2); j <= max(y1, y2); j++)
          v[i] += (a[x1][j] == 1), fr[j]++;
      else
        for (int j = min(y1, y2); j <= max(y1, y2); j++)
          v[i] += (a[x1][j] == 0), fr[j]++;
    } else {
      if (p == 1)
        for (int j = min(x1, x2); j <= max(x1, x2); j++)
          v[i] += (a[j][y1] == 1), fr[j]++;
      else
        for (int j = min(x1, x2); j <= max(x1, x2); j++)
          v[i] += (a[j][y1] == 0), fr[j]++;
    }
  }
}

void rezolvare() {
  for (int i = 1; i <= m; i++) {
    if (fr[i] == 0)
      nr++;
    else
      for (int j = 1; j <= k; j++)
        if (fr[i] == 1)
          v[j] += a[i][1] ^ 1;
  }
  fout << nr << '\n';
  nr = 0;
  for (int i = 1; i <= k; i++) {
    if (v[i] >= (dx + dy - 1) / 2)
      b[++nr] = i;
  }
  fout << nr << ' ';
  for (int i = 1; i <= nr; i++) fout << b[i] << ' ';
  fout << '\n';
}

int main() {
  citire();
  rezolvare();
  return 0;
}
