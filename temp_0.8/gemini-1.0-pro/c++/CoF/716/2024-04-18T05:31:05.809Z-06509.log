    if (i % q == 0) {
      if (vopsit[i]) {
        sc_violet++;
      } else {
        sc_albastru++;
        vopsit[i] = true;
      }
    }
    if (i % p == 0) {
      if (vopsit[i]) {
        sc_violet++;
      } else {
        sc_rosu++;
        vopsit[i] = true;
      }
    }
  }
  sc_nevopsite = n - sc_rosu - sc_albastru - sc_violet;
  out << sc_nevopsite << endl;
  out << sc_rosu << endl;
  out << sc_albastru << endl;
  out << sc_violet << endl;
  return 0;
}
