#include <fstream>
#include <vector>

using namespace std;

ifstream in("tbile.in");
ofstream out("tbile.out");

int main() {
  int type, n, m, v;
  in >> type >> n >> m;
  if (type == 2) in >> v;
  vector<int> x(m), y(m);
  int i = 1, j = 2;
  x[0] = 1;
  y[0] = 2;
  while (j <= n) {
    x[i] = x[i - 1] + y[i - 1];
    for (int k = x[i - 1] + 1; k < x[i]; k++) {
      if (k != 4) { y[j] = k; j++; }
    }
    i++;
  }
  if (x[i - 1] > n) {
    for (int k = x[i - 1] + 1; k <= n; k++) { y[j] = k; j++; }
  }
  if (type == 1) {
    int nr = (j + m - 1) / m;
    int sum = (nr * (nr + 1)) / 2 * m;
    out << nr << ' ' << sum << '\n';
  } else {
    int sum = 0;
    for (int k = 0; k < m; k++) {
      if (y[k] >= v) { sum += y[k]; }
    }
    out << sum << '\n';
  }
  return 0;
}
