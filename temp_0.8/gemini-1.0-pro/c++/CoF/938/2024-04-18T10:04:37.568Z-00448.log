        mat[i].resize(m);
        for (int j = 0; j < m; ++j) {
            in >> mat[i][j];
        }
    }
    in.close();
}

void write(vector<vector<int>>& mat, int n, int m) {
    ofstream out("labirint.out");
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < m; ++j) {
            out << mat[i][j];
        }
        out << '\n';
    }
    out.close();
}

int main() {
    vector<vector<vector<int>>> dp(2, vector<vector<int>>(1005, vector<int>(1005)));
    vector<vector<int>> mat;
    int n, m;
    read(mat, n, m);
    dp[0][1][1] = 1;
    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= m; ++j) {
            if (mat[i - 1][j] == 0) {
                dp[i & 1][i][j] = min({dp[(i - 1) & 1][i - 1][j], dp[(i - 1) & 1][i][j - 1], dp[(i - 1) & 1][i][j + 1]});
            }
            if (mat[i][j - 1] == 0) {
                dp[i & 1][i][j] = min({dp[i & 1][i][j], dp[(i - 1) & 1][i - 1][j - 1], dp[(i - 1) & 1][i][j]});
            }
            if (mat[i - 1][j - 1] == 0) {
                dp[i & 1][i][j] = min({dp[i & 1][i][j], dp[(i - 1) & 1][i - 1][j], dp[(i - 1) & 1][i][j - 1]});
            }
            if (dp[i & 1][i][j] != 0) {
                dp[i & 1][i][j] += 1;
            }
        }
    }
    vector<vector<int>> ans(n, vector<int>(m));
    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= m; ++j) {
            if (dp[n & 1][i][j] < dp[n & 1][n][m]) {
                ans[i - 1][j - 1] = 1;
            }
        }
    }
    write(ans, n, m);
    return 0;
}
