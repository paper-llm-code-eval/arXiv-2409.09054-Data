#include <fstream>
#include <vector>

using namespace std;

ifstream in("triunghi.in");
ofstream out("triunghi.out");

int n, q, linie, coloana, k, suma_max;
vector<vector<int>> A;
vector<tuple<int, int, int>> triunghiuri;

int main() {
    in >> n;
    A.resize(n + 1, vector<int>(n + 1));
    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= n; ++j) {
            in >> A[i][j];
        }
    }
    in >> q;
    triunghiuri.resize(q);
    for (int i = 0; i < q; ++i) {
        in >> linie >> coloana >> k;
        triunghiuri[i] = make_tuple(linie, coloana, k);
    }

    for (const auto& [linie, coloana, k] : triunghiuri) {
        int suma = 0;
        if (k > 0) {
            for (int i = 0; i < k; ++i) {
                for (int j = 0; j <= i; ++j) {
                    suma += A[linie + i][coloana + j];
                }
            }
        } else {
            k = -k;
            for (int i = 0; i < k; ++i) {
                for (int j = 0; j <= i; ++j) {
                    suma += A[linie - i][coloana - j];
                }
            }
        }
        suma_max = max(suma_max, suma);
    }
    out << suma_max;
    return 0;
}
```
