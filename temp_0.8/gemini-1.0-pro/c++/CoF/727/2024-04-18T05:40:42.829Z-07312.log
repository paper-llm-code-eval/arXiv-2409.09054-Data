#include <fstream>
#include <vector>
#include <cstring>
using namespace std;

int main() {
  ifstream fin("control.in");
  ofstream fout("control.out");
  int n, frec[201] = {0};
  fin >> n;
  while (n--) {
    int x;
    fin >> x;
    ++frec[x];
  }
  fin.close();
  int cnt1 = 0, cnt0 = 0;
  for (int i = 1; i <= 200; i++) {
    while (frec[i] > 0) {
      if (frec[i] % 3 == 0) {
        frec[i] -= 3;
        ++cnt1;
      } else {
        frec[i] -= 2;
        ++cnt0;
      }
    }
  }
  int nr_control = cnt1 * 10 + cnt0;
  fout << nr_control << endl;
  bool prim = true;
  for (int i = 2; i * i <= nr_control; i++) {
    if (nr_control % i == 0) {
      prim = false;
      break;
    }
  }
  fout << (prim ? 1 : 0) << endl;
  fout.close();
  return 0;
}
