#include <fstream>
#include <utility>
#include <vector>
#include <algorithm>

using namespace std;

typedef pair<int, int> Mage;

int main() {
    ifstream fin("vraji.in");
    ofstream fout("vraji.out");
    int n, max_objects = 0, max_boxes = 0;
    fin >> n;
    vector<Mage> mages;
    for (int i = 0; i < n; i++) {
        Mage mage;
        fin >> mage.first >> mage.second;
        mages.push_back(mage);
        max_objects = max(max_objects, mage.first * mage.second);
    }
    sort(mages.begin(), mages.end(), [](Mage &a, Mage &b) {
        return a.first > b.first;
    });
    int total = 0;
    for (const Mage &mage : mages) {
        total += mage.first * mage.second;
    }
    while (total % n != 0) {
        total -= mages.back().first * mages.back().second;
        mages.pop_back();
    }
    int objects_per_box = total / n;
    for (const Mage &mage : mages) {
        max_boxes = max(max_boxes, mage.first * mage.second / objects_per_box);
    }
    fout << max_objects << endl << max_boxes << endl;
    return 0;
}
