  ifstream fin("eq4.in");
  ofstream fout("eq4.out");

  int c;
  fin >> c;

  Expression expression;
  string expr;
  fin >> expr;
  int coefficient = 0;
  char variable;
  bool negative = false;
  for (char ch : expr) {
    if (ch == '+')
      negative = false;
    else if (ch == '-')
      negative = true;
    else if (ch >= '0' && ch <= '9')
      coefficient = coefficient * 10 + (ch - '0');
    else {
      variable = ch;
      expression.push_term(negative ? -coefficient : coefficient, variable);
      coefficient = 0;
    }
  }
  if (coefficient != 0) expression.push_term(negative ? -coefficient : coefficient, '\0');

  if (c == 1) {
    fout << expression.evaluate(1, 1, 1, 1) << '\n';
  } else {
    int a, b, e;
    fin >> a >> b >> e;
    int count = 0;
    for (int x = a; x <= b; x++)
      for (int y = a; y <= b; y++)
        for (int z = a; z <= b; z++)
          for (int t = a; t <= b; t++)
            if (expression.evaluate(x, y, z, t) == e) count++;
    fout << count << '\n';
  }

  return 0;
}
