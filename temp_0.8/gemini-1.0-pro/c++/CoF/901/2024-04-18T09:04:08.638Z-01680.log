            val[p] = s[i] - 'x' + 1;
            ++p;
        }
    }
    if (c == 1) {
        for (int i = 0; i < p; ++i) {
            ans += coef[i] * powmod(val[i], e) % MOD;
            ans = (ans + MOD) % MOD;
        }
        fout << ans << '\n';
    } else {
        for (int x = a; x <= b; ++x) {
            for (int y = a; y <= b; ++y) {
                for (int z = a; z <= b; ++z) {
                    for (int t = a; t <= b; ++t) {
                        int sum = 0;
                        for (int i = 0; i < p; ++i) {
                            sum += coef[i] * powmod(val[i], x) % MOD;
                            sum = (sum + MOD) % MOD;
                        }
                        if (sum == e) {
                            ++ans;
                        }
                    }
                }
            }
        }
        fout << ans << '\n';
    }
    return 0;
}
