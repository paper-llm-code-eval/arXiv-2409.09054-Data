#include <fstream>
#include <queue>
#include <tuple>
#include <vector>

using namespace std;

ifstream in("pulsar.in");
ofstream out("pulsar.out");

int main() {
  int C, N, P;
  in >> C >> N >> P;
  vector<tuple<int, int, int, int>> pulsars(P);
  for (auto& [x, y, r, t] : pulsars) in >> x >> y >> r >> t;
  int xs, ys, xf, yf;
  in >> xs >> ys >> xf >> yf;

  if (C == 1) {
    vector<vector<int>> dist(N + 1, vector<int>(N + 1, 0));
    for (auto [x, y, r, t] : pulsars)
      for (int dx = -r; dx <= r; ++dx)
        for (int dy = -r; dy <= r; ++dy) {
          int dist_man = abs(dx) + abs(dy);
          if (dist_man <= r && abs(x - xs + dx) <= N && abs(y - ys + dy) <= N) {
            dist[x - xs + dx][y - ys + dy] = 1;
          }
        }
    int sum = 0;
    for (int i = 1; i <= N; ++i)
      for (int j = 1; j <= N; ++j) sum += dist[i][j];
    out << sum << '\n';
    return 0;
  }

  vector<vector<int>> dist(N + 1, vector<int>(N + 1, N * N + 1));
  dist[xs][ys] = 0;
  priority_queue<tuple<int, int, int>, vector<tuple<int, int, int>>,
                   greater<tuple<int, int, int>>>
      pq;
  pq.emplace(0, xs, ys);
  while (!pq.empty()) {
    auto [dist_cur, x, y] = pq.top();
    pq.pop();
    if (x == xf && y == yf) {
      out << dist_cur << '\n';
      return 0;
    }
    if (dist_cur > dist[x][y]) continue;
    for (auto [x1, y1, r, t] : pulsars) {
      int dx = x1 - x, dy = y1 - y;
      if (abs(dx) + abs(dy) > r) continue;
      int dist_new = dist_cur + 1;
      if (dist_new < dist[x][y]) {
        dist[x][y] = dist_new;
        pq.emplace(dist_new, x, y);
      }
      dx = x1 - xf, dy = y1 - yf;
      if (abs(dx) + abs(dy) > r) continue;
      dist_new = dist_cur + 2;
      if (dist_new < dist[xf][yf]) {
        dist[xf][yf] = dist_new;
        pq.emplace(dist_new, xf, yf);
      }
    }
    if (x + 1 <= N && dist_cur + 1 < dist[x + 1][y]) {
      dist[x + 1][y] = dist_cur + 1;
      pq.emplace(dist_cur + 1, x + 1, y);
    }
    if (x - 1 >= 1 && dist_cur + 1 < dist[x - 1][y]) {
      dist[x - 1][y] = dist_cur + 1;
      pq.emplace(dist_cur + 1, x - 1, y);
    }
    if (y + 1 <= N && dist_cur + 1 < dist[x][y + 1]) {
      dist[x][y + 1] = dist_cur + 1;
      pq.emplace(dist_cur + 1, x, y + 1);
    }
    if (y - 1 >= 1 && dist_cur + 1 < dist[x][y - 1]) {
      dist[x][y - 1] = dist_cur + 1;
      pq.emplace(dist_cur + 1, x, y - 1);
    }
  }
}
