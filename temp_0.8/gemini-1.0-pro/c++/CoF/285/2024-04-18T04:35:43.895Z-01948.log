  in >> start_x >> start_y >> end_x >> end_y;
  --start_x;
  --start_y;
  --end_x;
  --end_y;
  vector<vector<uint32_t>> dist(n, vector<uint32_t>(n, kMaxDist + 1));
  auto compare = [](const pair<int, int>& a, const pair<int, int>& b) {
    return dist[a.first][a.second] > dist[b.first][b.second];
  };
  priority_queue<pair<int, int>, vector<pair<int, int>>, decltype(compare)> q(compare);
  q.push({start_x, start_y});
  dist[start_x][start_y] = 0;
  vector<vector<bool>> safe(n, vector<bool>(n, true));
  for (const auto& pulsar : pulsars) {
    for (int dx = -pulsar.r; dx <= pulsar.r; ++dx) {
      for (int dy = -pulsar.r; dy <= pulsar.r; ++dy) {
        int x = pulsar.x + dx;
        int y = pulsar.y + dy;
        if (x >= 0 && x < n && y >= 0 && y < n) {
          safe[x][y] = false;
        }
      }
    }
  }
  while (!q.empty()) {
    auto [x, y] = q.top();
    q.pop();
    if (x == end_x && y == end_y) {
      out << dist[x][y] << endl;
      return 0;
    }
    for (const auto& dxdy : {make_pair(0, 1), make_pair(1, 0), make_pair(0, -1), make_pair(-1, 0)}) {
      int nx = x + dxdy.first;
      int ny = y + dxdy.second;
      if (nx >= 0 && nx < n && ny >= 0 && ny < n && safe[nx][ny] && dist[x][y] + 1 < dist[nx][ny]) {
        dist[nx][ny] = dist[x][y] + 1;
        q.push({nx, ny});
      }
    }
  }
  out << -1 << endl;
  return 0;
}
