#include <bits/stdc++.h>
using namespace std;

int n, start[3], mn[3], mx[3];

int main() {
    ifstream in("schi.in");
    ofstream out("schi.out");

    for (int i = 0; i < 3; i++) in >> start[i];
    in >> n;
    for (int i = 0; i < n; i++) {
        int m, s;
        in >> m >> s;
        mn[0] = min(mn[0], start[0] + m);
        mn[1] = min(mn[1], start[1] + s);
        mn[2] = min(mn[2], start[2] + s - (s >= mn[1]));

        mx[0] = max(mx[0], start[0] + m);
        mx[1] = max(mx[1], start[1] + s);
        mx[2] = max(mx[2], start[2] + s - (s >= mx[1]));
    }

    for (int i = 0; i < 3; i++) {
        out << mn[i] << ' ';
        out << mx[i] << (i == 2 ? '\n' : ' ');
    }

    return 0;
}
```
