#include <fstream>
#include <algorithm>

using namespace std;

ifstream in("schi.in");
ofstream out("schi.out");

int main()
{
    int h, m, s, n, tmp, minT = INT_MAX, maxT = INT_MIN;

    in >> h >> m >> s >> n;

    for (int i = 0; i < n; ++i)
    {
        in >> m >> s;
        tmp = m * 60 + s;
        minT = min(minT, tmp);
        maxT = max(maxT, tmp);
    }

    h += minT / 60;
    h %= 24;
    m = (minT % 60) + m;
    m %= 60;
    s = (minT % 60 % 60) + s;
    s %= 60;

    out << h << ' ' << m << ' ' << s << '\n';

    h += maxT / 60;
    h %= 24;
    m = (maxT % 60) + m;
    m %= 60;
    s = (maxT % 60 % 60) + s;
    s %= 60;

    out << h << ' ' << m << ' ' << s << '\n';

    return 0;
}
```
