#include <fstream>
#include <vector>
#include <string>
#include <algorithm>
#include <numeric>
#include <iterator>
#include <sstream>

using namespace std;

bool is_palindrome(string& s) {
    int length = s.length();
    for (int i = 0; i < length / 2; i++) {
        if (s[i] != s[length - i - 1]) {
            return false;
        }
    }
    return true;
}

int main() {
    ifstream input("mare.in");
    ofstream output("mare.out");

    int n;
    input >> n;

    vector<int> numbers(n);
    for (int i = 0; i < n; i++) {
        input >> numbers[i];
    }

    string current_number;
    string max_palindrome;

    for (int i = 0; i < n; i++) {
        for (int j = i; j < n; j++) {
            current_number = accumulate(numbers.begin() + i, numbers.begin() + j + 1, string());
            if (is_palindrome(current_number) && current_number > max_palindrome) {
                max_palindrome = current_number;
            }
        }
    }

    output << max_palindrome << endl;

    return 0;
}
