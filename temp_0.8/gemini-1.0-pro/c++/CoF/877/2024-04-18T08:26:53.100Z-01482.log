#include <iostream>
#include <fstream>
#include <algorithm>
#include <vector>
using namespace std;

struct elev {
    int nota_real;
    int nota_uman;
    int id;
};

int main() {
    ifstream fin("admitere.in");
    ofstream fout("admitere.out");

    int cerinta;
    fin >> cerinta;

    int n, m;
    fin >> n >> m;

    vector<int> real(m), uman(m);
    for (int i = 0; i < m; i++) {
        fin >> real[i];
    }
    for (int i = 0; i < m; i++) {
        fin >> uman[i];
    }

    vector<elev> elevi(n);
    for (int i = 0; i < n; i++) {
        fin >> elevi[i].nota_real >> elevi[i].nota_uman;
        elevi[i].id = i;
    }

    if (cerinta == 1) {
        // Sortarea elevilor dupa nota la real
        sort(elevi.begin(), elevi.end(),
             [](const elev &a, const elev &b) -> bool { return a.nota_real > b.nota_real; });

        // Verificarea daca un elev poate fi admis la real
        int max_admisi = 0;
        for (int i = 0; i < n; i++) {
            if (real[m - 1] < elevi[i].nota_real) {
                max_admisi++;
            } else {
                break;
            }
        }

        // Verificarea daca un elev poate fi admis la uman
        sort(elevi.begin(), elevi.end(),
             [](const elev &a, const elev &b) -> bool { return a.nota_uman > b.nota_uman; });

        int max_admisi_uman = 0;
        for (int i = 0; i < n; i++) {
            if (uman[m - 1] < elevi[i].nota_uman) {
                max_admisi_uman++;
            } else {
                break;
            }
        }

        // Afisarea rezultatului
        fout << max(max_admisi, max_admisi_uman) << '\n';
        for (int i = 0; i < n; i++) {
            if (max_admisi > 0 && elevi[i].nota_real > real[m - 1]) {
                fout << 'R';
                max_admisi--;
            } else if (max_admisi_uman > 0 && elevi[i].nota_uman > uman[m - 1]) {
                fout << 'U';
                max_admisi_uman--;
            } else {
                fout << 'X';
            }
        }
        fout << '\n';
    } else {
        // Sortarea elevilor dupa nota la real
        sort(elevi.begin(), elevi.end(),
             [](const elev &a, const elev &b) -> bool { return a.nota_real > b.nota_real; });

        // Crearea unei multimi cu notele elevilor deja inscrisi
        set<int> note_real(real.begin(), real.end());
        set<int> note_uman(uman.begin(), uman.end());

        // Gasirea elevilor care pot fi admisi la real
        int max_admisi_real = 0;
        int nota_admitere_real = real[m - 1];
        for (int i = 0; i < n; i++) {
            if (elevi[i].nota_real > nota_admitere_real && note_real.find(elevi[i].nota_real) == note_real.end()) {
                max_admisi_real++;
            } else {
                break;
            }
        }

        // Gasirea elevilor care pot fi admisi la uman
        sort(elevi.begin(), elevi.end(),
             [](const elev &a, const elev &b) -> bool { return a.nota_uman > b.nota_uman; });

        int max_admisi_uman = 0;
        int nota_admitere_uman = uman[m - 1];
        for (int i = 0; i < n; i++) {
            if (elevi[i].nota_uman > nota_admitere_uman && note_uman.find(elevi[i].nota_uman) == note_uman.end()) {
                max_admisi_uman++;
            } else {
                break;
            }
        }

        // Afisarea rezultatului
        fout << max(max_admisi_real, max_admisi_uman) << '\n';
        for (int i = 0; i < n; i++) {
            if (max_admisi_real > 0 && elevi[i].nota_real > nota_admitere_real &&
                note_real.find(elevi[i].nota_real) == note_real.end()) {
                fout << 'R';
                note_real.insert(elevi[i].nota_real);
                max_admisi_real--;
            } else if (max_admisi_uman > 0 && elevi[i].nota_uman > nota_admitere_uman &&
                       note_uman.find(elevi[i].nota_uman) == note_uman.end()) {
                fout << 'U';
                note_uman.insert(elevi[i].nota_uman);
                max_admisi_uman--;
            } else {
                fout << 'X';
            }
        }
        fout << '\n';
    }

    return 0;
}
