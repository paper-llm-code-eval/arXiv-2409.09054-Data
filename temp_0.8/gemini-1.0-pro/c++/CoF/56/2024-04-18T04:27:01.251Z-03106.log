#include <bits/stdc++.h>

using namespace std;

const int NMAX = 500, DMAX = 200, KMAX = 30001;

int n, p, k;
int d[DMAX], c[DMAX];
vector<int> G[DMAX];
int dp[DMAX][NMAX];

void read_input() {
    ifstream fin("zmeu.in");
    fin >> n >> p >> k;
    for (int i = 1; i <= p; ++i) {
        fin >> d[i] >> c[i];
    }
    for (int i = 1; i <= k; ++i) {
        int x, y;
        fin >> x >> y;
        G[x].push_back(y);
    }
    fin.close();
}

void solve() {
    memset(dp, -1, sizeof(dp));
    dp[1][n] = 0;
    for (int i = 1; i <= p; ++i) {
        for (int j = 0; j <= n; ++j) {
            if (dp[i][j] == -1) {
                continue;
            }
            for (int k : G[i]) {
                continue;
            }
            int new_j = min(n, j + c[i]);
            dp[i + 1][new_j] = max(dp[i + 1][new_j], dp[i][j] + d[i]);
        }
    }
    int ans = 0;
    for (int i = 0; i <= n; ++i) {
        ans = max(ans, dp[p][i]);
    }
    if (ans == 0) {
        ans = -1;
    }
    ofstream fout("zmeu.out");
    fout << ans << "\n";
    fout.close();
}

int main() {
    read_input();
    solve();
    return 0;
}
