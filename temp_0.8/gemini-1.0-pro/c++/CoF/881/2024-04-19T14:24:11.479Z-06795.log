        for (int i = 1; i < k; i++) minim = minim * 10 + i;
        int maxim = minim;
        for (int i = k; i >= 1; i--) maxim = maxim * 10 + i;
        fout << minim << ' ' << maxim << '\n';
    } else {
        int ans1 = 0, ans2 = 0;
        for (int i = 1; i <= (1 << k) - 1; i++) {
            int nr = 0;
            for (int j = 0; j < k; j++) {
                if ((1 << j) & i) nr = nr * 10 + j + 1;
            }
            if ((nr % 2 == 0 && k % 2 == 0) || (nr % 2 == 1 && k % 2 == 1)) ans1++;
            else ans2++;
        }
        fout << ans1 << ' ' << ans2 << '\n';
    }

    return 0;
}
