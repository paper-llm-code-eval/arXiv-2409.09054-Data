bool is_prime(int x) {
    if(x < 2) {
        return false;
    }

    if(x % 2 == 0) {
        return x == 2;
    }

    for(int i = 3; i * i <= x; i += 2) {
        if(x % i == 0) {
            return false;
        }
    }

    return true;
}

bool validate(int x) {
    int digits = 0;
    int last_digit = -1;

    while(x) {
        int digit = x % 10;

        digits++;

