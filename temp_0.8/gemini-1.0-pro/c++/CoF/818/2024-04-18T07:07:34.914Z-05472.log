#include <bits/stdc++.h>
using namespace std;
ifstream f("roata.in");
ofstream g("roata.out");
int n, p, v[100001], rot[100001], poz[100001], ord[100001], k, sum = 0, ind = 1;
int main() {
  f >> n >> p;
  for (int i = 1; i <= p; i++) {
    f >> v[i];
    sum += v[i];
  }
  for (int i = 1; i <= p; i++) {
    rot[i] = v[i] + rot[i - 1];
  }
  for (int i = 1; i <= p; i++) {
    poz[rot[i] % n + 1] = i;
  }
  for (int i = 1; i <= n; i++) {
    if (poz[i] != 0) {
      ord[k++] = poz[i];
      poz[i] = 0;
    }
  }
  g << sum << '\n';
  for (int i = 1; i < k; i++) g << ord[i] << ' ';
  g << ord[k] << '\n' << rot[p] % n + 1 << '\n';
  return 0;
}
```
