#include <fstream>
#include <vector>

using namespace std;

ifstream fin("roata.in");
ofstream fout("roata.out");

const int NMAX = 100100;

int n, p, c[NMAX], s[NMAX], coada[NMAX];

int main() {
  fin >> n >> p;
  for (int i = 1; i <= p; ++i) fin >> c[i];
  int sum = 0, poz = 0, u = 1, v = 0;
  for (int i = 1; i <= p; ++i) {
    sum += c[i];
    coada[++v] = i;
    s[i] = poz + 1;
    poz = (poz + c[i]) % n;
  }
  fout << sum << '\n';
  while (u <= v) {
    fout << coada[u] << ' ';
    poz = (poz - c[coada[u]] + n) % n;
    ++u;
    while (s[coada[v]] != poz) --v;
    coada[++v] = coada[u];
  }
  fout << '\n' << poz + 1;
  return 0;
}
```
