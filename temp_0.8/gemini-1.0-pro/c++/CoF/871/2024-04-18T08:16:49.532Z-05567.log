        par[idx] = idx;
        max_cells = std::max(max_cells, x + y + 1);
        int px = x, py = y;
        while (px != n || py != m) {
            int dir = (px == 0 ? 1 : (py == m ? 2 : (px == n ? 3 : 4)));
            int nx = px, ny = py;
            if (dir == 1)
                nx++;
            else if (dir == 2)
                ny--;
            else if (dir == 3)
                nx--;
            else
                ny++;
            int nidx = nx * m + ny;
            if (par[nidx] == nidx) {
                merge(idx, nidx);
                max_len = std::max(max_len, x - nx + y - ny + 2);
            }
            px = nx;
            py = ny;
        }
    }
    FILE *out = fopen("galerie.out", "wb");
    fprintf(out, "%d\n", req == 1 ? max_cells : max_len);
    fclose(out);
    return 0;
}
