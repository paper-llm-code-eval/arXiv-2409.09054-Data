#include <iostream>
#include <vector>
#include <queue>
#include <algorithm>

using namespace std;

const int MAX_N = 10001;

int n, k;
vector<int> vecini[MAX_N];
int dist[MAX_N];
int cost[MAX_N];

void bfs(int start) {
    for (int i = 1; i <= n; ++i) {
        dist[i] = -1;
        cost[i] = 0;
    }
    queue<int> q;
    q.push(start);
    dist[start] = 0;
    while (!q.empty()) {
        int nod = q.front();
        q.pop();
        for (size_t i = 0; i < vecini[nod].size(); ++i) {
            int vecin = vecini[nod][i];
            if (dist[vecin] == -1) {
                dist[vecin] = dist[nod] + 1;
                cost[vecin] = cost[nod] + 1;
                q.push(vecin);
            } else if (dist[vecin] == dist[nod] + 1) {
                cost[vecin] = min(cost[vecin], cost[nod] + 1);
            }
        }
    }
}

int main() {
    ifstream fin("cezar.in");
    ofstream fout("cezar.out");
    fin >> n >> k;
    for (int i = 1; i < n; ++i) {
        int a, b;
        fin >> a >> b;
        vecini[a].push_back(b);
        vecini[b].push_back(a);
    }
    bfs(1);
    int min_cost = 1e9;
    for (int i = 1; i <= n; ++i) {
        if (cost[i] <= k) {
            int dist_to_i = dist[i];
            bfs(i);
            int max_dist_from_i = 0;
            for (int j = 1; j <= n; ++j) {
                max_dist_from_i = max(max_dist_from_i, dist[j]);
            }
            min_cost = min(min_cost, dist_to_i + max_dist_from_i);
        }
    }
    fout << min_cost << endl;
    return 0;
}
