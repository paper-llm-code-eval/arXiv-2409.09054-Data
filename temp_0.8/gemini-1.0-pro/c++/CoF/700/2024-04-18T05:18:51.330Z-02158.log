#include <fstream>
#include <iostream>
#include <limits>
#include <vector>

using namespace std;

int main() {
  ifstream fin("mouse.in");
  ofstream fout("mouse.out");

  int m, n;
  fin >> m >> n;
  vector<vector<int>> grid(m, vector<int>(n));
  for (int i = 0; i < m; ++i) {
    for (int j = 0; j < n; ++j) {
      fin >> grid[i][j];
    }
  }

  vector<vector<int>> dp(m, vector<int>(n, -1));
  dp[0][0] = grid[0][0];
  for (int i = 0; i < m; ++i) {
    for (int j = 0; j < n; ++j) {
      if (i > 0 && dp[i - 1][j] != -1) {
        dp[i][j] = max(dp[i][j], dp[i - 1][j]);
      }
      if (j > 0 && dp[i][j - 1] != -1) {
        dp[i][j] = max(dp[i][j], dp[i][j - 1]);
      }
      if (dp[i][j] != -1) {
        dp[i][j] += grid[i][j];
      }
    }
  }

  int max_food = 0;
  int end_i = -1, end_j = -1;
  for (int i = 0; i < m; ++i) {
    for (int j = 0; j < n; ++j) {
      if (dp[i][j] > max_food) {
        max_food = dp[i][j];
        end_i = i;
        end_j = j;
      }
    }
  }

  fout << max_food << endl;

  vector<pair<int, int>> path;
  while (end_i >= 0 && end_j >= 0) {
    path.push_back(make_pair(end_i + 1, end_j + 1));
    if (end_i > 0 && dp[end_i - 1][end_j] == dp[end_i][end_j]) {
      --end_i;
    } else if (end_j > 0 && dp[end_i][end_j - 1] == dp[end_i][end_j]) {
      --end_j;
    }
  }
  reverse(path.begin(), path.end());
  for (const auto& p : path) {
    fout << p.first << ' ' << p.second << endl;
  }
  return 0;
}
