#include <fstream>
using namespace std;

const int MAXN = 1e5 + 1;
ifstream fin("buldo.in");
ofstream fout("buldo.out");
int n, h[MAXN], dp[MAXN];

int main() {
    fin >> n;
    for (int i = 1; i <= n; ++ i)
        fin >> h[i];
    dp[n] = h[n];
    for (int i = n - 1; i; -- i)
        dp[i] = max(h[i], dp[i + 1] - 1);
    int ans = 0;
    for (int i = 1; i <= n; ++ i)
        ans = max(ans, min(dp[i], h[i]));
    fout << ans << '\n';
    return 0;
}
```
