  std::ofstream out("valori-panta.out");

  size_t n;
  in >> n;
  std::vector<size_t> v(n);
  in >> v;

  size_t max = v[0], min = v[0], nr = 0, pmax = 0, pmin = 0;
  for (size_t i = 0; i < n; ++i) {
    size_t x = v[i];
    bool d = 1, c = 1;
    while (x > 0) {
      size_t y = x % 10;
      x /= 10;
      if (d && y >= x % 10) d = 0;
      if (c && y <= x % 10) c = 0;
    }
    if (d || c) ++nr;
    if (max < v[i] && d) {
      max = v[i];
      pmax = i + 1;
    }
    if (min > v[i] && c) {
      min = v[i];
      pmin = i + 1;
    }
  }
  out << nr << '\n';
  if (nr > 0) {
    out << max << ' ' << pmax << '\n' << min << ' ' << pmin << '\n';
  } else {
    out << "NU EXISTA\n";
  }
  return 0;
}
