    if (min > v[i] && c) {
      min = v[i];
      pmin = i + 1;
    }
  }
  out << nr << '\n';
  if (nr > 0) {
    out << max << ' ' << pmax << '\n' << min << ' ' << pmin << '\n';
  } else {
    out << "NU EXISTA\n";
  }
  return 0;
}
