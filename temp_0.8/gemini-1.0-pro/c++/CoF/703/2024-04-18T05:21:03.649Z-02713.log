  fout << nr_valori_panta << endl;
  if (nr_valori_panta > 0) {
    fout << max_valoare_panta << " ";
    for (int pozitie : pozitii_max_valoare_panta) {
      fout << pozitie << " ";
    }
    fout << endl;
    fout << min_valoare_panta << " ";
    for (int pozitie : pozitii_min_valoare_panta) {
      fout << pozitie << " ";
    }
    fout << endl;
  } else {
    fout << "NU EXISTA" << endl;
  }

  return 0;
