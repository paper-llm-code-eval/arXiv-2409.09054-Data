#include <fstream>
#include <string>
#include <vector>
#include <iostream>

std::fstream fin("excel.in", std::ios::in);
std::fstream fout("excel.out", std::ios::out);

std::vector<std::vector<int>> get_excel_data(int n_cols, int n_rows) {
  std::vector<std::vector<int>> excel(n_rows, std::vector<int>(n_cols, 0));

  for (int row = 0; row < n_rows; ++row) {
    std::string line;
    std::getline(fin, line);

    int i = 0;
    for (int col = 0; col < n_cols; ++col) {
      int start = i;
      while (line[i] != ' ' && line[i] != '\n' && i < line.length()) {
        ++i;
      }
      std::string cell_value = line.substr(start, i - start);
      if (cell_value[0] == '=') {
        int sum = 0;
        int pos = 2;
        while (pos < cell_value.length()) {
          int col_num = cell_value[pos] - 'A';
          ++pos;
          int row_num = cell_value[pos] - '0';
          ++pos;
          if (row_num <= row) {  // formula uses values from previous rows
            sum += excel[row_num - 1][col_num];
          }
          if (pos < cell_value.length() && cell_value[pos] == '+') ++pos;
        }
        excel[row][col] = sum;
      } else {
        excel[row][col] = std::stoi(cell_value);
      }
      ++i;
    }
  }

  return excel;
}

void print_excel(std::vector<std::vector<int>>& excel, int n_rows, int n_cols) {
  for (int row = 0; row < n_rows; ++row) {
    for (int col = 0; col < n_cols; ++col) {
      fout << excel[row][col] << " ";
    }
    fout << "\n";
  }
}

int main() {
  int n_cols, n_rows;
  fin >> n_cols >> n_rows;
  auto excel = get_excel_data(n_cols, n_rows);
  print_excel(excel, n_rows, n_cols);
  return 0;
}
