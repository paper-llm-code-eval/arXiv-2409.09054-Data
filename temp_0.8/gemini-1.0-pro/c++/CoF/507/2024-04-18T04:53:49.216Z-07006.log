// partitura.cpp
#include <algorithm>
#include <vector>
#include <fstream>

using namespace std;

int main() {
    ifstream fin("partitura.in");
    ofstream fout("partitura.out");

    int n;
    fin >> n;
    vector<pair<int, int>> notes(n);
    for (auto& note : notes) {
        fin >> note.first >> note.second;
    }
    fin.close();

    sort(notes.begin(), notes.end(), [](const pair<int, int>& a, const pair<int, int>& b) {
        return a.second > b.second;
    });

    long long score = 0;
    int duration = 0;
    for (const auto& note : notes) {
        duration += note.first;
        if (duration == 1) {
            score += note.second * note.second;
            duration = 0;
        }
    }

    fout << score << endl;
    fout.close();

    return 0;
}
```
