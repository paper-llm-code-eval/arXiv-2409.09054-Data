#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream fin("reactivi.in");
    ofstream fout("reactivi.out");

    int n;
    fin >> n;
    vector<pair<int, int>> intervals(n);
    for (int i = 0; i < n; i++) {
        fin >> intervals[i].first >> intervals[i].second;
    }

    sort(intervals.begin(), intervals.end());

    int answer = 1;
    int current_min = intervals[0].first;
    int current_max = intervals[0].second;
    for (int i = 1; i < n; i++) {
        if (intervals[i].first > current_max) {
            answer++;
            current_min = intervals[i].first;
            current_max = intervals[i].second;
        } else {
            current_min = max(current_min, intervals[i].first);
            current_max = min(current_max, intervals[i].second);
        }
    }

    fout << answer << "\n";
    return 0;
}
