#include <bits/stdc++.h>
using namespace std;

ifstream cin("puncte.in");
ofstream cout("puncte.out");

const int MAXN = 250'000 + 1;

struct Point {
    int x, y;
    bool operator < (const Point &p) const {
        if (x != p.x) {
            return x < p.x;
        }
        return y < p.y;
    }
};
struct Line {
    int a, b, c;
    Line(Point p, Point q) {
        if (p.x == q.x) {
            a = 1;
            b = 0;
            c = -p.x;
        } else {
            a = q.y - p.y;
            b = p.x - q.x;
            c = p.y * (q.x - p.x) - p.x * (q.y - p.y);
        }
    }
    bool parallel(const Line &l) const {
        return a * l.b == b * l.a;
    }
};

int n;
Point points[MAXN];
vector<Point> ax, obl, lin, col;
vector<vector<int>> pointOnLine;
vector<Line> lines;
int nrp, nrd, xp;

int main() {
    cin >> n;
    for (int i = 0; i < n; i++) {
        cin >> points[i].x >> points[i].y;
    }
    sort(points, points + n);
    for (int i = 0; i < n; i++) {
        if (points[i].x == 0) pointOnLine[n].push_back(i);
        else if (points[i].y == 0) pointOnLine[n + 1].push_back(i);
        else if (points[i].x == points[i].y) pointOnLine[n + 2].push_back(i);
        else if (points[i].x + points[i].y == 0) pointOnLine[n + 3].push_back(i);
    }
    for (int i = 0; i < n - 1; i++) {
        for (int j = i + 1; j < n; j++) {
            if (points[i].x == points[j].x) {
                pointOnLine[points[i].x].push_back(i);
                pointOnLine[points[i].x].push_back(j);
            } else if (points[i].y == points[j].y) {
                pointOnLine[points[i].y + n + 1].push_back(i);
                pointOnLine[points[i].y + n + 1].push_back(j);
            } else {
                Line l(points[i], points[j]);
                for (int k = 0; k < n; k++) {
                    if (l.a * points[k].x + l.b * points[k].y + l.c == 0) {
                        pointOnLine[i * n + j].push_back(k);
                    }
                }
                lines.push_back(l);
            }
        }
    }
    for (int i = 0; i < n * n + n + 4; i++) {
        if (pointOnLine[i].empty()) continue;
        vector<int> v = pointOnLine[i];
        sort(v.begin(), v.end());
        for (int j = 0; j < v.size() - 1; j++) {
            nrp = max(nrp, v[j + 1] - v[j]);
        }
        for (int j = 0; j < v.size(); j++) {
            for (int k = j + 1; k < v.size(); k++) {
                for (int l = 0; l < lines.size(); l++) {
                    if (lines[l].a * points[v[j]].x + lines[l].b * points[v[j]].y + lines[l].c == 0
                        && lines[l].a * points[v[k]].x + lines[l].b * points[v[k]].y + lines[l].c == 0) {
                        nrd++;
                    }
                }
            }
        }
    }
    int cnt = 0;
    for (int i = 0; i < n; i++) {
        cnt++;
        if (cnt % 3 == 0) {
            xp = points[i].x;
        }
    }
    cout << nrp << '\n' << nrd << '\n' << xp << '\n';
    return 0;
}
