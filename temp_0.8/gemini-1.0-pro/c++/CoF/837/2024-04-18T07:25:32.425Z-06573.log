#include <fstream>

using namespace std;

const int MAXN = 250000;

int n, v[MAXN][2], fr[MAXN + 5], a, b, c, d, d1, d2, maxd;
bool ok[MAXN + 5];

int main() {
    ifstream fin("puncte.in");
    ofstream fout("puncte.out");
    fin >> n;
    int nrp = 1, nrd = 0, xp = 0, nr3 = 0;
    while (fin >> v[a][0] >> v[a][1]) {
        ok[v[a][0]] = 1;
        if (v[a][0] == 0 || v[a][1] == 0) nrp = max(nrp, ++fr[v[a][0] + v[a][1] * MAXN]);
        a++;
    }
    fin.close();
    for (b = -MAXN; b <= MAXN; b++)
        if (fr[b] > 1) nrp = max(nrp, fr[b]), fr[b] = 0;
    for (b = 0; b < a; b++) {
        if (v[b][0] == 0 || v[b][1] == 0) continue;
        for (c = b + 1; c < a; c++)
            if (v[c][0] == 0 || v[c][1] == 0) continue;
            else if (v[b][0] == v[c][0])
                nrp = max(nrp, ++fr[v[b][0] + MAXN]);
            else if (v[b][1] == v[c][1])
                nrp = max(nrp, ++fr[v[b][1] + MAXN * 2]);
            else if (v[b][0] + v[b][1] == v[c][0] + v[c][1])
                nrp = max(nrp, ++fr[v[b][0] + v[b][1] + MAXN * 3]);
            else if (v[b][0] - v[b][1] == v[c][0] - v[c][1])
                nrp = max(nrp, ++fr[v[b][0] - v[b][1] + MAXN * 4]);
    }
    for (b = 0; b < a; b++) {
        if (v[b][0] == 0 || v[b][1] == 0 || ok[v[b][0] + v[b][1]] || ok[v[b][0] - v[b][1]]) continue;
        if (v[b][0] == v[b][1])
            d1 = v[b][0], d2 = 0;
        else
            d1 = 0, d2 = v[b][0] + v[b][1];
        if (v[b][0] > v[b][1])
            for (c = -v[b][0]; c <= v[b][1]; c++) {
                if (ok[c + d1] || ok[c + d2]) continue;
                ok[c + d1] = ok[c + d2] = 1;
                nr3++;
                fr[c + d1 + MAXN * 6]++;
                fr[c + d2 + MAXN * 7]++;
            }
        else
            for (c = -v[b][1]; c <= v[b][0]; c++) {
                if (ok[c + d1] || ok[c + d2]) continue;
                ok[c + d1] = ok[c + d2] = 1;
                nr3++;
                fr[c + d1 + MAXN * 6]++;
                fr[c + d2 + MAXN * 7]++;
            }
        nrd++;
    }
    for (b = -MAXN; b <= MAXN; b++)
        if (fr[b + MAXN * 6] > 1) nrd += fr[b + MAXN * 6] - 1, fr[b + MAXN * 6] = 0;
    for (b = -MAXN; b <= MAXN; b++)
        if (fr[b + MAXN * 7] > 1) nrd += fr[b + MAXN * 7] - 1, fr[b + MAXN * 7] = 0;
    for (b = 0; b < a; b++)
        if (!nrp) { xp = v[b][0]; break; } else if (!--nr3) xp = v[b][0];
    fout << nrp << endl << nrd << endl << xp << endl;
    fout.close();
    return 0;
}
