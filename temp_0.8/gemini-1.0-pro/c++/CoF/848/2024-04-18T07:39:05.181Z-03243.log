#include <fstream>

using namespace std;

ifstream fin("arrows.in");
ofstream fout("arrows.out");

const int NMAX = 500, dir[5] = {0, 1, 4, 2, 3};

int n, m, a[NMAX + 5][NMAX + 5], p[NMAX + 5][NMAX + 5], v[NMAX + 5][NMAX + 5],
    l, c, rez;
bool ok;

void dfs(int x, int y, int d) {
  if (x < 1 || x > n || y < 1 || y > m)
    return;
  if (v[x][y] == 1)
    ok = 0;
  v[x][y] = 1;
  p[x][y] = rez + (d == 0);
  dfs(x, y + dir[a[x][y]], d + (d == a[x][y]));
}

int main() {
  int cer;
  fin >> cer;
  fin >> n >> m;
  for (int i = 1; i <= n; ++i)
    for (int j = 1; j <= m; ++j) fin >> a[i][j];
  fin >> l >> c;
  if (cer == 1) {
    dfs(l, c, 0);
    fout << p[l][c];
  } else if (cer == 2) {
    for (int i = 1; i <= n; ++i)
      for (int j = 1; j <= m; ++j) {
        ok = 1;
        dfs(i, j, 0);
        if (ok) rez++;
      }
    fout << rez;
  } else {
    for (int i = 1; i <= n; ++i)
      for (int j = 1; j <= m; ++j) {
        ok = 1;
        dfs(i, j, 0);
        if (ok) rez = max(rez, p[l][c]);
      }
    fout << rez;
  }
}
