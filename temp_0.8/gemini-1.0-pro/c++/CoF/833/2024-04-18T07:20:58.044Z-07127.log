#include <fstream>
#include <iostream>
#include <vector>
#include <algorithm>
#include <cstdio>

using namespace std;
ifstream fin("cladiri.in");
ofstream fout("cladiri.out");
const int maxN=10001;
vector<int> v[maxN],v2[maxN]; //v stocheaza turnurile din fiecare cladire in ordinea in care au fost vazute de Crina, v2 stocheaza turnurile in ordinea in care au fost vazute de Rares
int f[10],f2[10],vmax,cntvmax;

int main()
{
    int i,j,n,x,nr1=0,nr2=0;
    fin>>n;
    for(i=1;i<=n;i++)
    {
        fin>>x;
        v[i].push_back(x);
        fin>>x;
        v2[i].push_back(x);
    }
    for(i=n;i>0;i--)
    {
        for(j=0;j<v[i].size();j++)
        {
            f[v[i][j]]++;
        }
        for(j=0;j<v2[i].size();j++)
        {
            f2[v2[i][j]]++;
        }
    }
    for(i=1;i<=9;i++)
    {
        if(f[i]>vmax)
        {
            vmax=f[i];
            cntvmax=1;
        }
        if(f[i]==vmax)
        {
            cntvmax++;
        }
        if(f[i]==f2[i])
        {
            nr1++;
        }
    }
    for(i=n;i>0;i--)
    {
        for(j=0;j<v[i].size();j++)
        {
            if(f2[v[i][j]]==f[v[i][j]])
            {
                nr2++;
            }
        }
    }
    fout<<vmax<<' '<<cntvmax<<'\n';
    fout<<nr1<<'\n';
    fout<<nr2<<'\n';
    return 0;
}
