    out << '\n';
  } else if (c == 3) {
    int mx = *max_element(v.begin(), v.end());
    int l = -1, r = -1;
    for (int i = 0; i < n; i++) {
      if (v[i] == mx) {
        if (l == -1) {
          l = i;
        }
        r = i;
      }
    }
    int sum = 0;
    for (int i = l + 1; i < r; i++) {
      sum += mx - v[i];
    }
    out << sum << '\n';
  }

  return 0;
}
