  in >> n;
  vector<int> v(n);
  for (int i = 0; i < n; i++) {
    in >> v[i];
  }

  if (c == 1) {
    for (int i = n - 2; i >= 0; i--) {
      if (v[i] > v[i + 1]) {
        out << i + 1 << '\n';
        break;
      }
    }
  } else if (c == 2) {
