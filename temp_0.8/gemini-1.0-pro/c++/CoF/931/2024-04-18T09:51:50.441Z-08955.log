#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream in("sir.in");
  ofstream out("sir.out");

  int C, N;
  in >> C >> N;
  vector<int> sir(N + 1);

  for (int i = 1; i <= N; i++) {
    in >> sir[i];
  }

  if (C == 1) {
    int poz = -1;
    for (int i = 1; i < N; i++) {
      if (sir[i] > sir[i + 1]) {
        poz = i;
        break;
      }
    }
    out << poz << '\n';
  } else if (C == 2) {
    for (int i = 1; i <= N; i++) {
      bool valid = true;
      for (int j = 1; j < i; j++) {
        if (sir[j] >= sir[i]) {
          valid = false;
          break;
        }
      }
      if (valid) {
        out << i << ' ';
      }
    }
    out << '\n';
  } else {
    int mx = -1;
    for (int i = 1; i <= N; i++) {
      mx = max(mx, sir[i]);
    }

    int st = -1, dr = -1, sum = 0;
    for (int i = 1; i <= N; i++) {
      if (sir[i] == mx) {
        if (st == -1) {
          st = i;
        }
        dr = i;
      }
    }

    for (int i = st + 1; i < dr; i++) {
      sum += mx - sir[i];
    }

    out << sum << '\n';
  }

  return 0;
}
