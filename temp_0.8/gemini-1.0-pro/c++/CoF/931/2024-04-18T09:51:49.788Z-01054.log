#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("sir.in");
ofstream fout("sir.out");

int main() {
  int C, N, a, last_idx, mx, ans;
  fin >> C >> N;
  vector<int> v(N);
  for (int i = 0; i < N; i++) {
    fin >> a;
    v[i] = a;
    if (a > mx) {
      mx = a;
      last_idx = i;
    }
  }
  if (C == 1) {
    int idx = -1;
    for (int i = N - 1; i >= 0; i--) {
      if (v[i] > v[i + 1]) {
        idx = i;
        break;
      }
    }
    fout << idx << '\n';
  } else if (C == 2) {
    for (int i = 0; i < N; i++) {
      if (v[i] > v[i - 1]) {
        fout << i + 1 << ' ';
      }
    }
    fout << '\n';
  } else {
    ans = 0;
    for (int i = 0; i < N; i++) {
      if (v[i] < mx) {
        ans += mx - v[i];
      }
    }
    fout << ans << '\n';
  }
  return 0;
}
