        if (a[1][j] == fib[0])
            dp[1][j][1] = 1;
    }
    for (int j = 1; j <= m; ++j) {
        for (int i = 1; i <= n; ++i) {
            int val = a[i][j];
            int ind = upper_bound(all(fib), val) - fib.begin() - 1;
            dp[i][j][0] = max({dp[i - 1][j][0], dp[i - 1][j][1], dp[i][j - 1][0], dp[i][j - 1][1] + (a[i][j] == fib[ind])});
            if (a[i][j] == fib[ind])
                dp[i][j][1] = dp[i - 1][j][1] + 1;
            if (dp[i][j][1] > 0) {
                dp[i][j][0] = max(dp[i][j][0], dp[i][j][1]);
                if (i > 1)
                    dp[i][j][0] = max(dp[i][j][0], dp[i - 1][j][0] + 1);
                if (j > 1)
                    dp[i][j][0] = max(dp[i][j][0], dp[i][j - 1][0] + 1);
            }
        }
    }
    int ans = 0;
    if (c == 1) {
        for (int j = 1; j <= m; ++j)
            for (int i = 1; i <= n; ++i)
                ans += (a[i][j] == fib[upper_bound(all(fib), a[i][j]) - fib.begin() - 1]);
        out << ans << "\n";
    } else {
        for (int j = 1; j <= m; ++j)
            for (int i = 1; i <= n; ++i)
                ans = max(ans, dp[i][j][0]);
        out << ans << "\n";
    }
    return 0;
}
