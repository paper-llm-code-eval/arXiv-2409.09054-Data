#include <iostream>
#include <algorithm>

using namespace std;

const int MAX_N = 1501, MAX_M = 1501;

int n, m, c, k = 3, a[MAX_N][MAX_M], f[2][MAX_M], l[MAX_N][MAX_M], F[500000], r[MAX_N], cmax[MAX_M];

void fib() {
  F[1] = F[2] = 1;
  while (F[k - 1] <= 2e9) {
    F[k] = F[k - 1] + F[k - 2];
    k++;
  }
}

int main() {
  fib();
  freopen("fibosnek.in", "r", stdin);
  freopen("fibosnek.out", "w", stdout);
  scanf("%d%d%d", &c, &n, &m);
  for (int i = 1; i <= n; i++)
    for (int j = 1; j <= m; j++) {
      scanf("%d", &a[i][j]);
      if (c == 1)
        if (binary_search(F + 1, F + k, a[i][j]))
          f[i][j] = 1;
    }
  if (c == 1) {
    for (int i = 1; i <= n; i++)
      for (int j = 1; j <= m; j++) f[i][j] += f[i - 1][j] + f[i][j - 1] - f[i - 1][j - 1];
    printf("%d", f[n][m]);
    return 0;
  }
  for (int j = 1; j <= m; j++)
    for (int i = 1; i <= n; i++) {
      int p = lower_bound(F + 1, F + k, a[i][j]) - F;
      if (F[p] < a[i][j])
        l[i][j] = F[p - 1];
      else
        l[i][j] = F[p];
    }
  int ans = 0, cur = 0, mxr = 0, mxc = 0;
  for (int i = 1; i <= n; i++) {
    cur = 0;
    for (int j = 1; j <= m; j++) {
      if (a[i][j] == l[i][j])
        continue;
      if (a[i][j] <= l[i][j]) {
        cur++;
        l[i][j] = a[i][j];
        f[i][j] = 1;
      } else {
        r[i] = max(r[i], cur);
        cur = 1;
        f[i][j] = 1 + f[i - 1][j];
      }
      ans = max(ans, f[i][j]);
    }
    r[i] = max(r[i], cur);
    mxr = max(mxr, *max_element(r + 1, r + n + 1));
  }
  for (int j = 1; j <= m; j++) {
    cur = 0;
    for (int i = 1; i <= n; i++) {
      if (a[i][j] == l[i][j])
        continue;
      if (a[i][j] <= l[i][j]) {
        cur++;
        l[i][j] = a[i][j];
        f[i][j] = 1;
      } else {
        cmax[j] = max(cmax[j], cur);
        cur = 1;
        f[i][j] = 1 + f[i - 1][j];
      }
      ans = max(ans, f[i][j]);
    }
    cmax[j] = max(cmax[j], cur);
    mxc = max(mxc, *max_element(cmax + 1, cmax + m + 1));
  }
  ans += l[n][m] + max(mxr, mxc);
  printf("%d", ans);
  return 0;
}
