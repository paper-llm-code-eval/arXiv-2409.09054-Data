#include <fstream>
#include <vector>
#include <cstdint>

struct RollingHash {
  static const uint64_t MOD = 1000000007ull;
  static const uint64_t BASE = 29;
  std::vector<uint64_t> hash, pow;

  RollingHash(const std::string& s) {
    hash.resize(s.size() + 1);
    pow.resize(s.size() + 1);
    pow[0] = 1;
    for (size_t i = 0; i < s.size(); i++) {
      hash[i + 1] = (hash[i] * BASE + s[i]) % MOD;
      pow[i + 1] = (pow[i] * BASE) % MOD;
    }
  }

  uint64_t get(size_t l, size_t r) {
    return (hash[r] - hash[l] * pow[r - l]) % MOD;
  }
};

int main() {
  std::ifstream in("sdistante.in");
  std::ofstream out("sdistante.out");

  std::string s;
  in >> s;

  RollingHash rh(s);
  uint64_t sum = 0;
  for (uint32_t i = 0; i < s.size(); i++) {
    for (uint32_t j = i; j < s.size(); j++) {
      for (uint32_t k = i + 1; k < s.size(); k++) {
        for (uint32_t l = k; l < s.size(); l++) {
          if (j - i != l - k) continue;
          sum += rh.get(i, j) == rh.get(k, l);
        }
      }
    }
  }

  out << sum % RollingHash::MOD << '\n';
  return 0;
}
