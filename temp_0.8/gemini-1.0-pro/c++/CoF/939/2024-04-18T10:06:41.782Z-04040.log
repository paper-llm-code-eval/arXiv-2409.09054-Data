#include <bits/stdc++.h>
using namespace std;

typedef long long ll;

#define MOD 1000000007

string s;
int n, lg;
int dp[4000005][2][2];

int lgcom(int p1, int p2, bool lit1, bool lit2) {
    if (p1 == n) {
        if (p2 == n)
            return 0;
        else
            return MOD;
    }
    if (dp[p1][lit1][lit2] != -1)
        return dp[p1][lit1][lit2];
    int ans = MOD;
    if (p2 < n) {
        ans = min(ans, lgcom(p1, p2 + 1, lit1 || (s[p1] == s[p2]), lit2 || (s[p1] == s[p2])));
        if (s[p1] != s[p2])
            ans = min(ans, lgcom(p1 + 1, p2 + 1, lit1 || (s[p1] == s[p2]), lit2 || (s[p1] == s[p2])) + 1);
    }
    ans = min(ans, lgcom(p1 + 1, p2, lit1, lit2) + 1);
    dp[p1][lit1][lit2] = ans;
    return dp[p1][lit1][lit2];
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    freopen("sdistante.in", "r", stdin);
    freopen("sdistante.out", "w", stdout);
    cin >> s;
    n = (int)s.size();
    lg = (int)log2(n) + 1;
    memset(dp, -1, sizeof(dp));
    ll ans = 0;
    for (int i = 0; i < n; ++i) {
        for (int j = i; j < n; ++j) {
            ans += lgcom(i, i, 0, 0);
            if (ans >= MOD)
                ans -= MOD;
        }
    }
    cout << ans << '\n';
    return 0;
}
