#include <cstdio>
#include <stdint>
#include <vector>
using namespace std;

uint32_t a, b;
vector<uint32_t> tema, refren;

int main() {
    freopen("muzica.in", "r", stdin);
    freopen("muzica.out", "w", stdout);
    scanf("%u %u", &a, &b);

    uint32_t r, nr = 0;
    uint32_t v = 1;
    do {
        r = a / b;
        nr++;
        a = ((uint64_t)a * 10) % b;
        if (r == v) {
            tema.push_back(v);
        } else {
            refren.push_back(v);
            v = r;
        }
    } while (v != r);
    nr -= refren.size();

    for (uint32_t i = 0, st = tema.size(); i < st; i++) {
        printf("%u", tema[i]);
    }
    for (uint32_t i = 0, st = refren.size(); i < st; i++) {
        printf("%u", refren[i]);
    }
    printf(" %u", refren.size());

    return 0;
}
```
