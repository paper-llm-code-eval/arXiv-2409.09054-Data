#include <iostream>
#include <set>
#include <vector>
#include <fstream>
#include <algorithm>

using namespace std;
ifstream in("cursuri.in");
ofstream out("cursuri.out");

const int NMAX = 100000 + 5;

int n, k, c;
vector<pair<int, int> >  v;

int main() {
    in >> c >> n >> k;
    for (int i = 1; i <= n; ++i) {
        int x, y;
        in >> x >> y;
        v.push_back({x, y});
    }
    if (c == 1) {
        sort(v.begin(), v.end());
        int ans = 0, l = 0, r = 100000;
        while (l <= r) {
            int mid = (l + r) / 2;
            int ok = 1;
            vector<pair<int, int> >  aux;
            for (vector<pair<int, int> >::iterator it = v.begin(); it != v.end(); it++) {
                int t = it->first;
                while (aux.size() && t > aux.back().second) {
                    aux.pop_back();
                }
                if (aux.size() == k) {
                    ok = 0;
                    break;
                }
                aux.push_back({t, t + mid});
            }
            if (ok) {
                ans = mid;
                l = mid + 1;
            } else {
                r = mid - 1;
            }
        }
        out << ans << "\n";
    } else {
        sort(v.begin(), v.end());
        int ans = v[n - 1].second - v[n - 1].first;
        while (ans % k) {
            ++ans;
        }
        out << ans << "\n";
    }
    return 0;
}
