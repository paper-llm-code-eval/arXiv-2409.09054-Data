#include <fstream>
#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

struct interval {
    int start;
    int end;
};

bool compare(interval a, interval b) {
    return a.end < b.end;
}

int main() {
    ifstream fin("cursuri.in");
    ofstream fout("cursuri.out");

    int c;
    fin >> c;
    int n, k;
    fin >> n >> k;
    vector<interval> intervals(n);
    for (int i = 0; i < n; i++) {
        fin >> intervals[i].start >> intervals[i].end;
    }

    if (c == 1) {
        sort(intervals.begin(), intervals.end(), compare);
        int count = 0;
        int current_end = 0;
        for (int i = 0; i < n; i++) {
            if (intervals[i].start >= current_end) {
                count++;
                current_end = intervals[i].end;
            }
        }
        fout << count << endl;
    } else if (c == 2) {
        sort(intervals.begin(), intervals.end());
        int max_duration = intervals[n - 1].end - intervals[n - 1].start;
        int current_end = 0;
        int count = 0;
        for (int i = 0; i < n; i++) {
            if (current_end + max_duration <= intervals[i].end) {
                count++;
                current_end += max_duration;
                while (i < n && current_end >= intervals[i].end) {
                    i++;
                }
                i--;
            }
        }
        fout << max_duration << endl;
    }

    return 0;
}
