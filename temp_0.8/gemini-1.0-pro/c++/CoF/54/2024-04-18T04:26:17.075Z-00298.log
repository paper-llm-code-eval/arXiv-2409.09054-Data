#include <fstream>
#include <queue>

using namespace std;

const int INF = 1e9 + 5;
const int N = 55;
const int M = (N * N) / 2;

ifstream fin("lanterna.in");
ofstream fout("lanterna.out");
int dp[N][M]; // nr restanta de wati(0..M), nod curent(1..N)
int tmin[N][M]; // timpul minim pana la nodul curent(1..N)
int p[N], in[N], cost[N][N], t[N][N]; // p - tatal in DFS, in - noduri vizitate in DFS, cost - consumul de wati pe o muchie, t - durata parcurgerii unei muchii
vector<int> G[N];

inline int min(int a, int b) { return a < b ? a : b; }

void Dijkstra(int start) {
    for (int i = 0; i < N; i++) {
        for (int j = 0; j < M; j++) { dp[i][j] = INF; }
    }
    dp[start][M - 1] = 0;
    priority_queue<pair<int, pair<int, int>>> pq;
    pq.push({0, {M - 1, start}});
    while (!pq.empty()) {
        int w = pq.top().first;
        int nrW = pq.top().second.first;
        int node = pq.top().second.second;
        pq.pop();
        w = -w;
        if (w > dp[node][nrW]) continue;
        for (int vec : G[node]) {
            for (int i = 0; i < nrW; i++) {
                int nW = i, nT = dp[node][i] + t[node][vec];
                if (cost[node][vec] <= nW) {
                    nW -= cost[node][vec];
                    if (dp[vec][nW] > nT) {
                        dp[vec][nW] = nT;
                        p[vec] = node;
                        pq.push({-nT, {nW, vec}});
                    }
                }
            }
        }
    }
}

int DFS(int node, int nrW) {
    if (tmin[node][nrW] != -1) return tmin[node][nrW];
    tmin[node][nrW] = dp[node][nrW];
    if (node == N) return tmin[node][nrW];
    for (int vec : G[node]) {
        if (cost[node][vec] <= nrW) {
            tmin[node][nrW] = min(tmin[node][nrW], DFS(vec, nrW - cost[node][vec]));
        }
    }
    return tmin[node][nrW];
}

int32_t main() {
    int n, k, m;
    fin >> n >> k;
    int re[n + 1];
    fin >> re[1];
    for (int i = 2; i <= n; i++) {
        fin >> re[i];
        in[i] = 1;
    }
    fin >> m;
    for (int i = 0; i < m; i++) {
        int a, b, t1, t2;
        fin >> a >> b >> t1 >> t2;
        G[a].push_back(b);
        G[b].push_back(a);
        cost[a][b] = t1;
        cost[b][a] = t1;
        t[a][b] = t2;
        t[b][a] = t2;
    }
    Dijkstra(1);
    for (int i = 0; i < N; i++)
        for (int j = 0; j < M; j++) tmin[i][j] = -1;
    int ans = INF, ansW = 0;
    for (int i = 0; i < M; i++) {
        int t1 = DFS(N, i);
        if (t1 < ans) {
            ans = t1;
            ansW = i;
        }
    }
    fout << ans << " " << ansW << "\n";
    return 0;
}
