#include <iostream>
#include <fstream>
#include <vector>
#include <map>
#include <utility>
#include <queue>
#include <cassert>

using namespace std;

typedef unsigned int uint;
typedef unsigned long long ull;
typedef vector<int> vi;
typedef vector<uint> vui;
typedef map<int, vi> map_ii_vi;

struct Edge {
	uint v[2];  // end point
	uint w;    // weight
};

class Graph {
public:
	Graph(uint _n, uint _m);
	void addEdge(uint a, uint b, uint w);
	uint getWayOut(uint start) const;
	uint getLength(uint start) const;
private:
	uint n;
	uint m;
	vi inDegree;
	vi outDegree;
	vector<Edge> edges;
	map_ii_vi edgesMap;
};

Graph::Graph(uint _n, uint _m)
	: n(_n)
	, m(_m)
	, inDegree(n, 0)
	, outDegree(n, 0)
	, edges{}
	, edgesMap{}
{
	edges.reserve(m * 2);  // worst case
	edgesMap.reserve(n);
}

void Graph::addEdge(uint a, uint b, uint w)
{
	edgesMap[a].push_back(edges.size());
	edges.push_back({ { a, b }, w });
	++outDegree[a];
	++inDegree[b];
}

uint Graph::getWayOut(uint start) const
{
	queue<uint> q;
	q.push(start);
	if (start == n)
		return n;
	while (!q.empty()) {
		uint node = q.front();
		q.pop();
		if (inDegree[node] == 0)
			return node;
		for (uint edgeIdx : edgesMap.at(node)) {
			const Edge& edge = edges[edgeIdx];
			uint next = (node == edge.v[0] ? edge.v[1] : edge.v[0]);
			if (--inDegree[next] == 0)
				q.push(next);
		}
	}
	assert(false);
}

uint Graph::getLength(uint start) const
{
	queue<pair<uint, uint>> q;
	q.push({ start, outDegree[start] });
	uint length = 1;
	while (!q.empty()) {
		uint node, deg;
		tie(node, deg) = q.front();
		q.pop();
		for (uint edgeIdx : edgesMap.at(node)) {
			const Edge& edge = edges[edgeIdx];
			uint next = (node == edge.v[0] ? edge.v[1] : edge.v[0]);
			if (--inDegree[next] == 0)
				q.push({ next, outDegree[next] });
		}
		if (deg == 0)
			++length;
	}
	return length;
}

int main()
{
	ifstream in("tunel.in");
	ofstream out("tunel.out");
	uint c, n, m, x;
	in >> c >> n >> m >> x;
	Graph g(n, m);
	for (uint i = 1; i < n; ++i) {
		uint p;
		in >> p;
		for (uint j = 0; j < p; ++j) {
			uint a, b;
			in >> a >> b;
			g.addEdge(a, b, 1);
		}
	}
	if (c == 1)
		out << g.getWayOut(x) << '\n';
	else
		out << g.getLength(x) << '\n';
	return 0;
}
