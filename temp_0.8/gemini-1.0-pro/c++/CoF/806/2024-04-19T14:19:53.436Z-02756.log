    int i = N, j = M;
    while (i != 1 || j != 1) {
        path.push(make_pair(i, j));
        int prev = dp_prev[zone][i][j];
        if (prev == 1) {
            --i;
        } else if (prev == 2) {
            ++j;
        } else if (prev == 3) {
            ++i;
        } else if (prev == 4) {
            --j;
        }
    }
    path.push(make_pair(1, 1));

    out << zone << " " << max_sum << " " << path.size() - 1 << "\n";

    out.close();
}

int main() {
    solve();
    return 0;
}
