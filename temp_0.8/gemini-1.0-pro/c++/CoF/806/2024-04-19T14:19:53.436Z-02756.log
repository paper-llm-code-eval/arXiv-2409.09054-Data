#include <fstream>
#include <vector>
#include <climits>
#include <stack>
using namespace std;

const int MAX_N = 500;
int N, M, X, Y;
vector<int> G[MAX_N][MAX_N];
int dp[4][MAX_N][MAX_N];
int dp_prev[4][MAX_N][MAX_N];
int sum[4][MAX_N][MAX_N];
int quantity[MAX_N][MAX_N];

void solve() {
    ifstream in("cri.in");
    ofstream out("cri.out");

    in >> N >> M >> X >> Y;
    for (int i = 1; i <= N; ++i) {
        for (int j = 1; j <= M; ++j) {
            in >> quantity[i][j];
        }
    }
    in.close();

    for (int i = 1; i <= N; ++i) {
        for (int j = 1; j <= M; ++j) {
            if (i > 1) {
                G[1][i][j].push_back(1);
                G[1][i - 1][j].push_back(1);
            }
            if (j > 1) {
                G[1][i][j].push_back(2);
                G[1][i][j - 1].push_back(2);
            }
            if (i < N) {
                G[2][i][j].push_back(3);
                G[2][i + 1][j].push_back(3);
            }
            if (j < M) {
                G[2][i][j].push_back(4);
                G[2][i][j + 1].push_back(4);
            }
            if (i > 1) {
                G[3][i][j].push_back(1);
                G[3][i - 1][j].push_back(1);
            }
            if (j > 1) {
                G[3][i][j].push_back(3);
                G[3][i][j - 1].push_back(3);
            }
            if (i < N) {
                G[4][i][j].push_back(2);
                G[4][i + 1][j].push_back(2);
            }
            if (j < M) {
                G[4][i][j].push_back(4);
                G[4][i][j + 1].push_back(4);
            }
        }
    }

    for (int zone = 1; zone <= 4; ++zone) {
        for (int i = 1; i <= N; ++i) {
            for (int j = 1; j <= M; ++j) {
                dp[zone][i][j] = INT_MAX;
                sum[zone][i][j] = quantity[i][j];
                if (zone == 1 && i == 1 && j == 1) {
                    dp[zone][i][j] = 0;
                }
                if (zone == 2 && i == 1 && j == M) {
                    dp[zone][i][j] = 0;
                }
                if (zone == 3 && i == N && j == 1) {
                    dp[zone][i][j] = 0;
                }
                if (zone == 4 && i == N && j == M) {
                    dp[zone][i][j] = 0;
                }
            }
        }
    }

    for (int zone = 1; zone <= 4; ++zone) {
        for (int i = N; i >= 1; --i) {
            for (int j = M; j >= 1; --j) {
                for (int prev : G[zone][i][j]) {
                    int new_sum = sum[zone][i][j] + sum[prev][i][j];
                    int new_dp = dp[prev][i][j] + 1;
                    if (new_dp < dp[zone][i][j] || (new_dp == dp[zone][i][j] && new_sum > sum[zone][i][j])) {
                        dp[zone][i][j] = new_dp;
                        sum[zone][i][j] = new_sum;
                        dp_prev[zone][i][j] = prev;
                    }
                }
            }
        }
    }

    int zone = 0;
    int min_dp = INT_MAX;
    int max_sum = 0;
    for (int i = 1; i <= 4; ++i) {
        if (dp[i][N][M] < min_dp || (dp[i][N][M] == min_dp && sum[i][N][M] > max_sum)) {
            zone = i;
            min_dp = dp[i][N][M];
            max_sum = sum[i][N][M];
        }
    }

    stack<pair<int, int>> path;
    int i = N, j = M;
    while (i != 1 || j != 1) {
        path.push(make_pair(i, j));
        int prev = dp_prev[zone][i][j];
        if (prev == 1) {
            --i;
        } else if (prev == 2) {
            ++j;
        } else if (prev == 3) {
            ++i;
        } else if (prev == 4) {
            --j;
        }
    }
    path.push(make_pair(1, 1));

    out << zone << " " << max_sum << " " << path.size() - 1 << "\n";

    out.close();
}

int main() {
    solve();
    return 0;
}
