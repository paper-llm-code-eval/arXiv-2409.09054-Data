  in >> n >> m >> x >> y;
  for (int i = 1; i <= n; i++)
    for (int j = 1; j <= m; j++)
      in >> g[i][j];
  for (int i = 1; i <= n; i++)
    for (int j = 1; j <= m; j++) {
      dp[i][j][1] = g[i][j];
      dp[i][j][2] = g[i][j];
      dp[i][j][3] = g[i][j];
      dp[i][j][4] = g[i][j];
    }
  for (int i = 2; i <= n; i++)
    for (int j = 1; j < i; j++) {
      if (j <= y) {
        dp[i][j][1] = max(dp[i - 1][j][1], dp[i - 1][j][4]) + g[i][j];
        dp[i][j][3] = max(dp[i - 1][j][3], dp[i - 1][j][4]) + g[i][j];
      } else {
        dp[i][j][2] = max(dp[i - 1][j][2], dp[i - 1][j][1]) + g[i][j];
        dp[i][j][4] = max(dp[i - 1][j][2], dp[i - 1][j][3]) + g[i][j];
      }
    }
  for (int i = 2; i <= m; i++)
    for (int j = y + 1; j <= m; j++) {
      if (j <= y) {
        dp[i][j][1] = max(dp[i][j - 1][1], dp[i][j - 1][4]) + g[i][j];
        dp[i][j][3] = max(dp[i][j - 1][3], dp[i][j - 1][4]) + g[i][j];
      } else {
        dp[i][j][2] = max(dp[i][j - 1][2], dp[i][j - 1][1]) + g[i][j];
        dp[i][j][4] = max(dp[i][j - 1][2], dp[i][j - 1][3]) + g[i][j];
      }
    }
  for (int i = n - 1; i >= 2; i--)
    for (int j = i + 1; j <= m; j++) {
      if (j <= y) {
        dp[i][j][1] = max(dp[i + 1][j][1], dp[i + 1][j][4]) + g[i][j];
        dp[i][j][3] = max(dp[i + 1][j][3], dp[i + 1][j][4]) + g[i][j];
      } else {
        dp[i][j][2] = max(dp[i + 1][j][2], dp[i + 1][j][1]) + g[i][j];
        dp[i][j][4] = max(dp[i + 1][j][2], dp[i + 1][j][3]) + g[i][j];
      }
