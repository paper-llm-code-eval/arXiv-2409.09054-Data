        if (zona == 2 && (i == 1 || j == m))
            dp[i][j][zona] = max(dp[i][j][zona], dp[ii][jj][zona] + a[i][j]);
        if (zona == 3 && (i == n || j == 1))
            dp[i][j][zona] = max(dp[i][j][zona], dp[ii][jj][zona] + a[i][j]);
        if (zona == 4 && (i == n || j == m))
            dp[i][j][zona] = max(dp[i][j][zona], dp[ii][jj][zona] + a[i][j]);
    }
}

int main() {
    fin >> n >> m >> x >> y;
    for (int i = 1; i <= n; i++)
        for (int j = 1; j <= m; j++)
            fin >> a[i][j];

    DSU zone(n * m);
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            for (int k = 0; k < 4; k++) {
                int ii = i + dx[k];
                int jj = j + dy[k];
                if (ii < 1 || ii > n || jj < 1 || jj > m)
                    continue;
                zone.unite(i * m + j, ii * m + jj);
            }
        }
    }

    int z = 0, t = 0, k = 0;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            if (i == x && j == y)
                continue;
            int root = zone.find(i * m + j);
