    Edge(int u = 0, int v = 0, int w = 0) : u(u), v(v), w(w) {}
};

struct DSU {
    int *parinte, *rang;
    int n;

    DSU(int n) {
        this->n = n;
        parinte = new int[n + 1];
        rang = new int[n + 1];
        for (int i = 0; i <= n; i++) {
            parinte[i] = i;
            rang[i] = 0;
        }
    }

    int find(int x) {
        if (parinte[x] == x)
            return x;
        return parinte[x] = find(parinte[x]);
    }

    void unite(int x, int y) {
        int r1 = find(x);
        int r2 = find(y);
        if (r1 == r2)
            return;
        if (rang[r1] < rang[r2])
            swap(r1, r2);
        parinte[r2] = r1;
        if (rang[r1] == rang[r2])
            rang[r1]++;
    }
};

