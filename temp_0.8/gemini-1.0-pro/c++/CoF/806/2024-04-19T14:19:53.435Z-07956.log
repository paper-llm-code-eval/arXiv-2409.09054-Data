#include <cstdio>
#include <cstring>
#include <algorithm>

const int maxn = 500 + 3;
const int maxm = 500 + 3;
const int inf = 0x3f3f3f3f;

int n, m, x, y;
int h[maxn][maxm], cost1[maxn][maxm], cost2[maxn][maxm];
bool viz[maxn][maxm];

int dx[] = {0, 0, -1, 1};
int dy[] = {1, -1, 0, 0};

bool valid(int x, int y) { return x >= 1 && x <= n && y >= 1 && y <= m; }

void flood_fill(int x, int y, int cost, int **b) {
    if (!valid(x, y))
        return;
    if (viz[x][y])
        return;
    viz[x][y] = true;
    b[x][y] = cost;
    for (int i = 0; i < 4; i++)
        flood_fill(x + dx[i], y + dy[i], cost + 1, b);
}

int main() {
    freopen("cri.in", "r", stdin);
    freopen("cri.out", "w", stdout);

    scanf("%d%d%d%d", &n, &m, &x, &y);
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            scanf("%d", &h[i][j]);
        }
    }

    memset(cost1, inf, sizeof cost1);
    flood_fill(1, 1, 0, cost1);
    memset(cost2, inf, sizeof cost2);
    flood_fill(n, m, 0, cost2);

    int ans = -inf, ans_x, ans_y;
    for (int i = x; i <= n; i++) {
        for (int j = y; j <= m; j++) {
            if (cost1[i][j] + cost2[i][j] < ans)
                continue;
            int sum = 0, cnt = 0;
            for (int ii = 1; ii <= i; ii++) {
                for (int jj = 1; jj <= j; jj++) {
                    sum += h[ii][jj];
                    cnt++;
                }
            }
            for (int ii = x; ii <= i; ii++) {
                for (int jj = y; jj <= j; jj++) {
                    if (cost1[ii][jj] + cost2[i][j] == cost1[i][j] + cost2[ii][jj]) {
                        sum -= h[ii][jj];
                        cnt--;
                    }
                }
            }
            if (cost1[i][j] + cost2[i][j] > ans || (cost1[i][j] + cost2[i][j] == ans && cnt < ans_x) ||
                (cost1[i][j] + cost2[i][j] == ans && cnt == ans_x && sum > ans_y)) {
                ans = cost1[i][j] + cost2[i][j];
                ans_x = cnt;
                ans_y = sum;
            }
        }
    }
    int zona = 1;
    if (ans_x == cost1[x][y])
        zona = 1;
    else if (ans_x == cost1[x][m])
        zona = 2;
    else if (ans_x == cost1[n][y])
        zona = 3;
    else if (ans_x == cost1[n][m])
        zona = 4;
    else
        zona = -1;
    printf("%d %d %d", zona, ans_y, ans_x);
    return 0;
}
