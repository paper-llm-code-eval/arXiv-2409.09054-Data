#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAXN = 505;

ifstream fin("cri.in");
ofstream fout("cri.out");

int n, m, x, y;
int X[4][MAXN][MAXN];
int dist[4][MAXN][MAXN];
int sol[4], sol2[4];
int viz[MAXN][MAXN];
int dx[] = {0, 0, 1, -1};
int dy[] = {1, -1, 0, 0};

void bfs(int nr) {
    for (int i = 1; i <= n; ++i)
        for (int j = 1; j <= m; ++j) {
            dist[nr][i][j] = -1;
            viz[i][j] = 0;
        }

    dist[nr][x][y] = 0;

    int q[n * m * 2], fr = 0, bg = 0;

    q[bg++] = x;
    q[bg++] = y;
    viz[x][y] = 1;

    while (fr < bg) {
        int x = q[fr++];
        int y = q[fr++];

        for (int k = 0; k < 4; ++k) {
            int xx = x + dx[k];
            int yy = y + dy[k];

            if (xx < 1 || xx > n || yy < 1 || yy > m || viz[xx][yy] || X[nr][xx][yy] == -1)
                continue;

            dist[nr][xx][yy] = dist[nr][x][y] + 1;
            viz[xx][yy] = 1;
            q[bg++] = xx;
            q[bg++] = yy;
        }
    }

    sol[nr] = 0;
    sol2[nr] = 0;

    for (int i = 1; i <= n; ++i)
        for (int j = 1; j <= m; ++j) {
            sol[nr] = max(sol[nr], dist[nr][i][j]);
            sol2[nr] += min(dist[nr][i][j], dist[nr][i][j] + X[nr][i][j]);
        }
}
int main() {
    fin >> n >> m >> x >> y;

    for (int i = 1; i <= n; ++i)
        for (int j = 1; j <= m; ++j)
            fin >> X[1][i][j];

    for (int i = 1; i <= n; ++i)
        for (int j = 1; j <= m; ++j)
            fin >> X[2][i][j];

    for (int i = 1; i <= n; ++i)
        for (int j = 1; j <= m; ++j)
            fin >> X[3][i][j];

    for (int i = 1; i <= n; ++i)
        for (int j = 1; j <= m; ++j)
            fin >> X[4][i][j];

    for (int i = 1; i <= 4; ++i) {
        bfs(i);
    }

    int ans1 = -1, ans2 = -1, ans3 = -1;

    for (int i = 1; i <= 4; ++i) {
        if (ans1 == -1 || sol[i] > sol[ans1] || (sol[i] == sol[ans1] && sol2[i] < sol2[ans1])) {
            ans1 = i;
            ans2 = sol[i];
            ans3 = sol2[i];
        }
    }

    fout << ans1 << " " << ans2 << " " << ans3 << "\n";
    return 0;
}
