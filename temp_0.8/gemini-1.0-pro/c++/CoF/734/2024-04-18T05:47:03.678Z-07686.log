#include <fstream>
#include <cassert>
#include <queue>

using namespace std;

int n, m, sol_x, sol_y, sol_t;
char harta[100][100];
int dist[100][100];
int dx[] = {-1, -1, -1, 0, 0, 1, 1, 1};
int dy[] = {-1, 0, 1, -1, 1, -1, 0, 1};

class point {
public:
  int x, y, t;

  bool operator<(const point &other) const {
    return t > other.t;
  }
};

void read_data() {
  ifstream f("rj.in");
  f >> n >> m;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      f >> harta[i][j];
      if (harta[i][j] == 'R') {
        dist[i][j] = 0;
      } else {
        dist[i][j] = -1;
      }
    }
  }
  f.close();
}

void print_data() {
  ofstream f("rj.out");
  f << sol_t << " " << sol_x << " " << sol_y << endl;
  f.close();
}

int main() {
  read_data();
  priority_queue<point> q;
  q.push({1, 1, 0});
  while (!q.empty()) {
    point u = q.top();
    q.pop();
    if (harta[u.x][u.y] == 'J') {
      sol_t = u.t;
      sol_x = u.x;
      sol_y = u.y;
      break;
    }
    for (int i = 0; i < 8; i++) {
      int x = u.x + dx[i];
      int y = u.y + dy[i];
      if (x >= 1 && x <= n && y >= 1 && y <= m && harta[x][y] != 'X' &&
          dist[x][y] == -1) {
        dist[x][y] = u.t + 1;
        q.push({x, y, u.t + 1});
      }
    }
  }
  print_data();
  assert(sol_x != 0 && sol_y != 0);
  return 0;
}
