  for (int i = 1; i <= n; ++i) {
    fin >> (a[i] + 1);
  }

  for (int i = 1; i <= n; ++i) {
    for (int j = 1; j <= m; ++j) {
      if (a[i][j] == 'R') {
        q.push({i, j, 0});
        d[i][j] = 0;
      } else if (a[i][j] == 'J') {
        d[i][j] = -1;
      } else {
        d[i][j] = INT_MAX;
      }
    }
  }

  int dx[] = {-1, -1, -1, 0, 0, 1, 1, 1};
  int dy[] = {-1, 0, 1, -1, 1, -1, 0, 1};

  while (!q.empty()) {
    Point p = q.front();
    q.pop();

    if (d[p.x][p.y] != p.t) {
      continue;
    }

    for (int k = 0; k < 8; ++k) {
      int x = p.x + dx[k];
      int y = p.y + dy[k];

      if (x < 1 || x > n || y < 1 || y > m) {
        continue;
      }

      if (a[x][y] == 'X') {
        continue;
      }

      if (d[x][y] > p.t + 1) {
        d[x][y] = p.t + 1;
        q.push({x, y, p.t + 1});
      }
    }
  }

  int tmin = INT_MAX;
  int x = 0, y = 0;

  for (int i = 1; i <= n; ++i) {
    for (int j = 1; j <= m; ++j) {
      if (d[i][j] != -1 && d[i][j] < tmin) {
        tmin = d[i][j];
        x = i;
        y = j;
      }
    }
  }

  if (tmin == INT_MAX) {
    fout << "-1\n";
  } else {
    fout << x << " " << y << " " << tmin << "\n";
  }

  return 0;
}
