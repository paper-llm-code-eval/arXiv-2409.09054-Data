#include <queue>
#include <utility>

using namespace std;

ifstream fin("ubuntzei.in");
ofstream fout("ubuntzei.out");

const int NMAX = 2005;

vector<pair<int, int>> graf[NMAX];
int dist[NMAX];

int main() {
  int n, m, k;
  fin >> n >> m >> k;
  vector<int> prieteni(k + 1);
  for (int i = 0; i <= k; ++i) {
    fin >> prieteni[i];
  }
  for (int i = 0; i < m; ++i) {
    int x, y, z;
    fin >> x >> y >> z;
    graf[x].push_back({y, z});
    graf[y].push_back({x, z});
  }
  for (int i = 0; i <= n; ++i) {
    dist[i] = 1e9;
  }
  priority_queue<pair<int, int>, vector<pair<int, int>>, greater<pair<int, int>>> pq;
  pq.push({0, 1});
  dist[1] = 0;
  while (!pq.empty()) {
    int nod, cost;
    tie(cost, nod) = pq.top();
    pq.pop();
    if (cost > dist[nod]) {
      continue;
    }
    for (auto vec : graf[nod]) {
      int vecin, muchie;
      tie(vecin, muchie) = vec;
      if (dist[vecin] > cost + muchie) {
        dist[vecin] = cost + muchie;
        pq.push({dist[vecin], vecin});
      }
    }
