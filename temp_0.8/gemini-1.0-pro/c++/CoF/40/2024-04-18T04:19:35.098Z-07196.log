#include <fstream>
#include <vector>
#include <queue>

using namespace std;

const int maxn = 2000;
const int inf = 1e9;
vector<pair<int, int>> g[maxn + 5];
int n, m, k, dist[maxn + 5];

int main() {
    ifstream fin("ubuntzei.in");
    ofstream fout("ubuntzei.out");

    fin >> n >> m;
    fin >> k;
    vector<int> loc(k);
    for (int i = 0; i < k; ++i) {
        fin >> loc[i];
    }

    for (int i = 1; i <= m; ++i) {
        int x, y, z;
        fin >> x >> y >> z;
        g[x].push_back({y, z});
        g[y].push_back({x, z});
    }

    for (int i = 1; i <= n; ++i) {
        dist[i] = inf;
    }

    priority_queue<pair<int, int>> pq;
    dist[1] = 0;
    pq.push({0, 1});

    while (!pq.empty()) {
        int node = pq.top().second;
        int cost = -pq.top().first;
        pq.pop();

        if (dist[node] < cost) {
            continue;
        }

        for (auto& edge : g[node]) {
            int to = edge.first;
            int w = edge.second;
            if (dist[to] > cost + w) {
                dist[to] = cost + w;
                pq.push({-dist[to], to});
            }
        }
    }

    int ans = inf;
    for (int i = 0; i < k; ++i) {
        ans = min(ans, dist[loc[i]]);
    }

    fout << ans + dist[n] << "\n";

    return 0;
}
