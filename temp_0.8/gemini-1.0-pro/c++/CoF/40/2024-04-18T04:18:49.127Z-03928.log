#include <vector>
#include <queue>
#include <fstream>

using namespace std;

typedef long long ll;

const ll inf = LLONG_MAX;
const int maxK = 15;
const int maxN = 2000;
const int maxM = 10000;

int main() {
    ifstream fin("ubuntzei.in");
    ofstream fout("ubuntzei.out");

    int n, m, k;
    fin >> n >> m >> k;
    vector<int> locuri_prieteni(k);
    for (int i = 0; i < k; i++) {
        fin >> locuri_prieteni[i];
    }

    vector<vector<pair<int, ll>>> g(n + 1);
    for (int i = 0; i < m; i++) {
        int x, y, w;
        fin >> x >> y >> w;
        g[x].push_back(make_pair(y, w));
        g[y].push_back(make_pair(x, w));
    }

    vector<vector<ll>> d(n + 1, vector<ll>(1 << maxK, inf));
    d[1][0] = 0;
    priority_queue<pair<ll, pair<int, int>>> pq;
    pq.push(make_pair(-d[1][0], make_pair(1, 0)));
    while (!pq.empty()) {
        ll dist = -pq.top().first;
        int nod = pq.top().second.first;
        int masca = pq.top().second.second;
        pq.pop();
        if (dist > d[nod][masca]) {
            continue;
        }
        if (nod == n) {
            fout << dist << "\n";
            return 0;
        }
        for (const auto& vec : g[nod]) {
            int vecin = vec.first;
            ll cost = vec.second;
            int noua_masca = masca;
            for (int i = 0; i < k; i++) {
                if (locuri_prieteni[i] == vecin) {
                    noua_masca |= (1 << i);
                    break;
                }
            }
            if (d[vecin][noua_masca] > dist + cost) {
                d[vecin][noua_masca] = dist + cost;
                pq.push(make_pair(-d[vecin][noua_masca], make_pair(vecin, noua_masca)));
            }
        }
    }
    fout << "-1\n";
    return 0;
}
