#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>
#define pb push_back
#define pi pair<int, int>
using namespace std;
int n, m, k, c[15], dist[2001];
bool viz[2001];
vector<pi> G[2001];
int main() {
  ifstream fin("ubuntzei.in");
  ofstream fout("ubuntzei.out");
  fin >> n >> m >> k;
  for (int i = 1; i <= k; i++) {
    fin >> c[i];
    viz[c[i]] = 1;
  }
  for (int i = 1; i <= m; i++) {
