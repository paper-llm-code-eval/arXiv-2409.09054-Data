    int distanta = -pq.top().first;
    int nod = pq.top().second;
    pq.pop();
    if (viz[nod]) continue;
    viz[nod] = 1;
    dist[nod] = distanta;
    for (auto &vecin : G[nod]) {
      int cost = vecin.second;
      int vecinul = vecin.first;
      if (!viz[vecinul] || viz[vecinul] && dist[nod] + cost < dist[vecinul]) {
        pq.push({-(dist[nod] + cost), vecinul});
      }
    }
  }
  fout << dist[n] << "\n";
  return 0;
}
