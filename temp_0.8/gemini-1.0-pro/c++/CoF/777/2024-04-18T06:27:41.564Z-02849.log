#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream in("virus.in");
  ofstream out("virus.out");

  int n;
  in >> n;

  vector<pair<int, int>> intervals(n);
  for (int i = 0; i < n; i++) {
    in >> intervals[i].first >> intervals[i].second;
  }

  sort(intervals.begin(), intervals.end());

  int max_duration = 0;
  int max_gap = 0;
  int current_duration = 0;

  for (int i = 0; i < n; i++) {
    if (intervals[i].first > intervals[i - 1].second + 1) {
      max_gap = max(max_gap, intervals[i].first - intervals[i - 1].second - 1);
    }
    current_duration = max(current_duration, intervals[i].second - intervals[i].first + 1);
    max_duration = max(max_duration, current_duration);
  }

  out << max_duration << " " << max_gap << endl;

  return 0;
}
```
