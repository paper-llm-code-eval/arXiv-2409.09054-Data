#include <fstream>

using namespace std;

const int MAX_N = 10005;

int sums[MAX_N];

int main() {
    ifstream fin("numere.in");
    ofstream fout("numere.out");

    int t;
    fin >> t;

    int sum = 0;
    for (int i = 1; i < MAX_N; i++) {
        sums[i] = sum;
        sum += i;
    }

    if (t == 1) {
        int n;
        fin >> n;

        int x;
        for (int i = 0; i < n; i++) {
            fin >> x;
            if (x == 1)
                sum -= i;
            else
                sum += i;
        }

        fout << sums[sum] << '\n';
    } else {
        int k;
        fin >> k;

        sum = 0;
        for (int i = 1; i < MAX_N; i++) {
            if (sums[i] <= k) {
                int x = k - sums[i];

                if (x >= 0 && x < i)
                    sum = max(sum, sums[i] + x);
            }
        }

        fout << sum << '\n';
    }

    return 0;
}
