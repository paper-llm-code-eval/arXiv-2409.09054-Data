#include <fstream>
#include <vector>
#include <cmath>
#include <algorithm>

using namespace std;

ifstream fin("numere.in");
ofstream fout("numere.out");

const int MAXN = 100005;
vector<vector<int>> v(MAXN);
vector<int> sum(MAXN);

void build() {
    int last = 1;
    v[1].push_back(1);
    sum[1] = 1;
    for (int i = 2; i < MAXN; i++) {
        int sz = v[i - 1].size();
        for (int j = 0; j < sz; j++) {
            v[i].push_back(last + 1);
            sum[last + 1] = sum[last] + last + 1;
            last++;
        }
    }
}

void solve_cerinta_1() {
    int n;
    fin >> n;
    vector<int> path(n);
    for (int i = 0; i < n; i++) {
        fin >> path[i];
    }
    int curr = 1, i = 0;
    while (i < n) {
        if (path[i] == 1) {
            curr = v[curr][i];
        } else {
            curr = v[curr][i + 1];
            i++;
        }
        i++;
    }
    fout << curr << '\n';
}

void solve_cerinta_2() {
    int k;
    fin >> k;
    int ans = 0;
    for (int i = 1; i <= 100000; i++) {
        if (sum[i] <= k) {
            ans = max(ans, sum[i]);
        }
    }
    fout << ans << '\n';
}

int main() {
    int c;
    fin >> c;
    build();
    if (c == 1) {
        solve_cerinta_1();
    } else {
        solve_cerinta_2();
    }
    return 0;
}
