#include <fstream>
#include <vector>
using namespace std;

constexpr int kMaxN = 10000;
constexpr int kMaxK = 100000;
constexpr int kInf = 0x3f3f3f3f;
int a[kMaxN + 1];
int dp[kMaxN + 1][kMaxK + 1];

int main() {
  ifstream in("numere.in");
  ofstream out("numere.out");
  int c, n, k;
  in >> c;
  if (c == 1) {
    in >> n;
    for (int i = 1; i <= n; ++i) {
      int x;
      in >> x;
      a[i] = a[i - 1] + (x == 1 ? 1 : 2);
    }
    out << a[n] << "\n";
  } else {
    in >> k;
    for (int i = 1; i <= k; ++i) { dp[0][i] = -kInf; }
    for (int i = 1; i <= 10000; ++i) {
      for (int j = 1; j <= k; ++j) {
        dp[i][j] = max(dp[i - 1][j], max(dp[i - 1][j - 1] + i, dp[i - 1][j - 2] + i));
      }
    }
    int ans = 0;
    for (int i = 1; i <= k; ++i) { ans = max(ans, dp[10000][i]); }
    out << ans << "\n";
  }
  return 0;
}
