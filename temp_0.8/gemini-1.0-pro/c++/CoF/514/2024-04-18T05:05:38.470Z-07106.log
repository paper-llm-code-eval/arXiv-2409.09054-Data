  for (; pos <= n; pos += pos & -pos) s[pos] += val;
}

ll query(int pos) {
  ll sum = 0;
  for (; pos > 0; pos -= pos & -pos) sum += s[pos];
  return sum;
}

void add(int pos, ll val) {
  update(pos, val);
  update(pos + 1, -val);
}

ll sum(int l, int r) {
  return query(r) - query(l - 1);
}

int main() {
  freopen("primprim.in", "r", stdin);
  freopen("primprim.out", "w", stdout);

  int c;
  cin >> c;
  cin >> n;
  for (int i = 1; i <= n; i++) {
    cin >> v[i];
    p[i] = v[i];
  }
  sieve();
  if (c == 1) {
    for (int i = 1; i <= n; i++) {
      if (is_prime[v[i]])
        cost[i] = 0;
      else {
        int prv = -1, nxt = maxn;
        for (int j = v[i] - 1; j >= 2; j--) {
          if (is_prime[j]) {
            prv = j;
            break;
          }
        }
        for (int j = v[i] + 1; j <= maxn; j++) {
          if (is_prime[j]) {
            nxt = j;
            break;
          }
        }
        cost[i] = min(v[i] - prv, nxt - v[i]);
      }
      add(i, cost[i]);
    }
    cout << sum(1, n) << endl;
  } else {
    cin >> q;
    for (int i = 1; i <= q; i++) {
      int t, x, p;
      cin >> t >> x >> p;
      add(t, -cost[t]);
      v[t] = x;
      if (is_prime[v[t]])
        cost[t] = 0;
      else {
        int prv = -1, nxt = maxn;
        for (int j = v[t] - 1; j >= 2; j--) {
