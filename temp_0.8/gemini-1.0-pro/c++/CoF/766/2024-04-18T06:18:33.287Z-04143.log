#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream in("afise.in");
ofstream out("afise.out");

const int MAX_N = 1024;
int n, k, l;
bool v[MAX_N];

int main() {
  in >> l >> n >> k;
  for (int i = 1; i <= n; i++) {
    int x;
    in >> x;
    v[x] = true;
  }
  vector<int> intervals;
  int li = -1;
  for (int i = 1; i <= l; i++) {
    if (!v[i]) {
      if (li == -1) {
        li = i;
      }
    } else {
      if (li != -1) {
        intervals.push_back(i - li);
      }
      li = -1;
    }
  }
  if (li != -1) {
    intervals.push_back(l - li + 1);
  }

  int ans = 0, cnt = 0;
  sort(intervals.begin(), intervals.end());
  for (int i = 0; i < intervals.size() && cnt < k; i++) {
    ans += intervals[i];
    cnt++;
  }

  out << ans << ' ' << cnt << '\n';
  return 0;
}
```
