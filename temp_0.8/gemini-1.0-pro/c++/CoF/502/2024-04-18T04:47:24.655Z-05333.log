#include <vector>
#include <queue>
#include <iostream>
#include <fstream>
#include <climits>
#include <array>

template <typename T>
using vi = std::vector<T>;

void dfs1(int node, vi<int>& parent, vi<vi<int>>& graph, vi<int>& depth) {
    for (const auto& next : graph[node]) {
        if (parent[next] != -1)
            continue;
        parent[next] = node;
        depth[next] = depth[node] + 1;
        dfs1(next, parent, graph, depth);
    }
}

std::pair<int, vi<int>> dfs2(int node, int dest, vi<vi<int>>& graph, vi<int>& depth) {
    if (node == dest)
        return { 0, { node } };

    std::pair<int, vi<int>> ans = { INT_MAX, vi<int>() };
    for (const auto& next : graph[node]) {
        if (depth[next] <= depth[node])
            continue;
        auto next_ans = dfs2(next, dest, graph, depth);
        if (next_ans.first < INT_MAX) {
            next_ans.first++;
            next_ans.second.insert(next_ans.second.begin(), node);
            ans = std::min(ans, next_ans);
        }
    }
    return ans;
}

int main() {
    std::ifstream fin("veri.in", std::ios::in | std::ios::binary);
    std::ofstream fout("veri.out", std::ios::out | std::ios::binary);

    int c; fin >> c;
    int n, m; fin >> n >> m;
    int s, a, b; fin >> s >> a >> b;

    vi<int> parent(n + 1, -1);
    parent[s] = 0;
    vi<vi<int>> graph(n + 1, vi<int>());
    vi<int> depth(n + 1, 0);
    for (int i = 0; i < m; i++) {
        int x, y; fin >> x >> y;
        graph[x].push_back(y);
    }

    dfs1(s, parent, graph, depth);

    int z = -1;
    std::pair<int, vi<int>> path_to_z = { INT_MAX, vi<int>() };
    for (int i = 1; i <= n; i++) {
        if (parent[i] == -1)
            continue;
        auto path_to_i = dfs2(i, s, graph, depth);
        if (path_to_i.first < path_to_z.first) {
            path_to_z = path_to_i;
            z = i;
        }
    }

    if (c == 1) {
        std::pair<int, vi<int>> path_to_a = dfs2(z, a, graph, depth);
        std::pair<int, vi<int>> path_to_b = dfs2(z, b, graph, depth);
        fout << std::max(path_to_a.first, path_to_b.first) + path_to_z.first << '\n';
    } else {
        fout << path_to_z.first << '\n';
        for (const auto& node : path_to_z.second)
            fout << node << ' ';
        fout << '\n';
        std::pair<int, vi<int>> path_to_a = dfs2(z, a, graph, depth);
        std::pair<int, vi<int>> path_to_b = dfs2(z, b, graph, depth);
        fout << path_to_a.first << '\n';
        for (const auto& node : path_to_a.second)
