#include <fstream>
#include <iostream>
#include <queue>
#include <vector>
using namespace std;

struct point {
  int x, y;
};

int n, m, dist[176][176];
bool viz[176][176];
vector<point> v;
int dx[] = {1, -1, 0, 0};
int dy[] = {0, 0, 1, -1};

int bfs(point start, point end) {
  queue<point> q;
  q.push(start);
  dist[start.x][start.y] = 0;
  viz[start.x][start.y] = true;
  while (!q.empty()) {
    point u = q.front();
    q.pop();
    if (u.x == end.x && u.y == end.y) return dist[u.x][u.y];
    for (int i = 0; i < 4; i++) {
      int x = u.x + dx[i];
      int y = u.y + dy[i];
      if (x >= 1 && x <= n && y >= 1 && y <= n && !viz[x][y] && v[(x - 1) * n + y - 1].x != 0) {
        q.push(point{x, y});
        dist[x][y] = dist[u.x][u.y] + 1;
        viz[x][y] = true;
      }
    }
  }
  return -1;
}

int main() {
  ifstream fin("alee.in");
  ofstream fout("alee.out");
  fin >> n >> m;
  v.resize(n * n + 1);
  for (int i = 0; i < n * n + 1; i++) v[i].x = v[i].y = 0;
  for (int i = 0; i < m; i++) {
    int x, y;
    fin >> x >> y;
    v[(x - 1) * n + y - 1].x = x;
    v[(x - 1) * n + y - 1].y = y;
  }
  int x1, y1, x2, y2;
  fin >> x1 >> y1 >> x2 >> y2;
  fout << bfs({x1, y1}, {x2, y2}) << "\n";
  return 0;
}
