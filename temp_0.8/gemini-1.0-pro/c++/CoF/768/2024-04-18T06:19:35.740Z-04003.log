#include <fstream>
#include <vector>
#include <queue>
#include <utility>

using namespace std;
const int INF = 1 << 30;

int m, n, x1, y1, x2, y2;
vector<vector<int>> g, c;
vector<vector<bool>> viz;

void read() {
  ifstream fin("alee.in", ios::in);
  fin >> n >> m >> x1 >> y1 >> x2 >> y2;
  fin.close();
  c.assign(n + 1, vector<int>(n + 1, 0));
  g.assign(n + 1, vector<int>(n + 1, INF));
  viz.assign(n + 1, vector<bool>(n + 1, false));
}

int di[] = {-1, 0, 1, 0};
int dj[] = {0, 1, 0, -1};

void bfs() {
  queue<pair<int, int>> q;
  q.push({x1, y1});
  c[x1][y1] = 0;
  viz[x1][y1] = true;
  while (!q.empty()) {
    int i = q.front().first;
    int j = q.front().second;
    if (i == x2 && j == y2) break;
    for (int k = 0; k < 4; ++k) {
      int ii = i + di[k];
      int jj = j + dj[k];
      if (ii > 0 && ii <= n && jj > 0 && jj <= n && !viz[ii][jj] &&
          g[ii][jj] != INF) { // nu exista copac
        q.push({ii, jj});
        c[ii][jj] = c[i][j] + 1;
        viz[ii][jj] = true;
      }
    }
    q.pop();
  }
}

void write() {
  ofstream fout("alee.out", ios::out);
  fout << c[x2][y2] << '\n';
  fout.close();
}

int main() {
  read();
  for (int i = 0; i < m; ++i) {
    int x, y;
    ifstream fin("alee.in", ios::in);
    fin >> x >> y;
    g[x][y] = INF;
    fin.close();
  }
  bfs();
  write();
  return 0;
}
