#include <iostream>
#include <fstream>

using namespace std;

int main() {
  ifstream in("sss.in");
  ofstream out("sss.out");

  int c, n;
  in >> c >> n;

  int v[100001], ok = 0;
  long long sum = 0;
  for (int i = 1; i <= n; i++) {
    in >> v[i];
    sum += v[i];
    if (v[i] % 10 != 0) ok = i;
  }

  if (c == 1) out << sum - v[ok];
  else {
    long long mx = 0, sumi = 0;
    for (int i = n; i >= 1; i--) {
      sumi += v[i];
      if (mx < sumi) mx = sumi;
      sumi -= v[i - (i - 1) * (i != n)];
    }
    out << mx;
  }

  return 0;
}
```
