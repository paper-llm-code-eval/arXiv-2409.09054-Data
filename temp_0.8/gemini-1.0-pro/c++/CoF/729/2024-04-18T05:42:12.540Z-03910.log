#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream fin("siruri.in");
  ofstream fout("siruri.out");

  int n;
  fin >> n;

  vector<int> x(n);
  for (int i = 0; i < n; i++) {
    fin >> x[i];
  }

  vector<int> y(n);
  int curr = 1;
  for (int i = 0; i < n; i++) {
    for (int j = i + 1; j < n; j++) {
      if (x[i] < x[j]) {
        y[i] = curr++;
      }
    }
  }

  for (int i = n - 1; i >= 0; i--) {
    if (y[i] == 0) {
      y[i] = curr++;
    }
  }

  for (int i = 0; i < n; i++) {
    fout << y[i] << " ";
  }
  fout << endl;

  return 0;
}
```
