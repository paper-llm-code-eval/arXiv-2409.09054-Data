#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream in("siruri.in");
  ofstream out("siruri.out");
  int n;
  in >> n;
  vector<int> x(n);
  for (int i = 0; i < n; i++) in >> x[i];

  vector<pair<int, int>> y(n);
  for (int i = 0; i < n; i++) y[i] = make_pair(x[i], i);

  sort(y.begin(), y.end());

  vector<int> ord(n);
  for (int i = 0; i < n; i++) ord[y[i].second] = i + 1;

  for (int i = 0; i < n; i++) out << ord[i] << " ";

  return 0;
}
