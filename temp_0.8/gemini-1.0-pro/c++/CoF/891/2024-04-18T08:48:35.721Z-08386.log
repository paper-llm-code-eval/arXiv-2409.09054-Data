#include <fstream>
#include <algorithm>
#include <vector>

using namespace std;

const int maxn = 1000;

int n, m;
int a[maxn][maxn];
int dp[maxn][maxn][2];

int main() {
    ifstream fin("fadema.in");
    ofstream fout("fadema.out");

    fin >> n >> m;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            fin >> a[i][j];
        }
    }

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (i == 0) {
                dp[i][j][0] = 1 - a[i][j];
                dp[i][j][1] = a[i][j];
            } else {
                dp[i][j][0] = max(dp[i - 1][j][0], dp[i - 1][j][1] + 1 - a[i][j]);
                dp[i][j][1] = max(dp[i - 1][j][0] + a[i][j], dp[i - 1][j][1]);
            }
        }
    }

    int ans = 0;
    for (int i = 0; i < m; i++) {
        ans = max(ans, dp[n - 1][i][0]);
        ans = max(ans, dp[n - 1][i][1]);
    }

    fout << ans << '\n';
    return 0;
}
```
