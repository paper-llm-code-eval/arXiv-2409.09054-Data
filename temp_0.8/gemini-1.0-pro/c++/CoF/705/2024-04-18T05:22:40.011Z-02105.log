#include <fstream>
using namespace std;
ifstream fin("panglica.in");
ofstream fout("panglica.out");

const int MAXN = 10005;
const int MAXC = 205;

int a[MAXN];
int l[MAXN][MAXC], r[MAXN][MAXC];

int main() {
  int n, c;
  fin >> n >> c;
  for (int i = 1; i <= n; i++) {
    fin >> a[i];
  }
  for (int i = 1; i <= n; i++) {
    l[i][a[i]]++;
  }
  for (int j = 1; j <= c; j++) {
    for (int i = 1; i <= n; i++) {
      l[i][j] += l[i - 1][j];
    }
  }
  for (int i = n; i >= 1; i--) {
    r[i][a[i]]++;
  }
  for (int j = 1; j <= c; j++) {
    for (int i = n; i >= 1; i--) {
      r[i][j] += r[i + 1][j];
    }
  }
  int st = 0, dr = 0, maxlen = 0, cul = 0;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= c; j++) {
      if (l[i - 1][j] + r[i + 1][j] > maxlen) {
        maxlen = l[i - 1][j] + r[i + 1][j];
        st = l[i - 1][j];
        dr = r[i + 1][j];
        cul = j;
      }
    }
  }
  fout << n - st - dr << '\n' << cul << '\n' << st << '\n' << dr << '\n';
  return 0;
}
