#include <cstdio>
#include <vector>

using namespace std;

#define FOR(i, a, b) for (int i = a; i <= b; i++)
#define REP(i, a, b) for (int i = a; i < b; i++)

const int MAX_N = 200005;
const int MAX_P = 100005;

int n, p, c, start[MAX_P], end[MAX_P], cost[MAX_P];
int x[MAX_N], f[MAX_N];

int main() {
  FILE *fin = fopen("hibrid.in", "r");
  FILE *fout = fopen("hibrid.out", "w");
  int x;
  fscanf(fin, "%d%d%d", &c, &p, &n);
  fscanf(fin, "%d%d%d", &start[1], &end[1], &cost[1]);
  fscanf(fin, "%d%d%d", &start[2], &end[2], &cost[2]);
  FOR(i, 3, p) { fscanf(fin, "%d%d%d", &start[i], &end[i], &cost[i]); }
  FOR(i, 1, n) { fscanf(fin, "%d", &x); }
  fclose(fin);
  int cnt = 0, maxCnt = 0, minId = -1, sum = 0;
  FOR(i, 1, n - 1) {
    int st = x[i], dr = x[i + 1];
    REP(j, 1, p) {
      if (st <= end[j] && dr >= start[j]) {
        cnt++;
        sum += cost[j];
        if (cnt > maxCnt) { maxCnt = cnt; }
        if (maxCnt == cnt) { minId = j; }
      }
    }
  }
  if (c == 1) {
    if (!maxCnt) fputs("-1", fout);
    else fprintf(fout, "%d\n", minId);
  } else
    fprintf(fout, "%d\n", sum);
  fclose(fout);
  return 0;
}
