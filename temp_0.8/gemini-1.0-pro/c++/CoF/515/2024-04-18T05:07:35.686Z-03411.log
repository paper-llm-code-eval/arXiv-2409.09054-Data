#include <iostream>
#include <fstream>
#include <vector>

using namespace std;

struct Interval {
    int start, end, cost;
};

struct Query {
    int pos, id;
};

const int MAXN = 200005;
const int MAXP = 100005;
const int MAXVAL = 300001;

int n, p, c;
vector<Interval> intervals;
vector<Query> queries;
int ans[MAXN];
int cnt[MAXVAL];

int main() {
    ifstream fin("hibrid.in");
    ofstream fout("hibrid.out");

    fin >> c >> p >> n;

    for (int i = 0; i < p; ++i) {
        Interval interval;
        fin >> interval.start >> interval.end >> interval.cost;
        intervals.push_back(interval);
    }

    for (int i = 0; i < n; ++i) {
        int x;
        fin >> x;
        queries.push_back({x, i});
    }

    sort(queries.begin(), queries.end(), [](const Query &a, const Query &b) {
        return a.pos < b.pos;
    });

    int active_idx = 0;
    int ans_idx = -1;
    int ans_cnt = 0;
    int ans_sum = 0;

    for (const Query &query : queries) {
        while (active_idx < p && intervals[active_idx].start <= query.pos) {
            int start = intervals[active_idx].start;
            int end = intervals[active_idx].end;
            int cost = intervals[active_idx].cost;

            if (start <= query.pos && query.pos <= end) {
                cnt[cost]++;
                ans_sum += cost;

                if (cnt[cost] > ans_cnt) {
                    ans_cnt = cnt[cost];
                    ans_idx = active_idx;
                }
            }

            active_idx++;
        }

        ans[query.id] = ans_idx;
    }

    if (c == 1) {
        fout << ans_idx + 1 << '\n';
    } else {
        fout << ans_sum << '\n';
    }

    return 0;
}
