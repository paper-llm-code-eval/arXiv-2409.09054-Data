            res.winner = 'V';
            res.stickers = victor;
        } else if (radu > victor) {
            res.winner = 'R';
            res.stickers = radu;
        } else {
            res.winner = 'V';
            res.winner += 'R';
            res.stickers = victor;
        }

        os << res.winner << "\n" << res.stickers;
    }

    return 0;
}
