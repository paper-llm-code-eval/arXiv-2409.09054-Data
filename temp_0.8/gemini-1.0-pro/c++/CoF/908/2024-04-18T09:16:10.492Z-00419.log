#include <algorithm>
#include <array>
#include <fstream>
#include <vector>

struct sticker {
    int code = -1;
    std::array<int, 10> digits;
};

struct result {
    char winner;
    int stickers;
};

int main() {
    std::ifstream is("album.in");
    std::ofstream os("album.out");

    int part;
    is >> part;
    int n;
    is >> n;

    std::vector<sticker> stickers(n);
    for (auto& sticker : stickers) {
        is >> sticker.code;

        int code = sticker.code;
        int digit = 0;
        while (code > 0) {
            int d = code % 10;
            code /= 10;
            sticker.digits[digit++] = d;
        }
    }

    std::sort(stickers.begin(), stickers.end(),
              [](const sticker& a, const sticker& b) {
                  return a.code < b.code;
              });

    if (part == 1) {
        std::stable_sort(stickers.rbegin(), stickers.rend(),
                         [](const sticker& a, const sticker& b) {
                             return std::max(a.digits[0], a.digits[1]) <
                                    std::max(b.digits[0], b.digits[1]);
                         });
        os << stickers.empty() ? 0 : stickers[0].digits[0];
        os << " ";
        os << stickers.empty() ? 0 : stickers[0].digits[1];
    } else {
        int victor = 0, radu = 0;
        for (auto& sticker : stickers) {
            if (victor > radu) {
                radu += sticker.code;
                for (int i = 0; i < 10; ++i) {
                    if (sticker.digits[i] ==
                        std::max(sticker.digits[0], sticker.digits[1]))
                        radu += sticker.digits[i];
                }
            } else {
                victor += sticker.code;
                for (int i = 0; i < 10; ++i) {
                    if (sticker.digits[i] ==
                        std::max(sticker.digits[0], sticker.digits[1]))
                        victor += sticker.digits[i];
                }
            }
        }

        result res;
        if (victor > radu) {
            res.winner = 'V';
            res.stickers = victor;
        } else if (radu > victor) {
            res.winner = 'R';
            res.stickers = radu;
        } else {
            res.winner = 'V';
            res.winner += 'R';
            res.stickers = victor;
        }

        os << res.winner << "\n" << res.stickers;
    }

    return 0;
}
