#include <fstream>
#include <vector>
#include <iostream>

using namespace std;

int main() {
    ifstream in("text.in");
    ofstream out("text.out");

    // citirea textului
    string text;
    while (!in.eof()) {
        string line;
        getline(in, line);
        text += line + '\n';
    }

    // impartirea textului in cuvinte
    vector<string> words;
    string word;
    for (char c : text) {
        if (c == ' ' || c == '\n') {
            if (!word.empty()) {
                words.push_back(word);
            }
            word.clear();
        } else {
            word += c;
        }
    }
    if (!word.empty()) {
        words.push_back(word);
    }

    // determinarea numarului minim de cuvinte de eliminat
    int min_removals = 0;
    for (int i = 0; i < words.size() - 1; i++) {
        if (words[i].back() != words[i + 1].front()) {
            min_removals++;
        }
    }

    // afisarea numarului minim de cuvinte de eliminat si a numarului de cuvinte din text
    out << min_removals << endl;
    out << words.size() << endl;

    // afisarea cuvintelor ramase
    for (string word : words) {
        if (min_removals > 0 && word.back() != words[0].front()) {
            min_removals--;
        } else {
            out << word << endl;
        }
    }

    return 0;
}
