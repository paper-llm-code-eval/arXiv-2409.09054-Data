#include <fstream>
#include <vector>
#include <algorithm>

int main() {
    std::ifstream in("text.in");
    std::ofstream out("text.out");

    std::vector<std::string> words;
    std::string word;
    while (in >> word) {
        words.push_back(word);
    }

    int min_removals = 0;
    for (int i = 0; i < words.size() - 1; i++) {
        if (words[i].back() != words[i + 1].front()) {
            min_removals++;
        }
    }

    out << min_removals << std::endl;
    out << words.size() << std::endl;

    for (int i = 0; i < words.size(); i++) {
        if (i == 0 || words[i - 1].back() == words[i].front()) {
            out << words[i] << std::endl;
        }
    }

    return 0;
}
```
