#include <cstdio>
#include <vector>
#include <cmath>
#include <algorithm>
#include <set>
using namespace std;
const int NMAX = 100005;

struct Point {
    int x, y;
    Point(int _x = 0, int _y = 0): x(_x), y(_y) {}
};

int n;
vector<Point> points;
vector<pair<double, int>> lines;
set<double> angles;

double getAngle(const Point& p1, const Point& p2) {
    if (p1.x == p2.x) {
        return 1e9;
    }
    return atan2(p2.y - p1.y, p2.x - p1.x);
}

void solve() {
    lines.clear();
    angles.clear();
    for (int i = 1; i < n; ++i) {
        lines.emplace_back(getAngle(points[i - 1], points[i]), i - 1);
    }
    sort(lines.begin(), lines.end());

    double lastAngle = -1e9;
    int ans = 0;
    for (const auto& line : lines) {
        if (lastAngle != line.first) {
            lastAngle = line.first;
            ans = max(ans, 1);
        } else {
            ++ans;
        }
        angles.insert(line.first);
    }

    if (angles.size() >= 2) {
        ans = n;
    }
    printf("%d\n", ans);
}

int main() {
    freopen("arhitect.in", "r", stdin);
    freopen("arhitect.out", "w", stdout);
    scanf("%d", &n);
    points.resize(n);
    for (int i = 0; i < n; ++i) {
        scanf("%d%d%d%d", &points[i].x, &points[i].y, &points[i].x, &points[i].y);
    }
    solve();
    return 0;
}
