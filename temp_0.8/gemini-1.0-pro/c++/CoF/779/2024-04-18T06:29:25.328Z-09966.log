        if (towns[j].first >= x) sum++;
      }
    } else if (i == 1) {
      for (int j = 0; j < K; j++) {
        if (towns[j].second >= y) sum++;
      }
    } else if (i == 2) {
      for (int j = 0; j < K; j++) {
        if (towns[j].first <= x) sum++;
      }
    } else {
      for (int j = 0; j < K; j++) {
        if (towns[j].second <= y) sum++;
      }
    }

    max_sum = max(max_sum, sum);
  }

  fout << max_sum << endl;
  fout.close();

  return 0;
}
