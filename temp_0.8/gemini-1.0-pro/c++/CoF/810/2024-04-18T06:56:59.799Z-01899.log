#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    // Read input
    ifstream fin("grad.in");
    int n, k;
    fin >> n >> k;
    vector<int> x(n);
    for (int i = 0; i < n; ++i) {
        fin >> x[i];
    }
    fin.close();

    // Calculate the rank of each element in the original array
    vector<int> rank(n);
    iota(rank.begin(), rank.end(), 0);
    sort(rank.begin(), rank.end(), [&x](int i, int j) { return x[i] < x[j]; });

    // Calculate the degree of the entire array
    int degree = 0;
    for (int i = 0; i < n; ++i) {
        if (rank[i] == i) {
            ++degree;
        }
    }

    // Find the first sequence of length k with maximum degree
    int max_degree = 0;
    int start_position = -1;
    for (int i = 0; i + k - 1 < n; ++i) {
        int current_degree = 0;
        for (int j = i; j < i + k; ++j) {
            if (rank[j] >= i && rank[j] < i + k) {
                ++current_degree;
            }
        }
        if (current_degree > max_degree) {
            max_degree = current_degree;
            start_position = i;
        }
    }

    // Write output
    ofstream fout("grad.out");
    fout << degree << endl;
    fout << start_position + 1 << ' ' << max_degree << endl;
    fout.close();

    return 0;
}
```
