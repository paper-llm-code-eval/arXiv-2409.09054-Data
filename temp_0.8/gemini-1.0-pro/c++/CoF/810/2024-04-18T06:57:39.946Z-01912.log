#include <iostream>
#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int n, k;
vector<int> v;

int main() {
    ifstream in("grad.in");
    ofstream out("grad.out");

    in >> n >> k;

    for (int i = 0; i < n; i++) {
        int x;
        in >> x;
        v.push_back(x);
    }

    // Sort the vector and create a vector of positions
    vector<int> p(n);
    for (int i = 0; i < n; i++) {
        p[i] = i;
    }

    sort(v.begin(), v.end());
    sort(p.begin(), p.end(), [&](int i, int j) { return v[i] < v[j]; });

    // Calculate the degree of the entire sequence
    int deg = 0;
    for (int i = 0; i < n; i++) {
        if (i == p[i]) { deg++; }
    }

    // Determine the position and degree of the first sequence of length k with maximum degree
    int max_deg = 0;
    int pos = -1;

    for (int i = 0; i <= n - k; i++) {
        int cur_deg = 0;
        for (int j = 0; j < k; j++) {
            if (p[i + j] == i + j) { cur_deg++; }
        }
        if (cur_deg > max_deg) {
            max_deg = cur_deg;
            pos = i;
        }
    }

    out << deg << endl;
    out << pos + 1 << ' ' << max_deg << endl;

    return 0;
}
