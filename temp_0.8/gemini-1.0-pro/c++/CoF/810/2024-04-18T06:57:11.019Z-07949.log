#include <fstream>
#include <algorithm>
#include <vector>
using namespace std;

const int NMAX = 10010;

int v[NMAX], ord[NMAX], poz[NMAX], ord2[NMAX], perm[NMAX];
vector <int> secv[NMAX];
int n, k, grad, grad_max, poz_max;

int main() {
  ifstream fin("grad.in");
  ofstream fout("grad.out");
  fin >> n >> k;
  for (int i = 1; i <= n; i++) {
    fin >> v[i];
