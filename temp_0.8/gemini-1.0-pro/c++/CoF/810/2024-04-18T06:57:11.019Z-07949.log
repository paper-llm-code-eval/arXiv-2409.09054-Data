  for (int i = 1; i <= n; i++) {
    int j = i;
    while (j <= n && ord[j] - j < k) j++;
    if (j - i >= k) {
      perm[i] = j - i + 1;
      secv[perm[i]].push_back(i);
    }
  }
  for (int i = k; i <= n; i++) {
    if (perm[i] > perm[i - 1]) {
