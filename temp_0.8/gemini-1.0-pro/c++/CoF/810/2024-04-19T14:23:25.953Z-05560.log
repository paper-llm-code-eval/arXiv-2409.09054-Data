#include <iostream>
#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream fin("grad.in");
  ofstream fout("grad.out");

  int n, k;
  fin >> n >> k;
  vector<int> x(n);
  for (int i = 0; i < n; ++i) {
    fin >> x[i];
  }
  sort(x.begin(), x.end());

  int grad = 0;
  for (int i = 0; i < n; ++i) {
    if (x[i] == x[i + 1]) {
      ++grad;
      ++i;
    }
  }
  fout << grad << endl;

  int max_grad = 0;
  int pos = -1;
  for (int i = 0; i <= n - k; ++i) {
    int local_grad = 0;
    for (int j = 0; j < k; ++j) {
      if (x[i + j] == x[i + j + 1]) {
        ++local_grad;
      }
    }
    if (local_grad > max_grad) {
      max_grad = local_grad;
      pos = i;
    }
  }
  fout << pos + 1 << " " << max_grad << endl;

  return 0;
}
