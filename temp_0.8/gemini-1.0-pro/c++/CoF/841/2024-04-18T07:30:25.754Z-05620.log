#include <fstream>

using namespace std;
ifstream f("pseudobil.in");
ofstream g("pseudobil.out");

int n, D, K, p, m, a[55005][2], b[100005][2], ans, l[1505], r[1505], dp[1505][1505];

int main() {
  f >> p;
  f >> n >> K >> D;
  if (p == 1) {
    ans = n * n;
    for (int i = 1; i <= K; i++) {
      f >> a[i][0] >> a[i][1];
      l[a[i][0]]++;
      r[a[i][1]]++;
    }
    for (int i = 1; i <= n; i++) {
      l[i] = min(D / 2 + 1, l[i]);
      r[i] = min(D / 2 + 1, r[i]);
      ans -= l[i] + r[i] - 1;
    }
    g << ans;
  }
  if (p == 2) {
    for (int i = 1; i <= K; i++) {
      f >> a[i][0] >> a[i][1];
      dp[a[i][0]][a[i][1]] = 1;
    }
    f >> m;
    for (int i = 1; i <= m; i++) {
      f >> b[i][0] >> b[i][1];
      ans += dp[b[i][0]][b[i][1]];
      if (b[i][0] - (D / 2 - 1) < 1)
        ans += ((D / 2 - 1) - (b[i][0] - 1)) * 2;
      else
        ans += (n - (b[i][0] - (D / 2 - 1))) * 2;
      if (b[i][1] - (D / 2 - 1) < 1)
        ans += ((D / 2 - 1) - (b[i][1] - 1)) * 2;
      else
        ans += (n - (b[i][1] - (D / 2 - 1))) * 2;
      g << ans << '\n';
      ans = 0;
    }
  }
  f.close();
  g.close();
  return 0;
}
