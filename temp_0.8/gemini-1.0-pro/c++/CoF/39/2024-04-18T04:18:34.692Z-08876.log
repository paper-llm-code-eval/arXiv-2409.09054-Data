#include <fstream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
  ifstream f("suma.in");
  ofstream g("suma.out");
  int n;
  f >> n;
  vector<int> c(n + 1), dp(n + 1), t(n + 1);
  for (int i = 1; i <= n; ++i) f >> c[i];
  for (int i = n; i >= 1; --i) {
    int s = dp[i] = 1 << 30;
    if (i + 1 <= n) {
      if (dp[i] > dp[i + 1]) {
        dp[i] = dp[i + 1];
        t[i] = 0;
      }
      if (dp[i] > dp[i + 2]) {
        dp[i] = dp[i + 2];
        t[i] = 1;
      }
      if (dp[i] > dp[i + 4]) {
        dp[i] = dp[i + 4];
        t[i] = 2;
      }
    }
    if (i + 3 <= n) {
      if (dp[i] > dp[i + 3]) {
        dp[i] = dp[i + 3];
        t[i] = 3;
      }
    }
    s += c[i];
    dp[i] = min(s, dp[i]);
  }
  int x = 1;
  vector<int> r;
  while (x <= n) {
    r.push_back(x);
    x += 1 << t[x];
  }
  g << r.size() << " " << dp[1] << "\n";
  for (int x : r) g << x << " ";
  return 0;
}
