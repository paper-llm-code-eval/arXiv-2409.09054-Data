#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int n;
vector<int> costuri;
vector<vector<int>> cost_minime;
vector<vector<int>> drum_minime;

int main() {
    ifstream fin("suma.in");
    ofstream fout("suma.out");

    fin >> n;
