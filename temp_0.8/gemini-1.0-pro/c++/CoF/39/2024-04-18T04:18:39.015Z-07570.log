    // Calculeaza costurile minime pentru fiecare nivel
    for (int nivel = 1; nivel <= n; nivel++) {
        for (int camera = 1; camera <= nivel * nivel; camera++) {
            if (nivel == 1)
                cost_minime[nivel][camera] = costuri[camera];
            else {
                // Costul minim pentru camera curenta este suma costului minim pentru camera de deasupra si costul camerei curente
                int cost_min = costuri[camera];
                for (int camera_de_deasupra = camera - nivel; camera_de_deasupra > 0; camera_de_deasupra -= nivel)
                    cost_min += cost_minime[nivel - 1][camera_de_deasupra];
                cost_minime[nivel][camera] = cost_min;
            }
        }
    }

    // Calculeaza drumul minim
    int nivel_curent = n;
    int camera_curenta = n;
    while (nivel_curent > 0) {
        // Gaseste camera de deasupra cu costul minim
        int camera_de_deasupra_min = 0;
        int cost_min = cost_minime[nivel_curent][camera_curenta];
        for (int camera_de_deasupra = camera_curenta - nivel_curent; camera_de_deasupra > 0; camera_de_deasupra -= nivel_curent) {
            if (cost_minime[nivel_curent][camera_de_deasupra] < cost_min) {
                cost_min = cost_minime[nivel_curent][camera_de_deasupra];
                camera_de_deasupra_min = camera_de_deasupra;
            }
        }
        drum_minime[nivel_curent][camera_curenta] = camera_de_deasupra_min;
        nivel_curent--;
        camera_curenta = camera_de_deasupra_min;
    }

    // Afiseaza rezultatul
    int nr_niveluri = n;
    int cost_minim = cost_minime[nr_niveluri][n];
    fout << nr_niveluri << " " << cost_minim << endl;

    int camera_curenta = n;
    while (nr_niveluri > 0) {
        fout << camera_curenta << " ";
        camera_curenta = drum_minime[nr_niveluri][camera_curenta];
        nr_niveluri--;
    }

    return 0;
}
