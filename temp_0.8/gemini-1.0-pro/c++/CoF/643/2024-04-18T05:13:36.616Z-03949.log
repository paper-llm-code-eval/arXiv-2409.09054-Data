#include <fstream>
#include <vector>

using namespace std;

ifstream in("pergament.in");
ofstream out("pergament.out");

const int kN = 10000000;
const int kK = 50;
const int kQ = 100000;
const int kMaxN = 10000000;

struct Coord {
    int x, y;
};

int n, k, q;
int a, b, c, d;
int x[kMaxN], y[kMaxN];
int j[kQ], r[kQ], l[kQ];
int dp[kMaxN][kK][2];

inline int add(int a, int b, int mod) { return (a + b) % mod; }
inline int mul(int a, int b, int mod) { return ((long long)a * b) % mod; }
inline int put(int value, int pos, int mod) { return (value + mod - (1 << pos)) % mod; }

void solve(int _x1, int _y1) {
    vector<Coord> v;
    v.reserve(kMaxN + kQ);
    for (int i = 1; i <= n; ++i) {
        v.emplace_back(x[i], y[i]);
        x[i] = add(mul(x[i], a, kN), b, kN);
        y[i] = add(mul(y[i], c, k - x[i] + 1), d, k - x[i] + 1);
    }
    for (int i = 1; i <= q; ++i) {
        v.emplace_back(j[i], r[i]);
        v.emplace_back(j[i], r[i] + l[i] - 1);
    }

    sort(v.begin(), v.end(),
         [](const Coord &a, const Coord &b) { return a.x < b.x || (a.x == b.x && a.y < b.y); });
    int _dp[kMaxN][kK][2];
    int sum = 0;
    for (size_t i = 0; i < v.size(); ++i) {
        int &x = v[i].x;
        int &y = v[i].y;
        for (int j = 0; j < kK; ++j) {
            if (x - j >= 1) {
                _dp[y][j][0] = max(_dp[y][j][0], dp[y][j][1]);
                if (y - 1 >= 1) { _dp[y][j][1] = put(max(_dp[y - 1][j][1], _dp[y - 1][j][0]), 0, kMaxN); }
            }
            if (x + y - j - 1 < k && j < k) {
                if (y - 1 >= 1) { _dp[y][j][0] = max(_dp[y][j][0], _dp[y - 1][j + 1][1]); }
                _dp[y][j][1] = max(_dp[y][j][1], _dp[y][j + 1][0]);
            }
            if (j < k) { _dp[y][j][1] = max(_dp[y][j][1], sum); }
            sum = 0;
            if (x == _x1 && y == _y1) {
                _dp[y][j][0] = 1;
                _dp[y][j][1] = 1;
                sum = 1;
            }
        }
    }
    out << _dp[k][0][1] << "\n";
}

int main() {
    in >> n >> k >> q;
    in >> a >> b >> c >> d;
    in >> x[1] >> y[1];
    j[0] = k + 1;
    for (int i = 2; i <= n; ++i) { in >> x[i] >> y[i]; }
    for (int i = 1; i <= q; ++i) { in >> j[i] >> r[i] >> l[i]; }
    solve(x[1], y[1]);
    return 0;
}
