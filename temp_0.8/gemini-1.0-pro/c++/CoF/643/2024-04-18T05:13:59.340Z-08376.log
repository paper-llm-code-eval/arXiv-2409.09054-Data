        ll b = street_x[i] + street_y[i] - 1;
        for (ll j = 1; j <= n; j++) {
            bool b1 = j >= a && j <= b;
            ll x = j;
            for (ll s : vertical) {
                ll u = s.first, l = s.second;
                bool b2 = u <= x && u + l - 1 >= x;
                if (b1 && b2) { purple++; }
            }
        }
    }
    fout << purple;
}
