#include <algorithm>
#include <bitset>
#include <cmath>
#include <cstdio>
#include <fstream>
#include <iostream>
#include <string>
#include <vector>
#include <map>

using namespace std;

ifstream in("circular.in");
ofstream out("circular.out");

typedef long long ll;

const int MOD = 666013;
typedef bitset<26> bit;

bit albastre;
bit rosii;
vector<int> ordalbastre;
map<bit, vector<string>> dp;
vector<string> sol;
string s, smin;
int n, c, ans, mx;

void read() {
    in >> c >> s >> s;
    for (int i = 0; i < (int)s.size(); i++)
        albastre[s[i] - 'A'] = 1;
    for (int i = 0; i < (int)s.size(); i++)
        rosii[s[i] - 'A'] = 1;
    rosii.flip();
    albastre.flip();
}

int dist(char a, char b) {
    int i = a - 'A';
    int j = b - 'A';
    if (i > j)
        swap(i, j);
    return min(j - i, 26 - j + i);
}

void rez1() {
    for (int i = 1; i < (int)s.size(); i++)
        ans += dist(s[i], s[i - 1]);
    out << ans << '\n';
}

void rez2() {
    dp[albastre].push_back(s);
    for (int i = 1; i < (int)s.size(); i++)
        for (auto j : dp[albastre]) {
            bit aux = albastre;
            aux[s[i] - 'A'] = 1;
            ordalbastre.clear();
            for (int k = 0; k < 26; k++)
                if (aux[k])
                    ordalbastre.push_back(k);
            for (int k = 0; k < (int)ordalbastre.size() - 1; k++)
                aux.set(ordalbastre[k], 0);
            if (!dp.count(aux))
                dp[aux].push_back(j + s[i]);
            else if (j + s[i] < dp[aux][0])
                dp[aux][0] = j + s[i];
        }
    for (auto i : dp) {
        if ((int)i.second.size() > mx) {
            mx = i.second.size();
            sol.clear();
            for (auto j : i.second)
                sol.push_back(j);
        }
        sort(i.second.begin(), i.second.end());
        if (i.second[0].size() < smin.size() ||
            (i.second[0].size() == smin.size() && i.second[0] < smin))
            smin = i.second[0];
    }
    ans = smin.size() - (int)s.size();
    out << ans << '\n' << mx << '\n' << smin << '\n';
}

int main() {
    read();
    if (c == 1)
        rez1();
    else
        rez2();
    return 0;
}
