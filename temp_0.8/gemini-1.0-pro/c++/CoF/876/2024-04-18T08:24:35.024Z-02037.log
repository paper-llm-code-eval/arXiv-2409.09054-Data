#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAXN = 1005, INF = 1e9;
int n, m, cerinta;
int h[MAXN][MAXN];
int seen[MAXN][MAXN][2];
int dp[MAXN][MAXN][2];
int dx[] = { -1, 0, 1, 0 };
int dy[] = { 0, -1, 0, 1 };
ifstream fin("ace.in");
ofstream fout("ace.out");

int maximize(int x, int y, int dir)
{
  if (dp[x][y][dir] != -INF) return dp[x][y][dir];

  dp[x][y][dir] = h[x][y];

  for (int d = 0; d < 4; ++d)
  {
    int nx = x + dx[d], ny = y + dy[d];
    if (nx < 1 || nx > n || ny < 1 || ny > m || seen[nx][ny][dir] || h[nx][ny] < h[x][y]) continue;
    seen[nx][ny][dir] = 1;
    dp[x][y][dir] = max(dp[x][y][dir], maximize(nx, ny, dir) + h[nx][ny]);
  }

  return dp[x][y][dir];
}

int maximize_rot(int x, int y)
{
  int ans = 0;

  for (int dir = 0; dir < 2; ++dir)
  {
    for (int i = 1; i <= n; ++i)
      for (int j = 1; j <= m; ++j)
        seen[i][j][dir] = 0, dp[i][j][dir] = -INF;

    ans = max(ans, maximize(x, y, dir));
  }

  return ans;
}

int main()
{
  fin >> cerinta >> n >> m;
  for (int i = 1; i <= n; ++i)
    for (int j = 1; j <= m; ++j)
      fin >> h[i][j];

  if (cerinta == 1)
  {
    int ans = 0;
    for (int i = 1; i <= n; ++i)
