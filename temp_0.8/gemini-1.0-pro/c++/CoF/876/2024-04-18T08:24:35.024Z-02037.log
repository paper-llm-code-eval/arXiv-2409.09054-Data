    int nx = x + dx[d], ny = y + dy[d];
    if (nx < 1 || nx > n || ny < 1 || ny > m || seen[nx][ny][dir] || h[nx][ny] < h[x][y]) continue;
    seen[nx][ny][dir] = 1;
    dp[x][y][dir] = max(dp[x][y][dir], maximize(nx, ny, dir) + h[nx][ny]);
  }

  if (h[x][y] == h[x + 1][y] && h[x][y] == h[x - 1][y] && h[x][y] == h[x][y + 1] && h[x][y] == h[x][y - 1]) dp[x][y][dir] = 4 * h[x][y];

  return dp[x][y][dir];
}

int maximize_rot(int x, int y)
{
  int ans = 0;

  for (int dir = 0; dir < 2; ++dir)
  {
    for (int i = 1; i <= n; ++i)
      for (int j = 1; j <= m; ++j)
        seen[i][j][dir] = 0, dp[i][j][dir] = -INF;

    ans = max(ans, maximize(x, y, dir));
  }

  return ans;
}

int main()
{
  fin >> cerinta >> n >> m;
  for (int i = 1; i <= n; ++i)
    for (int j = 1; j <= m; ++j)
      fin >> h[i][j];

  if (cerinta == 1)
  {
    int ans = 0;
    for (int i = 1; i <= n; ++i)
    {
      for (int j = 1; j <= m; ++j)
      {
        seen[i][j][0] = 0, dp[i][j][0] = -INF;
        seen[i][j][1] = 0, dp[i][j][1] = -INF;
        ans = max(ans, max(maximize(i, j, 0), maximize(i, j, 1)));
      }
    }

    fout << ans << '\n';
  }
  else
  {
    int ans = 0;
    for (int i = 1; i <= n; ++i)
      for (int j = 1; j <= m; ++j)
        ans = max(ans, maximize_rot(i, j));
    fout << ans << '\n';
  }

  return 0;
}
