#include <iostream>
#include <fstream>
using namespace std;

int main() {
  ifstream fin("tcif.in");
  ofstream fout("tcif.out");

  int N, A, B, C, c1, c2, c3;
  fin >> A >> B >> C >> c1 >> c2 >> c3 >> N;

  string ans = to_string(N);
  while (true) {
    int cnt1 = 0, cnt2 = 0, cnt3 = 0;
    for (char c: ans) {
      if (c == c1 + '0') cnt1++;
      else if (c == c2 + '0') cnt2++;
      else if (c == c3 + '0') cnt3++;
      else break;
    }
    if (cnt1 == A && cnt2 == B && cnt3 == C) {
      fout << ans << endl;
      break;
    }
    ans = to_string(++N);
  }

  return 0;
}
