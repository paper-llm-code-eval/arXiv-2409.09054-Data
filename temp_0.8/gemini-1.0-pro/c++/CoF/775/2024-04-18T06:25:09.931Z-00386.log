#include <bits/stdc++.h>
using namespace std;

const int MAXN = 1e5;
int n, pages[MAXN + 5];
bitset<MAXN + 5> vis;
vector<int> adj[MAXN + 5];

int ans = 0, last = 0, maxlen = 0;

void dfs(int node) {
  vis[node] = true;
  if (node - last > 1) {
    ans++;
    last = node;
    maxlen = 1;
  }
  maxlen = max(maxlen, node - last + 1);
  for (int next : adj[node]) {
    if (!vis[next]) dfs(next);
  }
}

int main() {
  ifstream in("pagini.in");
  ofstream out("pagini.out");
  in >> n;
  for (int i = 1; i <= n; i++) {
    in >> pages[i];
  }
  for (int i = 1; i <= n; i++) adj[pages[i]].push_back(pages[i + 1]);
  for (int i = 1; i <= n; i++) {
    if (!vis[pages[i]]) dfs(pages[i]);
  }
  out << ans << '\n' << maxlen << '\n';
  return 0;
}
