#include <bits/stdc++.h>
using namespace std;

const int MAXN = 1e5;
int n, pages[MAXN + 5];
int parent[MAXN + 5], sz[MAXN + 5];

int find(int x) {
  if (parent[x] != x) parent[x] = find(parent[x]);
  return parent[x];
}

void unite(int a, int b) {
  a = find(a), b = find(b);
  if (a != b) {
    if (sz[a] > sz[b]) swap(a, b);
    parent[a] = b;
    sz[b] += sz[a];
  }
}

int main() {
  ifstream in("pagini.in");
  ofstream out("pagini.out");
  in >> n;
  for (int i = 1; i <= n; i++) {
    in >> pages[i];
    parent[i] = i;
    sz[i] = 1;
  }
  for (int i = 1; i <= n; i++) {
    unite(pages[i], pages[i + 1]);
  }
  int ans = 0, maxlen = 0;
  for (int i = 1; i <= n; i++) {
    if (find(i) == i) {
      int cnt = 0;
      for (int j = i; j <= n; j++) {
        if (find(j) == i) cnt++;
        else break;
      }
      ans++;
      maxlen = max(maxlen, cnt);
    }
  }
  out << ans << '\n' << maxlen << '\n';
  return 0;
}
