#include <iostream>
#include <vector>
#include <set>
#include <queue>
#include <fstream>

using namespace std;

const int maxn = 8000;

int p[maxn], sx[maxn], sy[maxn], dx[maxn], dy[maxn];
int m, n, mx, my;
int dp[maxn][maxn];
int viz[maxn][maxn];
bool ok;
int ans;

void dfs(int x, int y) {
  if(viz[x][y] == 1) {
    ok = false;
    return;
  }
  viz[x][y] = 1;
  if(dp[x][y] == 1) {
    ans++;
    return;
  }
  if(x > 0 && dp[x - 1][y] == 0) {
    dfs(x - 1, y);
  }
  if(x < mx && dp[x + 1][y] == 0) {
    dfs(x + 1, y);
  }
  if(y > 0 && dp[x][y - 1] == 0) {
    dfs(x, y - 1);
  }
  if(y < my && dp[x][y + 1] == 0) {
    dfs(x, y + 1);
  }
}

int main() {
  ifstream fin("colaj.in");
  ofstream fout("colaj.out");
  fin >> n >> m >> p;
  mx = m + 1;
  my = p + 1;
  for(int i = 1; i <= n; i++) {
    fin >> p[i] >> sx[i] >> dx[i] >> dy[i];
    dp[p[i]][sx[i]] = dp[p[i]][dx[i] + 1] = dp[dx[i] + 1][sx[i]] = dp[dx[i] + 1][dx[i] + 1] = 1;
  }
  for(int i = 1; i <= m + 1; i++) {
    dp[i][0] = dp[i][p + 1] = 1;
  }
  for(int i = 1; i <= p + 1; i++) {
    dp[0][i] = dp[m + 1][i] = 1;
  }
  for(int i = 1; i <= m + 1; i++) {
    for(int j = 1; j <= p + 1; j++) {
      ok = true;
      dfs(i, j);
      if(ok == false) {
        dp[i][j] = 1;
      }
    }
  }
  for(int i = 1; i <= m + 1; i++) {
    for(int j = 1; j <= p + 1; j++) {
      if(dp[i][j] == 0) {
        ans++;
      }
    }
  }
  fout << ans << '\n';
  fin.close();
  fout.close();
  return 0;
}
