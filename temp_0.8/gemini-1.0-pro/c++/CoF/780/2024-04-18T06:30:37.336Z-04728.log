const int maxn = 8000;

int p[maxn], sx[maxn], sy[maxn], dx[maxn], dy[maxn];
int m, n, mx, my;
int dp[maxn][maxn];
int viz[maxn][maxn];
bool ok;
int ans;

void dfs(int x, int y) {
  if(viz[x][y] == 1) {
    ok = false;
    return;
  }
  viz[x][y] = 1;
  if(dp[x][y] == 1) {
    ans++;
    return;
  }
  if(x > 0 && dp[x - 1][y] == 0) {
    dfs(x - 1, y);
  }
  if(x < mx && dp[x + 1][y] == 0) {
    dfs(x + 1, y);
  }
  if(y > 0 && dp[x][y - 1] == 0) {
    dfs(x, y - 1);
  }
  if(y < my && dp[x][y + 1] == 0) {
    dfs(x, y + 1);
  }
}

int main() {
  ifstream fin("colaj.in");
  ofstream fout("colaj.out");
  fin >> n >> m >> p;
  mx = m + 1;
  my = p + 1;
