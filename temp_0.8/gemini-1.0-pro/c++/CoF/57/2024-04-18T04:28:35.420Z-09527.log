    }
    void union_(int a, int b) {
        a = find(a), b = find(b);
        if (a == b)
            return;
        if (size[a] > size[b])
            std::swap(a, b);
        parent[a] = b;
        size[b] += size[a];
    }
};
int main() {
    std::ifstream fin("urgenta.in");
    std::ofstream fout("urgenta.out");
    int n, m, k;
    fin >> n >> m >> k;
    std::vector<std::vector<Edge>> adj(n + 1);
    for (int i = 0; i < m; ++i) {
        int u, v, p;
        fin >> u >> v >> p;
        adj[u].push_back({v, p});
        adj[v].push_back({u, p});
    }
    std::vector<int> ans(k + 1);
    for (ans[k] = n - 1, ans[k - 1] = m; k >= 1; --k) {
        DisjointSetUnion dsu;
        dsu.initialize(n + 1);
        std::priority_queue<Edge, std::vector<Edge>, std::greater<Edge>> pq;
        for (int i = 1; i <= n; ++i) {
            for (const auto& edge : adj[i]) {
                if (dsu.find(i) != dsu.find(edge.to)) {
                    pq.push({i, edge.to});
                }
            }
        }
        while (m - ans[k - 1] > ans[k] && !pq.empty()) {
            auto [u, v] = pq.top();
            pq.pop();
            if (dsu.find(u) != dsu.find(v)) {
                dsu.union_(u, v);
                --ans[k];
            }
        }
    }
    int gravmax = 0;
    for (int i = 1; i <= m; ++i) {
        auto [u, v] = ans[i];
        gravmax += u * v;
    }
    fout << gravmax << "\n";
    for (int i = 1; i < k; ++i) {
        fout << ans[i] << "\n";
    }
    std::stack<Edge> destroyed;
    for (int i = 1; i <= n; ++i) {
        for (const auto& edge : adj[i]) {
            if (dsu.find(i) != dsu.find(edge.to)) {
                destroyed.push({i, edge.to});
            }
        }
    }
    while (!destroyed.empty()) {
        auto [u, v] = destroyed.top();
        destroyed.pop();
        fout << u << " " << v << "\n";
    }
    return 0;
}
