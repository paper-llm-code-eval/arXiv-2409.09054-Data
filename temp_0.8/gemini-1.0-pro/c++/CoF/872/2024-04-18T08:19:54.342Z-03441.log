}

auto& get_ofstream() {
    static std::ofstream file("arma.out");
    return file;
}

inline void flush() { get_ofstream().flush(); }

void solve_task_1() {
    size_t n;
    get_ifstream() >> n;
    std::vector<unsigned long long> distances(n);
    for (auto& distance : distances) get_ifstream() >> distance;
    std::sort(distances.begin(), distances.end());
    unsigned long long total = 0;
    for (auto distance : distances) total += distance;
    get_ofstream() << total << "\n";
}

void solve_task_2() {
    size_t n;
    get_ifstream() >> n;
    std::vector<unsigned long long> distances(n);
    for (auto& distance : distances) get_ifstream() >> distance;
    std::sort(distances.begin(), distances.end());
    std::vector<int> levels(n);
    unsigned long long total = 0;
    for (size_t i = 0; i < n; ++i) {
        unsigned long long max_level = static_cast<unsigned long long>(std::sqrt(distances[i]));
        auto it = std::upper_bound(distances.begin(), distances.end(), max_level * max_level);
        auto level = it - distances.begin() - 1;
        levels[i] = level;
        total += distances[i] / (level + 1);
    }
    get_ofstream() << total << "\n";
    for (auto level : levels) get_ofstream() << level + 1 << "\n";
}

