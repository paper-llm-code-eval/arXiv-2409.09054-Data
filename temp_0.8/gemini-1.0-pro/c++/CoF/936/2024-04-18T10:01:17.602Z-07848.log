    for (size_t i = 1; i <= N; i++) {
      size_t st = (i % 2 == 1) ? 1 : M;
      size_t dr = (i % 2 == 1) ? M : 1;
      size_t inc = (i % 2 == 1) ? 1 : -1;
      for (size_t j = st; j != dr + inc; j += inc) {
        s += f.get(j);
      }
    }
    fout << s << '\n';
  } else if (C == 1) {
    for (size_t i = 1; i <= N; i++) {
      size_t st = (i % 2 == 1) ? 1 : M;
      size_t dr = (i % 2 == 1) ? M : 1;
      size_t inc = (i % 2 == 1) ? 1 : -1;
      for (size_t j = st; j != dr + inc; j += inc) {
        s += (f.get(j) == 0);
      }
    }
    fout << s << '\n';
