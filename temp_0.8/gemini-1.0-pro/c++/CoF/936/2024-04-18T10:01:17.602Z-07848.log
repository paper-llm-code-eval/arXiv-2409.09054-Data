struct Fibonacci {
  size_t size;
  uint64_t* arr;

  Fibonacci(size_t size) : size(size) { init(); }

  void init() {
    arr = new uint64_t[size];
    arr[0] = 0;
    arr[1] = 1;
    for (size_t i = 2; i < size; i++) arr[i] = (arr[i - 1] + arr[i - 2]) % 10;
  }

  uint64_t get(size_t i) { return arr[(i % size)]; }
};

int main() {
  std::chrono::steady_clock::time_point start = std::chrono::steady_clock::now();

  std::ifstream fin("cartofi.in", std::ios::binary);
  std::ofstream fout("cartofi.out", std::ios::binary);
  uint32_t C;
  fin >> C;
  uint64_t N, M, Q, a, b;
  fin >> N >> M;
  Fibonacci f(20);
  uint64_t s = 0;
  uint64_t max = 0;
  if (C == 3) {
    fin >> Q;
    uint64_t* cnt = new uint64_t[M + 1];
    for (size_t i = 0; i <= M; i++) cnt[i] = 0;
    for (size_t i = 1; i <= N; i++) {
      size_t st = (i % 2 == 1) ? 1 : M;
      size_t dr = (i % 2 == 1) ? M : 1;
      size_t inc = (i % 2 == 1) ? 1 : -1;
      for (size_t j = st; j != dr + inc; j += inc) {
        cnt[j]++;
      }
    }
    for (size_t i = 1; i <= M; i++) {
      cnt[i] += cnt[i - 1];
      if (cnt[i] == 0) s++;
      max = std::max(max, cnt[i]);
    }
    fout << s << '\n' << max << '\n';
    for (size_t i = 0; i < Q; i++) {
      fin >> a >> b;
      fout << (cnt[b] - cnt[a - 1]) << '\n';
