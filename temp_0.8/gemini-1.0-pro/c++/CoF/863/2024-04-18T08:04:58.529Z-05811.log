#include <fstream>

void read_data(std::ifstream& in, int& n, int& m, int& T, int& L, int& C, int& k, int& S, std::vector<std::vector<int>>& map, std::vector<std::vector<bool>>& reachable, std::vector<std::pair<int, int>>& food)
{
    in >> n >> m >> T;
    in >> L >> C >> k >> S;
    --L, --C;
    in >> map[L][C];
    for (int i = 0; i < T; ++i) {
        int x, y;
        in >> x >> y;
        --x, --y;
        food.emplace_back(x, y);
        in >> map[x][y];
    }
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < m; ++j) {
            in >> map[i][j];
        }
    }
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < m; ++j) {
            reachable[i][j] = (k & (1 << (S - 1))) == (map[i][j] & (1 << (S - 1)));
        }
    }
    reachable[L][C] = false;
}

void dfs(const std::vector<std::vector<int>>& map, std::vector<std::vector<bool>>& reachable, std::vector<std::vector<bool>>& visited, std::vector<std::vector<bool>>& food_visited, int i, int j, int steps, int& Smin, int& nt)
{
    visited[i][j] = true;
    if (food_visited[i][j]) {
        if (steps < Smin) {
            Smin = steps;
            nt = 0;
        }
        if (steps == Smin) {
            ++nt;
        }
    }
    if (reachable[i][j]) {
        if (i > 0 && !visited[i - 1][j]) {
            dfs(map, reachable, visited, food_visited, i - 1, j, steps + 1, Smin, nt);
        }
        if (i + 1 < n && !visited[i + 1][j]) {
            dfs(map, reachable, visited, food_visited, i + 1, j, steps + 1, Smin, nt);
        }
        if (j > 0 && !visited[i][j - 1]) {
            dfs(map, reachable, visited, food_visited, i, j - 1, steps + 1, Smin, nt);
        }
        if (j + 1 < m && !visited[i][j + 1]) {
            dfs(map, reachable, visited, food_visited, i, j + 1, steps + 1, Smin, nt);
        }
    }
    visited[i][j] = false;
}

int main()
{
    std::ifstream in("panda.in");
    std::ofstream out("panda.out");
    int p;
    in >> p;
    int n, m, T, L, C, k, S;
    std::vector<std::vector<int>> map(500, std::vector<int>(500, 0));
    std::vector<std::vector<bool>> reachable(500, std::vector<bool>(500, false));
    std::vector<std::pair<int, int>> food;
    read_data(in, n, m, T, L, C, k, S, map, reachable, food);
    if (p == 1) {
        int cnt = 0;
        for (int i = 0; i < n; ++i) {
            for (int j = 0; j < m; ++j) {
                if (reachable[i][j]) {
                    ++cnt;
                }
            }
        }
        out << cnt << '\n';
    } else if (p == 2) {
        int Smin = 500 * 500;
        int nt = 0;
        std::vector<std::vector<bool>> food_visited(500, std::vector<bool>(500, false));
        for (auto& f : food) {
            food_visited[f.first][f.second] = true;
        }
        std::vector<std::vector<bool>> visited(500, std::vector<bool>(500, false));
        dfs(map, reachable, visited, food_visited, L, C, 0, Smin, nt);
        out << Smin << ' ' << nt << '\n';
    }
    return 0;
}
