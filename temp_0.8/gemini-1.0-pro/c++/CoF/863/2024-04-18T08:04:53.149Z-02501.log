#include <fstream>
#include <vector>
#include <tuple>
using namespace std;

int n, m, t, l, c, p, s, k;
int coduri[505][505];
bool viz[505][505];
vector<tuple<int, int>> mancare;

bool Complementare(int a, int b, int s) {
  int masca = (1 << s) - 1;
  return (a & masca) == (~b & masca);
}

void Citire() {
  ifstream fin("panda.in");
  fin >> p;
  fin >> n >> m >> t;
  fin >> l >> c >> k >> s;
  for (int i = 1; i <= t; i++) {
    int x, y;
    fin >> x >> y;
    mancare.push_back(make_tuple(x, y));
  }
  for (int i = 1; i <= n; i++)
    for (int j = 1; j <= m; j++) {
      fin >> coduri[i][j];
    }
  fin.close();
}

int X() {
  int x = 0;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      if (i != l || j != c) {
        if (Complementare(coduri[i][j], k, s)) {
          x++;
        }
      }
    }
  }
  return x;
}

tuple<int, int> SminNT() {
  int dist[505][505];
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      dist[i][j] = -1;
      viz[i][j] = false;
    }
  }
  queue<tuple<int, int>> q;
  q.push(make_tuple(l, c));
  dist[l][c] = 0;
  viz[l][c] = true;
  int smin = 1e9;
  int nt = 0;
  while (!q.empty()) {
    int x, y;
    tie(x, y) = q.front();
    q.pop();
    for (auto& dir : vector<pair<int, int>>{{0, 1}, {0, -1}, {1, 0}, {-1, 0}}) {
      int nx = x + dir.first;
      int ny = y + dir.second;
      if (nx >= 1 && nx <= n && ny >= 1 && ny <= m) {
        if (Complementare(coduri[nx][ny], k, s)) {
          if (!viz[nx][ny]) {
            q.push(make_tuple(nx, ny));
            viz[nx][ny] = true;
            dist[nx][ny] = dist[x][y] + 1;
            if (find(mancare.begin(), mancare.end(), make_tuple(nx, ny)) !=
                mancare.end()) {
              nt++;
              smin = min(smin, dist[nx][ny]);
            }
          }
        }
      }
    }
  }
  if (smin == 1e9) {
    smin = -1;
  }
  return make_tuple(smin, nt);
}

int main() {
  Citire();
  if (p == 1) {
    ofstream fout("panda.out");
    fout << X() << '\n';
    fout.close();
  } else {
    ofstream fout("panda.out");
    int smin, nt;
    tie(smin, nt) = SminNT();
    fout << smin << ' ' << nt << '\n';
    fout.close();
  }
  return 0;
}
