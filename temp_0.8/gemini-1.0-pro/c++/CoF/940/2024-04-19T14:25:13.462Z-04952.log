#include <fstream>
#include <vector>
#include <algorithm>
#include <iterator>

using namespace std;

typedef unsigned long long ull;

int main()
{
    ifstream fin("tort.in");
    ofstream fout("tort.out");

    ull n;
    fin >> n;

    vector<ull> a(n);
    copy_n(istream_iterator<ull>(fin), n, a.begin());

    const ull SUM = accumulate(a.begin(), a.end(), 0ULL);
    const ull HALF = SUM / 2;

    vector<vector<ull>> dp(n + 1, vector<ull>(HALF + 1));
    dp[0][0] = 1;
    for(ull i = 1; i <= n; i++) {
        for(ull sum = 0; sum <= HALF; sum++) {
            dp[i][sum] = dp[i - 1][sum];
            if(sum >= a[i - 1])
                dp[i][sum] += dp[i - 1][sum - a[i - 1]];
        }
    }

    fout << dp[n][HALF] << endl;

    return 0;
}
```
