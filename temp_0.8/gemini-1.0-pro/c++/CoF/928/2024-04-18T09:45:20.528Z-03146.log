#include <fstream>
#include <vector>
#include <tuple>
#include <string>
#include <cassert>
#include <cstring>

using namespace std;

const int MAXN = 10001;

int n, sz;
char s[MAXN], t[100001];
vector<pair<int, int>> v;

void get_transformations() {
    int sp = 0, ep = 0;
    char c;
    for (int i = 0; s[i]; i++) {
        if (s[i] >= '0' && s[i] <= '9') {
            sp = i;
            while (s[i] >= '0' && s[i] <= '9') i++;
            i--;
            ep = i;
        } else if (s[i] == '(') {
            v.emplace_back(stoi(string(s + sp, s + ep + 1)), i);
        } else if (s[i] == '*' || s[i] == ']') {
            if (s[i] == '*') c = '[';
            else c = '(';
            while (s[i] != c) i++;
            v.emplace_back(-(i - sp - 1), i);
        }
    }
}

void apply_transformations() {
    int cnt = 0, pos = 0;
    for (auto& [x, y] : v) {
        if (x > 0) {
            for (int i = 0; i < x; i++) {
                memcpy(t + pos, &s[y + 1], v[cnt].second - y - 1);
                pos += v[cnt].second - y - 1;
            }
        } else {
            int lg = v[cnt].second - y - 2;
            for (int i = 0; i < lg / 2; i++) {
                t[pos + i] = s[y + i + 1];
                t[pos + lg - 1 - i] = s[y + lg - i];
            }
            if (lg % 2 == 1) t[pos + lg / 2] = s[y + lg / 2 + 1];
            pos += lg;
        }
        cnt++;
    }
    t[pos] = '\0';
}

int main() {
    ifstream in("arh.in");
    ofstream out("arh.out");
    in.getline(s, MAXN);
    n = strlen(s);
    get_transformations();
    apply_transformations();
    out << v.size() << '\n' << t << '\n';
    return 0;
}
