#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream fin("culori.in");
  ofstream fout("culori.out");

  int n, m, t;
  fin >> n >> m >> t;

  vector<vector<char>> culori(n, vector<char>(m));
  for (int i = 0; i < n; ++i) {
    for (int j = 0; j < m; ++j) {
      fin >> culori[i][j];
    }
  }

  vector<vector<char>> culori_t = culori;

  while (t--) {
    for (int i = 0; i < n; ++i) {
      for (int j = 0; j < m; ++j) {
        int rosii = 0, albastri = 0;
        int x = max(0, i - 1), y = max(0, j - 1), z = min(n - 1, i + 1), w = min(m - 1, j + 1);
        for (int ii = x; ii <= z; ++ii) {
          for (int jj = y; jj <= w; ++jj) {
            if (culori_t[ii][jj] == 'r') ++rosii;
          }
        }
        albastri = (z - x + 1) * (w - y + 1) - rosii;
        if (rosii > albastri) {
          culori[i][j] = 'r';
        } else if (rosii < albastri) {
          culori[i][j] = 'a';
        }
      }
    }
    culori_t = culori;
  }

  for (int i = 0; i < n; ++i) {
    for (int j = 0; j < m; ++j) {
      fout << culori[i][j];
    }
    fout << '\n';
  }
  return 0;
}
