void afis(int s) {
  for (int i = 1; i <= n; i++) {
    if (s == 1)
      g << v[i] << " ";
    if (s == 2)
      g << w[i] << " ";
  }
  g << "\n";
}
void perm(int k) {
  int i;
  if (k > n) {
    int x = 0, y = 0;
    for (i = 1; i <= n; i++) {
      p1[w[i]] = i;
      p2[v[i]] = i;
    }
    for (i = 1; i <= n; i++) {
      if (p1[i] == i && p2[i] == i) {
        x++;
        b1[x] = i;
      }
      if (p1[i] == n - i + 1 && p2[i] == n - i + 1) {
        y++;
        b2[y] = i;
      }
    }
    if (x == n && y == n) {
      ok = 1;
      if (!sol.size())
        sol = w;
      if (sol.size()) {
        int cnt = 0;
        for (i = 1; i <= n; i++)
          if (sol[i] != w[i]) cnt++;
        if (cnt < sol.size()) sol = w;
      }
    }
    return;
  }
  for (i = 1; i <= n; i++) {
    if (b1[i] == w[k - 1] || b2[i] == w[k - 1]) continue;
    w[k] = i;
    perm(k + 1);
  }
}
int main() {
  int i, j, x, y;
  f >> n;
  for (i = 1; i <= n; i++) {
    f >> x;
    v[i] = x;
    w[i] = x;
  }
  for (i = 1; i <= n; i++) {
    f >> x;
    w[i] = x;
  }
  perm(2);
  if (!ok)
    g << -1 << " " << -1 << "\n\n";
  else {
    for (i = 1; i <= n; i++) {
      for (j = 1; j <= n; j++) {
        if (w[i] == v[j]) {
          swap(v[i], v[j]);
          break;
        }
      }
    }
    for (i = 1; i <= n; i++)
      if (v[i] != sol[i]) y++;
    g << y << " " << y << "\n";
    afis(1);
    afis(2);
  }
  return 0;
}
