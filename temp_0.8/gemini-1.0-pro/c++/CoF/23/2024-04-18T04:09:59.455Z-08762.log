#include <fstream>
#include <vector>
using namespace std;
ifstream in("galeti.in");
ofstream out("galeti.out");
const int NMAX = 100000 + 5;

int n, e, w;
vector <int> a(NMAX), g[NMAX];
vector <pair <int, int> > sol;

int main()
{
  in >> n >> e;
  for (int i = 1; i < n; i++)
  {
    int x, y;
    in >> x >> y;
    g[x].push_back(y);
    g[y].push_back(x);
  }
  a[1] = e;
  bool ok = false;
  while (!ok)
  {
    w = 0;
    for (int i = 1; i <= n; i++)
    {
      while (a[i] >= w)
      {
        for (vector <int> :: iterator it = g[i].begin(); it != g[i].end(); it++)
        {
          if (a[i] - w >= a[*it])
          {
            ok = true;
            for (int j = 0; j < sol.size(); j++)
              if (sol[j].first == *it && sol[j].second == i)
              {
                ok = false;
                break;
              }
            if (ok)
            {
              w += a[*it];
              a[*it] = 0;
              sol.push_back(make_pair(i, *it));
            }
          }
        }
      }
    }
  }
  if (sol.empty())
    out << -1 << '\n';
  else
  {
    for (int i = 0; i < sol.size(); i++)
      out << sol[i].first << ' ' << sol[i].second << '\n';
  }
  return 0;
}
