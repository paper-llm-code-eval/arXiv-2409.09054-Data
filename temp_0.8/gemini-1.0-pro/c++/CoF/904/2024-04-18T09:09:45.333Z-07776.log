#include <fstream>
#include <algorithm>
#include <vector>

struct node{
    int x,y;
};

std::vector<node> v;
int n,m,l,c,V,mat[505][505],mark[505][505],nr,ans=1e9,nrmax=0;
int dl[]={-1,-1,-1,0,0,1,1,1};
int dc[]={-1,0,1,-1,1,-1,0,1};

bool cmp(const node &a, const node &b){
    return a.x<b.x;
}

void df(int x, int y,int &nrx,int &nry){
    nr++;
    mark[x][y]=1;
    for(int k=0;k<8;k++){
        int nx=x+dl[k];
        int ny=y+dc[k];
        if(nx>=1 && nx<=l && ny>=1 && ny<=c && !mark[nx][ny] && mat[nx][ny])
            df(nx,ny,nrx,nry);
    }
}

void dfs(int x, int y,int &nrx,int &nry){
    mark[x][y]=1;
    nrx=min(nrx,x);
    nry=min(nry,y);
    for(int k=0;k<8;k++){
        int nx=x+dl[k];
        int ny=y+dc[k];
        if(nx>=1 && nx<=l && ny>=1 && ny<=c && !mark[nx][ny] && mat[nx][ny])
            dfs(nx,ny,nrx,nry);
    }
}

int main()
{
    std::ifstream fin("deminare.in");
    fin>>V;
    fin>>l>>c;
    fin>>m;
    for(int i=1;i<=m;i++){
        node a;
        fin>>a.x>>a.y;
        v.push_back(a);
        mat[a.x][a.y]=1;
    }
    fin.close();
    sort(v.begin(),v.end(),cmp);
    for(int i=1;i<=m;i++){
        int lx=1e9,ly=1e9;
        if(!mark[v[i].x][v[i].y]){
            nr=0;
            df(v[i].x,v[i].y,lx,ly);
            if(ans>nr){
                ans=nr;
                nrmax=1;
            }
            else if(ans==nr)
                nrmax++;
        }
    }
    std::ofstream fout("deminare.out");
    if(V==1){
        bool stop=0;
        for(int i=1;i<=m;i++){
            if(lx==v[i].x){
                if(!stop){
                    fout<<lx<<' ';
                    stop=1;
                }
            }
            else{
                if(stop)
                    break;
            }
        }
    }
    else{
        if(ans==1e9)
            fout<<-1<<'\n';
        else{
            memset(mark,0,sizeof(mark));
            for(int i=1;i<=m;i++){
                int lx=1e9,ly=1e9;
                if(!mark[v[i].x][v[i].y]){
                    nr=0;
                    dfs(v[i].x,v[i].y,lx,ly);
                    for(int j=lx;j<=l;j++)
                        for(int k=ly;k<=c;k++)
                            mark[j][k]=1;
                }
            }
            int nrx=1e9,nry=1e9;
            bool gasit=0;
            for(int i=1;i<=l;i++)
                for(int j=1;j<=c;j++)
                    if(mark[i][j]){
                        gasit=1;
                        nrx=min(nrx,i);
                        nry=min(nry,j);
                    }
            if(gasit)
                fout<<ans<<'\n';
            else
                fout<<-1<<'\n';
        }
    }
    fout.close();
    return 0;
}
