#include <fstream>
#include <vector>
#include <algorithm>

int main() {
    std::ifstream in("deminare.in");
    std::ofstream out("deminare.out");

    int v, l, c, m, x, y, linie_max, maxim = 0, mutari_min = 0;
    in >> v >> l >> c >> m;

    std::vector<std::vector<int>> teren(l + 1, std::vector<int>(c + 1, 0)); // terenul de joc

    for (int i = 0; i < m; i++) {
        in >> x >> y;
        teren[x][y] = 1; // minare terenul
    }

    for (int i = 1; i <= l; i++) {
        for (int j = 1; j <= c; j++) {
            if (teren[i][j] == 1) {
                maxim++; // numaram minele
            }
        }
        if (maxim > mutari_min) {
            mutari_min = maxim; // retinem numarul maxim de mine
            linie_max = i;       // retinem linia cu numarul maxim de mine
        }
        maxim = 0; // resetam numarul de mine
    }

    if (v == 1) {
        out << linie_max << '\n'; // afisam linia cu numarul maxim de mine
    } else {
        if (mutari_min == l * c) {
            out << 0 << '\n'; // daca toate pozitiile sunt minate, nu trebuie facuta nicio mutare
        } else if (mutari_min <= l || mutari_min <= c) {
            out << mutari_min << '\n'; // daca minele pot fi asezate pe o singura linie sau coloana, afisam numarul minim de mutari
        } else {
            out << -1 << '\n'; // daca minele nu pot fi asezate intr-o zona compacta, afisam -1
        }
    }

    return 0;
}
