            if (Linii[i] == maxL) {
                out << i + 1 << ' ';
            }
        }
        out << '\n';
    } else if (V == 2) {
        int minM = L * C;
        int maxL = 0;
        int maxC = 0;
        for (int i = 0; i < L; ++i) {
            maxL = max(maxL, Linii[i]);
        }
        for (int i = 0; i < C; ++i) {
