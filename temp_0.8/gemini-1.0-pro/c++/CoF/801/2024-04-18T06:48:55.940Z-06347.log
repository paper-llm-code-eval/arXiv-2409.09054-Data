#include <fstream>
#include <vector>
#include <algorithm>

struct Point {
    int x, y;
};

int main() {
    std::ifstream fin("cladiri.in");
    std::ofstream fout("cladiri.out");
    int cx, cy, intensity;
    fin >> cx >> cy >> intensity;
    std::vector<Point> buildings;
    int x, y, strength;
    while (fin >> x >> y >> strength) {
        buildings.push_back({x, y});
    }
    fin.close();

    int n = buildings.size();
    std::vector<std::vector<int>> affected(intensity + 1);
    for (const auto& building : buildings) {
        int dist = (std::abs(building.x - cx) + std::abs(building.y - cy));
        if (dist <= intensity) {
            affected[dist].push_back(building.x);
        }
    }

    int totalAffected = 0;
    for (const auto& level : affected) {
        totalAffected += level.size();
    }

    int maxAffected = 0;
    for (const auto& level : affected) {
        maxAffected = std::max(maxAffected, (int)level.size());
    }

    std::vector<int> levelsWithMaxAffected;
    for (int i = 0; i <= intensity; i++) {
        if ((int)affected[i].size() == maxAffected) {
            levelsWithMaxAffected.push_back(i);
        }
    }

    fout << totalAffected << '\n';
    fout << maxAffected << '\n';
    for (const auto& level : levelsWithMaxAffected) {
        fout << level << ' ';
    }
    fout << '\n';
    fout.close();
    return 0;
}
```
