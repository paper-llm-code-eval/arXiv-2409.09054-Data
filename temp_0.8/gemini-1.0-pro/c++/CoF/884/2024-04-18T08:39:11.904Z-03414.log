std::vector<std::int32_t> get_line(std::ifstream & fin, std::int32_t & buffer_size) {
    static std::vector<std::uint8_t> buffer(buffer_size);
    fin.read(reinterpret_cast<char*>(buffer.data()), buffer_size);
    std::int32_t read = fin.gcount();
    if (read <= 0) {
        buffer_size = 0;
        return {};
    }
    buffer_size = read;
    std::vector<std::int32_t> ret;
    std::int32_t * ptr = reinterpret_cast<std::int32_t*>(buffer.data());
    std::int32_t * end = ptr + read / sizeof(std::int32_t);
    while (ptr != end) {
        ret.push_back(*ptr++);
    }
    return ret;
}

