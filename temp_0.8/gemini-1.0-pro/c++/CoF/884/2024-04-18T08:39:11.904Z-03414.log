                    }
                    mx = i + 1;
                    break;
                }
            }
            if (ddr) {
                if (drr[dt + ds + i]) {
                    if (t > s) {
                        ++tt;
                    } else {
                        ++ts;
                    }
                    mx = i + 1;
                    break;
                }
            }
        }
    }
    if (c == 1) {
        fout.write(reinterpret_cast<const char*>(&tt), sizeof(std::int32_t));
