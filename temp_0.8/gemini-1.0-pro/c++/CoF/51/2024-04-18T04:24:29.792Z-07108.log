        for (int j = 0; j <= k; j++) {
            for (int l = 0; l < n; l++) {
                if (similarities[i][l].first <= j) {
                    dp[i][j] = max(dp[i][j], dp[l][j - similarities[i][l].first] + 1);
                }
            }
        }
    }

    int result = 0;
    for (int i = 0; i < n; i++) {
        result = max(result, dp[i][k]);
    }

    printf("%d\n", result);

    return 0;
}
