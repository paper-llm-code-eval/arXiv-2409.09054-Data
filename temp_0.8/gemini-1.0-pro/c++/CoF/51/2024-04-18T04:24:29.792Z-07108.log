#include <cstring>
#include <cstdio>
#include <cstdlib>
#include <vector>
#include <utility>
#include <algorithm>

using namespace std;

const int max_len = 1000;
const int max_words = 150;
const int max_word_len = 30;

pair<int, int> similarities[max_words][max_words];

int k;
int n;
char line[max_len];
char words[max_words][max_word_len];

int main() {
    freopen("lant.in", "r", stdin);
    freopen("lant.out", "w", stdout);

    scanf("%d ", &k);
    while (fgets(line, max_len, stdin)) {
        char *p = strtok(line, ",:;.!?- ");
        while (p) {
            words[n][strlen(p)] = '\0';
            strcpy(words[n], p);
            n++;
            p = strtok(NULL, ",:;.!?- ");
        }
    }

    int dp[max_words][k + 1];
    memset(dp, 0, sizeof(dp));
    memset(similarities, 0, sizeof(similarities));

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            if (i == j) {
                similarities[i][j].first = 0;
                similarities[i][j].second = -1;
                continue;
            }

            int min_sim = max_len;
            int prev_word = -1;
            for (int l = 0; l < strlen(words[i]); l++) {
                int sim = 0;
                for (int m = 0; m < strlen(words[j]); m++) {
                    if (words[i][l] == words[j][m]) {
                        sim++;
                    }
                }
                sim = strlen(words[i]) - sim + strlen(words[j]) - sim;
                if (sim < min_sim) {
                    min_sim = sim;
                    prev_word = l;
                }
            }
            similarities[i][j].first = min_sim;
            similarities[i][j].second = prev_word;
        }
    }

    for (int i = 0; i < n; i++) {
        for (int j = 0; j <= k; j++) {
            for (int l = 0; l < n; l++) {
                if (similarities[i][l].first <= j) {
                    dp[i][j] = max(dp[i][j], dp[l][j - similarities[i][l].first] + 1);
                }
            }
        }
    }

    int result = 0;
    for (int i = 0; i < n; i++) {
        result = max(result, dp[i][k]);
    }

    printf("%d\n", result);

    return 0;
}
