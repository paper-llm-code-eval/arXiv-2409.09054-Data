#include <fstream>
#include <string>
#include <set>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream in("magic.in");
  ofstream out("magic.out");
  int n;
  in >> n;
  vector<char> v(n);
  for (int i = 0; i < n; ++i) in >> v[i];
  string s;
  set<int> digits;
  for (int i = 0; i < n; ++i) {
    if (isalpha(v[i])) {
      s += toupper(v[i]);
    } else {
      digits.insert(v[i] - '0');
    }
  }
  out << s << endl;
  vector<int> d(digits.begin(), digits.end());
  sort(d.begin(), d.end());
  for (int i = 0; i < d.size(); ++i) {
    if (d[i] != 0) {
      out << d[i];
      break;
    }
  }
  for (int i = 1; i < d.size(); ++i) out << d[i];
  out << endl;
  return 0;
}
```
