#include <fstream>
#include <string>

int main() {
  std::ifstream in("magic.in");
  std::ofstream out("magic.out");
  int n;
  in >> n;
  std::string s;
  in >> s;
  std::string magic;
  std::string number;
  bool found_digit = false;
  for (char c : s) {
    if (std::isdigit(c)) {
      number += c;
      if (!found_digit) {
        if (c == '0') {
          out << "IMPOSIBIL\n";
          return 0;
        }
        magic += c;
        found_digit = true;
      }
    } else {
      magic += std::toupper(c);
    }
  }
  if (magic.empty()) {
    out << "IMPOSIBIL\n";
    return 0;
  }
  std::sort(number.begin(), number.end());
  out << magic << '\n';
  out << number << '\n';
  return 0;
}
```
