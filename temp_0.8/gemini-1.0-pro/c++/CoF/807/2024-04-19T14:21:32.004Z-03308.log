#include <fstream>
#include <vector>

using namespace std;
#define endl '\n'

const int mxn = 1005;
char s[mxn];
int cnt[10];
int main() {
  ifstream fin("magic.in");
  ofstream fout("magic.out");
  int n;
  fin >> n;
  fin >> s;
  vector<char> v;
  for (int i = 0; i < n; ++i) {
    if (s[i] >= 'a' && s[i] <= 'z') {
      v.push_back(s[i] - 'a' + 'A');
    } else {
      cnt[s[i] - '0']++;
    }
  }
  for (char c : v) fout << c;
  fout << endl;
  for (int i = 1; i <= 9; ++i) {
    if (cnt[i]) {
      fout << i;
      cnt[i]--;
      break;
    }
  }
  for (int i = 0; i <= 9; ++i) {
    while (cnt[i]--) {
      fout << (i ? i : 1);
    }
  }
  fout << endl;
  return 0;
}
```
