#include <fstream>

using namespace std;

ifstream fin("spirala.in");
ofstream fout("spirala.out");

int labirint[51][51];
int n, k, ans;

int main()
{
    fin >> n >> k;
    int l = 1, c = n / 2 + 1, dl = 1, dc = 0, pas = 0, contor = 0;
    while (contor < k)
    {
        labirint[l][c] = ++pas;
        if (labirint[l - dl][c - dc] == pas)
            contor++;
        int aux = dl;
        dl = dc;
        dc = -aux;
        l += dl;
        c += dc;
        if (l < 1 || l > n || c < 1 || c > n)
        {
            dl = -dl;
            dc = -dc;
            l += dl;
            c += dc;
        }
        ans++;
    }
    fout << ans;
    return 0;
}
```
