#include <fstream>
#include <iostream>

using namespace std;

const int maxn = 52;
const int dx[] = {1, 0, -1, 0};
const int dy[] = {0, 1, 0, -1};

int n, k, a[maxn][maxn], nr, x, y, dx_, dy_, nx, ny, b[maxn][maxn];

void read() {
  ifstream fin("spirala.in");
  fin >> n >> k;
  fin.close();
}

void ini(int nx, int ny) {
  for (int i = 1; i <= n; ++i) {
    for (int j = 1; j <= n; ++j) {
      nx += dx_;
      ny += dy_;
      a[nx][ny] = ++nr;
    }
    dx_ = dx[i % 4];
    dy_ = dy[i % 4];
  }
}

void afis(int a[maxn][maxn]) {
  ofstream fout("spirala.out");
  for (int i = 1; i <= n; ++i) {
    for (int j = 1; j <= n; ++j) {
      fout << a[i][j] << ' ';
    }
    fout << '\n';
  }
  fout.close();
}

int calculeaza() {
  int x = 1, y = 1, ok = 0, cnt = 0;
  while (!ok) {
    for (int i = 1; i <= n; ++i) {
      for (int j = 1; j <= n; ++j) {
        nx = x + dx_;
        ny = y + dy_;
        if (nx < 1 || nx > n || ny < 1 || ny > n || a[nx][ny] < a[x][y]) {
          dx_ = dx[(i + 1) % 4];
          dy_ = dy[(i + 1) % 4];
          nx = x + dx_;
          ny = y + dy_;
          j = ny;
        } else {
          cnt++;
          x = nx;
          y = ny;
          if (cnt == k) {
            ok = 1;
            break;
          }
        }
      }
    }
  }
  return cnt / k;
}

int main() {
  read();
  ini(x, y);
  cout << calculeaza();
  return 0;
}
