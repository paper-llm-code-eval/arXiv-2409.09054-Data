#include <fstream>
#include <vector>
#include <queue>

using namespace std;

const int INF = 1e9;

struct point {
    int x, y;
};

struct node {
    point p;
    int cost, max_cost;
};

bool operator<(const node& a, const node& b) {
    return a.max_cost < b.max_cost;
}

int main() {
    ifstream in("traseu.in");
    ofstream out("traseu.out");

    int n, m;
    in >> n >> m;

    vector<vector<int>> costs(n, vector<int>(m));
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < m; ++j) {
            in >> costs[i][j];
        }
    }

    vector<vector<int>> dp(n, vector<int>(m, INF));
    dp[0][0] = costs[0][0];

    priority_queue<node> pq;
    pq.push({{0, 0}, costs[0][0], costs[0][0]});

    while (!pq.empty()) {
        node p = pq.top();
        pq.pop();

        if (p.p.x == n - 1 && p.p.y == m - 1) {
            out << p.max_cost - costs[0][0] << endl;
            return 0;
        }

        if (p.p.x + 1 < n && dp[p.p.x + 1][p.p.y] > p.cost) {
            dp[p.p.x + 1][p.p.y] = p.cost;
            pq.push({{p.p.x + 1, p.p.y}, p.cost, max(p.cost, costs[p.p.x + 1][p.p.y])});
        }

        if (p.p.y + 1 < m && dp[p.p.x][p.p.y + 1] > p.cost) {
            dp[p.p.x][p.p.y + 1] = p.cost;
            pq.push({{p.p.x, p.p.y + 1}, p.cost, max(p.cost, costs[p.p.x][p.p.y + 1])});
        }
    }

    out << 0 << endl;
    return 0;
}
