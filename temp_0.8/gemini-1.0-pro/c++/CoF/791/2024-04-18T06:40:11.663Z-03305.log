#include <fstream>
#include <algorithm>
#include <vector>

using namespace std;

ifstream fin("vecini.in");
ofstream fout("vecini.out");

const int dx[] = {0, 1, 0, -1, 1, 1, -1, -1};
const int dy[] = {1, 0, -1, 0, 1, -1, 1, -1};

int n, m, k, a[1005][1005];
int ans, cnt;

int main() {
    fin >> n >> m >> k;
    while (k--) {
        int x, y;
        fin >> x >> y;
        a[x][y] = 1;
    }
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            int vec = 0;
            for (int d = 0; d < 8; d++) {
                int nx = i + dx[d];
                int ny = j + dy[d];
                if (nx >= 1 && nx <= n && ny >= 1 && ny <= m) {
                    vec += (a[i][j] == a[nx][ny]);
                }
            }
            if (vec > ans) {
                ans = vec;
                cnt = 1;
            } else if (vec == ans) {
                cnt++;
            }
        }
    }
    fout << ans << ' ' << cnt << '\n';
    return 0;
}
