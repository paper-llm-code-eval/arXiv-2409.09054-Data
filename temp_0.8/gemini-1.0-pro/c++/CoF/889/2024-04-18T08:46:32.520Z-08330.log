#include <fstream>

using namespace std;

const int MAX_N = 100000 + 5;
const int MOD = 20173333;
int fact[MAX_N], invfact[MAX_N];
int pw[MAX_N];

int modpow(long long base, long long p) {
    if (!p) {
        return 1;
    }
    if (p == 1) {
        return base;
    }
    long long ans = modpow(base, p >> 1);
    ans = (ans * ans) % MOD;
    if (p & 1) {
        ans = (ans * base) % MOD;
    }
    return ans;
}

int com(int n, int k) {
    return (((long long)fact[n] * invfact[n - k]) % MOD) * invfact[k];
}

int cnp(int n, int k) {
    return ((long long)fact[n] * invfact[n - k]) % MOD;
}

