    if (p == 1) {
        fout << pw[n - 1] << '\n';
    } else {
        int ans = 0;
        for (int i = 0; i <= x; i++) {
            ans = (ans + cnp(n - 1, i)) % MOD;
        }
        ans = (1LL * ans * pw[x]) % MOD;
        fout << ans << '\n';
    }
    return 0;
}
