    ifstream fin("sir.in");
    ofstream fout("sir.out");
    int p, n, x;
    fin >> p >> n >> x;

    fact[0] = invfact[0] = pw[0] = 1;
    for (int i = 1; i <= n; i++) {
        fact[i] = (1LL * fact[i - 1] * i) % MOD;
        invfact[i] = modpow(fact[i], MOD - 2);
        pw[i] = (1LL * pw[i - 1] * x) % MOD;
