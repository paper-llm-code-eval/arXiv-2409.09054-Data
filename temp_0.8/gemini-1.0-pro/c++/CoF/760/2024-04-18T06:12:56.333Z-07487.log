    if (urcare) {
      consum += 3 * distanta;
    } else {
      consum += distanta;
    }
    numar_persoane += persoane[i];
  }

  fout << numar_persoane << '\n' << consum << '\n' << stații_speciale << '\n';
  return 0;
}
