#include <fstream>
#include <vector>
#include <iostream>

using namespace std;

bool is_boy(char gender) {
    return gender == 'b';
}

int main() {
    ifstream in("cartele.in");
    ofstream out("cartele.out");
    int task, n;
    in >> task >> n;
    vector<pair<pair<int, int>, pair<int, char>>> all_events;
    for (int i = 0; i < n; i++) {
        char gender, action;
        int hour, minute, second;
        in >> gender >> action >> hour >> minute >> second;
        all_events.push_back({{hour, minute}, {second, gender}});
    }
    int boys = 0, girls = 0;
    if (task == 1) {
        for (auto& event : all_events) {
            if (is_boy(event.second.second))
                boys++;
            else
                girls++;
        }
        out << boys << ' ' << girls << '\n';
    } else if (task == 2) {
        int seconds = 0;
        int diff = 0;
        for (auto& event : all_events) {
            diff += is_boy(event.second.second) ? 1 : -1;
            seconds += diff == 0;
        }
        out << seconds << '\n';
    } else if (task == 3) {
        int seconds = 0;
        int diff = 0;
        for (auto& event : all_events) {
            diff += is_boy(event.second.second) ? 1 : -1;
            seconds = max(seconds, diff == 0 ? 0 : diff % 2 == 1 ? diff : seconds);
        }
        out << seconds * 60 << '\n';
    }
    return 0;
}
