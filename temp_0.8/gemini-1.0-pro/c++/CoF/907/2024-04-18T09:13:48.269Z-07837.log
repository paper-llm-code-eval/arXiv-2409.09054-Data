#include <fstream>
#include <vector>

using namespace std;

struct event {
    char type;
    char action;
    int hour;
    int minute;
    int second;
};

int main() {
    ifstream fin("cartele.in");
    ofstream fout("cartele.out");

    int C, N;
    fin >> C >> N;

    vector<event> events;
    events.reserve(N);

    for (int i = 0; i < N; i++) {
        event e;
        fin >> e.type >> e.action >> e.hour >> e.minute >> e.second;
        events.push_back(e);
    }

    int boys = 0;
    int girls = 0;
    int equalTime = 0;
    int maxOddTime = 0;
    int oddTime = 0;

    for (const event& e : events) {
        if (C == 1) {
            e.type == 'b' ? boys++ : girls++;
        } else if (C == 2) {
            e.type == 'b' ? boys++ : girls--;
            if (boys == girls && boys != 0) {
                equalTime += e.second - events[i - 1].second;
            }
        } else if (C == 3) {
            e.type == 'b' ? boys++ : boys--;
            if (boys % 2 != 0) {
                oddTime += e.second - events[i - 1].second;
            } else {
                maxOddTime = max(maxOddTime, oddTime);
                oddTime = 0;
            }
        }
    }

    if (C == 1) {
        fout << boys << ' ' << girls;
    } else if (C == 2) {
        fout << equalTime;
    } else if (C == 3) {
        fout << maxOddTime;
    }

    fin.close();
    fout.close();
    return 0;
}
