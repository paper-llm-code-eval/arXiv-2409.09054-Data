#include <fstream>

using namespace std;

int main() {
  ifstream in("cartele.in");
  ofstream out("cartele.out");
  int cerinta, n, ora, minute, secunde;
  char sex, actiune;
  in >> cerinta >> n;
  int numar_baieti = 0, numar_fete = 0, numar_egalitate = 0, numar_impar = 0,
      timp_egalitate = 0, timp_impar = 0, timp_anterior = 0;
  while (n--) {
    in >> sex >> actiune >> ora >> minute >> secunde;
    if (actiune == 'i') {
      if (sex == 'b')
        numar_baieti++;
      else
        numar_fete++;
    } else {
      if (sex == 'b')
        numar_baieti--;
      else
        numar_fete--;
    }
    int timp_curent = ora * 3600 + minute * 60 + secunde;
    if (numar_baieti == numar_fete && numar_baieti > 0) {
      timp_egalitate += timp_curent - timp_anterior;
      numar_egalitate++;
    }
    if (numar_baieti % 2) {
      timp_impar += timp_curent - timp_anterior;
      numar_impar++;
    }
    timp_anterior = timp_curent;
  }
  if (cerinta == 1)
    out << numar_baieti << ' ' << numar_fete << '\n';
  else if (cerinta == 2) {
    if (numar_egalitate > 0)
      out << timp_egalitate << '\n';
    else
      out << 0 << '\n';
  } else {
    if (numar_impar > 0)
      out << timp_impar << '\n';
    else
      out << 0 << '\n';
  }
  in.close();
  out.close();
  return 0;
}
