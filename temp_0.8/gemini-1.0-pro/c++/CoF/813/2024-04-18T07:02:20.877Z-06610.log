#include <fstream>
#include <vector>

int main() {
  std::ifstream in("adunscad.in");
  std::ofstream out("adunscad.out");

  int n, m;
  in >> n >> m;
  std::vector<char> digits(m);
  for (int i = 0; i < m; ++i) in >> digits[i];

  int current = digits[0] - '0';
  int op = n - current;
  out << (op >= 0 ? "" : "-") << current;
  for (int i = 1; i < m; ++i) {
    current = digits[i] - '0';
    op = n - current;
    out << (op >= 0 ? "+" : "-") << current;
  }

  out << "\n";

  return 0;
}
```
