#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

typedef long long ll;

int main() {
    ifstream fin("fotbal.in");
    ofstream fout("fotbal.out");
    int n, k;
    fin >> n >> k;
    vector<pair<ll, ll>> st;
    vector<pair<ll, ll>> dr;
    ll x, y, p;
    for (int i = 0; i < n; i++) {
        fin >> x >> y >> p;
        if (p == 1) {
            dr.push_back({x, y});
        } else {
            st.push_back({x, y});
        }
    }
    int lst, rst;
    ll ans = 0;
    sort(dr.begin(), dr.end());
    sort(st.begin(), st.end());
    for (int i = 0; i < dr.size(); i++) {
        if (i > 0) {
            rst = max(rst, dr[i].second);
        } else {
            rst = dr[i].second;
        }
        for (int j = 0; j < st.size(); j++) {
            if (j > 0) {
                lst = max(lst, st[j].second);
            } else {
                lst = st[j].second;
            }
            if (dr[i].first <= lst && st[j].first <= rst) {
                ans += (j + 1) * (i + 1);
            }
        }
    }
    fout << ans % (1000000000 + 9) << "\n";
    return 0;
}
