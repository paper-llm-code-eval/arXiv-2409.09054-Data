#include <fstream>
#include <string>
#include <vector>

using namespace std;

int main() {
    ifstream fin("litere.in");
    ofstream fout("litere.out");

    // Read the input data
    int p, n;
    fin >> p >> n;
    string name;
    fin >> name;

    // 1.
    int stars = n % 3;
    if (stars == 1) stars++;
    if (p == 1) {
        fout << stars << endl;
        return 0;
    }

    // 2.
    vector<char> firstRow;
    for (int i = 0; i < n; i++) {
        if (i % 3 == 0) firstRow.push_back(name[i]);
    }
    if (p == 2) {
        for (int i = 0; i < firstRow.size(); i++) {
            fout << firstRow[i] << ' ';
        }
        fout << endl;
        return 0;
    }

    // 3.
    int rows = n / 3 + (stars > 0);
    vector<vector<char>> rotated(rows);
    int row = 0, col = 0;
    for (int i = n - 1; i >= 0; i--) {
        rotated[row][col++] = name[i];
        if (col == 3) {
            col = 0;
            row++;
        }
    }
    while (col < 3 && stars > 0) {
        rotated[row][col++] = '*';
        stars--;
    }

    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < 3; j++) {
            fout << rotated[i][j] << ' ';
        }
        fout << endl;
    }

    return 0;
}
