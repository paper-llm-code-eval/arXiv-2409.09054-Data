#include <vector>

namespace strips {

struct interval {
    std::int32_t start, end;
    interval(std::int32_t start = 0, std::int32_t end = 0)
        : start(start), end(end) {}
};

struct player_info {
    std::int32_t penalty, max_zone;
    player_info(std::int32_t penalty = 0, std::int32_t max_zone = 0)
        : penalty(penalty), max_zone(max_zone) {}
};

struct board_state {
    std::int32_t n;
    std::int32_t l;
    std::vector<interval> taken;

    board_state(std::int32_t n = 0, std::int32_t l = 0) : n(n), l(l) {}

    bool valid_position(std::int32_t position) const {
        if (position < 0 || position + l > n)
            return false;
        for (auto it = taken.begin(); it != taken.end(); it++)
            if (position >= it->start && position + l <= it->end + 1)
                return false;
        return true;
    }
};

std::array<player_info, 2> play_game(const board_state& board,
                                      std::int32_t nr_moves) {
    std::array<player_info, 2> result = {player_info(), player_info()};
    std::vector<std::int32_t> moves(nr_moves);
    std::ifstream(R"(strips.in)", std::ios_base::binary | std::ios_base::in)
        .read((char*)moves.data(), nr_moves * sizeof(std::int32_t));
    for (auto move : moves) {
        if (result[0].max_zone < move + 1)
            result[0].max_zone = move + 1;
        auto position = std::distance(moves.begin(),
                                      std::find(moves.begin(), moves.end(), move));
        if (position & 1) {
            if (board.valid_position(move))
                result[1].max_zone = std::max(result[1].max_zone, move + l);
            else
                result[1].penalty++;
        } else {
            if (board.valid_position(move))
                result[0].max_zone = std::max(result[0].max_zone, move + l);
            else
                result[0].penalty++;
        }
    }
    return result;
}

}  // namespace strips

int main() {
    std::int32_t c, n, nr, l;
    std::ifstream(R"(strips.in)", std::ios_base::binary | std::ios_base::in)
        .read((char*)&c, sizeof(std::int32_t))
        .read((char*)&n, sizeof(std::int32_t))
        .read((char*)&nr, sizeof(std::int32_t))
        .read((char*)&l, sizeof(std::int32_t));
    auto result = strips::play_game(strips::board_state{n, l}, nr);
    std::ofstream(R"(strips.out)", std::ios_base::binary | std::ios_base::out)
        .write((char*)result.data(), sizeof(std::int32_t) * 2);
    return 0;
}
