          dp2[i][1][0] = dp2[j][1][0] + p[i] * p[(i + 1) % n];
          next2[i][1][0] = j;
        }
        if (dp2[j][0][1] + p[i] * p[(i + 1) % n] > dp2[i][0][1]) {
          dp2[i][0][1] = dp2[j][0][1] + p[i] * p[(i + 1) % n];
          next2[i][0][1] = j;
        }
      }
      if (p[j] > p[i]) {
        // cazul in care adaugam robotul i intr-o subsecventa descrescatoare
        if (dp2[j][0][0] + p[i] * p[(i + 1) % n] > dp2[i][1][1]) {
          dp2[i][1][1] = dp2[j][0][0] + p[i] * p[(i + 1) % n];
          next2[i][1][1] = j;
        }
        if (dp2[j][1][0] + p[i] * p[(i + 1) % n] > dp2[i][0][0]) {
          dp2[i][0][0] = dp2[j][1][0] + p[i] * p[(i + 1) % n];
          next2[i][0][0] = j;
        }
      }
    }
    for (int j = 0; j < 2; ++j) {
      for (int k = 0; k < 2; ++k) {
        dp2[i + 1][j][k] = std::max(dp2[i + 1][j][k], dp2[i][j][k]);
      }
    }
  }
  ans = -inf;
  for (int j = 0; j < 2; ++j) {
    for (int k = 0; k < 2; ++k) {
      if (dp2[n][j][k] > ans) {
        ans = dp2[n][j][k];
        pos[n] = j;
        pos[n + 1] = k;
      }
    }
  }
  int i = n;
  while (i > 1) {
    pos[i] = next2[i][pos[i]][pos[i + 1]];
    i = pos[i];
  }
  for (int i = 1; i <= n; ++i) {
    out << p[pos[i]] << ' ';
  }
  out << std::endl;
  return 0;
}
