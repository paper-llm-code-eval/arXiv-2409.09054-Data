      dp[i][j][0] = max(max(dp[i - 1][j][0], dp[i - 1][j][1]), max(dp[i][j - 1][0], dp[i][j - 1][1]));
      dp[i][j][1] = max(max(dp[i - 1][j][1], dp[i - 1][j - 1][0]), max(dp[i][j - 1][1], dp[i - 1][j - k][1]));
      if (i >= k && j >= k) {
        dp[i][j][0] = max(dp[i][j][0], dp[i - k][j - k][1]);
        dp[i][j][1] = max(dp[i][j][1], dp[i - k][j - k][1]);
      }
      dp[i][j][0] += a[i][j];
      dp[i][j][1] += a[i][j];
    }
  }
  int val = dp[n][n][0];
  int type = 0;
  for (int i = n; i > n - k; i--) {
    if (dp[i][n][0] > val) {
      val = dp[i][n][0];
      type = 0;
    }
    if (dp[i][n][1] > val) {
      val = dp[i][n][1];
      type = 1;
    }
  }
  in.close();
  out << val << " ";
  if (type == 0) {
    out << n - k + 1 << " " << 1 << "\n";
  } else {
    out << n - k + 1 << " " << n - k + 1 << "\n";
  }
  out.close();
  return 0;
}
