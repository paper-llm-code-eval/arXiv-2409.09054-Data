#include <fstream>
#include <vector>

using namespace std;

int main() {
    ifstream in("cruce.in");
    ofstream out("cruce.out");

    int n, k;
    in >> n >> k;

    vector<vector<int>> matrix(n, vector<int>(n));
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            in >> matrix[i][j];
        }
    }

    int max_value = -1e9, max_line = -1, max_column = -1;
    for (int i = 0; i <= n - k; i++) {
        for (int j = 0; j <= n - k; j++) {
            int value = 0;
            for (int ii = i; ii < i + k; ii++) {
                for (int jj = j; jj < j + k; jj++) {
                    value += matrix[ii][jj];
                }
            }
            if (value > max_value) {
                max_value = value;
                max_line = i;
                max_column = j;
            }
        }
    }

    out << max_value << " " << max_line + 1 << " " << max_column + 1 << endl;

    return 0;
}
```
