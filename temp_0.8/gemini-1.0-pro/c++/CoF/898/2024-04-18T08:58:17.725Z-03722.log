#include <fstream>

using namespace std;

const int NMAX = 505;

int n, k;
int a[NMAX][NMAX], sum[NMAX][NMAX], sumc[NMAX][NMAX];
int ans;
pair<int, int> poz;

int main() {
	ifstream in("cruce.in");
	in >> n >> k;
	for (int i = 1; i <= n; i++)
		for (int j = 1; j <= n; j++) {
			in >> a[i][j];
			sum[i][j] = sum[i][j - 1] + sum[i - 1][j] - sum[i - 1][j - 1] + a[i][j];
			sumc[i][j] = sumc[i - 1][j] + sumc[i][j - 1] - sumc[i - 1][j - 1] + a[i][j];
		}
	in.close();

	for (int i = 1; i <= n - k + 1; i++) {
		for (int j = 1; j <= n - k + 1; j++) {
			int sum_c = sum[i + k - 1][j + k - 1] - sum[i + k - 1][j - 1] - sum[i - 1][j + k - 1] + sum[i - 1][j - 1];
			int sum_l = sumc[i + k - 1][j + k - 1] - sumc[i + k - 1][j - 1] - sumc[i - 1][j + k - 1] + sumc[i - 1][j - 1];
			if (sum_c + sum_l > ans) {
				ans = sum_c + sum_l;
				poz = {i, j};
			}
		}
	}

	ofstream out("cruce.out");
	out << ans << ' ' << poz.first << ' ' << poz.second << '\n';
	out.close();

	return 0;
}
