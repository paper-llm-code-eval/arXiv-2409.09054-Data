        if (cnt > 0) {
            if (A == 0) A = M;
            A = max(A, M);
        }
        
        nr += s;
        b += 2;
    }
    
    C = n - C;
    fout << P << '\n' << M << '\n' << C << '\n' << A << '\n';
    return 0;
}
