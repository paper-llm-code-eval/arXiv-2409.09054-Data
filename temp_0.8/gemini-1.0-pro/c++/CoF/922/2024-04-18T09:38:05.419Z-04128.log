    if (cerinta == 1) {
        timp += 2 * max_oriz + (foi.size() - 1) * 3;
        fout << timp << '\n';
    } else if (cerinta == 2) {
        fout << oriz << '\n';
    } else {
        for (; oriz < t; timp += 3) {
            oriz += foi[ord % n].first;
            ord++;
        }

        fout << ord % n + 1 << '\n';
    }
}
