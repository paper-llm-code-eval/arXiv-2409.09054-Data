ifstream fin("furnica.in");
ofstream fout("furnica.out");

int main() {
    int cerinta, n, t;
    long long timp = 0, max_oriz = 0, oriz = 0, ord = 1;
    vector<pair<int, int>> foi;

    fin >> cerinta >> n;
    if (cerinta == 3) {
        fin >> t;
    }

