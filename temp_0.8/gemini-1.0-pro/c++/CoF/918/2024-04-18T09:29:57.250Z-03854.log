  x = 1;
  y = 1;
  for (i = 1; i <= n * n; i++) {
    v[x][y] = i;
    if (x + y == n) {
      if (y == n) {
        y--;
        x++;
      } else {
        y++;
        x--;
      }
    } else if (x - y == 1) {
      y++;
    } else {
      x++;
    }
  }
  for (i = 1; i <= n; i++) {
    for (j = 1; j <= n; j++) {
      if (v[i][j] == n * n / 2) {
        out << i << ' ' << j << '\n';
        break;
      }
    }
  }
  for (i = n; i >= 1; i--) {
    for (j = n; j >= 1; j--) {
      if (v[i][j] == n * n / 2 + 1) {
        out << i << ' ' << j << '\n';
        break;
      }
    }
  }
  return 0;
}
