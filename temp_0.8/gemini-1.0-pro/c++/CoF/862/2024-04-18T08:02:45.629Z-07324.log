    if (s[i] > s[i - 1])
      alter++;
    else if (s[i] < s[i - 1])
      alter--, alterMax = max(alterMax, alter);
  }
  fout << alterMax << '\n';

  dp[n][0] = dp[n][1] = 0;
  for (int i = n - 1; i >= 0; i--) {
    int op0 = dp[i + 1][0];
    int op1 = f[val[s[i]]];
    if (i + 1 < n && s[i] < s[i + 1] && s[i] > s[i + 2])
      op1 = max(op1, f[val[s[i + 2]]]);
    if (i + 1 < n && s[i] > s[i + 1] && s[i] < s[i + 2])
      op1 = max(op1, f[val[s[i + 1]]]);
    dp[i][0] = max(dp[i + 1][0], op0);
    dp[i][1] = max(dp[i + 1][1], op1 + dp[i + 1][0]);
  }
  rez2 = dp[0][1];
  for (int i = 0, j = 0; i < n; i++) {
    int op1 = f[val[s[i]]];
    if (j + 1 < n && s[j] < s[j + 1] && s[j] > s[j + 2])
      op1 = max(op1, f[val[s[j + 2]]]);
    if (j + 1 < n && s[j] > s[j + 1] && s[j] < s[j + 2])
      op1 = max(op1, f[val[s[j + 1]]]);
    if (op1 + dp[i + 1][0] == rez2) {
      ss += s[j];
      j++;
    } else {
      fout << ss << '\n' << rez2 << '\n';
      return 0;
    }
  }
  fout << ss << '\n' << rez2 << '\n';
  return 0;
}
