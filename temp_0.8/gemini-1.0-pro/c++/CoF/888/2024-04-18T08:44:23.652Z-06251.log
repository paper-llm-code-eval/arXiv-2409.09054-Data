            for (int j = 0; j <= N; j++)
                dist[i][j] = INT_MAX;
        int cost = dijkstra();
        fout << (cost == INT_MAX ? -1 : cost) << endl;
    } else {
        fin >> N;
        for (int i = 1; i <= N; i++)
            for (int j = 1; j <= N; j++)
                fin >> A[i][j];
        fout << binary_search() << endl;
    }
    
    fin.close();
    fout.close();
    return 0;
}
