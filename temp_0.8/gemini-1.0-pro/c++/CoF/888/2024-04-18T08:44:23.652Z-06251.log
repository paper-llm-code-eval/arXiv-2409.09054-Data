#include <fstream>
#include <vector>
#include <queue>
#include <climits>

using namespace std;

const int MAX_DIM = 505;
int N, G, A[MAX_DIM][MAX_DIM], dist[MAX_DIM][MAX_DIM];
vector<pair<int, int>> moves = {{-1, 0}, {1, 0}, {0, -1}, {0, 1}};

int dijkstra() {
    priority_queue<pair<int, pair<int, int>>> pq;
    pq.push({0, {1, 1}});
    dist[1][1] = 0;
    while (!pq.empty()) {
        int cost = -pq.top().first;
        int i = pq.top().second.first;
        int j = pq.top().second.second;
        pq.pop();
        
        if (i == N && j == N)
            return cost;
        
        for (auto& move : moves) {
            int ii = i + move.first;
            int jj = j + move.second;
            if (ii < 1 || jj < 1 || ii > N || jj > N)
                continue;
            int newCost = cost;
            if (A[ii][jj] < G)
                newCost++;
            if (dist[ii][jj] > newCost) {
                dist[ii][jj] = newCost;
                pq.push({-newCost, {ii, jj}});
            }
        }
    }
    return INT_MAX;
}

int binary_search() {
    int low = 0, high = 10000;
    while (low < high) {
        G = (low + high) / 2;
        int cost = dijkstra();
        if (cost != INT_MAX)
            high = G;
        else
            low = G + 1;
    }
    return low;
}

int main() {
    ifstream fin("rover.in");
    ofstream fout("rover.out");
    
    int V;
    fin >> V;
    if (V == 1) {
        fin >> N >> G;
        for (int i = 1; i <= N; i++)
            for (int j = 1; j <= N; j++)
                fin >> A[i][j];
        fout << dijkstra() << endl;
    } else {
        fin >> N;
        for (int i = 1; i <= N; i++)
            for (int j = 1; j <= N; j++)
                fin >> A[i][j];
        fout << binary_search() << endl;
    }
    
    fin.close();
    fout.close();
    return 0;
}
