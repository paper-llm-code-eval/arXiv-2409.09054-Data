#include <fstream>
#include <cstring>
#include <algorithm>
#include <vector>
#include <stack>
#include <queue>

using namespace std;

struct Node {
  string name;
  vector<Node *> children;
};

Node *root;
vector<string> files;

void parse_tree(const string &s) {
  stack<Node *> st;
  Node *current_node;
  for (size_t i = 0; i < s.size(); i++) {
    if (s[i] == '(') {
      current_node = new Node;
      current_node->name = "";
      st.push(current_node);
    } else if (s[i] == ')') {
      current_node = st.top();
      st.pop();
      if (!st.empty()) {
        st.top()->children.push_back(current_node);
      } else {
        root = current_node;
      }
    } else {
      current_node->name += s[i];
    }
  }
}

void dfs(Node *node, string path) {
  if (node->children.empty() && islower(node->name[0])) {
    files.push_back(path + node->name);
  } else {
    for (size_t i = 0; i < node->children.size(); i++) {
      dfs(node->children[i], path + node->name + "\\");
    }
  }
}

bool compare_paths(const string &a, const string &b) {
  return a < b;
}

int main() {
  ifstream fin("dir.in");
  ofstream fout("dir.out");

  string s;
  fin >> s;

  parse_tree(s);
  dfs(root, "");

  sort(files.begin(), files.end(), compare_paths);

  fout << files.size() << "\n";
  for (size_t i = 0; i < files.size(); i++) {
    fout << files[i] << "\n";
  }

  return 0;
}
