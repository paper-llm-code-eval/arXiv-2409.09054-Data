#include <cstdio>
#include <cstring>
#include <vector>
#include <algorithm>

const int NMAX = 1e3 + 11;
const int INF = 1e9 + 7;

int n;
int a[NMAX], b[NMAX], sum[NMAX];

int calc_sum(int x) {
    int s = 0;
    for (int i = 1; i <= n; ++i) {
        s += std::min(x, a[i] + b[i]);
    }
    return s;
}

int solve() {
    int lim = INF;
    int l = 0, r = lim;
    while (l < r) {
        int mid = (l + r) >> 1;
        if (calc_sum(mid) == lim) {
            r = mid;
        } else {
            l = mid + 1;
        }
    }

    int ans_sum = calc_sum(l);
    int ans_len = l;
    int ans_nr = 0;
    
    for (int i = 1; i <= n; ++i) {
        int x = std::min(ans_len, a[i] + b[i]);
        if (x == ans_len) {
            ++ans_nr;
        }
    }

    return ans_sum - ans_nr * ans_len;
}

int main() {
    freopen("bete.in", "r", stdin);
    freopen("bete.out", "w", stdout);
    scanf("%d", &n);
    for (int i = 1; i <= n; ++i) {
        scanf("%d", &a[i]);
    }
    for (int i = 1; i <= n; ++i) {
        scanf("%d", &b[i]);
    }
    printf("%d\n", solve());
    return 0;
}
