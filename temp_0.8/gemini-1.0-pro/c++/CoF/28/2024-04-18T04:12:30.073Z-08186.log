    return !dp[a].cnt || dp[a].cnt == dp[b].cnt && dp[a].ans < dp[b].ans;
}

int main() {
    int n;
    fin >> n;
    int q;
    fin >> q;
    dp[0].cnt = 1LL;
    dp[0].ans = dp[0].pos = 0LL;
    queue<int> Q;
    Q.push(0);
    while (!Q.empty()) {
        int u = Q.front();
        Q.pop();
        if (u) {
            for (int i = 0, ni = u; i < MAX_N; ++i, ni >>= 1) {
                if (ni & 1) {
                    int v = u ^ (1 << i);
                    if (ok(v, u)) {
                        dp[u] = dp[v];
                        Q.push(u);
                        break;
                    }
                }
