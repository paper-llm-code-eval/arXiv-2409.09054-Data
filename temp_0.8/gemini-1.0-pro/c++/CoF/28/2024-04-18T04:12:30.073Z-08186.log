            if (u & (1 << i)) {
                continue;
            }
            p[j++] = i;
        }
        for (int i = 0, j = n; i < n; ++i) {
            if (u & (1 << (i + n))) {
                continue;
            }
            p[j++] = i;
        }
        pair<long long, long long> ans = permi(n, u);
        dp[u].cnt = ans.second;
        dp[u].ans = ans.first;
        dp[u].p = p;
        for (int v : G[u]) {
            if (ok(u, v)) {
                dp[v] = dp[u];
                Q.push(v);
            }
        }
    }
    for (; q--;) {
        int tip;
        fin >> tip;
        if (tip == 1) {
            int pos;
            fin >> pos;
            --pos;
            data_t ans = dp[pos];
            for (int i : ans.p) {
                fout << i + 1 << " ";
            }
            fout << "\n";
        } else {
            p.resize(n);
            for (int i = 0; i < 2 * n; ++i) {
                int x;
                fin >> x;
                --x;
                p[x] = i;
            }
            long long mask = 0LL;
            for (int i = 0; i < n; ++i) {
                mask |= 1LL << p[i];
            }
            for (int i = n; i < 2 * n; ++i) {
                mask |= 1LL << (p[i] + n);
            }
            fout << dp[mask].pos + 1 << "\n";
        }
    }
    return 0;
}
