bool cmp(int a, int b) {
    return p[a] < p[b];
}

pair<long long, long long> permi(int n, int k) {
    long long mask = (1LL << 2 * n) - 1LL;
    while (k) {
        int i = __builtin_ctzll(k);
        c.push_back(i);
        k -= 1LL << i;
    }
    int m = c.size();
    vector<int> perm(m);
    iota(perm.begin(), perm.end(), 0);
    sort(perm.begin(), perm.end(), cmp);
    d.resize(m);
