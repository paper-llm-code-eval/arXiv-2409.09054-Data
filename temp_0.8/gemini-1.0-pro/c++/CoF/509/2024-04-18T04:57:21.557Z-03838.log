    ll duration(bool inverted) const {
        ll duration = (H2 - H1) * 60 + (M2 - M1);
        if (duration < 0) {
            duration += 24 * 60;
        }
        return inverted ? duration : -duration;
    }
};

ll is_prime(ll n) {
    if (n <= 1) {
        return false;
