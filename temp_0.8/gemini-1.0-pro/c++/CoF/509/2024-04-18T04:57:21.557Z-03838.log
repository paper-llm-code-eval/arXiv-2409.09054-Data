    Point(ll _x, ll _y, ll _z, ll _w) : x(_x), y(_y), z(_z), w(_w) {}

    Point operator+(const Point& other) const {
        return Point((x + other.x) % MOD, (y + other.y) % MOD, (z + other.z) % MOD, (w + other.w) % MOD);
    }

    Point operator-(const Point& other) const {
