#include <fstream>
#include <tuple>

using namespace std;

struct Flight {
  int dep_airport, arr_airport, dep_hours, dep_minutes, arr_hours, arr_minutes;
  Flight() {}
  Flight(int d, int a, int dh, int dm, int ah, int am)
      : dep_airport(d),
        arr_airport(a),
        dep_hours(dh),
        dep_minutes(dm),
        arr_hours(ah),
        arr_minutes(am) {}
};

bool is_prime(int n) {
  if (n <= 1) return false;
  if (n <= 3) return true;
  if (n % 2 == 0 || n % 3 == 0) return false;
  int i = 5;
  while (i * i <= n) {
    if (n % i == 0 || n % (i + 2) == 0) return false;
    i += 6;
  }
  return true;
}

int sum_digits(int n) {
  int sum = 0;
  while (n) {
    sum += n % 10;
    n /= 10;
  }
  return sum;
}

tuple<int, int> get_flight_duration(const Flight &f) {
  int hours = f.arr_hours - f.dep_hours;
  int minutes = f.arr_minutes - f.dep_minutes;
  if (minutes < 0) {
    hours--;
    minutes += 60;
  }
  return make_tuple(hours, minutes);
}

int main() {
  ifstream in("aeriana.in");
  ofstream out("aeriana.out");

  int c, n;
  in >> c >> n;

  vector<Flight> flights(n);
  for (int i = 0; i < n; i++) {
    in >> flights[i].dep_airport >> flights[i].arr_airport >> flights[i].dep_hours >>
        flights[i].dep_minutes >> flights[i].arr_hours >> flights[i].arr_minutes;
  }

  int max_duration_normal = 0, max_duration_reversed = 0;
  for (int i = 0; i < n; i++) {
    auto [hours_n, minutes_n] = get_flight_duration(flights[i]);
    auto [hours_r, minutes_r] = get_flight_duration(
        Flight(flights[i].arr_airport, flights[i].dep_airport, flights[i].arr_hours,
               flights[i].arr_minutes, flights[i].dep_hours, flights[i].dep_minutes));

    if (is_prime(flights[i].dep_airport) &&
        flights[i].arr_airport % sum_digits(flights[i].dep_airport) == 0) {
      max_duration_reversed = max(max_duration_reversed, hours_r * 60 + minutes_r);
    }
    max_duration_normal = max(max_duration_normal, hours_n * 60 + minutes_n);
  }

  if (c == 1) {
    out << max_duration_normal / 60 << " " << max_duration_normal % 60 << endl;
  } else {
    out << max(max_duration_normal, max_duration_reversed) / 60 << " "
        << max(max_duration_normal, max_duration_reversed) % 60 << endl;
  }

  return 0;
}
