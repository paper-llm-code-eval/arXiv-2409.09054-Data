#include <iostream>
#include <string>
#include <unordered_map>
#include <vector>
#include <utility>
#include <algorithm>
using namespace std;

unordered_map<string, int> frecv;

int main() {
    ifstream fin("datorii.in");
    ofstream fout("datorii.out");
    int c, n, x, y, s;
    fin >> c >> n;
    if (c == 1) {
        for (int i = 0; i < n; i++) {
            string s1, s2;
            fin >> s1 >> s2 >> y;
            frecv[s1]++;
            frecv[s2]++;
        }
        fout << frecv.size() << "\n";
    } else if (c == 2) {
        vector<pair<string, pair<int, int>>> v;
        for (int i = 0; i < n; i++) {
            string s1, s2;
            fin >> s1 >> s2 >> y;
            if (s1 > s2) swap(s1, s2);
            frecv[s1] -= y;
            frecv[s2] += y;
        }
        for (auto it : frecv) {
            if (it.second > 0)
                v.push_back(make_pair(it.first, make_pair(0, it.second)));
            else if (it.second < 0)
                v.push_back(make_pair(it.first, make_pair(-it.second, 0)));
        }
        sort(v.begin(), v.end());
        for (auto it : v)
