#include <fstream>
#include <iostream>
#include <set>
#include <string>
#include <utility>
#include <vector>

using namespace std;

int main() {
  ifstream in("datorii.in");
  ofstream out("datorii.out");
  int c;
  in >> c;
  int d;
  in >> d;
  set<string> firms;
  vector<pair<string, pair<int, int>>> debts(6000);
  for (int i = 0; i < d; i++) {
    string a, b;
    int s;
    in >> a >> b >> s;
    firms.insert(a);
    firms.insert(b);
    debts[i] = {a, {s, 0}};
  }
  if (c == 1) {
    out << firms.size() << '\n';
  } else {
    for (const string& firm : firms) {
      int d = 0, c = 0;
      for (const auto& deb : debts) {
        if (deb.first == firm) {
          d += deb.second.first;
        } else if (deb.second.first == firm) {
          c += deb.second.second;
        }
      }
      for (auto& deb : debts) {
        if (deb.second.first == firm) {
          deb.second.second = c;
          break;
        }
      }
      debts.push_back({firm, {d, c}});
    }
    for (const auto& deb : debts) {
      if (deb.first == "") continue;
      out << deb.first << ' ' << deb.second.first << ' ' << deb.second.second
          << '\n';
    }
  }
  return 0;
}
