    in >> T;

    int N;
    in >> N;

    vector<int> puncte(N, 0);
    for (int i = 0; i < N; ++i) {
        in >> puncte[i];
    }

    int D;
    in >> D;
    vector<bool> meciuri(N * N, false);
    for (int i = 0; i < D; ++i) {
        int a, b;
        in >> a >> b;
        meciuri[(a - 1) * N + b - 1] = true;
        meciuri[(b - 1) * N + a - 1] = true;
    }

    if (T == 1) {
        // Punctaj maxim daca toate meciurile se termina la egalitate
        int max_puncte = *max_element(puncte.begin(), puncte.end());
        vector<int> teams;
        for (int i = 0; i < N; ++i) {
            if (puncte[i] == max_puncte) {
                teams.push_back(i + 1);
            }
        }
        sort(teams.begin(), teams.end());
        for (int i = 0; i < teams.size(); ++i) {
            out << teams[i] << ' ';
        }
        out << '\n';
    } else if (T == 2) {
        // Echipe care depind strict de rezultatele lor pentru a deveni campioane
        vector<int> max_puncte(N, 0);
        for (int i = 0; i < N; ++i) {
            max_puncte[i] = puncte[i];
            int played_matches = 0;
            for (int j = 0; j < N; ++j) {
                if (i == j) {
                    continue;
                }
                if (meciuri[i * N + j]) {
                    played_matches++;
                    if (puncte[j] > puncte[i]) {
                        max_puncte[i] -= 3;
                    }
                } else {
                    max_puncte[i] += 3;
                }
            }
            max_puncte[i] += (N - played_matches - 1) * 1;
        }
        int max_max_puncte = *max_element(max_puncte.begin(), max_puncte.end());
        vector<int> teams;
        for (int i = 0; i < N; ++i) {
            if (max_puncte[i] == max_max_puncte) {
                teams.push_back(i + 1);
            }
        }
        if (teams.empty()) {
            out << 0 << '\n';
        } else {
            sort(teams.begin(), teams.end());
            for (int i = 0; i < teams.size(); ++i) {
                out << teams[i] << ' ';
            }
            out << '\n';
        }
    }

    return 0;
