pair<int, int> m[DMAX];
bool ok[NMAX];

int main() {
  fin >> t >> n >> d;
  for (int i = 1; i <= n; ++i) { fin >> p[i]; }
  for (int i = 1; i <= d; ++i) {
    fin >> m[i].first >> m[i].second;
  }

  if (t == 1) {
    for (int i = 1; i <= d; ++i) {
      ++p[m[i].first];
      ++p[m[i].second];
    }
    int maxi = 0;
    for (int i = 1; i <= n; ++i) { maxi = max(maxi, p[i]); }
    for (int i = 1; i <= n; ++i) {
      if (p[i] == maxi) { fout << i << " "; }
    }
    fout << "\n";
  } else {
    int ok = 0;
    for (int i = 1; i <= n; ++i) {
      int sum = p[i];
      for (int j = 1; j <= d; ++j) {
        if (m[j].first == i) {
          if (p[m[j].first] < p[m[j].second] && !ok[i]) { sum += 3; }
        }
        if (m[j].second == i) {
          if (p[m[j].second] < p[m[j].first] && !ok[i]) { sum += 1; }
        }
      }
      v[i] = sum;
      if (sum > p[i]) { ok = 1; }
    }
    if (!ok) {
      fout << "0\n";
      return 0;
    }
    for (int i = 1; i <= n; ++i) {
      int sum = v[i];
      int cnt = 0;
      for (int j = 1; j <= d; ++j) {
        if (m[j].first == i || m[j].second == i) { ++cnt; }
      }
      sum += 3 * cnt;
      for (int j = 1; j <= n; ++j) {
        if (i == j) { continue; }
        sum = max(sum, v[j]);
      }
      if (sum > v[i]) { continue; }
      fout << i << " ";
      ok[i] = true;
    }
    fout << "\n";
  }

  return 0;
}
