#include <fstream>
#include <vector>
#include <algorithm>
#include <cstring>

using namespace std;

const int MAXN = 200000;

int a[MAXN + 1], b[MAXN + 1];
int n, maxi;
vector<int> fr[MAXN + 1];
vector<pair<int, int>> v;

int main() {
  ifstream fin("maxp.in");
  ofstream fout("maxp.out");
  fin >> n;
  for (int i = 1; i <= n; i++) {
    fin >> a[i];
    if (i > 1 && a[i] > a[i - 1])
      b[i] = b[i - 1] + 1;
    else
      b[i] = 1;
  }
  for (int i = n; i >= 1; i--) {
    if (i < n && a[i] > a[i + 1])
      fr[a[i]].push_back(b[i]);
    else
      fr[a[i]].push_back(b[i] + 1);
    if (fr[a[i]].size() > v.size() ||
        (fr[a[i]].size() == v.size() && fr[a[i]][0] > v[0].first))
      v.assign(fr[a[i]].begin(), fr[a[i]].end()), maxi = a[i];
    else if (fr[a[i]].size() == v.size() && fr[a[i]][0] == v[0].first)
      maxi = -1;
  }
  fout << v[0].first << '\n';
  fout << v.size() << '\n';
  return 0;
}
