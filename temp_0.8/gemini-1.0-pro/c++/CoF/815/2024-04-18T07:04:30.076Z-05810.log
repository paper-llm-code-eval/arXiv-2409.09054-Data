#include <fstream>
#include <iostream>
#include <queue>
#include <vector>

using namespace std;

typedef pair<int, int> Point;

ifstream fin("ai.in");
ofstream fout("ai.out");

const int MAX_N = 1005, INF = 0x3f3f3f3f;
int n, k, ans1, ans2 = INF;
Point target, laser1, laser2, robot1, robot2, obs[150005];
vector<Point> walls[MAX_N];
bool vis[MAX_N][MAX_N], visited[150005];
int dist[MAX_N][MAX_N];
int dy[] = {-1, 0, 1, 0}, dx[] = {0, 1, 0, -1};

void bfs(Point start) {
    queue<Point> q;
    q.push(start);
    dist[start.first][start.second] = 0;
    while (!q.empty()) {
        Point u = q.front();
        q.pop();
        for (int i = 0; i < 4; i++) {
            Point v = {u.first + dy[i], u.second + dx[i]};
            if (v.first <= 0 || v.first > n || v.second <= 0 || v.second > n ||
                vis[v.first][v.second])
                continue;
            bool ok = true;
            for (int j = 0; j < (int)walls[v.second].size(); j++)
                if (v == walls[v.second][j]) {
                    ok = false;
                    break;
                }
            if (ok) {
                vis[v.first][v.second] = true;
                dist[v.first][v.second] = dist[u.first][u.second] + 1;
                q.push(v);
            }
        }
    }
}

int main() {
    fin >> n >> target.first >> target.second >> laser1.first >> laser1.second >>
        laser2.first >> laser2.second >> robot1.first >> robot1.second >> robot2.first >>
        robot2.second >> k;
    fout << ans1 << "\n";
    if (k == 0) {
        fout << ans2 << "\n";
        return 0;
    }
    for (int i = 1; i <= k; i++) {
        fin >> obs[i].first >> obs[i].second;
    }
    for (int i = 1; i <= k; i++) {
        walls[obs[i].second].push_back(obs[i]);
    }
    for (int i = 1; i <= n; i++) {
        bfs({1, i});
        bfs({n, i});
        bfs({i, 1});
        bfs({i, n});
        int cnt = 0;
        for (int j = 1; j <= n; j++) {
            if (dist[i][j] >= INF) {
                if (cnt) {
                    ans1 = max(ans1, cnt);
                    walls[i].push_back({i, j - cnt});
                }
                cnt = 0;
            } else
                cnt++;
        }
        if (cnt) {
            ans1 = max(ans1, cnt);
            walls[i].push_back({i, n - cnt + 1});
        }
    }
    ans2 = dist[robot1.first][robot1.second];
    if (dist[robot2.first][robot2.second] < ans2) ans2 = dist[robot2.first][robot2.second];
    fout << ans2 << "\n";
    return 0;
}
