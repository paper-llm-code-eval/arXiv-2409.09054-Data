#include <algorithm>
#include <cctype>
#include <fstream>
#include <functional>
#include <iostream>
#include <numeric>
#include <sstream>
#include <stack>
#include <unordered_map>
#include <unordered_set>
#include <vector>

namespace {

using namespace std;

struct Point {
  int x;
  int y;

  bool operator<(const Point &p) const { return tie(y, x) < tie(p.y, p.x); }
};

struct Wall {
  Point start;
  Point end;
};

int abs(int x) { return (x < 0) ? -x : x; }

int main() {
  ifstream fin("ai.in");
  ofstream fout("ai.out");

  int n;
  fin >> n;

  Point target, laser1, laser2, robot1, robot2;
  fin >> target.y >> target.x >> laser1.y >> laser1.x >> laser2.y >> laser2.x >>
    robot1.y >> robot1.x >> robot2.y >> robot2.x;

  int k;
  fin >> k;

  vector<Point> obstacles;

  for (int i = 0; i < k; ++i) {
    Point p;
    fin >> p.y >> p.x;
    obstacles.push_back(p);
  }

  // Initialize the grid
  vector<vector<int>> grid(n + 1, vector<int>(n + 1, -1));
  grid[target.y][target.x] = 0;
  grid[laser1.y][laser1.x] = 1;
  grid[laser2.y][laser2.x] = 2;

  // Mark the obstacles
  for (const Point &p : obstacles) { grid[p.y][p.x] = 0; }

  // Find the walls
  vector<Wall> walls;
  for (int i = 1; i <= n; ++i) {
    for (int j = 1; j <= n; ++j) {
      if (grid[i][j] == 0) { continue; }

      int k = j + 1;
      while (k <= n && grid[i][k] == 0) { ++k; }
      --k;

      if (k > j) {
        walls.push_back(Wall{Point{i, j}, Point{i, k}});
        for (int m = j; m <= k; ++m) { grid[i][m] = 0; }
      }

      k = i + 1;
      while (k <= n && grid[k][j] == 0) { ++k; }
      --k;

      if (k > i) {
        walls.push_back(Wall{Point{i, j}, Point{k, j}});
        for (int m = i; m <= k; ++m) { grid[m][j] = 0; }
      }
    }
  }

  // Find the length of the longest wall
  int max_length = 0;
  for (const Wall &wall : walls) {
    int length = max(abs(wall.start.x - wall.end.x), abs(wall.start.y - wall.end.y)) + 1;
    max_length = max(max_length, length);
  }

  fout << max_length << '\n';

  // Initialize the distances
  vector<vector<int>> distance(n + 1, vector<int>(n + 1, 100000000));
  distance[robot1.y][robot1.x] = 0;
  distance[robot2.y][robot2.x] = 0;

  // Find the minimum number of seconds
  int min_seconds = 100000000;

  function<void(const Point &, int, int)> dijkstra = [&](const Point &p, int seconds,
                                                        int robot) {
    if (seconds >= distance[p.y][p.x] || grid[p.y][p.x] == 0) { return; }
    distance[p.y][p.x] = seconds;

    if (grid[p.y][p.x] == robot + 1) { min_seconds = min(min_seconds, seconds); }

    dijkstra(Point{p.y, p.x - 1}, seconds + 1, robot);
    dijkstra(Point{p.y, p.x + 1}, seconds + 1, robot);
    dijkstra(Point{p.y - 1, p.x}, seconds + 1, robot);
    dijkstra(Point{p.y + 1, p.x}, seconds + 1, robot);
  };

  dijkstra(robot1, 0, 0);
  dijkstra(robot2, 0, 1);

  fout << min_seconds << '\n';

  return 0;
}

}  // namespace
