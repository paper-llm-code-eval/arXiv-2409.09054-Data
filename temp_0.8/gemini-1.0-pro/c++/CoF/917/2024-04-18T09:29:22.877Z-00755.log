#include <fstream>
#include <cstring>

using namespace std;

int n, m, k, x;
int c[401][401];
int dp[401][401][401][401];
int ans = -1e9, ans1 = -1e9, ax, ay, ax1, ay1;

int main() {
  ifstream cin("cetate.in");
  ofstream cout("cetate.out");

  cin >> x;
  cin >> n >> m >> k;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      cin >> c[i][j];
    }
  }

  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      for (int p = 1; p <= k; p++) {
        dp[i][j][i + p - 1][j + p - 1] =
            c[i][j] + c[i][j + 1] + c[i + 1][j] + c[i + 1][j + 1];
      }
    }
  }

  for (int l = 1; l <= k; l++) {
    for (int i = 1; i <= n - l + 1; i++) {
      for (int j = 1; j <= m - l + 1; j++) {
        for (int i1 = i; i1 <= i + l - 1; i1++) {
          for (int j1 = j; j1 <= j + l - 1; j1++) {
            dp[i][j][i + l - 1][j + l - 1] = max(
                dp[i][j][i + l - 1][j + l - 1],
                dp[i][j][i1 - 1][j + l - 1] + dp[i1][j][i + l - 1][j1 - 1] +
                    dp[i1][j1][i + l - 1][j + l - 1] - c[i][j] - c[i1][j] -
                    c[i][j1] - c[i1][j1]);
          }
        }
        if (dp[i][j][i + l - 1][j + l - 1] > ans) {
          ans = dp[i][j][i + l - 1][j + l - 1];
          ax = i;
          ay = j;
        }
        if (x == 2 && dp[i][j][i + l - 1][j + l - 1] > ans1) {
          ans1 = dp[i][j][i + l - 1][j + l - 1];
          ax1 = i;
          ay1 = j;
        }
      }
    }
  }

  if (x == 1) {
    cout << ans << '\n' << ax << ' ' << ay << ' ' << ax + k - 1 << ' '
         << ay + k - 1 << '\n';
  }

  if (x == 2) {
    cout << ans1 << '\n' << ax1 << ' ' << ay1 << ' ' << ax1 + k - 1 << ' '
         << ay1 + k - 1 << '\n';
  }

  return 0;
}
