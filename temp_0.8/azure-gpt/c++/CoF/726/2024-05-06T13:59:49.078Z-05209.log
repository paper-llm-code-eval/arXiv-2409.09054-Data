#include <fstream>

int main() {
    std::ifstream fin("concurs.in");
    std::ofstream fout("concurs.out");

    int H, n, code, cityH, schoolH, prizesCity = 0, prizesSchool = 0;
    bool isWinner = false;
    
    fin >> H >> n;
    cityH = H / 100;
    schoolH = (H / 10) % 10;
    
    while (n--) {
        fin >> code;
        if (code == H) {
            isWinner = true;
        }
        if (code / 100 == cityH) {
            prizesCity++;
        }
        if ((code / 10) % 10 == schoolH && code / 100 == cityH) {
            prizesSchool++;
        }
    }
    
    fout << (isWinner ? "DA" : "NU") << '\n';
    fout << prizesCity << '\n';
    fout << prizesSchool << '\n';

    fin.close();
    fout.close();
    
    return 0;
}
```
