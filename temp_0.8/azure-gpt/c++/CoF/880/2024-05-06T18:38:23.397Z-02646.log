#include <fstream>
#include <vector>
#include <string>

int main() {
    std::ifstream fin("robot.in");
    std::ofstream fout("robot.out");

    int C;
    fin >> C;
    fin.ignore(); // Skip newline after reading C

    if (C == 1) {
        std::string program;
        std::getline(fin, program);
        int count = 0;
        for (char inst : program) {
            if (inst == 'A') ++count;
        }
        fout << count << '\n';
    } else if (C == 2) {
        std::string program;
        std::getline(fin, program);
        int pos = 0;
        for (char inst : program) {
            if (inst == 'D') {
                pos = (pos + (fin.get() - '0')) % 10;
            } else if (inst == 'S') {
                pos = (pos - (fin.get() - '0') + 10) % 10;
            } else if (inst == 'A') {
                fout << pos;
            }
        }
        fout << '\n';
    } else if (C == 3) {
        unsigned int N;
        fin >> N;
        std::vector<int> digits;
        while (N > 0) {
            digits.push_back(N % 10);
            N /= 10;
        }

        int pos = 0;
        for (auto it = digits.rbegin(); it != digits.rend(); ++it) {
            int move_right = (*it - pos + 10) % 10;
            int move_left = (pos - *it + 10) % 10;
            if (move_right <= move_left) {
                if (move_right > 0) fout << 'D' << move_right;
            } else {
                if (move_left > 0) fout << 'S' << move_left;
            }
            fout << 'A';
            pos = *it;
        }
        fout << 'T' << '\n';
    }

    fin.close();
    fout.close();
    return 0;
}
