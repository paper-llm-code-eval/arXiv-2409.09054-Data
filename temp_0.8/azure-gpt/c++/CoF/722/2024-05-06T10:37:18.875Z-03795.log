#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAX_N = 100;
const int INF = 1e9;

int S, n;
int taxes[MAX_N][MAX_N];
int dp[MAX_N][MAX_N];

int main() {
    ifstream fin("taxe.in");
    ofstream fout("taxe.out");

    fin >> S >> n;
    for (int i = 0; i < n; ++i)
        for (int j = 0; j < n; ++j)
            fin >> taxes[i][j];

    // Initialize the base case
    dp[0][0] = S - taxes[0][0];

    // Fill the first row and first column
    for (int i = 1; i < n; ++i) {
        if (dp[i - 1][0] - taxes[i][0] >= 0)
            dp[i][0] = dp[i - 1][0] - taxes[i][0];
        else
            dp[i][0] = -INF;
    }
    for (int j = 1; j < n; ++j) {
        if (dp[0][j - 1] - taxes[0][j] >= 0)
            dp[0][j] = dp[0][j - 1] - taxes[0][j];
        else
            dp[0][j] = -INF;
    }

    // Fill the rest of the dp array
    for (int i = 1; i < n; ++i) {
        for (int j = 1; j < n; ++j) {
            int up = (dp[i - 1][j] != -INF) ? dp[i - 1][j] - taxes[i][j] : -INF;
            int left = (dp[i][j - 1] != -INF) ? dp[i][j - 1] - taxes[i][j] : -INF;
            dp[i][j] = max(up, left);
        }
    }

    // Check if it is possible to reach the exit
    if (dp[n - 1][n - 1] < 0)
        fout << "-1\n"; // Not possible to exit
    else
        fout << dp[n - 1][n - 1] << "\n"; // Maximum amount of money left

    fin.close();
    fout.close();
    return 0;
}
```

