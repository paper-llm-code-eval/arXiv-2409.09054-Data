#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

const int MOD = 1e9 + 7;

void solveMaxPackages() {
    int N, K;
    cin >> N >> K;
    vector<int> days(N);
    for(int i = 0; i < N; ++i) {
        for(int j = 0; j < K; ++j) {
            char c;
            cin >> c;
            days[i] |= (c - '0') << j;
        }
    }

    int mask = (1 << K) - 1;
    vector<int> lastSeen(1 << K, -1);
    lastSeen[0] = 0;
    int currentMask = 0, maxPackages = 0;

    for(int i = 0; i < N; ++i) {
        currentMask |= days[i];
        if (currentMask == mask) {
            maxPackages++;
            currentMask = 0;
            fill(lastSeen.begin(), lastSeen.end(), -1);
            lastSeen[0] = i + 1;
        } else {
            lastSeen[currentMask] = i + 1;
        }
    }
    cout << maxPackages << '\n';
}

void solveMaxWays() {
    int N, K;
    cin >> N >> K;
    vector<int> days(N);
    for(int i = 0; i < N; ++i) {
        for(int j = 0; j < K; ++j) {
            char c;
            cin >> c;
            days[i] |= (c - '0') << j;
        }
    }

    int mask = (1 << K) - 1;
    vector<int> dp(N + 1), lastSeen(1 << K, 0);
    dp[0] = 1;
    lastSeen[0] = 1;
    int currentMask = 0;

    for(int i = 0; i < N; ++i) {
        currentMask |= days[i];
        if (currentMask == mask) {
            for (int j = 0; j <= mask; ++j) {
                dp[i + 1] = (dp[i + 1] + lastSeen[j]) % MOD;
                lastSeen[j] = 0;
            }
            currentMask = 0;
        }
        lastSeen[currentMask] = (lastSeen[currentMask] + dp[i + 1]) % MOD;
    }
    cout << dp[N] << '\n';
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int P, T;
    cin >> P >> T;
    while (T--) {
        if (P == 1) {
            solveMaxPackages();
        } else {
            solveMaxWays();
        }
    }
    return 0;
}
```
