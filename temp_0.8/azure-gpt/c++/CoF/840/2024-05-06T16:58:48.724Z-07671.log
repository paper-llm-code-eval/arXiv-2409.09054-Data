#include <fstream>
#include <unordered_map>

using namespace std;

ifstream fin("cool.in");
ofstream fout("cool.out");

const int MAX_N = 5000;
int A[MAX_N];

bool is_sequence_cool(int *begin, int *end, int &max_value) {
    unordered_map<int, bool> seen;
    int local_max = 0;
    for (auto it = begin; it != end; ++it) {
        if (seen.find(*it) != seen.end()) return false;
        seen[*it] = true;
        local_max = max(local_max, *it);
    }
    for (int i = local_max; i > local_max - (end - begin); --i) {
        if (!seen[i]) return false;
    }
    max_value = local_max;
    return true;
}

int distinct_count(int *begin, int *end) {
    unordered_map<int, int> freq;
    for (auto it = begin; it != end; ++it) {
        freq[*it]++;
    }
    int count = 0;
    for (const auto &pair : freq) {
        if (pair.second == 1) count++;
    }
    return count;
}

void solve_task1(int N, int K) {
    int max_value;
    if (is_sequence_cool(A, A + K, max_value)) {
        fout << max_value << "\n";
    } else {
        fout << distinct_count(A, A + K) << "\n";
    }
}

void solve_task2(int N) {
    int max_length = 0, num_sequences = 0;
    for (int i = 0; i < N; ++i) {
        unordered_map<int, int> seen;
        int local_length = 0, local_max = 0, min_value = A[i];
        for (int j = i; j < N; ++j) {
            if (seen.find(A[j]) != seen.end()) break;
            seen[A[j]] = j;
            local_max = max(local_max, A[j]);
            min_value = min(min_value, A[j]);
            local_length++;
            if (local_max - min_value + 1 == local_length) {
                if (local_length > max_length) {
                    max_length = local_length;
                    num_sequences = 1;
                } else if (local_length == max_length) {
                    num_sequences++;
                }
            }
        }
    }
    fout << max_length << "\n" << num_sequences << "\n";
}

int main() {
    int p, N, K;
    fin >> p >> N >> K;
    for (int i = 0; i < N; ++i) {
        fin >> A[i];
    }
    if (p == 1) {
        solve_task1(N, K);
    } else {
        solve_task2(N);
    }
    fin.close();
    fout.close();
    return 0;
}
```
