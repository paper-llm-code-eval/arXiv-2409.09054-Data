#include <algorithm>
#include <fstream>
#include <vector>

struct OZN {
    int x1, y1, x2, y2, nr;
};

struct Laser {
    int pos, index, kills;
};

bool compareLaserPos(const Laser &a, const Laser &b) {
    return a.pos < b.pos;
}

bool compareLaserIndex(const Laser &a, const Laser &b) {
    return a.index < b.index;
}

int main() {
    std::ifstream fin("ozn.in");
    std::ofstream fout("ozn.out");

    int N, K, x1, y1, x2, y2, nr;
    fin >> N >> K;
    
    std::vector<OZN> ozns(N);
    for (int i = 0; i < N; ++i) {
        fin >> x1 >> y1 >> x2 >> y2 >> nr;
        ozns[i] = {x1, y1, x2, y2, nr};
    }

    std::vector<Laser> lasers(K);
    for (int i = 0; i < K; ++i) {
        fin >> lasers[i].pos;
        lasers[i].index = i;
        lasers[i].kills = 0;
    }
    fin.close();

    // Sort the lasers according to their position
    std::sort(lasers.begin(), lasers.end(), compareLaserPos);

    // Check each OZN for intersection with lasers
    for (const auto& ozn : ozns) {
        Laser query = {ozn.x1, 0, 0};
        auto it1 = std::lower_bound(lasers.begin(), lasers.end(), query, compareLaserPos);

        query.pos = ozn.x2;
        auto it2 = std::upper_bound(lasers.begin(), lasers.end(), query, compareLaserPos);

        for (auto it = it1; it != it2; ++it) {
            it->kills += ozn.nr;
        }
    }

    // Sort lasers back to their original order
    std::sort(lasers.begin(), lasers.end(), compareLaserIndex);

    // Output results
    for (const auto& laser : lasers) {
        fout << laser.kills << '\n';
    }
    fout.close();

    return 0;
}
