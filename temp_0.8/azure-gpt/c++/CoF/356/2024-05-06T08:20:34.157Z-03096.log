#include <fstream>
#include <vector>

int main() {
    std::ifstream fin("cochilie.in");
    std::ofstream fout("cochilie.out");

    int C, N, P;
    fin >> C;
    if (C == 1) {
        fin >> N;
        fout << 2 * N - 1 << " " << N << "\n";
    } else if (C == 2) {
        fin >> N >> P;
        std::vector<int> line(2 * N - 1);
        int val = N, limit = N, step = -1;

        // Fill the middle part of the line
        for(int i = N - 1; i < 2 * N - 1; ++i) {
            line[i] = val;
        }

        // Fill the left and right parts of the line
        for(int i = N - 2; i >= 0; --i) {
            if (P <= limit || P > (2 * N - 1 - limit)) {
                ++val;
            } else {
                --val;
            }
            line[i] = val;
            line[2 * N - 2 - i] = val;

            limit += step;
            if (limit == 1) {
                step = 1;
                val -= (N - 1) * 2 - 1;
            }
        }

        // Output the result
        for (int v : line) {
            fout << v << ' ';
        }
        fout << "\n";
    }

    fin.close();
    fout.close();
    return 0;
}
```
