#include <fstream>
#include <vector>
using namespace std;

int main() {
    ifstream fin("cochilie.in");
    ofstream fout("cochilie.out");

    int C, N, P;
    fin >> C;

    if (C == 1) {
        fin >> N;
        fout << 2 * N - 1 << ' ' << 2 * N - 1 << '\n';
    } else if (C == 2) {
        fin >> N >> P;
        vector<int> line(2 * N - 1);

        // Fill the top part of the shell
        int value = 1;
        for (int i = 0; i < P; ++i) {
            for (int j = max(0, N - 1 - i); j < min(2 * N - 1, N + i); ++j) {
                line[j] = value;
            }
            ++value;
        }

        // Fill the bottom part of the shell
        for (int i = P - 2; i >= 0; --i) {
            for (int j = max(0, N - 1 - i); j < min(2 * N - 1, N + i); ++j) {
                line[j] = value;
            }
            ++value;
        }

        for (int elem : line) {
            fout << elem << ' ';
        }
        fout << '\n';
    }

    fin.close();
    fout.close();
    return 0;
}
```

