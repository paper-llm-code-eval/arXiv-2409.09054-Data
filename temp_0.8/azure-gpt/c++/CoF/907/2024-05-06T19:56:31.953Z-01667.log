#include <fstream>
#include <vector>

using namespace std;

// Util function to convert time to seconds
inline int to_seconds(int hours, int minutes, int seconds) {
    return hours * 3600 + minutes * 60 + seconds;
}

int main() {
    ifstream fin("cartele.in");
    ofstream fout("cartele.out");

    int C, N, boys = 0, girls = 0, last_time, current_time, equal_time = 0, max_odd_boy_time = 0;
    char gender, action;
    int hours, minutes, seconds;
    int last_boy_change = 0, last_equal_time = -1;
    bool odd_boys = false;

    fin >> C >> N;

    vector<int> boy_times(86400, 0); // 24 * 60 * 60 seconds in a day

    for (int i = 0; i < N; ++i) {
        fin >> gender >> action >> hours >> minutes >> seconds;
        current_time = to_seconds(hours, minutes, seconds);

        // Update boys' and girls' count
        if (gender == 'b') {
            boys += (action == 'i' ? 1 : -1);
        } else {
            girls += (action == 'i' ? 1 : -1);
        }

        // For the second and third requirements, we need to track the changes over time
        if (C != 1) {
            // Check if the number of boys is odd and if there was a change since last check
            if (odd_boys != ((boys & 1) == 1)) {
                odd_boys = !odd_boys;

                // Update the maximum time with odd number of boys if the current span is larger
                if (odd_boys) {
                    boy_times[boys]++;
                    last_boy_change = current_time;
                } else {
                    boy_times[boys]--;
                    max_odd_boy_time = max(max_odd_boy_time, current_time - last_boy_change);
                }
            }

            // Check if the number of boys and girls is equal and non-zero
            if (boys == girls && boys > 0) {
                if (last_equal_time < 0) {
                    last_equal_time = current_time;
                }
            } else if (last_equal_time >= 0) {
                equal_time += current_time - last_equal_time;
                last_equal_time = -1;
            }
        }

        // Remember the time of the last action for the second requirement
        last_time = current_time;
    }

    // If the last state was with an equal number of boys and girls
    if (last_equal_time >= 0 && boys == girls) {
        equal_time += last_time - last_equal_time;
    }

    switch (C) {
        case 1:
            fout << boys << ' ' << girls << '\n';
            break;
        case 2:
            fout << equal_time << '\n';
            break;
        case 3:
            // Check for the last remaining time with odd number of boys
            if (odd_boys) {
                max_odd_boy_time = max(max_odd_boy_time, last_time - last_boy_change);
            }
            fout << max_odd_boy_time << '\n';
            break;
    }

    fin.close();
    fout.close();

    return 0;
}
```
