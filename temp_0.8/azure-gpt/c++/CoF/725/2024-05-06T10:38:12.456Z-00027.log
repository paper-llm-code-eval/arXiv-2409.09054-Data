#include <fstream>
#include <algorithm>

using namespace std;

int main() {
    ifstream fin("cifre.in");
    ofstream fout("cifre.out");
    
    unsigned int a, b, i;
    fin >> a >> b;
    
    bool digits_a[10] = {false};
    bool digits_b[10] = {false};
    bool common_digits[10] = {false};
    bool all_digits[10] = {false};
    
    // Process digits for number a
    while (a > 0) {
        digits_a[a % 10] = true;
        all_digits[a % 10] = true;
        a /= 10;
    }

    // Process digits for number b, identify common digits
    while (b > 0) {
        digits_b[b % 10] = true;
        all_digits[b % 10] = true;
        if (digits_a[b % 10]) {
            common_digits[b % 10] = true;
        }
        b /= 10;
    }
    
    bool has_common_digits = false;
    for (i = 0; i < 10; ++i) {
        if (common_digits[i]) {
            if (has_common_digits) {
                fout << " ";
            }
            fout << i;
            has_common_digits = true;
        }
    }
    
    if (!has_common_digits) {
        fout << "-1";
    }
    
    fout << "\n";
    
    // Output the largest number formed by all digits
    for (i = 9; i > 0; --i) {
        while (all_digits[i]) {
            fout << i;
            all_digits[i] = false;
        }
    }
    
    fin.close();
    fout.close();
    return 0;
}
```
