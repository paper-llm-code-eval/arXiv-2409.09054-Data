#include <fstream>
#include <bitset>
#include <vector>
#include <algorithm>

int main() {
    std::ifstream in("numere.in");
    std::ofstream out("numere.out");
    std::vector<int> nums(10);
    int sum = 0;
    for (int &num : nums) {
        in >> num;
        sum += num;
    }
    in.close();

    const int MAX_SUM = sum;
    std::vector<std::pair<int, int>> dp(MAX_SUM + 1, {-1, -1}); // pair<mask, last_added>
    dp[0] = {0, 0};

    for (int i = 0; i < nums.size(); ++i) {
        for (int j = MAX_SUM - nums[i]; j >= 0; --j) {
            if (dp[j].first != -1 && dp[j + nums[i]].first == -1) {
                dp[j + nums[i]] = {dp[j].first | (1 << i), i};
            }
        }
    }

    int max_equal_subsets = -1, max_subset_sum = 0;
    for (int s = 1; s <= sum / 2; ++s) {
        if (dp[s].first != -1) {
            for (int i = 0; i < nums.size(); ++i) {
                if (!(dp[s].first & (1 << i)) && dp[s + nums[i]].first != -1) {
                    max_equal_subsets = std::max(max_equal_subsets, s + nums[i]);
                }
            }
        }
    }

    std::vector<int> subset1, subset2;
    int mask = dp[max_equal_subsets].first;
    while (mask) {
        int last = dp[max_equal_subsets].second;
        subset1.push_back(nums[last]);
        max_equal_subsets -= nums[last];
        mask ^= (1 << last);
    }

    mask = dp[max_equal_subsets].first;
    while (mask) {
        int last = dp[max_equal_subsets].second;
        subset2.push_back(nums[last]);
        mask ^= (1 << last);
    }

    int pairs = 0;
    std::bitset<1024> used;
    for (int i = 0; i < dp.size(); ++i) {
        if (!used[dp[i].first] && dp[i].first != -1) {
            for (int j = i + 1; j < dp.size(); ++j) {
                if ((dp[i].first & dp[j].first) == 0 && dp[j].first != -1 && i == j - i) {
                    used[dp[j].first] = true;
                    pairs++;
                }
            }
        }
    }

    std::sort(subset1.begin(), subset1.end());
    std::sort(subset2.begin(), subset2.end());

    out << pairs << " " << max_equal_subsets << std::endl;
    for (int num : subset1) {
        out << num << " ";
    }
    out << std::endl;
    for (int num : subset2) {
        out << num << " ";
    }
    out.close();

    return 0;
}
```
Above is the C++ code to solve the challenge. This code uses dynamic programming with bitmasks to find the number of pairs and the maximal sum of equal subset pairs as required. It reads from `numere.in` and writes to `numere.out` without using `cin`, `cout`, `scanf`, or `printf`. Please note that this is one possible solution, and optimizations may be possible depending on constraints and requirements not detailed in the challenge.
