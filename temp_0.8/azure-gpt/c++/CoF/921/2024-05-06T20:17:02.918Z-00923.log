#include <fstream>
#include <vector>
#include <algorithm>
#include <unordered_map>

using namespace std;

int countDivisors(int x) {
  int count = 0;
  for (int i = 1; i * i <= x; ++i) {
    if (x % i == 0) {
      count += 2;
      if (i * i == x) count -= 1;
    }
  }
  return count;
}

int main() {
  ifstream fin("forta.in");
  ofstream fout("forta.out");

  int c, n;
  fin >> c >> n;

  vector<pair<int, int>> nums(n);
  for (int i = 0; i < n; ++i) {
    fin >> nums[i].second;
    nums[i].first = countDivisors(nums[i].second);
  }

  if (c == 1) {
    sort(nums.begin(), nums.end(), [](const pair<int, int>& a, const pair<int, int>& b) {
      return a.first > b.first || (a.first == b.first && a.second < b.second);
    });
    fout << nums[0].second << '\n';
  }
  else if (c == 2) {
    unordered_map<int, int> forceCounts;
    int maxLength = 0;
    for (const auto& num : nums) {
      forceCounts[num.first]++;
      maxLength = max(maxLength, forceCounts[num.first]);
    }
    fout << maxLength << '\n';
  }

  fin.close();
  fout.close();
  return 0;
}
```
