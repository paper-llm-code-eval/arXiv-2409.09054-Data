#include <iostream>
#include <fstream>
#include <vector>
#include <algorithm>

// Function to check if a number is prime
bool is_prime(int num) {
    if (num <= 1) return false;
    if (num <= 3) return true;
    if (num % 2 == 0 || num % 3 == 0) return false;
    for (int i = 5; i * i <= num; i += 6)
        if (num % i == 0 || num % (i + 2) == 0)
            return false;
    return true;
}

int main() {
    std::ifstream fin("control.in");
    std::ofstream fout("control.out");

    int n, weight;
    fin >> n;
    std::vector<int> weights(201, 0); // Since the highest weight is 200

    for (int i = 0; i < n; ++i) {
        fin >> weight;
        weights[weight]++;
    }

    std::vector<int> counts;
    for (int i = 1; i <= 200; ++i) {
        if (weights[i] > 0) {
            counts.push_back(weights[i]);
        }
    }

    int ones = 0, zeros = 0;
    for (size_t i = 0; i + 2 < counts.size(); i += 3) {
        bool allEven = (counts[i] % 2 == 0 && counts[i + 1] % 2 == 0 && counts[i + 2] % 2 == 0);
        bool allOdd = (counts[i] % 2 != 0 && counts[i + 1] % 2 != 0 && counts[i + 2] % 2 != 0);
        if (allEven || allOdd) {
            ones++;
        } else {
            zeros++;
        }
    }

    int controlNumber = ones * 10 + zeros;
    fout << controlNumber << '\n' << is_prime(controlNumber) << '\n';

    // Close the files
    fin.close();
    fout.close();

    return 0;
}
