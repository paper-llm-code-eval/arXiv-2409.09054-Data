#include <fstream>
#include <vector>
#include <string>
#include <limits>

// Function to count the number of divisors of a number
int count_divisors(int n) {
    int count = 0;
    for (int i = 1; i * i <= n; ++i) {
        if (n % i == 0) {
            count += (i * i == n) ? 1 : 2;
        }
    }
    return count;
}

// Function to find the best permutation of the number with minimum divisors
int best_permutation(const std::string& num) {
    int min_divisors = std::numeric_limits<int>::max();
    int min_num = std::numeric_limits<int>::max();

    for (size_t i = 1; i < num.size(); ++i) {
        if (num[i] != '0') {
            std::string perm = num.substr(i) + num.substr(0, i);
            int perm_num = std::stoi(perm);
            int divisors = count_divisors(perm_num);

            if (divisors < min_divisors || (divisors == min_divisors && perm_num < min_num)) {
                min_divisors = divisors;
                min_num = perm_num;
            }
        }
    }
    return min_num;
}

int main() {
    std::ifstream in("forus.in");
    std::ofstream out("forus.out");

    int C, N;
    in >> C >> N;

    std::vector<int> numbers(N);
    for (int& num : numbers) {
        in >> num;
    }

    if (C == 1) {
        int count = 0;
        for (int num : numbers) {
            std::string numStr = std::to_string(num);
            bool can_cut = true;
            for (size_t i = 1; i < numStr.size(); ++i) {
                if (numStr[i] == '0') {
                    can_cut = false;
                    break;
                }
            }
            if (can_cut) {
                ++count;
            }
        }
        out << count << '\n';
    } else if (C == 2) {
        for (int num : numbers) {
            std::string numStr = std::to_string(num);
            out << best_permutation(numStr) << ' ';
        }
        out << '\n';
    }

    in.close();
    out.close();

    return 0;
}
