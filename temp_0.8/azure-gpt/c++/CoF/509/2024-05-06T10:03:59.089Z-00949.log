#include <fstream>

// Function to calculate the duration of a flight
inline int calculateDuration(int H1, int M1, int H2, int M2) {
    int startTime = H1 * 60 + M1;
    int endTime = H2 * 60 + M2;
    if (endTime < startTime) {
        endTime += 24 * 60; // Next day
    }
    return endTime - startTime;
}

// Function to check if a number is prime
inline bool isPrime(int number) {
    if (number <= 1) return false;
    if (number <= 3) return true;
    if (number % 2 == 0 || number % 3 == 0) return false;
    for (int i = 5; i * i <= number; i += 6)
        if (number % i == 0 || number % (i + 2) == 0)
            return false;
    return true;
}

// Function to calculate the sum of digits
inline int sumOfDigits(int number) {
    int sum = 0;
    while (number > 0) {
        sum += number % 10;
        number /= 10;
    }
    return sum;
}

int main() {
    std::ifstream in("aeriana.in");
    std::ofstream out("aeriana.out");

    int C, N;
    in >> C >> N;

    int maxDurationBefore = 0, maxDurationAfter = 0;
    for (int i = 0; i < N; ++i) {
        int A1, A2, H1, M1, H2, M2;
        in >> A1 >> A2 >> H1 >> M1 >> H2 >> M2;

        int duration = calculateDuration(H1, M1, H2, M2);
        maxDurationBefore = std::max(maxDurationBefore, duration);

        if (C == 2 && isPrime(A1) && A2 % sumOfDigits(A1) == 0) {
            duration = calculateDuration(H2, M2, H1, M1); // Swap times for special flights
        }

        maxDurationAfter = std::max(maxDurationAfter, duration);
    }

    in.close();

    if (C == 1) {
        out << maxDurationBefore / 60 << ' ' << maxDurationBefore % 60 << '\n';
    } else {
        out << maxDurationAfter / 60 << ' ' << maxDurationAfter % 60 << '\n';
    }

    out.close();
    return 0;
}
```

