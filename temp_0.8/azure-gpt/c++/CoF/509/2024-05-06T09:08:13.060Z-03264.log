#include <fstream>
#include <cmath>

// Check if a number is prime
bool is_prime(int number) {
    if (number <= 1) return false;
    if (number <= 3) return true;

    if (number % 2 == 0 || number % 3 == 0) return false;

    for (int i = 5; i * i <= number; i += 6) {
        if (number % i == 0 || number % (i + 2) == 0) return false;
    }
    return true;
}

// Calculate sum of the digits of a number
int sum_of_digits(int number) {
    int sum = 0;
    while (number != 0) {
        sum += number % 10;
        number /= 10;
    }
    return sum;
}

// Calculate duration of a flight
int calculate_duration(int H1, int M1, int H2, int M2, bool reverse) {
    int duration_minutes = (H2 * 60 + M2) - (H1 * 60 + M1);
    if (duration_minutes < 0 || reverse) {
        duration_minutes += 24 * 60; // if reverse or next day
    }
    return duration_minutes;
}

int main() {
    std::ifstream in("aeriana.in");
    std::ofstream out("aeriana.out");

    int C, N;
    in >> C >> N;

    int max_duration_before = 0, max_duration_after = 0;
    int A1, A2, H1, M1, H2, M2;

    for (int i = 0; i < N; ++i) {
        in >> A1 >> A2 >> H1 >> M1 >> H2 >> M2;

        int current_duration = calculate_duration(H1, M1, H2, M2, false);
        max_duration_before = std::max(max_duration_before, current_duration);

        bool special = is_prime(A1) && (A2 % sum_of_digits(A1) == 0);
        if (special) {
            current_duration = calculate_duration(H2, M2, H1, M1, true);
        }
        max_duration_after = std::max(max_duration_after, current_duration);
    }

    in.close();

    int hours, minutes;
    if (C == 1) {
        hours = max_duration_before / 60;
        minutes = max_duration_before % 60;
    } else {
        hours = max_duration_after / 60;
        minutes = max_duration_after % 60;
    }

    out << hours << " " << minutes << std::endl;
    out.close();

    return 0;
}
