#include <fstream>
#include <cmath>

void multiplyNonZeroDigits(unsigned int &num) {
    unsigned int result = 1;
    while (num > 0) {
        unsigned int digit = num % 10;
        if (digit != 0) {
            result *= digit;
        }
        num /= 10;
    }
    num = result;
}

int main() {
    std::ifstream in("galbeni.in");
    std::ofstream out("galbeni.out");

    unsigned int S, K, N;
    in >> S >> K >> N;
    in.close();

    unsigned int multiplier = pow(10, K);

    // First, let's calculate the product of non-zero digits for S
    multiplyNonZeroDigits(S);
    
    // Now apply the rule N-1 times
    for (unsigned int i = 2; i <= N; ++i) {
        S = (S * 8) % (multiplier);
        if (S < multiplier / 10) S = S * 10 + 9;
    }

    out << S;
    out.close();
    return 0;
}
```

