#include <fstream>
#include <vector>
#include <tuple>
#include <algorithm>

std::ifstream fin("immortal.in");
std::ofstream fout("immortal.out");

const int MAXN = 21;
bool board[MAXN][MAXN];
int dx[] = {-1, 1, 0, 0};
int dy[] = {0, 0, -1, 1};

bool find_solution(std::vector<std::tuple<int, int, int, int>> &moves, int n, int m, int x, int y) {
    for (int direction = 0; direction < 4; ++direction) {
        int nx = x + dx[direction];
        int ny = y + dy[direction];
        int jump_x = nx + dx[direction];
        int jump_y = ny + dy[direction];
        if (nx > 0 && nx <= n && ny > 0 && ny <= m && board[nx][ny] &&
            jump_x > 0 && jump_x <= n && jump_y > 0 && jump_y <= m && !board[jump_x][jump_y]) {
            board[x][y] = board[nx][ny] = false;
            board[jump_x][jump_y] = true;
            moves.push_back(std::make_tuple(x, y, jump_x, jump_y));
            for (int i = 1; i <= n; ++i) {
                for (int j = 1; j <= m; ++j) {
                    if (board[i][j] && find_solution(moves, n, m, i, j)) {
                        return true;
                    }
                }
            }
            moves.pop_back();
            board[x][y] = board[nx][ny] = true;
            board[jump_x][jump_y] = false;
        }
    }
    return std::count(&board[0][0], &board[MAXN][0], true) == 1;
}

int main() {
    int n, m, I;
    fin >> n >> m >> I;
    std::vector<std::tuple<int, int, int, int>> moves;
