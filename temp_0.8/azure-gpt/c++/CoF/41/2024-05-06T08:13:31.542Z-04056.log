                for (const auto &move : moves) {
                    fout << std::get<0>(move) << ' ' << std::get<1>(move) << ' '
                         << std::get<2>(move) << ' ' << std::get<3>(move) << '\n';
                }
                fout.close();
                return 0;
            }
        }
    }
}
