#include <fstream>
#include <vector>
#include <cmath>

using namespace std;

ifstream fin("maxd.in");
ofstream fout("maxd.out");

int countDivisors(int n) {
    int count = 0;
    for (int i = 1; i <= sqrt(n); i++) {
        if (n % i == 0) {
            // If divisors are equal, count only one
            if (n / i == i) count++;
            else count += 2; // Otherwise, count both
        }
    }
    return count;
}

int main() {
    int a, b;
    fin >> a >> b;

    int maxDivisors = 0, minValue = 0, countMaxDiv = 0, divCount;

    for (int i = a; i <= b; i++) {
        divCount = countDivisors(i);
        if (divCount > maxDivisors) {
            maxDivisors = divCount;
            minValue = i;
            countMaxDiv = 1;
        } else if (divCount == maxDivisors) {
            countMaxDiv++;
        }
    }

    fout << minValue << ' ' << maxDivisors << ' ' << countMaxDiv << '\n';

    fin.close();
    fout.close();

    return 0;
}
