#include <fstream>
#include <string>
#include <unordered_map>

int main() {
    std::ifstream fin("eq4.in");
    std::ofstream fout("eq4.out");

    int C;
    fin >> C;

    std::string expression;
    fin >> expression; // Reads the expression as a string.

    long long a, b, E;
    fin >> a >> b >> E;

    std::unordered_map<char, int> monomialSum; // Map to hold the sum of coefficients for x, y, z, t.
    int term = 0; // Holds the sum of free terms.
    int sign = 1; // Holds the sign of the current term or monomial.
    int num = 0; // Temporary holds the current number being read.
    bool hasCoefficient = false; // Checks if the current monomial has a coefficient.

    for (char c : expression) {
        if (c == '+') {
            term += sign * num; // Add the current number to the term.
            num = 0; // Reset number for the next term or coefficient.
            sign = 1; // Set sign to positive for the next term or monomial.
            hasCoefficient = false; // Reset coefficient flag.
        } else if (c == '-') {
            term += sign * num; // Add the current number to the term.
            num = 0; // Reset number for the next term or coefficient.
            sign = -1; // Set sign to negative for the next term or monomial.
            hasCoefficient = false; // Reset coefficient flag.
        } else if (isdigit(c)) {
            num = num * 10 + (c - '0'); // Update the current number.
            hasCoefficient = true; // We have a coefficient now.
        } else { // If c is a variable.
            monomialSum[c] += sign * (hasCoefficient ? num : 1); // Add the coefficient to the corresponding variable.
            num = 0; // Reset number for the next term or coefficient.
            hasCoefficient = false; // Reset coefficient flag.
        }
    }
    term += sign * num; // Add the last number to the term.

    if (C == 1) {
        fout << term + monomialSum['x'] + monomialSum['y'] + monomialSum['z'] + monomialSum['t'] << "\n";
    } else if (C == 2) {
        long long count = 0;
        for (int x = a; x <= b; ++x) {
            for (int y = a; y <= b; ++y) {
                for (int z = a; z <= b; ++z) {
                    for (int t = a; t <= b; ++t) {
                        if (term + monomialSum['x'] * x + monomialSum['y'] * y + 
                              monomialSum['z'] * z + monomialSum['t'] * t == E) {
                            ++count;
                        }
                    }
                }
            }
        }
        fout << count << "\n";
    }

    fin.close();
    fout.close();
    return 0;
}
```
