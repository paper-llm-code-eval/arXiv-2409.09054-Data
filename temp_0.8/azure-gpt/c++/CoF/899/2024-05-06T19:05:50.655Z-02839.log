#include <fstream>
#include <vector>
#include <algorithm>

std::ifstream fin("pal.in");
std::ofstream fout("pal.out");

// Function to find the next palindrome greater than or equal to a given number
std::string nextPalindrome(std::string num) {
    int n = num.size();
    std::string left = num.substr(0, (n + 1) / 2);
    std::string pal = left + std::string(left.rbegin() + (n & 1), left.rend());

    if (pal >= num) return pal;

    for (int i = (n - 1) / 2; i >= 0; --i) {
        if (left[i] != '9') {
            left[i]++;
            std::string newPal = left + std::string(left.rbegin() + (n & 1), left.rend());
            return newPal;
        }
        left[i] = '0';
    }

    return "1" + std::string(n - 1, '0') + "1";
}

// Function to determine the longest magic series and its elements
std::pair<int, std::vector<std::string>> longestMagicSeries(const std::vector<std::string>& palindromes) {
    std::vector<std::string> sortedPalindromes = palindromes;
    std::sort(sortedPalindromes.begin(), sortedPalindromes.end(), [](const std::string& a, const std::string& b) {
        if (a.size() == b.size()) return a < b;
        return a.size() < b.size();
    });

    int maxLength = 0;
    std::vector<std::string> maxSeries;
    std::vector< std::vector<std::string> > allSeries(1);

    for (const std::string& pal : sortedPalindromes) {
        bool added = false;
        for (auto& series : allSeries) {
            if (series.empty() || pal.substr((pal.size() - series.back().size()) / 2,
                                             series.back().size()) == series.back()) {
                series.push_back(pal);
                added = true;
                break;
            }
        }
        if (!added) allSeries.push_back({pal});
    }

    for (const auto& series : allSeries) {
        if (series.size() > maxLength) {
            maxLength = series.size();
            maxSeries = series;
        } else if (series.size() == maxLength && series.back() > maxSeries.back()) {
            maxSeries = series;
        }
    }

    return {maxLength, maxSeries};
}

int main() {
    int P, N;
    fin >> P >> N;
    std::vector<std::string> numbers(N), correctedList;

    for (int i = 0; i < N; ++i) {
        fin >> numbers[i];
        correctedList.push_back(nextPalindrome(numbers[i]));
    }

    if (P == 1) {
        for (const auto& num : correctedList) {
            fout << num << " ";
        }
    } else {
        auto [maxLength, maxSeries] = longestMagicSeries(correctedList);
        if (P == 2) {
            fout << maxLength;
        } else if (P == 3) {
            for (const auto& num : maxSeries) {
                fout << num << " ";
            }
        }
    }

    fout << "\n";
    fin.close();
    fout.close();
    return 0;
}
