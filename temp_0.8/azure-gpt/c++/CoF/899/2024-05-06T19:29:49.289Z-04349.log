#include <fstream>
#include <vector>
#include <algorithm>
#include <string>

using namespace std;

// Function to find next palindrome of a given number string
string nextPalindrome(string s) {
    int n = s.length();
    string left = s.substr(0, (n + 1) / 2);
    string palin = left + string(left.rbegin() + (n % 2), left.rend());
    if (palin > s) return palin;
    for (int i = (n - 1) / 2; i >= 0; --i) {
        if (left[i] != '9') {
            left[i]++;
            return left + string(left.rbegin() + (n % 2), left.rend());
        } else {
            left[i] = '0';
        }
    }
    return "1" + string(n - 1, '0') + "1";
}

// Function to check if smaller is a magical substring of larger
bool isMagical(const string &larger, const string &smaller) {
    if (larger.length() <= smaller.length()) return false;
    int trim = (larger.length() - smaller.length()) / 2;
    return larger.substr(trim, smaller.length()) == smaller;
}

int main() {
    ifstream fin("pal.in");
    ofstream fout("pal.out");

    int P, N;
    fin >> P >> N;

    vector<string> numbers(N);
    vector<string> palindromes;
    string num;

    for (int i = 0; i < N; ++i) {
        fin >> num;
        numbers[i] = nextPalindrome(num);
    }

    if (P == 1) {
        for (int i = 0; i < N; ++i) {
            fout << numbers[i] << (i < N - 1 ? ' ' : '\n');
        }
    } else {
        // Remove duplicates and sort
        sort(numbers.begin(), numbers.end());
        numbers.erase(unique(numbers.begin(), numbers.end()), numbers.end());

        vector<int> maxLength(N, 1);
        vector<int> prevIndex(N, -1);
        int maxLen = 0, lastIndex = -1;

        for (int i = 1; i < numbers.size(); ++i) {
            for (int j = 0; j < i; ++j) {
                if (isMagical(numbers[i], numbers[j]) && maxLength[j] + 1 > maxLength[i]) {
                    maxLength[i] = maxLength[j] + 1;
                    prevIndex[i] = j;
                }
            }
            if (maxLength[i] > maxLen) {
                maxLen = maxLength[i];
                lastIndex = i;
            }
        }

        if (P == 2) {
            fout << maxLen << '\n';
        } else {
            // Backtrack to find the magical sequence
            while (lastIndex != -1) {
                palindromes.push_back(numbers[lastIndex]);
                lastIndex = prevIndex[lastIndex];
            }
            reverse(palindromes.begin(), palindromes.end());
            for (const string &palindrome : palindromes) {
                fout << palindrome << '\n';
            }
        }
    }

    fin.close();
    fout.close();

    return 0;
}
