#include <fstream>
#include <vector>
#include <queue>

using namespace std;

const int dx[] = { -1, 0, 1, 0 };
const int dy[] = { 0, 1, 0, -1 };
const int MAX_N = 176;

int n, m, x1, y1, x2, y2;
int matrix[MAX_N][MAX_N]; // We use a matrix to represent the park zones.
queue<pair<int, int>> q;

void readInput() {
  ifstream fin("alee.in");
  fin >> n >> m;
  int x, y;
  for (int i = 0; i < m; ++i) {
    fin >> x >> y;
    matrix[x][y] = -1; // Mark the tree positions with -1
  }
  fin >> x1 >> y1 >> x2 >> y2;
  fin.close();
}

void writeOutput(int result) {
  ofstream fout("alee.out");
  fout << result << "\n";
  fout.close();
}

void bfs() {
  q.push({x1, y1});
  matrix[x1][y1] = 1; // Mark the starting point with distance 1.
  while (!q.empty()) {
    pair<int, int> current = q.front();
    q.pop();
    for (int i = 0; i < 4; ++i) {
      int nx = current.first + dx[i];
      int ny = current.second + dy[i];
      if (nx >= 1 && nx <= n && ny >= 1 && ny <= n && matrix[nx][ny] == 0) {
        matrix[nx][ny] = matrix[current.first][current.second] + 1;
        q.push({nx, ny});
        if (nx == x2 && ny == y2) {
          writeOutput(matrix[nx][ny] - 1); // Subtract 1 to exclude starting point.
          return;
        }
      }
    }
  }
}

int main() {
  readInput();
  bfs();
  return 0;
}
