            result += digit * factor;
            factor *= 10;
        }
        number /= 10;
    }
    return result;
}

int countQuartz(long long x) {
    if (x == 0) return 0;
    int k = 1, count = 0, sum = 0;
    while (sum <= x) {
        sum += k;
        if (sum == x) count++;
        k += 4;
    }
    return count;
}

