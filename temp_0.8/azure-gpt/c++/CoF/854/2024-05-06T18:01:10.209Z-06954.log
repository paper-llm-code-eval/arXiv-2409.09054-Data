long long extractOddDigits(long long number) {
    long long result = 0, factor = 1;
    while (number > 0) {
        int digit = number % 10;
        if (digit % 2 != 0) {
            result += digit * factor;
            factor *= 10;
        }
        number /= 10;
    }
    return result;
}

long long extractEvenDigits(long long number) {
    long long result = 0, factor = 1;
    while (number > 0) {
        int digit = number % 10;
