#include <fstream>
#include <vector>
#include <map>
#include <algorithm>

using namespace std;

// Helper function to extract label and count from a compartment label
void extractLabelAndCount(int compartmentLabel, int& label, int& count) {
    label = compartmentLabel % 100;
    count = compartmentLabel / 100;
}

int main() {
    ifstream in("cufere.in");
    ofstream out("cufere.out");
    
    int c, n;
    in >> c >> n;

    // Read all compartment labels and count objects
    map<int, int> itemCount; // Map of item label -> total count
    for (int i = 0; i < n * 27; ++i) {
        int compartmentLabel;
        in >> compartmentLabel;
        
        if (compartmentLabel > 0) {
            int label, count;
            extractLabelAndCount(compartmentLabel, label, count);
            itemCount[label] += count;
        }
    }
    in.close();

    if (c == 1) {
        for (const auto& item : itemCount) {
            out << item.first << ' ' << item.second << '\n';
        }
    } else if (c == 2) {
        // Rearrange items
        auto it = itemCount.begin();
        for (int i = 0; i < n; ++i) {
            for (int j = 0; j < 3; ++j) { // Rows
                for (int k = 0; k < 9; ++k) { // Columns
                    if (it == itemCount.end()) {
                        out << '0' << ' ';
                        continue;
                    }

                    int capacity = (it->first > 10) ? 64 : 16;
                    int toStore = min(it->second, capacity);
                    out << toStore * 100 + it->first << ' ';

                    it->second -= toStore;
                    if (it->second <= 0) {
                        ++it;
                    }
                }
                out << '\n';
            }
        }
    }

    out.close();
    return 0;
}
```

