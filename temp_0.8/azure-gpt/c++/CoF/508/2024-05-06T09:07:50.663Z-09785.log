#include <fstream>
#include <vector>
#include <map>
#include <algorithm>

using namespace std;

// Function to check if a number is prime
bool isPrime(int number) {
    if (number <= 1) return false;
    if (number <= 3) return true;
    if (number % 2 == 0 || number % 3 == 0) return false;
    for (int i = 5; i * i <= number; i += 6)
        if (number % i == 0 || number % (i + 2) == 0)
            return false;
    return true;
}

// Helper function to extract label and count from a compartment label
void extractLabelAndCount(int compartmentLabel, int& label, int& count) {
    label = compartmentLabel % 100;
    count = compartmentLabel / 100;
}

int main() {
