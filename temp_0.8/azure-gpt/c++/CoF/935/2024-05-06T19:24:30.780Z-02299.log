#include <fstream>
#include <vector>
#include <unordered_map>

int main() {
    std::ios::sync_with_stdio(false);

    std::ifstream fin("exclusiv.in");
    std::ofstream fout("exclusiv.out");

    int M, N;
    fin >> M >> N;

    std::vector<int> s(M + 1), v(N + 1);
    std::unordered_map<int, int> index;
    std::vector<int> max_len(N + 1, 0);

    for (int i = 1; i <= M; ++i) {
        fin >> s[i];
    }

    for (int i = 1; i <= N; ++i) {
        fin >> v[i];
    }

    int start = 1, current_len = 0;
    for (int i = 1; i <= M; ++i) {
        if (index.find(s[i]) != index.end()) {
            for (int j = start; j < i; ++j) {
                if (s[j] == s[i]) {
                    start = j + 1;
                    break;
                }
                if (index[s[j]] == 0) {
                    continue;
                }
                max_len[index[s[j]]] = std::max(max_len[index[s[j]]], current_len - (j - start + 1));
            }
            current_len = i - start;
        }
        ++current_len;
        index[s[i]] = 0;
    }

    for (int j = start; j <= M; ++j) {
        if (index[s[j]] != 0) {
            max_len[index[s[j]]] = std::max(max_len[index[s[j]]], current_len - (j - start));
        }
    }

    for (int i = 1; i <= N; ++i) {
        index[v[i]] = i;
        for (int j = i; j <= N; ++j) {
            max_len[j] = std::max(max_len[j], max_len[i - 1]);
        }
        fout << max_len[N] << '\n';
    }

    fin.close();
    fout.close();

    return 0;
}
