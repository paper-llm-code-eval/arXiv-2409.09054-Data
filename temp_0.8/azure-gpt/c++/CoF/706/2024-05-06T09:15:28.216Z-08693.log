#include <fstream>
#include <vector>
#include <algorithm>

int main() {
    std::ifstream fin("joc.in");
    std::ofstream fout("joc.out");
    
    int N;
    fin >> N;
    std::vector<int> heaps(N);
    
    for (int i = 0; i < N; ++i) {
        fin >> heaps[i];
    }
    
    int totalPebbles = 0, lastHeap = 0, currentTarget = 1;
    while (lastHeap < N) {
        // Try to reach the current target by adding pebbles from the available heaps
        while (lastHeap < N && heaps[lastHeap] <= currentTarget) {
            currentTarget += heaps[lastHeap++];
        }

        if (lastHeap == N && currentTarget <= totalPebbles) {
            // If we have used all heaps and the current target is not greater than the previous total
            // then the previous total is the last successful move
            break;
        }
        
        // If we have reached the current target, increase it for the next round
        totalPebbles = currentTarget;
        currentTarget++;
    }
    
    fout << totalPebbles << std::endl;
    
    fin.close();
    fout.close();
    
    return 0;
}
