#include <fstream>
#include <vector>
#include <algorithm>

int main() {
    std::ifstream in("cifru.in");
    std::ofstream out("cifru.out");

    int N;
    in >> N;

    std::vector<int> discs(N);
    std::vector<int> freq(10, 0);
    int max_digit = 0;
    
    for (int i = 0; i < N; ++i) {
        in >> discs[i];
        max_digit = std::max(max_digit, discs[i]);
        freq[discs[i]]++;
    }

    in.close();

    int min_moves = 1e9, best_digit = 0, min_digit = 9;
    std::vector<int> moves_to_make_identical(10, 0);
    
    for (int target_digit = 0; target_digit <= 9; ++target_digit) {
        int moves = 0;
        for (int i = 0; i < N; ++i) {
            int dist = std::min(std::abs(discs[i] - target_digit), 10 - std::abs(discs[i] - target_digit));
            moves += dist;
        }
        
        moves_to_make_identical[target_digit] = moves;
        
        if (moves < min_moves) {
            min_moves = moves;
            best_digit = target_digit;
        }

        min_digit = std::min(min_digit, target_digit);
    }

    int combinations = 0;
    for (int i = 0; i <= 9; ++i) {
        if (moves_to_make_identical[i] == min_moves) {
            combinations++;
        }
    }

    out << max_digit << '\n';
    out << min_moves << '\n';
    out << best_digit << '\n';
    out << combinations << '\n';

    out.close();
    
    return 0;
}
```

