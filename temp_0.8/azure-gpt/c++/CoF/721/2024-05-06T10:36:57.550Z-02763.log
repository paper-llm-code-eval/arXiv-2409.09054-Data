    }

    int cycleCount = 0;
    while (true) {
        int count = 0;
        for (int i = 0; i < n * n; i++) {
            if (initialPattern[i] == spiralPattern[i]) {
                count++;
            }
        }
        if (count == k) {
            break;
        }
        cycleCount++;

        vector<int> newSpiralPattern(n * n);
        for (int i = 0; i < n * n; i++) {
            newSpiralPattern[i] = spiralPattern[newPosition[i]];
        }
        spiralPattern = move(newSpiralPattern);
    }

    fout << cycleCount << '\n';

    fin.close();
    fout.close();

    return 0;
}
