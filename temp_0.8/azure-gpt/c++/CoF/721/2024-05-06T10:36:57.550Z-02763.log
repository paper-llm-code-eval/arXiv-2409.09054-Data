#include <fstream>
#include <vector>

using namespace std;

vector<int> computeSpiralPattern(int n) {
    vector<int> spiral(n * n);
    int left = 0, right = n - 1, top = 0, bottom = n - 1;
    int idx = 0;

    while (left <= right && top <= bottom) {
        for (int i = left; i <= right; i++)
            spiral[idx++] = top * n + (top % 2 ? right + left - i : i) + 1;
        top++;
        for (int i = top; i <= bottom; i++)
            spiral[idx++] = i * n + (i % 2 ? left : right) + 1;
        right--;
        if (top <= bottom) {
            for (int i = right; i >= left; i--)
                spiral[idx++] = bottom * n + (bottom % 2 ? right + left - i : i) + 1;
            bottom--;
        }
        if (left <= right) {
            for (int i = bottom; i >= top; i--)
                spiral[idx++] = i * n + (i % 2 ? left : right) + 1;
            left++;
        }
    }
    return spiral;
}

int main() {
    ifstream fin("spirala.in");
    ofstream fout("spirala.out");

    int n, k;
    fin >> n >> k;

    vector<int> initialPattern = computeSpiralPattern(n);
    vector<int> spiralPattern = initialPattern;
    vector<int> newPosition(n * n + 1);
    for (int i = 0; i < n * n; i++) {
