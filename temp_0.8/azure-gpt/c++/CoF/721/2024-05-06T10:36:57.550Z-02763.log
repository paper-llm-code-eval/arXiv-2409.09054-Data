using namespace std;

vector<int> computeSpiralPattern(int n) {
    vector<int> spiral(n * n);
    int num = 1;
    for (int i = 0; i < n; ++i) {
        if (i % 2 == 0) {
            for (int j = 0; j < n; ++j) {
                spiral[i * n + j] = num++;
            }
        } else {
            for (int j = n - 1; j >= 0; --j) {
                spiral[i * n + j] = num++;
            }
        }
    }
    return spiral;
}

int main() {
