}

void find_path(int src, int dest, vector<int>& path, int dist[]) {
    while (src != dest) {
        path.push_back(src);
        for (int neighbor : graph[src]) {
            if (dist[neighbor] == dist[src] - 1) {
                src = neighbor;
                break;
            }
        }
    }
    path.push_back(dest);
}

