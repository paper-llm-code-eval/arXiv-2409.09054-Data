#include <fstream>
#include <vector>
#include <queue>
#include <cstring>

using namespace std;

const int MAXN = 5005;
const int INF = 1 << 30;

vector<int> g[MAXN];
int distA[MAXN], distB[MAXN], distS[MAXN];
int prevA[MAXN], prevB[MAXN], prevS[MAXN];
bool visited[MAXN];

void bfs(int start, int dist[], int prev[]) {
    queue<int> q;
    memset(dist, INF, MAXN * sizeof(int));
    memset(prev, -1, MAXN * sizeof(int));
    memset(visited, 0, MAXN * sizeof(bool));
    q.push(start);
    dist[start] = 0;
    visited[start] = true;

    while (!q.empty()) {
        int node = q.front(); q.pop();
        for (int i = 0; i < g[node].size(); ++i) {
            int to = g[node][i];
            if (!visited[to]) {
                visited[to] = true;
                q.push(to);
                dist[to] = dist[node] + 1;
                prev[to] = node;
            }
        }
    }
}

vector<int> buildPath(int start, int end, int prev[]) {
    vector<int> path;
    for (int v = end; v != start; v = prev[v])
        path.push_back(v);
    path.push_back(start);
    reverse(path.begin(), path.end());
    return path;
}

int main() {
    ifstream fin("veri.in");
    ofstream fout("veri.out");

    int c, n, m, S, A, B;
    fin >> c >> n >> m >> S >> A >> B;

    while (m--) {
        int x, y;
        fin >> x >> y;
        g[x].push_back(y);
    }

    bfs(A, distA, prevA);
    bfs(B, distB, prevB);
    bfs(S, distS, prevS);

    int minTime = INF;
    int Z = -1;
    for (int i = 1; i <= n; ++i) {
        if (distS[i] < INF && distA[i] < INF && distB[i] < INF) {
            int t = distS[i];
            int maxTime = max(t + distA[i], t + distB[i]);
            if (minTime > maxTime) {
                minTime = maxTime;
                Z = i;
            }
        }
    }

    if (c == 1) {
        fout << minTime << '\n';
    } else { // c == 2
        vector<int> pathS = buildPath(S, Z, prevS);
        vector<int> pathA = buildPath(Z, A, prevA);
        vector<int> pathB = buildPath(Z, B, prevB);

        fout << pathS.size() - 1 << '\n';
        for (size_t i = 0; i < pathS.size(); ++i)
            fout << pathS[i] << (i + 1 == pathS.size() ? '\n' : ' ');

        fout << pathA.size() - 1 << '\n';
        for (size_t i = 0; i < pathA.size(); ++i)
            fout << pathA[i] << (i + 1 == pathA.size() ? '\n' : ' ');

        fout << pathB.size() - 1 << '\n';
        for (size_t i = 0; i < pathB.size(); ++i)
            fout << pathB[i] << (i + 1 == pathB.size() ? '\n' : ' ');
    }

    fin.close();
    fout.close();

    return 0;
}
```

This solution first performs a BFS from nodes A, B, and S to compute the shortest distances and previous nodes for each node. It then finds the optimal meeting node Z that minimizes the maximum of the total times taken to reach A and B. Depending on the value of c, it either outputs the minimum time or the paths from S to Z to A and B. The paths are reconstructed by tracing back the previous nodes starting from each destination.

Please note that this code assumes the input file `veri.in` and output file `veri.out` are in the same directory as the executable. To test this code, you would need to create an input file named `veri.in` with the given format and then compile and run the code to produce the output file `veri.out`.
