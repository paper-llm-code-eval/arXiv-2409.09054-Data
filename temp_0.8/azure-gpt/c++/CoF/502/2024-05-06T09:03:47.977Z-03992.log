#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>

using namespace std;

void readGraph(int &n, int &m, int &S, int &A, int &B,
               vector<vector<int>> &adj, ifstream &fin) {
    fin >> n >> m;
    fin >> S >> A >> B;
    adj.resize(n + 1);
    for (int i = 0, u, v; i < m; ++i) {
        fin >> u >> v;
        adj[u].push_back(v);
    }
}

vector<int> shortestPath(const vector<vector<int>> &adj, int start, int n) {
    vector<int> dist(n + 1, -1);
    queue<int> q;
    q.push(start);
    dist[start] = 0;
    while (!q.empty()) {
        int u = q.front(); q.pop();
        for (int v : adj[u]) {
            if (dist[v] == -1) {
                dist[v] = dist[u] + 1;
                q.push(v);
            }
        }
    }
    return dist;
}

int main() {
    ifstream fin("veri.in");
    ofstream fout("veri.out");

    int c, n, m, S, A, B;
    fin >> c;
    vector<vector<int>> adj;
    readGraph(n, m, S, A, B, adj, fin);

    vector<int> distA = shortestPath(adj, A, n);
    vector<int> distB = shortestPath(adj, B, n);

    int minTime = -1;
    int Z = -1;

    for (int i = 1; i <= n; ++i) {
        if (distA[i] != -1 && distB[i] != -1) {
            int maxTime = max(distA[i], distB[i]);
            if (minTime == -1 || maxTime < minTime) {
                minTime = maxTime;
                Z = i;
            }
        }
    }

    if (c == 1) {
        fout << minTime << '\n';
    } else {
        fout << 1 << '\n';
        fout << S << ' ' << Z << '\n';
        fout << distA[Z] << '\n';
        for (int u = Z; u != A; ) {
            for (int v : adj[u]) {
                if (distA[v] == distA[u] - 1) {
                    fout << u << ' ';
                    u = v;
                    break;
                }
            }
        }
        fout << A << '\n';
        fout << distB[Z] << '\n';
        for (int u = Z; u != B; ) {
            for (int v : adj[u]) {
                if (distB[v] == distB[u] - 1) {
                    fout << u << ' ';
                    u = v;
                    break;
                }
            }
        }
        fout << B << '\n';
    }

    fin.close();
    fout.close();
    return 0;
}
