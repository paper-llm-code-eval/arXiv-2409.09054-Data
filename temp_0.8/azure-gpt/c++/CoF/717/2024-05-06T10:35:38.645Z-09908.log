#include <fstream>
#include <string>
using namespace std;

string generate_series_element(int n) {
    if (n == 1) return "a";
    if (n == 2) return "b";
    return generate_series_element(n - 2) + generate_series_element(n - 1);
}

int main() {
    ifstream fin("sir.in");
    ofstream fout("sir.out");

    int n;
    fin >> n;

    string nth_element = generate_series_element(n);
    fout << nth_element;

    fin.close();
    fout.close();

    return 0;
}
```
