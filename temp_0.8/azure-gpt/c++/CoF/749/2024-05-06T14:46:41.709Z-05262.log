#include <fstream>

int main() {
    std::ifstream inFile("case.in");
    std::ofstream outFile("case.out");

    int n, houseNumber;
    int familyRelations[3] = {0, 0, 0};
    int population[3] = {0, 0, 0};  // men, women, children

    inFile >> n;
    while (n--) {
        inFile >> houseNumber;
        familyRelations[houseNumber / 1000 - 1]++;
        population[0] += (houseNumber / 100) % 10;   // men
        population[1] += (houseNumber / 10) % 10;    // women
        population[2] += houseNumber % 10;           // children
    }

    inFile.close();

    for (int i = 0; i < 3; ++i) {
        outFile << familyRelations[i] << '\n';
    }
    for (int i = 0; i < 3; ++i) {
        outFile << population[i] << (i < 2 ? '\n' : "");
    }

    outFile.close();
    return 0;
}
```

