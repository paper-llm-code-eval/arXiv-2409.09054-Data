#include <fstream>
#include <vector>

std::vector<int> primes;
std::vector<bool> is_prime;

void sieve(int max_val) {
    is_prime.resize(max_val + 1, true);
    is_prime[0] = is_prime[1] = false;

    for (int i = 2; i <= max_val; ++i) {
        if (is_prime[i]) {
            primes.push_back(i);
            for (int j = 2 * i; j <= max_val; j += i) {
                is_prime[j] = false;
            }
        }
    }
}

int findKthPrimeFactor(int num, int k) {
    int prime_idx = 0;
    int prime_factor_count = 0;
    for (const auto& prime : primes) {
        while (num % prime == 0) {
            num /= prime;
            prime_factor_count++;
            if (prime_factor_count == k) {
                return prime;
            }
        }
        if (num == 1) break; 
    }
    return -1; // This should never be reached given the guaranteed constraints.
}

int main() {
    std::ifstream in("cufar.in");
    std::ofstream out("cufar.out");

    int p, n;
    in >> p >> n;

    sieve(1000000); // Precompute primes up to 1,000,000.

    if (p == 1) {
        int num, k;
        in >> num >> k;
        int magic_value = findKthPrimeFactor(num, k);
        out << magic_value << '\n';
    } else if (p == 2) {
        long long sum_magic_values = 0;
        for (int i = 0; i < n; ++i) {
            int num, k;
            in >> num >> k;
            sum_magic_values += findKthPrimeFactor(num, k);
        }
        out << sum_magic_values << '\n';
    }

    in.close();
    out.close();
    return 0;
}
```
