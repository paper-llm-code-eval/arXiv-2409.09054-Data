#include <fstream>
#include <vector>

struct Duck {
    int x, y, dir;
};

void executeCommand(Duck& duck, char commandType, int value, int N) {
    switch (commandType) {
        case 'A': // Advance
            switch (duck.dir) {
                case 1: duck.x = (duck.x - value + N) % N; break; // North
                case 2: duck.y = (duck.y + value) % N; break; // East
                case 3: duck.x = (duck.x + value) % N; break; // South
                case 4: duck.y = (duck.y - value + N) % N; break; // West
            }
            break;
        case 'R': // Rotate
            duck.dir = (duck.dir + value - 1) % 4 + 1;
            break;
        case 'Z': // Fly to position
            duck.x = value / N;
            duck.y = value % N;
            break;
    }
}

int main() {
    std::ifstream fin("pelican.in");
    std::ofstream fout("pelican.out");

    int N, P, K;
    fin >> N >> P >> K;

    std::vector<Duck> ducks(P);
    for (int i = 0; i < P; ++i) {
        fin >> ducks[i].x >> ducks[i].y >> ducks[i].dir;
    }

    char commandType;
    int value;
    for (int i = 0; i < K; ++i) {
        fin >> commandType >> value;
        for (Duck& duck : ducks) {
            executeCommand(duck, commandType, value, N);
        }
    }

    for (const Duck& duck : ducks) {
        fout << duck.x << ' ' << duck.y << '\n';
    }

    fin.close();
    fout.close();

    return 0;
}
```
