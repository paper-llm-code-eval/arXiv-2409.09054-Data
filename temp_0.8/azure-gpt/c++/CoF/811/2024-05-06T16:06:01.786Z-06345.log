#include <fstream>
#include <vector>
#include <algorithm>
#include <map>

using namespace std;

int count_divisors(int number) {
    int divisors = 0;
    for (int i = 1; i <= number; ++i) {
        if (number % i == 0) ++divisors;
    }
    return divisors;
}

int main() {
    ifstream in("grupe.in");
    ofstream out("grupe.out");

    int m, n, x;
    in >> m >> n;
    
    map<int, vector<int>, greater<int>> divisors_to_elements;
    
    for (int i = 0; i < m; ++i) {
        for (int j = 0; j < n; ++j) {
            in >> x;
            int divisors = count_divisors(x);
            divisors_to_elements[divisors].push_back(x);
        }
    }
    
    vector<pair<int, pair<int, int>>> groups;
    
    for (auto &entry : divisors_to_elements) {
        auto &elements = entry.second;
        int max_element = *max_element(elements.begin(), elements.end());
        groups.push_back(make_pair(entry.first, make_pair(elements.size(), max_element)));
    }
    
    sort(groups.begin(), groups.end(), [](const pair<int, pair<int, int>> &a, const pair<int, pair<int, int>> &b) {
        if (a.second.first == b.second.first)
            return a.first > b.first;
        return a.second.first > b.second.first;
    });
    
    if (groups.size() > 0) {
        out << groups[0].first << " " << groups[0].second.first << " " << groups[0].second.second << "\n";
    }
    
    if (groups.size() > 1) {
        out << groups[1].first << " " << groups[1].second.first << " " << groups[1].second.second << "\n";
    } else {
        out << "0 0 0\n";
    }
    
    in.close();
    out.close();
    return 0;
}
```
This C++ code reads the input from "grupe.in", processes the data to find the number of divisors for each element in the matrix, groups them accordingly, sorts the groups by size and then by the number of divisors, and finally writes the output to "grupe.out".
