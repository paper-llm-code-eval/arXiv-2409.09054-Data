#include <fstream>

int main() {
    std::ifstream fin("vanatoare.in");
    std::ofstream fout("vanatoare.out");

    int ka, kb;
    fin >> ka >> kb;
    fin.close();
    
    int alb = 1, negru = 0; // Initial duck colors in the first row
    int last_alb = 0, last_negru = 1; // Count of ducks for the two previous rows (0 and 1 initially)
    int randuri = 0; // Number of rows hunted
    
    while (true) {
        // Check if there are enough arrows for both white and black ducks in the current row
        if (ka >= alb && kb >= negru) {
            // Subtract the arrows used for the current row
            ka -= alb;
            kb -= negru;
            // Increment the number of hunted rows
            ++randuri;
            // Prepare for the next row: calculate new duck counts
            int next_alb = alb + last_alb;
            int next_negru = negru + last_negru;
            // Update previous row counts
            last_alb = alb;
            last_negru = negru;
            // Update current row counts
            alb = next_alb;
            negru = next_negru;
        } else {
            // Not enough arrows to hunt the current row
            break;
        }
    }

    // Write the results to the output file
    fout << randuri << '\n';
    fout << ka << '\n';
    fout << kb << '\n';
    fout.close();
    
    return 0;
}
