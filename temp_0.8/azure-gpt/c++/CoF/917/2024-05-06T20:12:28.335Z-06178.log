        for (int j = 1; j <= M; ++j)
            sm[i][j] = sm[i-1][j] + sm[i][j-1] - sm[i-1][j-1] + mat[i][j];
}

long long subMatrixSum(int i1, int j1, int i2, int j2) {
    return sm[i2][j2] - sm[i1-1][j2] - sm[i2][j1-1] + sm[i1-1][j1-1];
}

int main() {
    int c, N, M, K, maxProfit = -1e9, maxI1, maxI2, maxJ1, maxJ2;
