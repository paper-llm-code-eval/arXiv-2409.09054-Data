#include <fstream>
#include <vector>
#include <algorithm>

int main() {
    std::ifstream fin("campionat.in");
    std::ofstream fout("campionat.out");

    int T, N, D;
    fin >> T >> N;
    std::vector<int> scores(N), maxPossibleScores(N), matchesLeft(N, 0);

    for (int i = 0; i < N; ++i) {
        fin >> scores[i];
        maxPossibleScores[i] = scores[i];
    }

    fin >> D;
    for (int i = 0; i < D; ++i) {
        int team1, team2;
        fin >> team1 >> team2;
        --team1; --team2; // Adjusting for 0-based indexing
        maxPossibleScores[team1] += 3;
        matchesLeft[team1]++;
        maxPossibleScores[team2] += 3;
        matchesLeft[team2]++;
    }

    if (T == 1) {
        // Question 1: Teams that will be at the top if all remaining games end in a draw
        int maxScore = 0;
        // Find the maximum score if all the games end in a draw
        for (int i = 0; i < N; ++i) {
            scores[i] += matchesLeft[i];
            maxScore = std::max(maxScore, scores[i]);
        }
        // Output all the teams with the maximum score
        for (int i = 0; i < N; ++i) {
            if (scores[i] == maxScore) {
                fout << (i + 1) << " ";
            }
        }
    } else {
        // Question 2: Teams that can win the championship depending only on their own results
        // First, find the maximum score possible for each team if they win all remaining matches.
        int absoluteMaxScore = *std::max_element(maxPossibleScores.begin(), maxPossibleScores.end());
        std::vector<int> potentialChampions;
        for (int i = 0; i < N; ++i) {
            // Teams that can become champions if they win all their matches, regardless of other results
            if (maxPossibleScores[i] > absoluteMaxScore - 3 * matchesLeft[i]) {
                potentialChampions.push_back(i + 1);
            }
        }
        if (potentialChampions.empty()) {
            fout << "0";
        } else {
            for (int team : potentialChampions) {
                fout << team << " ";
            }
        }
    }

    fin.close();
    fout.close();
    return 0;
}
```
