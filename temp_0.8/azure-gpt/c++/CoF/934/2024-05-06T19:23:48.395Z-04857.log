#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream fin("campionat.in");
    ofstream fout("campionat.out");

    int T, N, D, i, j;
    fin >> T >> N;
    vector<int> points(N + 1, 0);
    vector<int> max_points(N + 1, 0);
    vector<int> games(N + 1, 0);

    for (i = 1; i <= N; ++i) {
        fin >> points[i];
        max_points[i] = points[i];
    }

    fin >> D;
    for (i = 0; i < D; ++i) {
        fin >> j;
        fin.ignore(256, ' ');
        fin >> j;
        games[j]++;
    }

    if (T == 1) {
        int max_score = *max_element(points.begin(), points.end());
        for (i = 1; i <= N; ++i) {
            if (points[i] == max_score) {
                fout << i << ' ';
            }
        }
    } else {
        int overall_max = *max_element(points.begin(), points.end());
        for (i = 1; i <= N; ++i) {
            max_points[i] += games[i] * 3;
        }

        vector<int> potential_winners;
        for (i = 1; i <= N; ++i) {
            if (max_points[i] > overall_max) {
                potential_winners.push_back(i);
            }
        }

        if (potential_winners.empty()) {
            fout << "0";
        } else {
            for (int winner : potential_winners) {
                fout << winner << ' ';
            }
        }
    }

    fin.close();
    fout.close();
    return 0;
}
```

