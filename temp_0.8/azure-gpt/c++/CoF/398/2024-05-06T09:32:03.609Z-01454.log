    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (!visited[i][j]) {
                if (map[i][j] == '1') {
                    int count = 0;
                    FloodFill(i, j, 1, count);
                    if (count > 0) insuleR++;
                } else if (map[i][j] == '2') {
                    int count = 0;
                    FloodFill(i, j, 2, count);
                    if (count > 0) insuleG++;
                } else if (map[i][j] == '3') {
                    int count = 0;
                    FloodFill(i, j, 3, count);
                    if (count > 0) insuleB++;
                }
            }
        }
    }

    memset(distR, INF, sizeof(distR));
    memset(distG, INF, sizeof(distG));
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (map[i][j] == '1') BFS(i, j, distR);
            if (map[i][j] == '2') BFS(i, j, distG);
        }
    }

    int minBridge = INF;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (map[i][j] == '0' && distR[i][j] != INF && distG[i][j] != INF) {
                minBridge = min(minBridge, distR[i][j] + distG[i][j] - 1);
            }
        }
    }

    fout << insuleR << " " << insuleG << " " << insuleB << " " << minBridge << "\n";

    fin.close();
    fout.close();
    return 0;
}
