using namespace std;

const int MAX_N = 100;
const int MAX_M = 100;
const int INF = 1e9;
const int dx[] = {0, 1, 0, -1};
const int dy[] = {1, 0, -1, 0};
int n, m;
char map[MAX_N][MAX_M + 1];
bool visited[MAX_N][MAX_M];
int distR[MAX_N][MAX_M], distG[MAX_N][MAX_M];

void FloodFill(int x, int y, int value, int &count) {
    if (x < 0 || y < 0 || x >= n || y >= m || visited[x][y] || map[x][y] != value + '0') return;
    visited[x][y] = true;
    count++;
    for (int d = 0; d < 4; d++) {
        FloodFill(x + dx[d], y + dy[d], value, count);
    }
}

void BFS(int srcX, int srcY, int (&dist)[MAX_N][MAX_M]) {
    queue<pair<int, int>> q;
    q.push({srcX, srcY});
    dist[srcX][srcY] = 0;
    while (!q.empty()) {
        int x = q.front().first, y = q.front().second;
        q.pop();
        for (int d = 0; d < 4; d++) {
            int newX = x + dx[d], newY = y + dy[d];
            if (newX >= 0 && newY >= 0 && newX < n && newY < m && map[newX][newY] == '0' && dist[newX][newY] > dist[x][y] + 1) {
                dist[newX][newY] = dist[x][y] + 1;
                q.push({newX, newY});
            }
        }
    }
}

