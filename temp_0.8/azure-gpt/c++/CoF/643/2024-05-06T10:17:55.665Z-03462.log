#include <fstream>
#include <vector>

struct Street {
    int start, end;
};

int main() {
    std::ifstream in("pergament.in");
    std::ofstream out("pergament.out");

    int N, K, Q;
    in >> N >> K >> Q;

    std::vector<Street> horizontal(N + 1); // Store start and end points of horizontal streets
    int A, B, C, D, X, Y;
    in >> A >> B >> C >> D >> X >> Y;

    // We calculate the horizontal streets and store their start and end points
    for (int i = 1; i <= N; ++i) {
        horizontal[i].start = X;
        horizontal[i].end = X + Y - 1; // End point is start + length - 1
        X = 1 + (static_cast<long long>(A) * X + B) % K;
        Y = 1 + (static_cast<long long>(C) * Y + D) % (K - X + 1);
    }

    int J, R, L, purpleSquares = 0;
    // We calculate the purple squares by checking each vertical street
    for (int i = 0; i < Q; ++i) {
        in >> J >> R >> L;
        for (int r = R; r < R + L; ++r) {
            if (r <= N && J >= horizontal[r].start && J <= horizontal[r].end) {
                purpleSquares++; // Increment if the vertical street intersects with a horizontal street
            }
        }
    }

    out << purpleSquares;
    in.close();
    out.close();

    return 0;
}
```

