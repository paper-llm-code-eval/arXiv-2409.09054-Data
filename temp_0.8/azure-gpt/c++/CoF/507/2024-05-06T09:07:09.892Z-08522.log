#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAX_N = 300000;

int n;
pair<int, int> notes[MAX_N];
vector<int> heights[19]; // Durations are powers of 2, up to 2^18.

long long getGroupScore(vector<int> &group) {
    long long sum = 0;
    for (int h : group) sum += h;
    return sum * sum;
}

void readInput(const char *filename) {
    ifstream fin(filename);
    fin >> n;
    for (int i = 0; i < n; ++i) {
        fin >> notes[i].first >> notes[i].second;
        heights[notes[i].first].push_back(notes[i].second);
    }
    fin.close();
}

void writeOutput(const char *filename, long long maxScore) {
    ofstream fout(filename);
    fout << maxScore;
    fout.close();
}

int main() {
    readInput("partitura.in");

    // Sort notes in decreasing order of height within each duration group
    for (int i = 1; i < 19; ++i) {
        sort(heights[i].rbegin(), heights[i].rend());
    }

    long long maxScore = 0;
    vector<int> group;

    // Merge the groups of the same duration into groups of duration 1
    for (int i = 18; i > 0; --i) {
        // Combine pairs of groups from the next lower duration
        while (heights[i].size() >= 2) {
            group.clear();
            group.push_back(heights[i].back());
            heights[i].pop_back();
            group.push_back(heights[i].back());
            heights[i].pop_back();
            maxScore += getGroupScore(group);
        }

        // If there is an unpaired group, merge it with groups from the next lower duration
        if (!heights[i].empty()) {
            int remaining = heights[i].back();
            heights[i].pop_back();
            for (int j = i - 1; j > 0; j -= 2) {
                if (!heights[j].empty()) {
                    remaining += heights[j].back();
                    heights[j].pop_back();
                }
                if (j == 1 || heights[j - 1].empty()) {
                    maxScore += (long long)remaining * remaining;
                    break;
                }
            }
        }
    }

    writeOutput("partitura.out", maxScore);

    return 0;
}
