#include <fstream>
#include <algorithm>
#include <vector>

// Function to find the most significant two digits of a number
std::pair<int, int> findMaxTwoDigits(int num) {
    std::vector<int> digits;
    while (num > 0) {
        digits.push_back(num % 10);
        num /= 10;
    }
    std::sort(digits.rbegin(), digits.rend());
    return {digits[0], digits.size() > 1 ? digits[1] : digits[0]};
}

// Function to process the task 1
void processTask1(std::ifstream& fin, std::ofstream& fout, int n) {
    int lastNumber;
    for (int i = 0; i < n; ++i) {
        fin >> lastNumber;
    }
    auto maxTwoDigits = findMaxTwoDigits(lastNumber);
    fout << maxTwoDigits.second << " " << maxTwoDigits.first << "\n";
}

// Function to check if a number contains the two digits
bool containsDigits(int num, int first, int second) {
    bool hasFirst = false, hasSecond = false;
    while (num > 0) {
        int digit = num % 10;
        if (digit == first) hasFirst = true;
        if (digit == second) hasSecond = true;
        num /= 10;
    }
    return hasFirst && hasSecond;
}

// Function to process the task 2
void processTask2(std::ifstream& fin, std::ofstream& fout, int n) {
    std::vector<int> stickers;
    stickers.reserve(n);
    int num;
    for (int i = 0; i < n; ++i) {
        fin >> num;
        stickers.push_back(num);
    }

    int victorStickers = 0, raduStickers = 0;
    bool victorTurn = true;

    while (!stickers.empty()) {
        num = stickers.front();
        stickers.erase(stickers.begin());
        auto maxTwoDigits = findMaxTwoDigits(num);
        int count = 1;

        for (auto it = stickers.begin(); it != stickers.end();) {
            if (containsDigits(*it, maxTwoDigits.first, maxTwoDigits.second)) {
                it = stickers.erase(it);
                ++count;
            } else {
                ++it;
            }
        }

        if (victorTurn)
            victorStickers += count;
        else
            raduStickers += count;

        victorTurn = !victorTurn; // swap turns
    }

    if (victorStickers > raduStickers)
        fout << "V\n" << victorStickers << "\n";
    else if (raduStickers > victorStickers)
        fout << "R\n" << raduStickers << "\n";
    else
        fout << "V R\n" << victorStickers << "\n";
}

int main() {
    std::ifstream fin("album.in");
    std::ofstream fout("album.out");

    int c, n;
    fin >> c >> n;

    if (c == 1) {
        processTask1(fin, fout, n);
    } else if (c == 2) {
        processTask2(fin, fout, n);
    }

    fin.close();
    fout.close();
    return 0;
}
