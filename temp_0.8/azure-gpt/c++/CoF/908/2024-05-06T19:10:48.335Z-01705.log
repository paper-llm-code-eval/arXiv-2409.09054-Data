#include <fstream>
#include <vector>
#include <algorithm>

std::pair<int, int> findMaxTwoDigits(int number) {
    std::vector<int> digits;
    while (number > 0) {
        digits.push_back(number % 10);
        number /= 10;
    }
    std::sort(digits.rbegin(), digits.rend());
    return {digits[0], digits[1]};
}

bool hasDigits(int number, int digit1, int digit2) {
    bool found1 = false, found2 = false;
    while (number > 0) {
        int digit = number % 10;
        if (digit == digit1) found1 = true;
        if (digit == digit2) found2 = true;
        number /= 10;
    }
    return found1 && found2;
}

int main() {
    std::ifstream fin("album.in");
    std::ofstream fout("album.out");

    int c, n;
    fin >> c >> n;

    std::vector<int> stickers(n);
    for (int i = 0; i < n; ++i) {
        fin >> stickers[i];
    }

    if (c == 1) {
        auto [max1, max2] = findMaxTwoDigits(stickers.back());
        fout << max2 << ' ' << max1 << '\n';
    } else {
        int victorStickers = 0, raduStickers = 0;
        bool victorTurn = true;

        for (int i = 0; i < n; /* i incremented in loop */) {
            int currSticker = stickers[i];
            auto [maxDigit1, maxDigit2] = findMaxTwoDigits(currSticker);
            int count = 0;

            while (i < n && hasDigits(stickers[i], maxDigit1, maxDigit2)) {
                count++;
                i++;
            }

            if (victorTurn) {
                victorStickers += count;
            } else {
                raduStickers += count;
            }

            victorTurn = !victorTurn;
        }

        if (victorStickers > raduStickers) {
            fout << 'V' << '\n' << victorStickers << '\n';
        } else if (raduStickers > victorStickers) {
            fout << 'R' << '\n' << raduStickers << '\n';
        } else {
            fout << 'V' << ' ' << 'R' << '\n' << victorStickers << '\n';
        }
    }

    fin.close();
    fout.close();
    return 0;
}
```
