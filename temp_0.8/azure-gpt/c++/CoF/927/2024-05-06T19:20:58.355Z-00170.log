#include <fstream>
#include <algorithm>
#include <numeric>
#include <vector>

int gcd(int a, int b) {
    while (b != 0) {
        int t = b;
        b = a % b;
        a = t;
    }
    return a;
}

int lcm(int a, int b) {
    return a / gcd(a, b) * b;
}

int main() {
    std::ifstream in("alinieri.in");
    std::ofstream out("alinieri.out");

    int N, P, Z;
    in >> N >> P >> Z;

    std::vector<int> V(N);
    for (int& v : V) {
        in >> v;
    }

    // Precompute the least common multiple (LCM) of all pairs of velocities
    std::vector<int> pairwise_lcm(N * (N - 1) / 2);
    int idx = 0;
    for (int i = 0; i < N; ++i) {
        for (int j = i + 1; j < N; ++j) {
            pairwise_lcm[idx++] = lcm(V[i], V[j]);
        }
    }

    // Count the number of days when at least P planets align
    int alignments = 0;
    for (int day = 1; day <= Z; ++day) {
        int count = 0;
        for (int lcm_val : pairwise_lcm) {
            if (day % lcm_val == 0) {
                ++count;
            }
            if (count >= P * (P - 1) / 2) {
                ++alignments;
                break;
            }
        }
    }

    out << alignments << '\n';

    in.close();
    out.close();

    return 0;
}
```
