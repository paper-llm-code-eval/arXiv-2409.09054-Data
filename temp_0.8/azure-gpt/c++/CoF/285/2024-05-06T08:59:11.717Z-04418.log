#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>
#include <cstring>

using namespace std;

struct Pulsar {
    int x, y, r, t;
};

struct State {
    int x, y, time;
};

ifstream fin("pulsar.in");
ofstream fout("pulsar.out");

int C, N, P;
vector<Pulsar> pulsars;
int xs, ys, xf, yf;
int dX[4] = {-1, 1, 0, 0};
int dY[4] = {0, 0, -1, 1};

bool in_danger(int x, int y, int time) {
    for (const auto &p : pulsars) {
        int pulsarTime = (p.t + time) % p.r;
        if (abs(p.x - x) + abs(p.y - y) <= pulsarTime) {
            return true;
        }
    }
    return false;
}

int max_affected_sectors() {
    int count, maxCount = 0;
    for (int time = 0; time <= 6; ++time) { // Maximum period is 6
        count = 0;
        for (int i = 1; i <= N; ++i) {
            for (int j = 1; j <= N; ++j) {
                if (in_danger(i, j, time)) {
                    count++;
                }
            }
        }
        maxCount = max(maxCount, count);
    }
    return maxCount;
}

int min_time_to_destination() {
    queue<State> q;
    bool visited[501][501][7]; // Max N and Max period
    memset(visited, false, sizeof(visited));
    q.push(State{xs, ys, 0});
    visited[xs][ys][0] = true;
    while (!q.empty()) {
        State current = q.front();
        q.pop();
        if (current.x == xf && current.y == yf) {
            return current.time;
        }

        for (int i = 0; i < 4; ++i) {
            int nx = current.x + dX[i];
            int ny = current.y + dY[i];
            int nt = (current.time + 1) % 6;

            if (nx >= 1 && nx <= N && ny >= 1 && ny <= N && !visited[nx][ny][nt] && !in_danger(nx, ny, nt)) {
                visited[nx][ny][nt] = true;
                q.push(State{nx, ny, current.time + 1});
            }
        }
    }
    return -1; // Should never happen due to problem guarantees
}

int main() {
    fin >> C >> N >> P;
    pulsars.resize(P);
    for (int i = 0; i < P; ++i) {
        fin >> pulsars[i].x >> pulsars[i].y >> pulsars[i].r >> pulsars[i].t;
    }
    fin >> xs >> ys >> xf >> yf;

    if (C == 1) {
        fout << max_affected_sectors();
    } else {
        fout << min_time_to_destination();
    }

    fin.close();
    fout.close();
    return 0;
}
```
