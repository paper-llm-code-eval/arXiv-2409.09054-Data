#include <fstream>
#include <vector>
#include <unordered_map>

using namespace std;

int main() {
    ifstream fin("livada.in");
    ofstream fout("livada.out");

    int m, n, p, soi, rows_with_major = 0, max_consecutive = 0;
    fin >> m >> n >> p;

    for (int i = 0; i < m; ++i) {
        int prev_soi = 0, count_soi = 0, max_count_on_row = 0;
        unordered_map<int, int> frequency;

        for (int j = 0; j < n; ++j) {
            fin >> soi;
            ++frequency[soi];
            if (soi == prev_soi) {
                ++count_soi;
            } else {
                prev_soi = soi;
                count_soi = 1;
            }
            if (count_soi > max_count_on_row) {
                max_count_on_row = count_soi;
            }
        }

        if (max_count_on_row > max_consecutive) {
            max_consecutive = max_count_on_row;
        }

        for (auto &entry : frequency) {
            if (entry.second > n / 2) {
                ++rows_with_major;
                break;
            }
        }
    }

    fout << rows_with_major << "\n" << max_consecutive << "\n";

    fin.close();
    fout.close();

    return 0;
}
