#include <fstream>
#include <vector>
#include <algorithm>
#include <string>

int main() {
    std::ifstream in("templu.in");
    std::ofstream out("templu.out");

    int N, L;
    in >> N >> L;
    in.close();

    std::string sum_str = "";
    int total_sum = 0, max_digit_pair = -1;

    for (int i = 0; i < L; ++i) {
        int line_sum = 0;
        for (int j = 0; j < L; ++j) {
            line_sum += N + std::min({i, j, L - i - 1, L - j - 1});
        }
        sum_str += std::to_string(line_sum);
        total_sum += line_sum;
    }

    // Find the maximum two digit number in the concatenated string
    for (size_t i = 0; i < sum_str.size() - 1; ++i) {
        int pair = (sum_str[i] - '0') * 10 + (sum_str[i + 1] - '0');
        max_digit_pair = std::max(max_digit_pair, pair);
    }

    out << total_sum << '\n' << max_digit_pair << '\n';
    out.close();

    return 0;
}
```
