#include <fstream>
#include <vector>

const int MOD = 20173333;

std::ifstream fin("sir.in");
std::ofstream fout("sir.out");

std::vector<long long> factorial;
std::vector<long long> inv_factorial;

long long fast_pow(long long base, int exponent) {
    long long result = 1;
    while (exponent > 0) {
        if (exponent & 1)
            result = (result * base) % MOD;
        base = (base * base) % MOD;
        exponent >>= 1;
    }
    return result;
}

void compute_factorials(int N) {
    factorial.resize(N + 1);
    inv_factorial.resize(N + 1);
    factorial[0] = 1;
    for (int i = 1; i <= N; ++i)
        factorial[i] = (factorial[i - 1] * i) % MOD;
    inv_factorial[N] = fast_pow(factorial[N], MOD - 2);
    for (int i = N - 1; i >= 0; --i)
        inv_factorial[i] = (inv_factorial[i + 1] * (i + 1)) % MOD;
}

long long comb(int n, int k) {
    if (k > n) return 0;
    return (((factorial[n] * inv_factorial[k]) % MOD) * inv_factorial[n - k]) % MOD;
}

long long count_sequences_ending_with_U(int N, int U) {
    return comb(N - 1, U - 1);
}

long long count_sequences_with_maximum_R(int N, int R) {
    long long result = 0;
    std::vector<long long> dp(N + 1, 0);
    dp[0] = 1; // There's 1 way to count 0 numbers
    for (int i = 1; i <= R; ++i) { // Only counting sequences with numbers not exceeding R repetitions
        for (int j = i; j <= N; ++j) {
            dp[j] = (dp[j] + dp[j - i]) % MOD;
        }
    }
    result = dp[N];
    return result;
}

int main() {
    int P, N, X;
    fin >> P >> N >> X;
    compute_factorials(N);
    long long answer = 0;
    if (P == 1) {
        answer = count_sequences_ending_with_U(N, X);
    } else if (P == 2) {
        answer = count_sequences_with_maximum_R(N, X);
    }
    fout << answer << '\n';
    
    fin.close();
    fout.close();

    return 0;
}
```
Please note that this code assumes that a modern C++ compiler is being used, which supports C++11 or later standards for features like `auto` type deduction and range-based for loops.
