#include <fstream>
#include <vector>
#include <set>
#include <algorithm>

std::ifstream fin("colaj.in");
std::ofstream fout("colaj.out");

struct Event {
    int x, y1, y2, type;
    bool operator<(const Event& o) const {
        return x < o.x || (x == o.x && type < o.type);
    }
};

int main() {
    int n, m, p;
    fin >> n >> m >> p;

    std::vector<Event> events;
    std::set<int> ys;

    for (int i = 0, x1, y1, x2, y2; i < n; ++i) {
        fin >> x1 >> y1 >> x2 >> y2;
        events.push_back({x1, y1, y2, 1});
        events.push_back({x2, y1, y2, -1});
        ys.insert(y1);
        ys.insert(y2);
    }
    fin.close();

    std::sort(events.begin(), events.end());
    std::vector<int> yUnique(ys.begin(), ys.end());
    std::vector<int> sweep(yUnique.size(), 0);

    int countWhiteAreas = 0;
    bool lastWasWhite = true;

    for (size_t i = 0; i < events.size(); ++i) {
        Event e = events[i];
        int y1_idx = std::lower_bound(yUnique.begin(), yUnique.end(), e.y1) - yUnique.begin();
        int y2_idx = std::lower_bound(yUnique.begin(), yUnique.end(), e.y2) - yUnique.begin();

        for (int j = y1_idx; j < y2_idx; ++j) {
            sweep[j] += e.type;
        }

        if (i < events.size() - 1) {
            int nextX = events[i + 1].x;
            for (size_t j = 0; j < sweep.size(); ++j) {
                if (sweep[j] == 0) {
                    if (!lastWasWhite) {
                        countWhiteAreas++;
                        lastWasWhite = true;
                    }
                } else {
                    lastWasWhite = false;
                }
            }
        }
    }

    fout << countWhiteAreas << '\n';
    fout.close();

    return 0;
}
