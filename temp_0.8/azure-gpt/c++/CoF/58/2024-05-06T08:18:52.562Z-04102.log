#include <fstream>
#include <set>
#include <vector>

int main() {
    std::ifstream fin("nunta.in");
    std::ofstream fout("nunta.out");

    int n;
    fin >> n;
    std::vector<int> pretenders(n);
    for (int i = 0; i < n; ++i) {
        fin >> pretenders[i];
    }
    fin.close();

    // Use a set to store all unique possible values of P
    std::set<int> possible_values;

    // Recursive lambda function to explore all negotiation possibilities
    std::function<void(int, std::vector<int>)> negotiate = [&](int pos, std::vector<int> p) {
        if (pos == p.size() - 1) {
            possible_values.insert(p[pos]);
            return;
        }
        if (p[pos] < p[pos + 1]) {
            std::vector<int> next_p = p;
            next_p[pos + 1] -= p[pos];
            next_p.erase(next_p.begin() + pos);
            negotiate(pos, next_p);
        } else if (p[pos] > p[pos + 1]) {
            std::vector<int> next_p = p;
            next_p[pos] -= p[pos + 1];
            next_p.erase(next_p.begin() + pos + 1);
            negotiate(pos, next_p);
        } else {
            std::vector<int> next_p = p;
            next_p.erase(next_p.begin() + pos + 1);
            negotiate(pos, next_p);

            next_p = p;
            next_p.erase(next_p.begin() + pos);
            negotiate(pos, next_p);
        }
    };

    // Start negotiating from the beginning
    negotiate(0, pretenders);

    // Write the result to the output file
    fout << possible_values.size() << "\n";
    for (auto value : possible_values) {
        fout << value << " ";
    }
    fout << "\n";
    fout.close();

    return 0;
}
