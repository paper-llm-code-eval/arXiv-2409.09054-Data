#include <fstream>
#include <vector>

struct ExchangeHouse {
    char action;
    int buyRate;
    int sellRate;
};

struct Player {
    long long lei;
    long long euros;
    bool hasPass;
    bool eliminated;
};

int main() {
    std::ifstream in("valutar.in");
    std::ofstream out("valutar.out");

    int task;
    in >> task;

    int a, b, nr, N, M, L, E, X;
    in >> a >> b >> nr;
    in >> N >> M >> L >> E >> X;

    std::vector<ExchangeHouse> houses(N + 1);
    std::vector<Player> players(M + 1, {L, E, false, false});

    for (int i = 1; i <= N; ++i) {
        in >> houses[i].action >> houses[i].buyRate >> houses[i].sellRate;
    }

    int remainingPlayers = M;
    int currentPlayer = 1;
    for (int move = 1; move <= X; ++move) {
        if (players[currentPlayer].eliminated) {
            currentPlayer = currentPlayer % M + 1;
            continue;
        }

        nr = (a * nr + b) % N + 1;
        
        int targetHouse = (nr - 1) % N + 1;
        ExchangeHouse &house = houses[targetHouse];
        
        switch (house.action) {
            case 'A':
                break;
            case 'R':
                players[currentPlayer].hasPass = true;
                break;
            case 'G':
                if (players[currentPlayer].lei < house.buyRate * targetHouse) {
                    if (players[currentPlayer].hasPass) {
                        players[currentPlayer].hasPass = false;
                    } else {
                        players[currentPlayer].eliminated = true;
                        --remainingPlayers;
                    }
                } else {
                    players[currentPlayer].lei -= house.buyRate * targetHouse;
                    players[currentPlayer].euros += targetHouse;
                }
                break;
            case 'V':
                if (players[currentPlayer].euros < targetHouse) {
                    if (players[currentPlayer].hasPass) {
                        players[currentPlayer].hasPass = false;
                    } else {
                        players[currentPlayer].eliminated = true;
                        --remainingPlayers;
                    }
                } else {
                    players[currentPlayer].euros -= targetHouse;
                    players[currentPlayer].lei += house.sellRate * targetHouse;
                }
                break;
        }

        currentPlayer = currentPlayer % M + 1;
    }

    if (task == 1) {
        out << remainingPlayers << '\n';
    } else {
        int maxEurosPlayer = 0;
        long long maxEuros = 0;
        for (int i = 1; i <= M; ++i) {
            if (!players[i].eliminated && players[i].euros > maxEuros) {
                maxEuros = players[i].euros;
                maxEurosPlayer = i;
            }
        }
        out << maxEurosPlayer << '\n';
    }

    in.close();
    out.close();
    
    return 0;
}
