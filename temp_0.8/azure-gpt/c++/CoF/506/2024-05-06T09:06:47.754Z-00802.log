#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAX_VAL = 2147483647;
vector<int> fibs;

void generateFibonacci() {
    fibs.push_back(1);
    fibs.push_back(1);
    for (int i = 2; fibs[i-1] + fibs[i-2] > 0 && fibs[i-1] + fibs[i-2] < MAX_VAL; ++i) {
        fibs.push_back(fibs[i-1] + fibs[i-2]);
    }
}

bool isFibonacci(int x) {
    return binary_search(fibs.begin(), fibs.end(), x);
}

int findClosestFib(int x) {
    auto it = lower_bound(fibs.begin(), fibs.end(), x);
    if (it == fibs.end()) return fibs.back();
    if (it == fibs.begin()) return *it;
    int hi = *it, lo = *(--it);
    if (x - lo <= hi - x) return lo;
    return hi;
}

int main() {
    ifstream cin("fibosnek.in");
    ofstream cout("fibosnek.out");

    generateFibonacci();

    int c, n, m;
    cin >> c >> n >> m;

    if (c == 1) {
        int cnt = 0, x;
        for (int i = 0; i < n * m; ++i) {
