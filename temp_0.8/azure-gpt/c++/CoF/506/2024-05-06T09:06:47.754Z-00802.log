                    longest_non_fib = current_non_fib;
                    longest_non_fib_sum = current_non_fib_sum;
                }
                current_non_fib = 0;
                current_non_fib_sum = 0;
            } else {
                current_non_fib++;
                current_non_fib_sum += findClosestFib(x);
                current_fib_sum = 0;
            }

            longest_fib_sum = max(longest_fib_sum, current_fib_sum);
        }

        if (current_non_fib > longest_non_fib) {
            longest_non_fib = current_non_fib;
            longest_non_fib_sum = current_non_fib_sum;
        }

        cout << max(longest_fib_sum, longest_non_fib_sum) << "\n";
    }

    cin.close();
    cout.close();
    return 0;
}
