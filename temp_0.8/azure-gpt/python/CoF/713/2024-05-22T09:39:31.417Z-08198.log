# Efficient solution to calculate the power of k in n!

def count_factors(n, k):
    count = 0
    while n > 0:
        n //= k
        count += n
    return count

with open('exponent.in', 'r') as infile:
    n, k = map(int, infile.readline().split())

exponent = count_factors(n, k)

with open('exponent.out', 'w') as outfile:
    outfile.write(str(exponent))
