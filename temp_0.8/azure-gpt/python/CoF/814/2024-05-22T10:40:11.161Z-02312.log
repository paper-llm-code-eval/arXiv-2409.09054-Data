def eudorian_to_decimal(eudorian_str):
    value, multiplier = 0, {'m': 1000, 's': 100, 'z': 10, 'u': 1}
    num = ""
    for char in eudorian_str:
        if char.isdigit():
            num += char
        else:
            value += int(num) * multiplier[char]
            num = ""
    return value

def evaluate_expression(expression):
    parts = expression.split('+')
    return sum(eudorian_to_decimal(part) for part in parts)

def evaluate_comparison(comparison):
    if '<' in comparison:
        left, right = comparison.split('<')
        return evaluate_expression(left) < evaluate_expression(right), '<'
    elif '>' in comparison:
        left, right = comparison.split('>')
        return evaluate_expression(left) > evaluate_expression(right), '>'

with open('comp.in', 'r') as file_in:
    N = int(file_in.readline())
    comparisons = [file_in.readline().strip() for _ in range(N)]

less_than_count = 0
results = []

for comparison in comparisons:
    result, symbol = evaluate_comparison(comparison)
    results.append(int(result))
    if symbol == '<':
        less_than_count += 1

with open('comp.out', 'w') as file_out:
    file_out.write(f"{less_than_count}\n")
    file_out.writelines([f"{result}\n" for result in results])
