# Function to convert Eudorian numbers to integers
def eudorian_to_int(eudorian_num):
    value, multiplier = 0, 1
    for part in reversed(eudorian_num.split('+')):
        part_value = 0
        for section in part:
            if section.isdigit():
                part_value = part_value * 10 + int(section)
            else:
                if section == 'm':
                    multiplier = 1000
                elif section == 's':
                    multiplier = 100
                elif section == 'z':
                    multiplier = 10
                elif section == 'u':
                    multiplier = 1
                value += part_value * multiplier
                part_value = 0
    return value

# Function to evaluate Eudorian expressions and comparisons
def evaluate_comparisons(comparisons):
    less_than_count = 0
    results = []

    for comparison in comparisons:
        # Identify the comparison sign and split the expressions
        if '>' in comparison:
            expr1, expr2 = comparison.split('>')
            result = eudorian_to_int(expr1) > eudorian_to_int(expr2)
        else:
            expr1, expr2 = comparison.split('<')
            result = eudorian_to_int(expr1) < eudorian_to_int(expr2)
            less_than_count += 1

        # Store the result (1 for True, 0 for False)
        results.append(1 if result else 0)

    return less_than_count, results

# Read input data from comp.in
with open('comp.in', 'r') as file:
    n = int(file.readline().strip())
    comparisons = [file.readline().strip() for _ in range(n)]

# Evaluate the comparisons and write the output to comp.out
less_than_count, results = evaluate_comparisons(comparisons)
with open('comp.out', 'w') as file:
    file.write(f"{less_than_count}\n")
    file.writelines(f"{result}\n" for result in results)
