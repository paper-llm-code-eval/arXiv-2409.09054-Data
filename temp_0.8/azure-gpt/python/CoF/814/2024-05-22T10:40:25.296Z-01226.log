# Function to convert Eudorian number representation to integer
def eudorian_to_int(eudorian_num):
    num_map = {'m': 1000, 's': 100, 'z': 10, 'u': 1}
    result = 0
    current_value = ''
    for char in eudorian_num:
        if char.isnumeric():
            current_value += char
        else:
            result += int(current_value) * num_map[char]
            current_value = ''
    return result

# Function to evaluate Eudorian expressions and return the integer result
def eval_expression(expr):
    parts = expr.split('+')
    return sum(eudorian_to_int(part) for part in parts)

# Read input file, process data, and write output file
with open('comp.in', 'r') as file_in, open('comp.out', 'w') as file_out:
    n = int(file_in.readline().strip())
    less_than_count = 0
    results = []

    for _ in range(n):
        line = file_in.readline().strip()
        if '<' in line:
            less_than_count += 1
            left_expr, right_expr = line.split('<')
            comparison_result = eval_expression(left_expr) < eval_expression(right_expr)
        elif '>' in line:
            left_expr, right_expr = line.split('>')
            comparison_result = eval_expression(left_expr) > eval_expression(right_expr)
        results.append(str(int(comparison_result)))

    # Write results to output file
    file_out.write(str(less_than_count) + '\n')
    file_out.write('\n'.join(results) + '\n')
```

