# Code to solve the problem described

def generate_powers(max_power):
    powers = set()
    base = 2
    while base**2 <= max_power:
        power = base**2
        while power <= max_power:
            powers.add(power)
            power *= base
        base += 1
    return sorted(powers)

def find_closest_power(x, powers):
    left, right = 0, len(powers) - 1
    while left <= right:
        mid = (left + right) // 2
        if powers[mid] == x:
            return powers[mid]
        elif powers[mid] < x:
            left = mid + 1
        else:
            right = mid - 1

    # Get the closest power, with preference to the smaller one if distances are equal
    if left >= len(powers):
        return powers[-1]
    if right < 0:
        return powers[0]
    if abs(powers[left] - x) < abs(x - powers[right]):
        return powers[left]
    else:
        return powers[right]


# Read input from file
with open('abx.in', 'r') as file:
    N, M = map(int, file.readline().split())
    x_values = [int(file.readline()) for _ in range(N)]

# Generate all possible powers within [1, M]
powers = generate_powers(M)

# Find the closest power for each value
closest_powers = [find_closest_power(x, powers) for x in x_values]

# Write output to file
with open('abx.out', 'w') as file:
    file.write('\n'.join(map(str, closest_powers)) + '\n')
