def execute_program(program):
    position = 0
    output = []
    for instruction in program:
        if instruction[0] == 'D':
            position = (position + int(instruction[1])) % 10
        elif instruction[0] == 'S':
            position = (position - int(instruction[1])) % 10
        elif instruction == 'A':
            output.append(str(position))
    return output

def construct_program(number):
    number = str(number)
    program = []
    position = 0
    for digit in number:
        target = int(digit)
        right_dist = (target - position) % 10
        left_dist = (position - target) % 10
        if right_dist <= left_dist:
            if right_dist > 0:
                program.append(f'D{right_dist}')
            position = target
        else:
            if left_dist > 0:
                program.append(f'S{left_dist}')
            position = target
        program.append('A')
    program.append('T')
    return ''.join(program)

with open('robot.in', 'r') as file:
