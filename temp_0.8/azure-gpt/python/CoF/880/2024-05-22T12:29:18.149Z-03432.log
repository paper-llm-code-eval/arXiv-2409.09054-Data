# Solution in Python

def execute_program(program):
    pointer, screen = 0, []
    for action in program:
        if action[0] == 'D':
            pointer = (pointer + int(action[1])) % 10
        elif action[0] == 'S':
            pointer = (pointer - int(action[1])) % 10
        elif action == 'A':
            screen.append(str(pointer))
    return screen

def construct_program(number):
    number = str(number)
    program, pointer = [], 0
    for digit in number:
        target = int(digit)
        right_distance = (target - pointer) % 10
        left_distance = (pointer - target) % 10
        if right_distance <= left_distance:
            if right_distance > 0:
                program.append('D' + str(right_distance))
        else:
            if left_distance > 0:
                program.append('S' + str(left_distance))
        program.append('A')
        pointer = target
    program.append('T')
    return ''.join(program)

with open('robot.in', 'r') as fin:
