def replace_balls(drawn, remaining):
    remaining = sorted(remaining)
    min_ball = min(drawn)
    max_ball = max(drawn)
    drawn.remove(min_ball)
    drawn.remove(max_ball)
    min_replacement = max(remaining)
    max_replacement = min(remaining)
    for ball in remaining:
        if ball < min_ball and (min_ball - ball) <= (min_ball - min_replacement):
            min_replacement = ball
        if ball > max_ball and (ball - max_ball) <= (max_replacement - max_ball):
            max_replacement = ball
    drawn.append(min_replacement)
    drawn.append(max_replacement)
    return sorted(drawn)

with open('loto.in', 'r') as fin:
    N = int(fin.readline().strip())
    all_balls = list(map(int, fin.readline().strip().split()))
    drawn_balls = list(map(int, fin.readline().strip().split()))

remaining_balls = list(set(all_balls) - set(drawn_balls))
final_drawn = replace_balls(drawn_balls, remaining_balls)

with open('loto.out', 'w') as fout:
    fout.write(' '.join(map(str, final_drawn)) + '\n')
```

