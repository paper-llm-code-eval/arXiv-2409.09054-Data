# Ruleta.py

def process_ruleta(file_in, file_out):
    # Read the input data from ruleta.in
    with open(file_in, 'r') as fin:
        n = int(fin.readline().strip())
        numbers = list(map(int, fin.readline().split()))

    rotations = 0
    changed = True

    while changed:
        changed = False
        rotations += 1
        for i in range(n):
            current = numbers[i]
            next_index = (i + 1) % n
            if current != 0 and current <= numbers[next_index]:
                numbers[next_index] -= current
                changed = True

    # Count non-zero elements
    non_zero_count = sum(1 for number in numbers if number != 0)

    # Write the output to ruleta.out
    with open(file_out, 'w') as fout:
        fout.write(f"{rotations} {non_zero_count}\n")

# Call the function with the required file names
process_ruleta('ruleta.in', 'ruleta.out')
