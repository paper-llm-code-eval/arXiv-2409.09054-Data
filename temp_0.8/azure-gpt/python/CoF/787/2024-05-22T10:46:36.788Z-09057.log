    roulette_numbers = list(map(int, file.readline().strip().split()))

# initialize the variables for the number of full rotations and non-zero elements
rotations = 0
non_zero_elements = n

# perform the rotations
while True:
    has_changed = False
    initial_value = roulette_numbers[0]

    for i in range(n):
        next_index = (i + 1) % n
        if roulette_numbers[i] != 0 and roulette_numbers[i] <= roulette_numbers[next_index]:
            roulette_numbers[next_index] -= roulette_numbers[i]
            if roulette_numbers[next_index] == 0:
                non_zero_elements -= 1
            has_changed = True

    # increment the number of full rotations
    rotations += 1

    # if no modifications are made during a full rotation, stop the game
    if not has_changed:
        break

# write the output to "ruleta.out"
