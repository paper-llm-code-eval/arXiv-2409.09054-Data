def perform_rotations(numbers):
    n = len(numbers)
    rotations = 0

    while True:
        changes = False
        for i in range(n):
            if numbers[i] > 0 and numbers[(i + 1) % n] >= numbers[i]:
                numbers[(i + 1) % n] -= numbers[i]
                changes = True

        rotations += 1
        if not changes:
            break

    return rotations, sum([1 for number in numbers if number > 0])


def read_input(filename):
