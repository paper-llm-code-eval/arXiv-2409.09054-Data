def solve_equation(equation):
    parts = equation.split('=')
    left, right = parts[0], parts[1]

    def process_side(side):
        x_coeff = 0
        constant = 0
        sign = 1
        num = "0"
        for char in side:
            if char in '+-':
                if num.endswith('x'):
                    x_coeff += sign * int(num[:-1] or '1')
                else:
                    constant += sign * int(num)
                sign = 1 if char == '+' else -1
                num = "0"
            else:
                num += char
        if num.endswith('x'):
            x_coeff += sign * int(num[:-1] or '1')
        else:
            constant += sign * int(num)
        return x_coeff, constant

    left_x, left_const = process_side(left)
    right_x, right_const = process_side(right)

    x_coeff = left_x - right_x
    constant = right_const - left_const
    
    if x_coeff == 0:
        if constant == 0:
            return 'infinit\n'
        else:
            return 'imposibil\n'
    else:
        return '{:.4f}\n'.format(constant / x_coeff)

with open('ecuatii.in', 'r') as file_in:
    n = int(file_in.readline())
    equations = [file_in.readline().strip() for _ in range(n)]

results = [solve_equation(eq) for eq in equations]

with open('ecuatii.out', 'w') as file_out:
    file_out.writelines(results)
