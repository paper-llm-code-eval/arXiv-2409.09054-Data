def justify_line(words, max_width):
    if len(words) == 1:
        return words[0] + (' ' * (max_width - len(words[0])))
    total_spaces = max_width - sum(len(word) for word in words)
    spaces_between_words, extra_spaces = divmod(total_spaces, len(words) - 1)
    line = (' ' * (spaces_between_words + 1)).join(words[:extra_spaces + 1])
    line += ' ' * spaces_between_words
    line += (' ' * spaces_between_words).join(words[extra_spaces + 1:])
    return line

def justify_text(text, max_width):
    words = text.split()
    lines, current_words, current_len = [], [], 0
    for word in words:
        if current_len + len(word) + len(current_words) > max_width:
            lines.append(justify_line(current_words, max_width))
            current_words, current_len = [], 0
        current_words.append(word)
        current_len += len(word)
    lines.append(' '.join(current_words))
    return '\n'.join(lines)

def process_file(input_file, output_file, max_width):
    with open(input_file, 'r') as infile, open(output_file, 'w') as outfile:
        max_width = int(infile.readline())
        text = infile.read().strip()
        paragraphs = text.split('\n\n')
        for i, paragraph in enumerate(paragraphs):
            justified_paragraph = justify_text(paragraph.replace('\n', ' '), max_width)
            if i > 0:
                outfile.write('\n\n')
            outfile.write(justified_paragraph)

process_file('text.in', 'text.out', MAX)
