def divisors_count(n):
    count = 0
    for i in range(1, n//2 + 1):
        if n % i == 0:
            count += 1
    return count

def solve_task_1(n):
    return divisors_count(n)

def solve_task_2(n, values):
    prefix_sum = [0] * (n + 1)
    for i in range(1, n + 1):
        prefix_sum[i] = prefix_sum[i - 1] + values[i - 1]
    
    min_diff = float('inf')
    best_k = 0
    best_E = 0
    
    for k in range(1, n // 2 + 1):
        if n % k == 0:
            max_sum = max(prefix_sum[i * k] - prefix_sum[(i - 1) * k] for i in range(1, n // k + 1))
            min_sum = min(prefix_sum[i * k] - prefix_sum[(i - 1) * k] for i in range(1, n // k + 1))
            diff = max_sum - min_sum
            if diff < min_diff:
                min_diff = diff
                best_k = n // k
                for i in range(1, n // k + 1):
                    if prefix_sum[i * k] - prefix_sum[(i - 1) * k] == max_sum:
                        best_E = (i - 1) * k + 1
                        break
    
    return best_k, best_E

