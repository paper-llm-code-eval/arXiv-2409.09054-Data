def read_livada_input(filename):
    with open(filename, 'r') as file:
        m, n, p = map(int, file.readline().split())
        orchard = [list(map(int, file.readline().split())) for _ in range(m)]
    return m, n, p, orchard

def write_livada_output(filename, rows_with_majority, max_consecutive):
    with open(filename, 'w') as file:
        file.write(f"{rows_with_majority}\n{max_consecutive}\n")

def find_majority_soil(orchard_row, n):
    count = {}
    for fruit in orchard_row:
        if fruit in count:
            count[fruit] += 1
        else:
            count[fruit] = 1
        if count[fruit] > n // 2:
            return True
    return False

def find_max_consecutive_soil(orchard_row):
    max_consecutive = current = 1
    for i in range(1, len(orchard_row)):
        if orchard_row[i] == orchard_row[i-1]:
            current += 1
            max_consecutive = max(max_consecutive, current)
        else:
            current = 1
    return max_consecutive

def process_livada(m, n, orchard):
    rows_with_majority = 0
    max_consecutive = 0
    for row in orchard:
        if find_majority_soil(row, n):
            rows_with_majority += 1
        max_consecutive = max(max_consecutive, find_max_consecutive_soil(row))
    return rows_with_majority, max_consecutive

def main():
    m, n, p, orchard = read_livada_input('livada.in')
    rows_with_majority, max_consecutive = process_livada(m, n, orchard)
    write_livada_output('livada.out', rows_with_majority, max_consecutive)

if __name__ == "__main__":
    main()
```

