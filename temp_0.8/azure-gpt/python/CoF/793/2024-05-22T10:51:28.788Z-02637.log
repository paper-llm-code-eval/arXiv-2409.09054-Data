def count_majority_and_max_consecutive(row, n):
    threshold = n // 2 + 1
    counts = {}
    max_consecutive = 1
    current_consecutive = 1
    previous_fruit = row[0]
    counts[previous_fruit] = 1

    for i in range(1, n):
        fruit = row[i]
        if fruit == previous_fruit:
            current_consecutive += 1
        else:
            current_consecutive = 1
            previous_fruit = fruit

        max_consecutive = max(max_consecutive, current_consecutive)
        counts[fruit] = counts.get(fruit, 0) + 1

    has_majority = any(count >= threshold for count in counts.values())
    return has_majority, max_consecutive


def process_orchard(file_in, file_out):
    with open(file_in, 'r') as f:
        m, n, _ = map(int, f.readline().split())
        rows_with_majority = 0
        overall_max_consecutive = 0

        for _ in range(m):
            row = list(map(int, f.readline().split()))
            majority, max_consecutive = count_majority_and_max_consecutive(row, n)
            rows_with_majority += majority
            overall_max_consecutive = max(overall_max_consecutive, max_consecutive)

    with open(file_out, 'w') as f:
        f.write(f'{rows_with_majority}\n{overall_max_consecutive}\n')

