from collections import Counter

def read_input(file_name):
    with open(file_name, 'r') as file:
        c, n = map(int, file.readline().split())
        chocolates = list(map(int, file.readline().split()))
    return c, chocolates

def write_output(file_name, result):
    with open(file_name, 'w') as file:
        file.write(str(result) + '\n')

def solve_cerinta_1(chocolates):
    counter = Counter(chocolates)
    most_common = counter.most_common()
    max_occurrences = most_common[0][1]
    min_gramaj = min(gramaj for gramaj, count in most_common if count == max_occurrences)
    return min_gramaj

def solve_cerinta_2(chocolates):
    total_sum = sum(chocolates)
    irina = 0
    min_diff = total_sum
    for x in chocolates:
        irina += x
        mihaela = total_sum - irina
        diff = irina - mihaela
        if diff < min_diff and diff >= 0:
            min_diff = diff
    return min_diff

def main():
    c, chocolates = read_input('ciocolata.in')
    result = solve_cerinta_1(chocolates) if c == 1 else solve_cerinta_2(chocolates)
    write_output('ciocolata.out', result)

if __name__ == '__main__':
    main()
```

