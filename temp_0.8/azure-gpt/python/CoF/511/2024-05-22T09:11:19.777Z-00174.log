def solve_cerinta_1(chocolates):
    counter = Counter(chocolates)
    most_common = counter.most_common()
    max_occurrences = most_common[0][1]
    min_gramaj = min(gramaj for gramaj, count in most_common if count == max_occurrences)
    return min_gramaj

def solve_cerinta_2(chocolates):
    total_sum = sum(chocolates)
    irina = 0
    mihaela = 0
    min_diff = float('inf')

