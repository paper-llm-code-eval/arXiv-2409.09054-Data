def filter_odd_digits(n):
    return int(''.join(filter(lambda x: int(x) % 2 != 0, str(n))) or '0')

def filter_even_digits(n):
    return int(''.join(filter(lambda x: int(x) % 2 == 0, str(n))) or '0')

def is_quartz(x):
    k = 1
    total = 0
    while total <= x:
        total += k
        k += 4
    return total - k + 4 == x

def quartz_count(lst):
    quartz_found = 0
    for num in lst:
        if is_quartz(num):
            quartz_found += 1
    return quartz_found

def read_and_solve():
    with open('cuart.in', 'r') as fin:
        P = int(fin.readline().strip())
        N = int(fin.readline().strip())
        cards = list(map(int, fin.readline().split()))

    gina_cards = cards[:N]
    mihai_cards = cards[N:]

    ignored_numbers = []
    gina_filtered = []
    mihai_filtered = []

    for num in gina_cards:
        filtered_num = filter_odd_digits(num)
        if filtered_num != 0:
            gina_filtered.append(filtered_num)
        else:
            ignored_numbers.append(num)

    for num in mihai_cards:
        filtered_num = filter_even_digits(num)
        if filtered_num != 0:
            mihai_filtered.append(filtered_num)
        else:
            ignored_numbers.append(num)

    if P == 1:
        return max(ignored_numbers) if ignored_numbers else 0
    elif P == 2:
        gina_quartz = quartz_count(gina_filtered)
        mihai_quartz = quartz_count(mihai_filtered)
        if gina_quartz > mihai_quartz:
            return 1
        elif mihai_quartz > gina_quartz:
            return 2
        else:
            if gina_filtered and mihai_filtered:
                return 1 if gina_filtered[0] > mihai_filtered[0] else 2
            else:
                return 0
    elif P == 3:
        gina_quartz = quartz_count(gina_filtered)
        mihai_quartz = quartz_count(mihai_filtered)
        if gina_quartz != mihai_quartz:
            return max(gina_quartz, mihai_quartz)
        else:
            if gina_filtered and mihai_filtered:
                return max(gina_filtered[0], mihai_filtered[0])
            else:
                return 0

result = read_and_solve()

with open('cuart.out', 'w') as f:
    f.write(str(result) + '\n')
