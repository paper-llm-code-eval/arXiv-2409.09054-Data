from collections import deque

# Direction vectors for neighbors (up, down, left, right)
directions = [(-1, 0), (1, 0), (0, -1), (0, 1)]

def read_map(file_name):
    with open(file_name, 'r') as f:
        n, m = map(int, f.readline().split())
        grid = [list(map(int, list(f.readline().strip()))) for _ in range(n)]
    return n, m, grid

def write_result(file_name, result):
    with open(file_name, 'w') as f:
        f.write(' '.join(map(str, result)))

def bfs_island_count(grid, n, m):
    def in_bounds(y, x):
        return 0 <= y < n and 0 <= x < m

    def bfs(y, x, visited, value):
        queue = deque([(y, x)])
        visited.add((y, x))
        while queue:
            cy, cx = queue.popleft()
            for dy, dx in directions:
                ny, nx = cy + dy, cx + dx
                if in_bounds(ny, nx) and (ny, nx) not in visited and grid[ny][nx] == value:
                    visited.add((ny, nx))
                    queue.append((ny, nx))

    island_count = [0, 0, 0, 0]
    visited = set()
    for y in range(n):
        for x in range(m):
            if (y, x) not in visited and grid[y][x] > 0:
                island_count[grid[y][x]] += 1
                bfs(y, x, visited, grid[y][x])
    return island_count

def bfs_bridge_length(grid, n, m, r_islands, g_islands):
    visited = set()
    queue = deque()
    for y, x in r_islands:
        for dy, dx in directions:
            ny, nx = y + dy, x + dx
            if 0 <= ny < n and 0 <= nx < m and grid[ny][nx] == 0:
                queue.append((ny, nx, 1))  # (y, x, distance)
                visited.add((ny, nx))

    while queue:
        cy, cx, dist = queue.popleft()
        for dy, dx in directions:
            ny, nx = cy + dy, cx + dx
            if 0 <= ny < n and 0 <= nx < m and (ny, nx) not in visited:
                if grid[ny][nx] == 0:
                    visited.add((ny, nx))
                    queue.append((ny, nx, dist + 1))
                elif (ny, nx) in g_islands:
                    return dist
    return -1

def find_rg_islands(grid, n, m):
    r_islands = set()
    g_islands = set()
    for y in range(n):
        for x in range(m):
            if grid[y][x] == 1:
                r_islands.add((y, x))
            elif grid[y][x] == 2:
                g_islands.add((y, x))
    return r_islands, g_islands

# Read the map from the file
n, m, grid = read_map('insule.in')

# Find and count the islands for each country
island_count = bfs_island_count(grid, n, m)

# Find the sets of coordinates for R and G islands
r_islands, g_islands = find_rg_islands(grid, n, m)

# Find the minimum bridge length
bridge_length = bfs_bridge_length(grid, n, m, r_islands, g_islands)

# Write the result to the file
write_result('insule.out', [island_count[1], island_count[2], island_count[3], bridge_length])
