MOD = 1234567

def calculate_good_deeds(n, k, zv, zt):
    max_young = max_old = (k + 1) // 2
    good_deeds_done = 0
    young = old = 1  # Trevor is considered young
    t = 1

    # Calculate good deeds per day until n
    while t <= n and (young > 0 or old > 0):
        if t % zt == 0 and young > 0:
            good_deeds_done += young * k  # Each young does k good deeds
            if young < max_young * t:
                good_deeds_done -= (max_young * t - young) * k // zt
            young += young
        if t % zv == 0 and old > 0:
            good_deeds_done += old * k  # Each old does k good deeds
            if old < max_old * t:
                good_deeds_done -= (max_old * t - old) * k // zv
            old += old

        t += 1
        young = min(young, max_young * t)
        old = min(old, max_old * t)

    # Calculate the remaining good deeds to be done after n days
    remaining_good_deeds = (young + old) * k - good_deeds_done
    return remaining_good_deeds % MOD

def main():
    with open('pif.in', 'r') as fin:
        n = int(fin.readline().strip())
        k = int(fin.readline().strip())
        zv, zt = map(int, fin.readline().split())

    result = calculate_good_deeds(n, k, zv, zt)

    with open('pif.out', 'w') as fout:
        fout.write(str(result))

if __name__ == '__main__':
    main()
