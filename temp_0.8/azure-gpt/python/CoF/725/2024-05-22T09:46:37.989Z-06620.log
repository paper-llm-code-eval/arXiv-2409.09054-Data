# Read numbers from file
with open('cifre.in', 'r') as file:
    a, b = map(int, file.readline().strip().split())

# Find distinct common digits
common_digits = sorted(set(str(a)) & set(str(b)))

# Write common digits to file or -1 if there are none
with open('cifre.out', 'w') as file:
    if common_digits:
        file.write(' '.join(common_digits) + '\n')
    else:
        file.write('-1\n')

    # Construct the largest number from all digits of a and b
    all_digits = sorted(str(a) + str(b), reverse=True)
    largest_number = ''.join(all_digits)
