# Calculate the minimum time for robots to protect the target (task 2)
def bfs(obstacles_set, source, target):
    from collections import deque
    queue = deque([(source, 0)])
    visited = {source}
    while queue:
        (x, y), time = queue.popleft()
        if (x == target[0] and y != target[1]) or (y == target[1] and x != target[0]):
            return time
        for dx, dy in [(-1, 0), (1, 0), (0, -1), (0, 1)]:
