                queue.append((x+dx, y+dy, dist+1))
    return float('inf')

def is_collinear(p1, p2, p3):
    return (p1[0] - p2[0]) * (p1[1] - p3[1]) == (p1[1] - p2[1]) * (p1[0] - p3[0]) and \
            min(p2[0], p3[0]) <= p1[0] <= max(p2[0], p3[0]) and \
            min(p2[1], p3[1]) <= p1[1] <= max(p2[1], p3[1])

def min_time_to_protect(T, S1, S2, R1, R2, obstacles):
    obstacles_set = set(obstacles)
    obstacles_set.add(T)  # Add the target as an obstacle to avoid robots moving into it
    time1 = bfs(R1, T, obstacles_set, S1)
    time2 = bfs(R2, T, obstacles_set, S2)
    return max(time1, time2)

