# Read input data from ai.in
with open('ai.in', 'r') as file:
    n = int(file.readline().strip())
    T_1, T_2, S_1, S_2, S_3, S_4, R_1, R_2, R_3, R_4 = map(int, file.readline().strip().split())
    k = int(file.readline().strip())
    obstacles = [tuple(map(int, file.readline().strip().split())) for _ in range(k)]

# Calculate the maximum length of a wall (task 1)
def longest_wall(obstacles):
    rows, cols = {}, {}
    for x, y in obstacles:
        rows.setdefault(x, []).append(y)
        cols.setdefault(y, []).append(x)

    # Sort the positions in each wall
    for row in rows:
        rows[row].sort()
    for col in cols:
        cols[col].sort()

    max_length = 0
    # Check each row and column for the longest consecutive sequence
    for row in rows:
        max_length = max(max_length, max_consecutive(rows[row]))
    for col in cols:
        max_length = max(max_length, max_consecutive(cols[col]))

    return max_length

def max_consecutive(seq):
    max_len, current_len = 0, 1
    for i in range(1, len(seq)):
        if seq[i] == seq[i-1] + 1:
            current_len += 1
        else:
            max_len = max(max_len, current_len)
            current_len = 1
    return max(max_len, current_len)

# Calculate the minimum time for robots to protect the target (task 2)
from collections import deque

def bfs(start, target, obstacles, other_source):
    queue = deque([(start[0], start[1], 0)])
    visited = set()
    while queue:
        x, y, dist = queue.popleft()
