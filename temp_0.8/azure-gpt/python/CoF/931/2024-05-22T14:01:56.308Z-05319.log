# Read data from file
with open('sir.in', 'r') as file:
    C = int(file.readline().strip())
    N = int(file.readline().strip())
    numbers = list(map(int, file.readline().strip().split()))

def write_output(data):
    with open('sir.out', 'w') as file:
        file.write(data)

# Function to answer question type 1
def question_type_1():
    for i in range(N - 2, -1, -1):
        if numbers[i] > numbers[i + 1]:
            return str(i + 1)
    return str(N)

# Function to answer question type 2
def question_type_2():
    max_left = 0
    result = []
    for i in range(N):
        if numbers[i] > max_left:
            max_left = numbers[i]
            result.append(str(i + 1))
    return ' '.join(result)

# Function to answer question type 3
def question_type_3():
    max_value = max(numbers)
    first_idx = numbers.index(max_value)
    last_idx = N - numbers[::-1].index(max_value) - 1
    total = sum(max_value - numbers[i] for i in range(first_idx, last_idx + 1))
    return str(total)

# Logic to call the correct function based on the question type
if C == 1:
    result = question_type_1()
elif C == 2:
    result = question_type_2()
elif C == 3:
    result = question_type_3()

write_output(result)
```

