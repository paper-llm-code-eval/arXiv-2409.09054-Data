# Laser shooting function
def shoot(matrix, n, m, laser):
    i, j, d = laser
    if d == 1:  # North
        for row in range(i-2, -1, -1):
            matrix[row][j-1] -= 1
    elif d == 2:  # East
        for col in range(j, n):
            matrix[i-1][col] -= 1
    elif d == 3:  # South
        for row in range(i, n):
            matrix[row][j-1] -= 1
    elif d == 4:  # West
        for col in range(j-2, -1, -1):
            matrix[i-1][col] -= 1

# Groove check function
def is_groove(matrix, n, i, j):
    for dx in range(-1, 2):
        for dy in range(-1, 2):
            if dx == 0 and dy == 0:
                continue
            if i + dx < 0 or i + dx >= n or j + dy < 0 or j + dy >= n:
                continue
            if matrix[i][j] > matrix[i + dx][j + dy]:
                return False
    return True

# Count grooves
def count_grooves(matrix, n):
    count = 0
    for i in range(n):
        for j in range(n):
            if is_groove(matrix, n, i, j):
                count += 1
    return count

# Count trenches
def count_trenches(matrix, n):
    count = 0
    for i in range(n):
        groove_sequence = 0
        for j in range(n):
            if is_groove(matrix, n, i, j):
                groove_sequence += 1
                if j == n - 1 and groove_sequence > 1:
                    count += 1
            else:
                if groove_sequence > 1:
                    count += 1
                groove_sequence = 0
    return count

# Read input
with open('lasere.in', 'r') as fin:
    c = int(fin.readline())
    n, m = map(int, fin.readline().split())
    matrix = [list(map(int, fin.readline().split())) for _ in range(n)]
    lasers = [list(map(int, fin.readline().split())) for _ in range(m)]

# Shoot all lasers
for laser in lasers:
    shoot(matrix, n, m, laser)

# Output results
result = count_grooves(matrix, n) if c == 1 else count_trenches(matrix, n)
with open('lasere.out', 'w') as fout:
    fout.write(str(result) + '\n')
