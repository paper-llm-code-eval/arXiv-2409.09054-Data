from itertools import combinations

# Function to check if the team covers all disciplines
def is_valid_team(team, student_disciplines, D):
    disciplines = [False] * D
    for student in team:
        for discipline in student_disciplines[student]:
            disciplines[discipline - 1] = True
    return all(disciplines)

# Function to generate all valid teams
def generate_teams(n, k, D, student_disciplines):
    all_teams = combinations(range(n), k)
    valid_teams = []
    for team in all_teams:
        if is_valid_team(team, student_disciplines, D):
            valid_teams.append(team)
    return valid_teams

# Read input data from file
def read_input_file(filename):
    with open(filename, 'r') as file:
        n, k, D = map(int, file.readline().split())
        student_disciplines = [list(map(int, line.split()[1:])) for line in file]
    return n, k, D, student_disciplines

# Write output data to file
def write_output_file(filename, teams):
    with open(filename, 'w') as file:
        for team in teams:
            file.write(' '.join(str(student + 1) for student in team) + '\n')

def main():
    n, k, D, student_disciplines = read_input_file('pluricex.in')
    valid_teams = generate_teams(n, k, D, student_disciplines)
    write_output_file('pluricex.out', valid_teams)

if __name__ == '__main__':
    main()
