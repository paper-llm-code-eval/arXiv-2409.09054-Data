from itertools import combinations

def check_team_validity(team, discipline_matrix, D):
    disciplines_covered = [False] * D
    for member in team:
        for discipline in discipline_matrix[member]:
            disciplines_covered[discipline - 1] = True
    return all(disciplines_covered)

def generate_teams(n, k, D, discipline_matrix):
    valid_teams = []
    for team in combinations(range(n), k):
        if check_team_validity(team, discipline_matrix, D):
            valid_teams.append(team)
    return valid_teams

def read_input(filename):
    with open(filename, 'r') as file:
        n, k, D = map(int, file.readline().split())
        discipline_matrix = [[] for _ in range(n)]
        for i in range(n):
            line = list(map(int, file.readline().split()))
            discipline_matrix[i] = line[1:]
    return n, k, D, discipline_matrix

def write_output(filename, teams):
    with open(filename, 'w') as file:
        for team in teams:
            file.write(' '.join(str(member + 1) for member in team) + '\n')

def main():
    n, k, D, discipline_matrix = read_input('pluricex.in')
    valid_teams = generate_teams(n, k, D, discipline_matrix)
    write_output('pluricex.out', valid_teams)

if __name__ == '__main__':
    main()
