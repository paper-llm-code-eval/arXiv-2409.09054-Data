from itertools import combinations

# Read the list of students and their disciplines from file
def read_students(filename):
    with open(filename, 'r') as file:
        n, k, D = map(int, file.readline().split())
        student_disciplines = [0] * n
        for i in range(n):
            data = list(map(int, file.readline().split()))
            # Create a bitmask for each student's disciplines
            for discipline in data[1:]:
                student_disciplines[i] |= 1 << (discipline - 1)
    return n, k, D, student_disciplines

# Check if the team covers all disciplines
def check_team(team, D, student_disciplines):
    disciplines_covered = 0
    for student in team:
        disciplines_covered |= student_disciplines[student]
    # Check if all bits are set in disciplines_covered
    return disciplines_covered == (1 << D) - 1

# Find all possible teams that cover all disciplines
def find_teams(n, k, D, student_disciplines):
    all_combinations = combinations(range(n), k)
    valid_teams = []
    for team in all_combinations:
        if check_team(team, D, student_disciplines):
            # Convert to 1-based indexing and store the team
            valid_teams.append([student + 1 for student in team])
    return valid_teams

# Write the list of valid teams to the output file
def write_teams(filename, teams):
    with open(filename, 'w') as file:
        for team in teams:
            file.write(' '.join(map(str, team)) + '\n')

# Main function to solve the PluriCEX challenge
def solve_pluricex():
    n, k, D, student_disciplines = read_students('pluricex.in')
    valid_teams = find_teams(n, k, D, student_disciplines)
    write_teams('pluricex.out', valid_teams)

# Solve the PluriCEX problem
solve_pluricex()
```
