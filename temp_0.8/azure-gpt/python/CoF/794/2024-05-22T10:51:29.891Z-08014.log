from math import gcd

# Read the input data from the file
with open('numar.in', 'r') as file:
    ni, nz = map(int, file.readline().split())
    parte_intreaga = int(''.join(file.readline().split()))
    parte_zecimala = int(''.join(file.readline().split()))

# If there is no fractional part, the solution is trivial
if nz == 0:
    a, b = parte_intreaga, 1
else:
    # Calculate the numerator and denominator
    numerator = parte_intreaga * (10**nz) + parte_zecimala
    denominator = 10**nz

    # Simplify the fraction
    common_divisor = gcd(numerator, denominator)
    a = numerator // common_divisor
    b = denominator // common_divisor

# Write the output data to the file
with open('numar.out', 'w') as file:
    file.write(f"{len(str(a))}\n{a}\n{len(str(b))}\n{b}\n")
