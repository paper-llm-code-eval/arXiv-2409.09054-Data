def is_palindrome(num_str):
    return num_str == num_str[::-1]

def min_digits_to_palindrome(num_str):
    if is_palindrome(num_str):
        return 0
    for i in range(len(num_str)):
        if is_palindrome(num_str[i:]):
            return i
    return len(num_str) - 1

def count_min_digits(numbers):
    return sum(min_digits_to_palindrome(str(num)) for num in numbers)

def max_consecutive_palindromes(numbers, max_digits):
    count, max_count, total_digits_added = 0, 0, 0
    for num_str in numbers:
        digits_to_add = min_digits_to_palindrome(num_str)
        if total_digits_added + digits_to_add <= max_digits:
            count += 1
            total_digits_added += digits_to_add
        else:
            max_count = max(max_count, count)
            count, total_digits_added = 0, 0
    return max(max_count, count)

# Read data from input file
with open('palindrom.in', 'r') as file:
    lines = file.readlines()
    task = int(lines[0].strip())
    n = int(lines[1].strip())
    numbers = [line.strip() for line in lines[2:2+n]]
    S = int(lines[-1].strip()) if task == 2 else None

result = 0
if task == 1:
    result = count_min_digits(numbers)
elif task == 2:
    result = max_consecutive_palindromes(numbers, S)

# Write data to output file
with open('palindrom.out', 'w') as file:
    file.write(str(result))
