# Read from dreptc.in
with open('dreptc.in', 'r') as fin:
    N, MaxC = map(int, fin.readline().split())
    points_by_color = [{} for _ in range(MaxC + 1)]
    for _ in range(N):
        x, y, c = map(int, fin.readline().split())
        if x not in points_by_color[c]:
            points_by_color[c][x] = []
        points_by_color[c][x].append(y)

# Calculate the maximum number of correct rectangles
max_rectangles = 0
for color_points in points_by_color:
    for x1 in color_points:
        ys1 = set(color_points[x1])
        for x2 in color_points:
            if x1 < x2:
                ys2 = color_points[x2]
                common_ys = ys1.intersection(ys2)
                max_rectangles += len(common_ys) * (len(common_ys) - 1) // 2

# Write to dreptc.out
with open('dreptc.out', 'w') as fout:
    fout.write(str(max_rectangles) + '\n')
```
