from collections import deque

def read_input(file_name):
    with open(file_name, 'r') as file:
        p = int(file.readline().strip())
        n, m, T = map(int, file.readline().strip().split())
        L, C, k, S = map(int, file.readline().strip().split())
        food_coords = [tuple(map(int, file.readline().strip().split())) for _ in range(T)]
        grid_codes = [list(map(int, file.readline().strip().split())) for _ in range(n)]
    return p, n, m, T, L, C, k, S, food_coords, grid_codes

def write_output(file_name, result):
    with open(file_name, 'w') as file:
        if isinstance(result, tuple):
            file.write(f"{result[0]} {result[1]}\n")
        else:
            file.write(f"{result}\n")

def is_accessible(k, code, S):
    mask = (1 << S) - 1
    return ((k ^ code) & mask) == mask

def bfs(grid, start, n, m, k, S, food_coords):
    queue = deque([start])
    distances = [[-1] * m for _ in range(n)]
    distances[start[0]][start[1]] = 0
    food_distances = []
    while queue:
        x, y = queue.popleft()
        if (x + 1, y + 1) in food_coords:
            food_distances.append(distances[x][y])
        for dx, dy in [(-1, 0), (1, 0), (0, -1), (0, 1)]:
            nx, ny = x + dx, y + dy
            if 0 <= nx < n and 0 <= ny < m and distances[nx][ny] == -1 and is_accessible(k, grid[nx][ny], S):
                distances[nx][ny] = distances[x][y] + 1
                queue.append((nx, ny))
    return food_distances

def solve_problem_1(grid, n, m, k, S):
    count = 0
    for i in range(n):
        for j in range(m):
            if is_accessible(k, grid[i][j], S):
                count += 1
    return count - 1  # Exclude the starting cell

def solve_problem_2(food_distances):
    Smin = min(food_distances)
    nt = food_distances.count(Smin)
    return Smin, nt

def solve_panda_challenge():
    p, n, m, T, L, C, k, S, food_coords, grid_codes = read_input('panda.in')
    if p == 1:
        result = solve_problem_1(grid_codes, n, m, k, S)
        write_output('panda.out', result)
    else:
        food_distances = bfs(grid_codes, (L - 1, C - 1), n, m, k, S, food_coords)
        result = solve_problem_2(food_distances)
        write_output('panda.out', result)

solve_panda_challenge()
