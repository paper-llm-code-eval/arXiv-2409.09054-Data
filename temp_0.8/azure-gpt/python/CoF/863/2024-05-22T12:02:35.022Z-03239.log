from collections import deque

def read_input(file_name):
    with open(file_name, 'r') as file:
        p = int(file.readline().strip())
        n, m, T = map(int, file.readline().strip().split())
        L, C, k, S = map(int, file.readline().strip().split())
        food_coords = [tuple(map(int, file.readline().strip().split())) for _ in range(T)]
        grid_codes = [list(map(int, file.readline().strip().split())) for _ in range(n)]
    return p, n, m, T, L, C, k, S, food_coords, grid_codes

def write_output(file_name, result):
    with open(file_name, 'w') as file:
        file.write(' '.join(map(str, result)))

def is_accessible(k, code, S):
    bin_k = bin(k)[-S:].zfill(S)
    bin_code = bin(code)[-S:].zfill(S)
    return all(c1 != c2 for c1, c2 in zip(bin_k, bin_code))

def bfs(grid, start, n, m, k, S, food_coords):
    queue = deque([start])
    distances = [[float('inf')] * m for _ in range(n)]
    distances[start[0]][start[1]] = 0
    food_distances = []
    while queue:
        x, y = queue.popleft()
        for dx, dy in [(-1, 0), (1, 0), (0, -1), (0, 1)]:
            nx, ny = x + dx, y + dy
            if 0 <= nx < n and 0 <= ny < m and is_accessible(k, grid[nx][ny], S):
                if distances[nx][ny] == float('inf'):
                    distances[nx][ny] = distances[x][y] + 1
                    queue.append((nx, ny))
                    if (nx + 1, ny + 1) in food_coords:
                        food_distances.append(distances[nx][ny])
    return distances, food_distances

def solve_problem_1(grid, n, m, k, S, start):
    count = 0
    for i in range(n):
        for j in range(m):
            if is_accessible(k, grid[i][j], S) and (i + 1, j + 1) != start:
                count += 1
    return count

def solve_problem_2(distances, food_distances):
    Smin = min(food_distances)
    nt = food_distances.count(Smin)
    return Smin, nt

def solve_panda_challenge():
    p, n, m, T, L, C, k, S, food_coords, grid_codes = read_input('panda.in')
    if p == 1:
        result = solve_problem_1(grid_codes, n, m, k, S, (L, C))
    else:
        distances, food_distances = bfs(grid_codes, (L - 1, C - 1), n, m, k, S, food_coords)
        result = solve_problem_2(distances, food_distances)
    write_output('panda.out', [result])

solve_panda_challenge()
