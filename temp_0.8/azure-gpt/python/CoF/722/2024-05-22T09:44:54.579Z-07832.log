def read_input(file_name):
    with open(file_name, 'r') as file:
        S, n = map(int, file.readline().split())
        grid = [list(map(int, line.split())) for line in file]
    return S, n, grid

def write_output(file_name, data):
    with open(file_name, 'w') as file:
        file.write(str(data))

def find_max_remaining_money(S, n, grid):
    # Initialize a dynamic programming table with -1 (unreachable)
    dp = [[-1 for _ in range(n)] for _ in range(n)]
    # Starting point
    dp[0][0] = S - grid[0][0]

    # Populate the DP table
    for i in range(n):
        for j in range(n):
            # Skip the starting point
            if i == 0 and j == 0:
                continue
            # Look up and left for possible paths, update the current cell if a path is found
            if i > 0 and dp[i-1][j] >= grid[i][j]:
                dp[i][j] = max(dp[i][j], dp[i-1][j] - grid[i][j])
            if j > 0 and dp[i][j-1] >= grid[i][j]:
                dp[i][j] = max(dp[i][j], dp[i][j-1] - grid[i][j])

    # If the bottom-right corner is reachable, return the remaining money, otherwise return -1
    return dp[n-1][n-1] if dp[n-1][n-1] >= 0 else -1

def main():
    S, n, grid = read_input('taxe.in')
    result = find_max_remaining_money(S, n, grid)
    write_output('taxe.out', result)

if __name__ == "__main__":
    main()
