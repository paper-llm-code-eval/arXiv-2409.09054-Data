def read_input(file_name):
    with open(file_name, 'r') as file:
        S, n = map(int, file.readline().split())
        grid = [list(map(int, line.split())) for line in file]
    return S, n, grid

def write_output(file_name, data):
    with open(file_name, 'w') as file:
        file.write(str(data))

def find_max_remaining_money(S, n, grid):
    # Create a 2D list to store the maximum money left when reaching each cell
    dp = [[-1 for _ in range(n)] for _ in range(n)]
    dp[0][0] = S - grid[0][0]  # Start with the top-left cell
    
    # Calculate the maximum money left for each cell
    for i in range(n):
        for j in range(n):
            if i > 0 and dp[i-1][j] >= grid[i][j]:  # Check the cell above
                dp[i][j] = max(dp[i][j], dp[i-1][j] - grid[i][j])
            if j > 0 and dp[i][j-1] >= grid[i][j]:  # Check the cell to the left
                dp[i][j] = max(dp[i][j], dp[i][j-1] - grid[i][j])
    
    # The goal is to reach the bottom-right cell
    return dp[n-1][n-1] if dp[n-1][n-1] >= 0 else -1

def main():
    S, n, grid = read_input('taxe.in')
    result = find_max_remaining_money(S, n, grid)
    write_output('taxe.out', result)

