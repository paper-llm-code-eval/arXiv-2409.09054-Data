def process_intervals(intervals):
    # Sort intervals based on start day
    intervals.sort()
    
    max_working_period = 0
    max_idle_period = 0
    
    # Initialize with the first interval
    current_start, current_end = intervals[0]
    
    for start, end in intervals[1:]:
        if start <= current_end:  # Overlapping or adjacent
            # Extend the current working period
            current_end = max(current_end, end)
        else:
            # Calculate working period and update max_working_period
            working_period = current_end - current_start
            max_working_period = max(max_working_period, working_period)
            
            # Calculate idle period and update max_idle_period
            idle_period = start - current_end
            max_idle_period = max(max_idle_period, idle_period)
            
            # Update current working period
            current_start, current_end = start, end
    
    # Check the last interval for max working period
    max_working_period = max(max_working_period, current_end - current_start)
    
    return max_working_period, max_idle_period

# Read intervals from file
