    with open(file_in, 'r') as fin:
        n = int(fin.readline().strip())
        intervals = [tuple(map(int, line.split())) for line in fin.readlines()]

    intervals.sort()

    max_work, max_gap = 0, 0
    current_start, current_end = intervals[0]

    for start, end in intervals[1:]:
