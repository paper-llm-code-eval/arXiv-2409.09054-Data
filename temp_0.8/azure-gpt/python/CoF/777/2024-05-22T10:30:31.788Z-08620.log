            current_end = max(current_end, end)
        else:
            max_gap = max(max_gap, start - current_end)  # Non-overlapping, so calculate gap
            max_work = max(max_work, current_end - current_start)  # End of a work period
            current_start, current_end = start, end  # Start a new work period

    max_work = max(max_work, current_end - current_start)  # Check last interval

    with open(file_out, 'w') as fout:
        fout.write(f"{max_work} {max_gap}\n")

