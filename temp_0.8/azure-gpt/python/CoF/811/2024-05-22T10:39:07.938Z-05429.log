def count_divisors(n):
    divisors = 0
    for i in range(1, int(n ** 0.5) + 1):
        if n % i == 0:
            divisors += 2 if i * i != n else 1
    return divisors

def read_matrix(file):
    with open(file, 'r') as f:
        m, n = map(int, f.readline().split())
        matrix = [list(map(int, line.split())) for line in f]
    return matrix, m, n

def write_groups(file, group_a, group_b):
    with open(file, 'w') as f:
        f.write(f"{group_a[0]} {group_a[1]} {group_a[2]}\n")
        f.write(f"{group_b[0]} {group_b[1]} {group_b[2]}\n")

def solve(matrix, m, n):
    divisors_dict = {}
    for i in range(m):
        for j in range(n):
            divisors = count_divisors(matrix[i][j])
            if divisors in divisors_dict:
                divisors_dict[divisors].append(matrix[i][j])
            else:
                divisors_dict[divisors] = [matrix[i][j]]
    
    groups = sorted([(k, len(v), max(v)) for k, v in divisors_dict.items()], key=lambda x: (-x[1], -x[0]))
    
    group_a = groups[0]
    group_b = groups[1] if len(groups) > 1 else (0, 0, 0)

    return group_a, group_b

# Read the matrix from the input file
matrix, m, n = read_matrix('grupe.in')

# Solve the problem
group_a, group_b = solve(matrix, m, n)

# Write the groups to the output file
write_groups('grupe.out', group_a, group_b)
```
