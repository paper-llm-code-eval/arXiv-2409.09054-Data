# Solution to the flower gathering girls challenge

def read_data(file_name):
    with open(file_name, 'r') as file:
        n, k = map(int, file.readline().split())
        bouquets = [list(map(int, file.readline().split())) for _ in range(n)]
    return n, k, bouquets

def write_groups(file_name, groups):
    with open(file_name, 'w') as file:
        for group in groups:
            file.write(' '.join(map(str, group)) + '\n')

def find_groups(n, k, bouquets):
    groups = []
    used = [False] * n

    for i in range(n):
        if not used[i]:
            group = [i + 1]
            used[i] = True
            for j in range(i + 1, n):
                if not used[j] and any(flower in bouquets[i] for flower in bouquets[j]):
                    group.append(j + 1)
                    used[j] = True
            groups.append(group)

    return groups

def main():
    n, k, bouquets = read_data('flori.in')
    groups = find_groups(n, k, bouquets)
    write_groups('flori.out', groups)

if __name__ == "__main__":
    main()
```

