# Read the input from flori.in
with open("flori.in", "r") as file:
    n, k = map(int, file.readline().split())
    flower_types = [list(map(int, file.readline().split())) for _ in range(n)]

# Create a dictionary to map flower types to the girls who picked them
flower_to_girls = {}
for girl_index, flowers in enumerate(flower_types):
    for flower in flowers:
        if flower not in flower_to_girls:
            flower_to_girls[flower] = set()
        flower_to_girls[flower].add(girl_index + 1)

# Create a list to store the groups
groups = []

# Create a set to store girls that have been grouped
grouped_girls = set()

for girl_index in range(1, n + 1):
    if girl_index not in grouped_girls:
        # Start a new group with the current girl
        current_group = {girl_index}
        flower_set = set(flower_types[girl_index - 1])
        grouped_girls.add(girl_index)

        # Find all girls that can join the current group
        for flower in flower_set:
            for other_girl in flower_to_girls[flower]:
                if other_girl not in grouped_girls:
                    current_group.add(other_girl)
                    grouped_girls.add(other_girl)
                    flower_set.update(flower_types[other_girl - 1])

        # Add the group to the list of groups
        groups.append(sorted(list(current_group)))

# Write the output to flori.out
with open("flori.out", "w") as file:
    for group in sorted(groups, key=lambda x: x[0]):
        file.write(' '.join(map(str, group)) + '\n')
