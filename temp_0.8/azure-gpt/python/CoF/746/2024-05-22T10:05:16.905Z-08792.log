from itertools import permutations

# Function to calculate the effort required to climb the given staircase configuration
def calculate_effort(stairs, M, p):
    effort = 0
    i = 0
    while i < len(stairs):
        step_sum = 0
        step_count = 0
        while i < len(stairs) and step_sum + stairs[i] <= M:
            step_sum += stairs[i]
            step_count += 1
            i += 1
        effort += step_sum / step_count + p
    return effort

# Function to generate all possible sequences of steps heights that sum up to H
def generate_sequences(H, N, M):
    sequences = []
    
    def backtrack(curr_seq, curr_sum):
        if len(curr_seq) == N:
            if curr_sum == H:
                sequences.append(curr_seq[:])
            return
        
        for height in range(1, min(M, H - curr_sum) + 1):
            if height not in curr_seq:
                curr_seq.append(height)
                backtrack(curr_seq, curr_sum + height)
                curr_seq.pop()
    
    backtrack([], 0)
    return sequences

# Read input
with open('scara.in', 'r') as file:
    H, N, M, p = map(int, file.readline().strip().split())

# Generate all possible sequences of steps that sum up to H and have distinct heights
sequences = generate_sequences(H, N, M)

# Find the sequence with the minimum effort and lexicographically smallest
min_effort = float('inf')
min_sequence = None
for seq in permutations(range(1, N+1)):
    if sum(seq) == H:
        effort = calculate_effort(seq, M, p)
        if effort < min_effort:
            min_effort = effort
            min_sequence = seq

# Write output
with open('scara.out', 'w') as file:
    file.write(f'{min_effort:.2f}\n')
    file.write(' '.join(map(str, min_sequence)) + '\n')
```

