def sieve_of_eratosthenes(max_number):
    sieve = [True] * (max_number + 1)
    sieve[0] = sieve[1] = False
    primes = []
    for i in range(2, max_number + 1):
        if sieve[i]:
            primes.append(i)
            for j in range(i * i, max_number + 1, i):
                sieve[j] = False
    return primes

MAX_NUMBER = 1000000
primes = sieve_of_eratosthenes(MAX_NUMBER)

def kth_prime_divisor(card_value, k):
    prime_count = 0
    for prime in primes:
        while card_value % prime == 0:
            card_value //= prime
            prime_count += 1
            if prime_count == k:
                return prime
        if prime * prime > card_value:
            break
    if card_value > 1 and prime_count + 1 == k:
        return card_value
    return -1  # should not happen as per problem statement guarantees

def task1(card_value, k):
    return kth_prime_divisor(card_value, k)

def task2(cards):
    total = 0
    for card_value, k in cards:
        total += kth_prime_divisor(card_value, k)
    return str(total)

# Read from 'cufar.in' and decide which task to perform
with open('cufar.in', 'r') as fin:
    p, n = map(int, fin.readline().split())
    result = 0
    if p == 1:
        card_value, k = map(int, fin.readline().split())
        result = task1(card_value, k)
    else:
        cards = [tuple(map(int, fin.readline().split())) for _ in range(n)]
        result = task2(cards)

# Write to 'cufar.out'
with open('cufar.out', 'w') as fout:
    fout.write(f'{result}\n')
