from collections import Counter

# Function to find the k-th prime factor of a number
def find_kth_prime_factor(n, k):
    prime_factors = Counter()
    d = 2
    while d * d <= n:
        while n % d == 0:
            prime_factors[d] += 1
            n //= d
        d += 1
    if n > 1:
        prime_factors[n] += 1
    for prime in sorted(prime_factors.keys()):
        if prime_factors[prime] >= k:
            return prime
        k -= prime_factors[prime]
    return n

# Read from 'cufar.in'
with open('cufar.in', 'r') as fin:
    p, n = map(int, fin.readline().split())
    magic_values = []

    if p == 1:
        number, k = map(int, fin.readline().split())
        magic_value = find_kth_prime_factor(number, k)
        magic_values.append(magic_value)

    elif p == 2:
        total_magic_value = 0
        for _ in range(n):
            number, k = map(int, fin.readline().split())
            magic_value = find_kth_prime_factor(number, k)
            total_magic_value += magic_value
        magic_values.append(total_magic_value)

# Write to 'cufar.out'
with open('cufar.out', 'w') as fout:
    if p == 1:
        fout.write(f'{magic_values[0]}\n')
    elif p == 2:
        # Convert total magic value to string and write digits
        fout.write(f'{"".join(map(str, map(int, str(magic_values[0]))))}\n')
