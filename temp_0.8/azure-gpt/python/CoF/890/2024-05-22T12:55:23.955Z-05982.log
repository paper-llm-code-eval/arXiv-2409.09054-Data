# Function to get the k-th prime divisor of a number
def kth_prime_divisor(num, k):
    prime_count = 0
    divisor = 2
    while num >= divisor:
        if num % divisor == 0:
            prime_count += 1
            if prime_count == k:
                return divisor
            num //= divisor
        else:
            divisor += 1
            if divisor * divisor > num:
                if prime_count == k - 1:
                    return num
                divisor = num
    return -1

