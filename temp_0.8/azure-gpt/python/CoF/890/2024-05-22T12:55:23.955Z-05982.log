def kth_prime_divisor(num, k):
    prime_count = 0
    divisor = 2
    while num >= divisor:
        if num % divisor == 0:
            prime_count += 1
            if prime_count == k:
                return divisor
            while num % divisor == 0:
                num //= divisor
        else:
            divisor += 1
            if divisor * divisor > num:
                if prime_count == k - 1:
                    return num
                divisor = num
    return -1

def solve_problem_one(number, k):
    magical_value = kth_prime_divisor(number, k)
    with open('cufar.out', 'w') as output:
        output.write(str(magical_value) + '\n')

def solve_problem_two(pairs):
    sum_magical_values = 0
    for number, k in pairs:
        sum_magical_values += kth_prime_divisor(number, k)
    with open('cufar.out', 'w') as output:
        output.write(str(sum_magical_values) + '\n')

with open('cufar.in', 'r') as file:
    p, n = map(int, file.readline().split())
    if p == 1:
        number, k = map(int, file.readline().split())
        solve_problem_one(number, k)
    elif p == 2:
        pairs = [tuple(map(int, file.readline().split())) for _ in range(n)]
        solve_problem_two(pairs)
