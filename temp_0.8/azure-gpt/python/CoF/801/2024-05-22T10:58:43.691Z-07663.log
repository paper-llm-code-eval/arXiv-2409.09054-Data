# cladiri.py
def read_data(file_name):
    with open(file_name, 'r') as file:
        lines = file.readlines()
    return lines

def write_result(file_name, result):
    with open(file_name, 'w') as file:
        for item in result:
            file.write(f"{item}\n")

def compute_distance_and_effects(lines):
    epicenter = [int(x) for x in lines.pop(0).split()]
    epicenter_x, epicenter_y, intensity = epicenter
    affected_buildings = 0
    level_counts = {}
    max_buildings_per_level = 0
    levels_with_max_buildings = []
    
    for line in lines:
        building = [int(x) for x in line.split()]
        building_x, building_y, resistance = building
        level = max(abs(building_x - epicenter_x), abs(building_y - epicenter_y))
        if resistance <= intensity - level:
            affected_buildings += 1
            level_counts[level] = level_counts.get(level, 0) + 1
            if level_counts[level] > max_buildings_per_level:
                max_buildings_per_level = level_counts[level]
                levels_with_max_buildings = [level]
            elif level_counts[level] == max_buildings_per_level:
                levels_with_max_buildings.append(level)
                
    return affected_buildings, max_buildings_per_level, sorted(levels_with_max_buildings)

def main():
    lines = read_data('cladiri.in')
    result = compute_distance_and_effects(lines)
    write_result('cladiri.out', result)

if __name__ == "__main__":
    main()
```

