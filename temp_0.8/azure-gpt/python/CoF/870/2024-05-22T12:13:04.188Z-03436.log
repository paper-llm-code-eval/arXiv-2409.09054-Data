from itertools import permutations

# Function to find the next smaller number than X
def next_smaller(num_str):
    # Convert the string to a list of integers
    num = list(map(int, num_str))
    n = len(num)
    
    # Find the first digit that is greater than the next digit when traversing from right to left
    for i in range(n-2, -1, -1):
        if num[i] > num[i+1]:
            break
    else:  # If no such digit is found, return the original number
        return ''.join(map(str, num))

    # Find the largest digit to the right of 'i' that is smaller than num[i]
    x = num[i]
    smallest = i + 1
    for j in range(i+1, n):
        if 0 < num[j] < x and num[j] > num[smallest]:
            smallest = j

    # Swap the found digit with num[i]
    num[smallest], num[i] = num[i], num[smallest]

    # Sort the digits to the right of 'i' in descending order
    num[i+1:] = sorted(num[i+1:], reverse=True)

    # Convert the list back to a number and return as a string
    return ''.join(map(str, num))

# Function to count the number of ways to form A from X
def count_ways(A, num_str):
    A_str = str(A)
    positions = [i for i in range(len(num_str)) if num_str[i] in A_str]
    combos = set(permutations(positions, len(A_str)))
    ways = 0
    for combo in combos:
        if ''.join([num_str[i] for i in combo]) == A_str and sorted(combo) == list(combo):
            ways += 1
    return ways

# Read from the input file
with open('axyz.in', 'r') as file:
    p = int(file.readline().strip())
    A = int(file.readline().strip())
    N = int(file.readline().strip())
    X = file.readline().strip().replace(' ', '')

# Write to the output file
with open('axyz.out', 'w') as file:
    if p == 1:
        # Compute Y
        Y = next_smaller(X)
        file.write(Y + '\n')
    elif p == 2:
        # Compute Z
        Z = count_ways(A, X)
        file.write(str(Z) + '\n')
