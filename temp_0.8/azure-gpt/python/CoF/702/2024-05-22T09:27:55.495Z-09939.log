def main():
    with open('lascoala.in', 'r') as file:
        n = int(file.readline().strip())

    # Find the largest square number less than n
    k = int(n**0.5)
    if k**2 == n:
        k -= 1
    k = k**2

    # Calculate the number of awarded students
    awarded = n - k

    # Calculate the side length of the square formed by non-awarded students
    p = int(k**0.5)

    # Generate the layout of non-awarded students
    layout = [[str(j * p + i) for i in range(p, 0, -1)] for j in range(p-1, -1, -1)]
    
    with open('lascoala.out', 'w') as file:
        # Write the number of awarded students
        file.write(f"{awarded}\n")
        
        # Write the layout of non-awarded students
        for row in layout:
            file.write(" ".join(row) + "\n")

if __name__ == "__main__":
    main()
