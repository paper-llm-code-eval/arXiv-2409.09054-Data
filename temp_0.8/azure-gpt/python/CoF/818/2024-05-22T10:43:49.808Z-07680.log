# Read data from 'roata.in'
with open('roata.in', 'r') as fin:
    n = int(fin.readline().strip())
    p = int(fin.readline().strip())
    c = list(map(int, fin.readline().strip().split()))

# Initialize variables
total_sum = sum(c)
order_of_exit = []
last_cabin_number = 0

# Simulate the rotation of the Ferris wheel
queue = c[:n]  # Clients currently on the wheel
clients_in_cabins = list(range(1, min(n+1, p+1)))  # Client numbers in respective cabins
next_client = n  # Index of the next client to board
total_rotations = 0  # Total number of rotations made by the wheel

while queue:
    total_rotations += 1  # Rotate the wheel
    for i in range(len(queue)):
        queue[i] -= 1  # One rotation for each client on the wheel
        if queue[i] == 0:  # Client i needs to exit
            order_of_exit.append(clients_in_cabins[i])
            last_cabin_number = (total_rotations % n) if (total_rotations % n) != 0 else n
            if next_client < p:  # There are still clients waiting
                queue[i] = c[next_client]
                clients_in_cabins[i] = next_client + 1
                next_client += 1
            else:  # No more clients waiting
                queue[i] = -1  # Mark as empty
    queue = [client for client in queue if client > 0]  # Remove empty cabins

# Write data to 'roata.out'
with open('roata.out', 'w') as fout:
    fout.write(str(total_sum) + '\n')
    fout.write(' '.join(map(str, order_of_exit)) + '\n')
    fout.write(str(last_cabin_number) + '\n')
```
