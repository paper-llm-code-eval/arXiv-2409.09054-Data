    elif C == 2:
        result = max(split_max_prime(num, 2) for num in numbers)
    elif C == 3:
        result = max(split_max_prime(num, 3) for num in numbers)

    with open(output_path, 'w') as file:
        file.write(str(result) + '\n')

process_file('tai.in', 'tai.out')
