from math import isqrt

def is_prime(n):
    if n <= 1:
        return False
    if n <= 3:
        return True
    if n % 2 == 0 or n % 3 == 0:
        return False
    i = 5
    while i * i <= n:
        if n % i == 0 or n % (i + 2) == 0:
            return False
        i += 6
    return True

def split_max_prime(num, parts):
    max_prime = 0
    str_num = str(num)
    for i in range(1, len(str_num)):
        if parts == 2:
            left, right = str_num[:i], str_num[i:]
            left, right = int(left), int(right)
            if is_prime(left):
                max_prime = max(max_prime, left)
            if is_prime(right):
                max_prime = max(max_prime, right)
        elif parts == 3:
            for j in range(i+1, len(str_num)):
                left, middle, right = str_num[:i], str_num[i:j], str_num[j:]
                left, middle, right = int(left), int(middle), int(right)
                if is_prime(left):
                    max_prime = max(max_prime, left)
                if is_prime(middle):
                    max_prime = max(max_prime, middle)
                if is_prime(right):
                    max_prime = max(max_prime, right)
    return max_prime

def process_file(input_path, output_path):
    with open(input_path, 'r') as file:
        lines = file.readlines()
        C = int(lines[0].strip())
        N = int(lines[1].strip())
