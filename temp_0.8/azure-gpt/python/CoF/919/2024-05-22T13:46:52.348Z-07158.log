def read_input(file_path):
    with open(file_path, 'r') as file:
        task = int(file.readline().strip())
        n = int(file.readline().strip())
        cards = [tuple(map(int, file.readline().split())) for _ in range(n)]
    return task, n, cards

def write_output(file_path, result):
    with open(file_path, 'w') as file:
        file.write(str(result) + '\n')

def matching_pairs(cards):
    return sum(1 for i in range(len(cards) - 1) if cards[i][1] == cards[i + 1][0])

def longest_matching_sequence(cards):
    max_length = 1
    current_length = 1
    for i in range(1, len(cards)):
        if cards[i][0] == cards[i - 1][1]:
            current_length += 1
            max_length = max(max_length, current_length)
        else:
            current_length = 1
    return max_length

def count_max_sequences(cards, max_length):
    count = 0
    current_length = 1
    for i in range(1, len(cards)):
        if cards[i][0] == cards[i - 1][1]:
            current_length += 1
        else:
            if current_length == max_length:
                count += 1
            current_length = 1
    return count

def solve(task, n, cards):
    if task == 1:
        return matching_pairs(cards)
    elif task == 2:
        return longest_matching_sequence(cards)
    elif task == 3:
        max_length = longest_matching_sequence(cards)
        return count_max_sequences(cards, max_length)

# Main program execution
task, n, cards = read_input('cartonase.in')
result = solve(task, n, cards)
write_output('cartonase.out', result)
