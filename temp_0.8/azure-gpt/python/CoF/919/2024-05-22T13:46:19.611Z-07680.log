# Read the problem specification and the sequence of cards from the input file
with open('cartonase.in', 'r') as file:
    C = int(file.readline().strip())
    N = int(file.readline().strip())
    cards = [tuple(map(int, file.readline().split())) for _ in range(N)]

# Initialize the variables for each task
matching_pairs_count = 0
max_seq_length = 1
max_seq_count = 0
current_seq_length = 1

# Task 1: Count the number of matching pairs
for i in range(1, N):
    if cards[i - 1][1] == cards[i][0]:
        matching_pairs_count += 1

# Variables needed for task 2 and 3
if C in (2, 3):
    for i in range(1, N):
        if cards[i - 1][1] == cards[i][0]:
            current_seq_length += 1
        else:
            max_seq_length = max(max_seq_length, current_seq_length)
            current_seq_length = 1
    # Handle the case when the sequence continues till the end of the cards
    max_seq_length = max(max_seq_length, current_seq_length)

    # Task 3: Count the number of sequences with the maximum length
    if C == 3:
        current_seq_length = 1
        for i in range(1, N):
            if cards[i - 1][1] == cards[i][0]:
                current_seq_length += 1
            else:
                if current_seq_length == max_seq_length:
                    max_seq_count += 1
                current_seq_length = 1
        if current_seq_length == max_seq_length:  # Handle the case when the sequence continues till the end
            max_seq_count += 1

# Write the result to the output file based on the problem specification
