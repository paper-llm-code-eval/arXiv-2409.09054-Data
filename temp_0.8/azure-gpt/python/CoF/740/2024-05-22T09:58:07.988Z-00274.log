# Read k from sir.in
with open('sir.in', 'r') as file:
    k = int(file.readline().strip())

# Calculate the first value and initialize sequence and seen number set
k_squared = k * k
a1 = k_squared // 100 % 10 * 10 + k_squared // 10 % 10
sequence = [a1]
seen = {a1}

# Generate the sequence until a repeated value is found
while True:
    last_val_squared = sequence[-1] ** 2
    next_val = last_val_squared // 100 % 10 * 10 + last_val_squared // 10 % 10
    if next_val in seen:
        break
    sequence.append(next_val)
    seen.add(next_val)

# Write the original sequence to sir.out
with open('sir.out', 'w') as file:
    file.write(' '.join(map(str, sequence)) + '\n')

    # Sort by the first digit and write the sorted sequence
    sorted_sequence = sorted(sequence, key=lambda x: str(x)[0])
    file.write(' '.join(map(str, sorted_sequence)) + '\n')
