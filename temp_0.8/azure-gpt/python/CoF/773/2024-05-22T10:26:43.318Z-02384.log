def read_input(file_name):
    with open(file_name, 'r') as file:
        start_time = list(map(int, file.readline().split()))
        n = int(file.readline().strip())
        competitors = [list(map(int, file.readline().split())) for _ in range(n)]
    return start_time, competitors

def write_output(file_name, first_competitor_time, last_competitor_time):
    with open(file_name, 'w') as file:
        file.write('{} {} {}\n'.format(*first_competitor_time))
        file.write('{} {} {}\n'.format(*last_competitor_time))

def add_time(start_time, duration):
    hours, minutes, seconds = start_time
    m, s = duration
    seconds += s
    minutes += m + seconds // 60
    hours += minutes // 60
    return [hours % 24, minutes % 60, seconds % 60]

def find_first_and_last_competitor_times(competitors, start_time):
    competitors_times = [add_time(start_time, competitor) for competitor in competitors]
    first_competitor_time = min(competitors_times)
    last_competitor_time = max(competitors_times)
    return first_competitor_time, last_competitor_time

def main():
    start_time, competitors = read_input('schi.in')
    first_competitor_time, last_competitor_time = find_first_and_last_competitor_times(competitors, start_time)
    write_output('schi.out', first_competitor_time, last_competitor_time)

if __name__ == "__main__":
    main()
```

