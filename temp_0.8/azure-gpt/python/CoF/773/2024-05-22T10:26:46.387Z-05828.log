# schi.py
def compute_finish_time(start_time, duration):
    h, m, s = start_time
    dm, ds = duration
    finish_hour = h
    finish_minute = m + dm
    finish_second = s + ds

    if finish_second >= 60:
        finish_minute += finish_second // 60
        finish_second %= 60

    if finish_minute >= 60:
        finish_hour += finish_minute // 60
        finish_minute %= 60

    # Adjust for 24 hour clock if the hour is greater than 23
    finish_hour %= 24

    return finish_hour, finish_minute, finish_second

def read_input():
    with open('schi.in', 'r') as file:
        start_time = tuple(map(int, file.readline().split()))
        n = int(file.readline().strip())

        durations = [tuple(map(int, line.split())) for line in file]

    return start_time, durations

def write_output(first_finish, last_finish):
    with open('schi.out', 'w') as file:
        file.write(f"{first_finish[0]} {first_finish[1]} {first_finish[2]}\n")
        file.write(f"{last_finish[0]} {last_finish[1]} {last_finish[2]}\n")

def main():
    start_time, durations = read_input()
    finish_times = [compute_finish_time(start_time, duration) for duration in durations]
    
    first_finish = min(finish_times)
    last_finish = max(finish_times)

    write_output(first_finish, last_finish)

if __name__ == '__main__':
    main()
```

