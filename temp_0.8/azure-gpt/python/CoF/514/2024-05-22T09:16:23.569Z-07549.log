with open('primprim.in', 'r') as fin:
    C = int(fin.readline().strip())
    n = int(fin.readline().strip())
    elements = list(map(int, fin.readline().split()))
    if C == 2:
        q = int(fin.readline().strip())
        operations = [list(map(int, fin.readline().split())) for _ in range(q)]

# Pre-compute prime numbers using Sieve of Eratosthenes
MAX_NUM = 10**6 + 1
is_prime = [False, False] + [True] * (MAX_NUM - 2)
for i in range(2, int(MAX_NUM**0.5) + 1):
    if is_prime[i]:
        for j in range(i*i, MAX_NUM, i):
            is_prime[j] = False
primes = [i for i in range(2, MAX_NUM) if is_prime[i]]

def find_closest_prime(x):
    # Find the closest prime number to x using binary search
    index = bisect_left(primes, x)
    if index == len(primes):
        return primes[-1]
    if index == 0:
        return primes[0]
    before = primes[index - 1]
    after = primes[index]
    if after - x < x - before:
        return after
    else:
        return before

# Function to calculate the total minimum cost
def total_cost(elements, p=None):
    sorted_elements = sorted(elements)[:p] if p is not None else sorted(elements)
    return sum(abs(e - find_closest_prime(e)) for e in sorted_elements)

# Write outputs to the output file
