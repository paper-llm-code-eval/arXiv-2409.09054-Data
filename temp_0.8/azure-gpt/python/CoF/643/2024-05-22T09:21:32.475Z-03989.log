def count_purple_squares(N, K, Q, A, B, C, D, X1, Y1, vertical_streets):
    horizontal_streets = calculate_horizontal_streets_parameters(N, K, A, B, C, D, X1, Y1)
    square_colors = [[0] * K for _ in range(N)] # 0: none, 1: red, 2: blue, 3: purple

    for r, X, Y in horizontal_streets:
        for k in range(X, X + Y):
            square_colors[r-1][k-1] += 2
    
    for J, R, L in vertical_streets:
        for n in range(R-1, R-1 + L):
            square_colors[n][J-1] += 1

    return sum(square_color == 3 for row in square_colors for square_color in row)

