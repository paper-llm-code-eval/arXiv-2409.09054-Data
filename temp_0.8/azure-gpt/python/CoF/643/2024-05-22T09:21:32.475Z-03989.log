def calculate_horizontal_streets_parameters(N, K, A, B, C, D, X1, Y1):
    horizontal_streets = [(X1, Y1)]
    for i in range(1, N):
        X = 1 + (horizontal_streets[-1][0] * A + B) % K
        Y = 1 + (horizontal_streets[-1][1] * C + D) % (K - X + 1)
        horizontal_streets.append((X, Y))
    return horizontal_streets

def read_input(filepath):
    with open(filepath, 'r') as file:
        N, K, Q = map(int, file.readline().split())
        A, B, C, D = map(int, file.readline().split())
