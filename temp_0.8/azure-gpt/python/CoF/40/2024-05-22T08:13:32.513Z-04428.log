from collections import defaultdict
import heapq
import itertools

def read_data(file_path):
    with open(file_path, 'r') as file:
        n, m = map(int, file.readline().split())
        k, *cities = map(int, file.readline().split())
        graph = defaultdict(list)
        for _ in range(m):
            x, y, z = map(int, file.readline().split())
            graph[x].append((y, z))
            graph[y].append((x, z))
        return n, k, cities, graph

def write_result(file_path, result):
    with open(file_path, 'w') as file:
        file.write(str(result))

def dijkstra(graph, start, targets_set):
    distances = defaultdict(lambda: float('inf'))
    distances[start] = 0
    queue = [(0, start)]

    while queue:
        current_distance, current_node = heapq.heappop(queue)

        if current_distance > distances[current_node]:
            continue

        for neighbor, weight in graph[current_node]:
            distance = current_distance + weight
            if distance < distances[neighbor]:
                distances[neighbor] = distance
                if neighbor in targets_set:
                    continue
                heapq.heappush(queue, (distance, neighbor))

    return distances

def solve(n, k, cities, graph):
    all_cities = [1] + cities + [n]
    targets_set = set(all_cities)
    distances_from_cities = {city: dijkstra(graph, city, targets_set) for city in all_cities}
    min_distance = float('inf')
    for perm in itertools.permutations(cities):
        path = [1] + list(perm) + [n]
        path_distance = sum(distances_from_cities[path[i]][path[i+1]] for i in range(len(path) - 1))
        min_distance = min(min_distance, path_distance)
    return min_distance

def main():
    n, k, cities, graph = read_data('ubuntzei.in')
    result = solve(n, k, cities, graph)
    write_result('ubuntzei.out', result)

main()
