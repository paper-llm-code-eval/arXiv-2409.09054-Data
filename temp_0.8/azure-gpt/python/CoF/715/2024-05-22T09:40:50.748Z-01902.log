# Function to check if a number is prime
def is_prime(num):
    if num in (0, 1):
        return False
    for i in range(2, int(num**0.5) + 1):
        if num % i == 0:
            return False
    return True

# Function to check if a number has the required properties
def has_properties(number, n):
    # Check the first digit and length
    if int(str(number)[0]) != n or len(str(number)) != n:
        return False
    
    # Check for the prime sequences of two digits
    number_str = str(number)
    for i in range(len(number_str) - 1):
        if not is_prime(int(number_str[i:i+2])):
            return False
    return True

# Read the number of evil spirits defeated from "visul.in"
with open("visul.in", "r") as infile:
    n = int(infile.readline().strip())

# Generate the number for Sinbad
if n == 1:
    solution = "Nu exista"
else:
    start = int("1" * n)  # The smallest number with n digits
    end = int("9" * n)    # The largest number with n digits
    found = False
    
    for number in range(start, end + 1):
        if has_properties(number, n):
            solution = number
            found = True
            break
    
    if not found:
        solution = "Nu exista"

# Write the result to "visul.out"
with open("visul.out", "w") as outfile:
    outfile.write(str(solution))

