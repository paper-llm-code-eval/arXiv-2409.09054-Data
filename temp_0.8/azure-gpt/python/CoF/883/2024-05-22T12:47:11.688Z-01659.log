# Code for reading from 'cursuri.in' and writing to 'cursuri.out'
def read_inputs(file_path):
    with open(file_path, 'r') as file:
        C = int(file.readline().strip())
        N, K = map(int, file.readline().strip().split())
        intervals = [tuple(map(int, file.readline().strip().split())) for _ in range(N)]
    return C, N, K, intervals

def write_output(file_path, result):
    with open(file_path, 'w') as file:
        file.write(str(result) + '\n')

def schedule_max_courses(intervals, K):
    intervals.sort(key=lambda x: x[1])
    end_times = [0] * K
    count = 0
    for start, end in intervals:
        for i in range(K):
            if end_times[i] <= start:
                end_times[i] = end
                count += 1
                break
    return count

def max_course_duration(intervals, K):
    max_duration = max(end - start for start, end in intervals)
    left, right = 0, max_duration
    intervals.sort()
    while left < right:
        mid = (left + right + 1) // 2
        end_times = [0] * K
        count = 0
        for start, _ in intervals:
            for i in range(K):
                if end_times[i] <= start:
                    end_times[i] = start + mid
                    count += 1
                    break
        if count >= len(intervals):
            left = mid
        else:
            right = mid - 1
    return left

def solve_challenge(C, N, K, intervals):
    if C == 1:
        return schedule_max_courses(intervals, K)
    elif C == 2:
        return max_course_duration(intervals, K)

# Main logic
C, N, K, intervals = read_inputs('cursuri.in')
result = solve_challenge(C, N, K, intervals)
write_output('cursuri.out', result)
```

