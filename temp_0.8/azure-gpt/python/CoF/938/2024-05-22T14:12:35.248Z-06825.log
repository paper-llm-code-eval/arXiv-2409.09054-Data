from collections import deque

def read_matrix(filename):
    with open(filename, 'r') as file:
        n, m = map(int, file.readline().split())
        matrix = [list(map(int, list(line.strip()))) for line in file]
    return n, m, matrix

def write_matrix(filename, matrix):
    with open(filename, 'w') as file:
        for row in matrix:
            file.write(''.join(map(str, row)) + '\n')

def bfs(matrix, start, n, m):
    queue = deque([start])
    distances = [[-1 for _ in range(m)] for _ in range(n)]
    distances[start[0]][start[1]] = 1
    
    while queue:
        x, y = queue.popleft()
        
        for dx, dy in [(-1, 0), (1, 0), (0, -1), (0, 1)]:
            nx, ny = x + dx, y + dy
            if 0 <= nx < n and 0 <= ny < m and matrix[nx][ny] == 0 and distances[nx][ny] == -1:
                distances[nx][ny] = distances[x][y] + 1
                queue.append((nx, ny))
                
    return distances

def solve_labirint(n, m, matrix):
    original_distances = bfs(matrix, (0, 0), n, m)
    d0 = original_distances[n-1][m-1]
    result_matrix = [[0 for _ in range(m)] for _ in range(n)]

    for i in range(n):
        for j in range(m):
            if matrix[i][j] == 1:
                matrix[i][j] = 0
                distances = bfs(matrix, (0, 0), n, m)

                if distances[n-1][m-1] >= 0 and distances[n-1][m-1] < d0:
                    result_matrix[i][j] = 1

                matrix[i][j] = 1

    return result_matrix

def main():
    n, m, matrix = read_matrix('labirint.in')
    result_matrix = solve_labirint(n, m, matrix)
    write_matrix('labirint.out', result_matrix)

if __name__ == '__main__':
    main()
```

