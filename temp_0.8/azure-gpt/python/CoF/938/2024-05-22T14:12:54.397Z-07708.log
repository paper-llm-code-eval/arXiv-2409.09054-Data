from collections import deque

def read_matrix(filename):
    with open(filename, 'r') as file:
        n, m = map(int, file.readline().split())
        matrix = [list(map(int, list(file.readline().strip()))) for _ in range(n)]
    return n, m, matrix

def write_matrix(filename, matrix):
    with open(filename, 'w') as file:
        for line in matrix:
            file.write(''.join(map(str, line)) + '\n')

def bfs(matrix, n, m):
    visited = [[False] * m for _ in range(n)]
    distances = [[float('inf')] * m for _ in range(n)]
    queue = deque([(0, 0)])
    visited[0][0] = True
    distances[0][0] = 1

    while queue:
        x, y = queue.popleft()
        for dx, dy in [(-1, 0), (1, 0), (0, -1), (0, 1)]:
            nx, ny = x + dx, y + dy
            if 0 <= nx < n and 0 <= ny < m and not visited[nx][ny] and matrix[nx][ny] == 0:
                visited[nx][ny] = True
                distances[nx][ny] = distances[x][y] + 1
                queue.append((nx, ny))

    return distances[n-1][m-1], distances

def main():
    n, m, matrix = read_matrix('labirint.in')

    # Compute the initial minimum path length
    d0, distances = bfs(matrix, n, m)

    result = [[0] * m for _ in range(n)]

    # Compute d(i, j) for each cell and compare with d0
    for i in range(n):
        for j in range(m):
            if matrix[i][j] == 1:
                matrix[i][j] = 0
                d, _ = bfs(matrix, n, m)
                matrix[i][j] = 1
                if d < d0:
                    result[i][j] = 1

    write_matrix('labirint.out', result)

if __name__ == '__main__':
    main()
```
