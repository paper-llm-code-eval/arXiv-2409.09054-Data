def calculate_area_and_steps(path, X, Y):
    visited = set()
    visited.add((X, Y))
    last_x, last_y = X, Y
    first_intersection = None
    step_count = 0
    area = 0
    directions = [(0, -1), (1, 0), (0, 1), (-1, 0)]  # N, E, S, W

    for i, move in enumerate(path):
        step_count += 1
        dx, dy = directions[move - 1]
        last_x += dx
        last_y += dy
        if (last_x, last_y) in visited and first_intersection is None:
            first_intersection = step_count
        visited.add((last_x, last_y))

        # Count the area using edge crossing number algorithm
        if move == 1 or move == 3:
            area += last_y if move == 1 else -last_y

    # Adjust the area value since we counted the edges
    area = abs(area) - first_intersection // 2

    return first_intersection, area

