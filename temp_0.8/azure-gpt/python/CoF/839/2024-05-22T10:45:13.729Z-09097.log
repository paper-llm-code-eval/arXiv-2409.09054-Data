# Define the function to calculate area and steps
def calculate_area_and_steps(path, X, Y):
    visited = {(X, Y)}
    x, y = X, Y
    area = 0
    first_pass_steps = 0
    # Directions correspond to N, E, S, W
    direction_offsets = {1: (-1, 0), 2: (0, 1), 3: (1, 0), 4: (0, -1)}

    for move in path:
        dx, dy = direction_offsets[move]
        x, y = x + dx, y + dy

        # For area calculation using the shoelace formula
        if move in [1, 3]:  # N or S
            area += (y - dy + y) * (dx)  # Trapezoidal area

        if (x, y) in visited:
            if first_pass_steps == 0:
                first_pass_steps = len(visited)
            else:
                break  # Stop as soon as we get to an already visited point
        else:
            visited.add((x, y))

    # Correct the area
    area = abs(area) // 2
    return first_pass_steps, area

