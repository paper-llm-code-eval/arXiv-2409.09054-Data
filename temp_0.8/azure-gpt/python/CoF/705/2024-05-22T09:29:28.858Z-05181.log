# Read from input file
with open('panglica.in', 'r') as fin:
    N, C = map(int, fin.readline().split())
    ribbons = [int(fin.readline().strip()) for _ in range(N)]

# Initialize variables to store the best solution
max_length = 0
color = 0
start_cut = 0
end_cut = 0

# Find the longest sequence from the start
start_seq = {}
for i in range(N):
    if ribbons[i] in start_seq:
        break
    start_seq[ribbons[i]] = i

# Find the longest sequence from the end
end_seq = {}
for i in range(N - 1, -1, -1):
    if ribbons[i] in end_seq:
        break
    end_seq[ribbons[i]] = N - 1 - i

# Check for the color with the longest sequence on both ends
for c in start_seq:
    if c in end_seq:
        left = start_seq[c]
        right = end_seq[c]
        seq_length = N - left - right
        if seq_length > max_length or (seq_length == max_length and left < start_cut):
            max_length = seq_length
            color = c
            start_cut = left
            end_cut = right

# Write to output file
with open('panglica.out', 'w') as fout:
    fout.write(f"{max_length}\n{color}\n{start_cut}\n{end_cut}\n")
