def check_winner(board, N):
    # Check rows and columns
    for i in range(N):
        if len(set(board[i])) == 1 and board[i][0] != 0:
            return board[i][0]
        if len(set(board[j][i] for j in range(N))) == 1 and board[0][i] != 0:
            return board[0][i]
    
    # Check main diagonal and adjacent semi-diagonals
    if len(set(board[i][i] for i in range(N))) == 1 and board[0][0] != 0:
        return board[0][0]
    if N > 3:
        if len(set(board[i][i + 1] for i in range(N - 1))) == 1 and board[0][1] != 0:
            return board[0][1]
        if len(set(board[i][i - 1] for i in range(1, N))) == 1 and board[1][0] != 0:
            return board[1][0]

    # Check secondary diagonal and adjacent semi-diagonals
    if len(set(board[i][N - i - 1] for i in range(N))) == 1 and board[0][N - 1] != 0:
        return board[0][N - 1]
    if N > 3:
        if len(set(board[i][N - i - 2] for i in range(N - 1))) == 1 and board[0][N - 2] != 0:
            return board[0][N - 2]
        if len(set(board[i + 1][N - i - 1] for i in range(N - 1))) == 1 and board[1][N - 1] != 0:
            return board[1][N - 1]
    
    return 0

def solve_task1(N, K, rounds):
    teodora_wins = 0
    stefan_wins = 0
    for round_number in range(K):
        board = [[0] * N for _ in range(N)]
        for i, cell in enumerate(rounds[round_number]):
            row = (cell - 1) // N
            col = (cell - 1) % N
            board[row][col] = 'X' if (round_number + i) % 2 == 0 else '0'
            if i >= N - 1 and check_winner(board, N):
                if (round_number + i) % 2 == 0:
                    teodora_wins += 1
                else:
                    stefan_wins += 1
                break
    return teodora_wins, stefan_wins

def solve_task2(N, K, rounds):
    max_moves = 0
    for round_number in range(K):
        board = [[0] * N for _ in range(N)]
        for i, cell in enumerate(rounds[round_number]):
            row = (cell - 1) // N
            col = (cell - 1) % N
            board[row][col] = 'X' if (round_number + i) % 2 == 0 else '0'
            if i >= N - 1 and check_winner(board, N):
                max_moves = max(max_moves, i + 1)
                break
    return max_moves

with open('joc.in', 'r') as fin:
    lines = fin.readlines()
