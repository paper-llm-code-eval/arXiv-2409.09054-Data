mod = 10**9 + 7

def solve(s):
    n = len(s)
    s = s.lower()
    cnt = [[0] * 26 for _ in range(n + 1)]
    for i in range(n - 1, -1, -1):
        for j in range(26):
            cnt[i][j] = cnt[i + 1][j]
        cnt[i][ord(s[i]) - ord('a')] += 1
    ans = 0
    for i in range(n):
        for j in range(i + 1, n):
            if s[i] != s[j]:
                ans += cnt[i][ord(s[j]) - ord('a')] * (j - i) * (j - i - 1) // 2
                ans %= mod
    return ans

with open('sdistante.in', 'r') as f:
    s = f.readline().strip()
with open('sdistante.out', 'w') as f:
    f.write(str(solve(s)))
