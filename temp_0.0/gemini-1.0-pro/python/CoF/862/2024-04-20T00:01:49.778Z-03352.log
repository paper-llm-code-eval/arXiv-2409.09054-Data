with open('charlie.in', 'r') as f:
    p = int(f.readline())
    s = f.readline().strip()

if p == 1:
    ans = 0
    for i in range(1, len(s)):
        if s[i] < s[i - 1]:
            ans += 1
    with open('charlie.out', 'w') as f:
        f.write(str(ans))
else:
    n = len(s)
    f = [0] * n
    for i in range(n):
        f[i] = ord(s[i]) - ord('a') + 1
    dp = [[0] * 3 for _ in range(n + 1)]
    dp[n][0] = dp[n][1] = dp[n][2] = 0
    for i in range(n - 1, -1, -1):
        for j in range(3):
            dp[i][j] = max(dp[i + 1][j], dp[i + 1][(j + 1) % 3] + f[i])
    ans = dp[0][0]
    t = []
    i = j = 0
    while i < n:
        if dp[i][j] == dp[i + 1][j]:
            t.append(s[i])
            i += 1
        else:
            j = (j + 1) % 3
    with open('charlie.out', 'w') as f:
        f.write(''.join(t) + '\n')
        f.write(str(ans))
