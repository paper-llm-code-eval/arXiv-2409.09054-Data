import sys
from collections import deque

def read_input():
    n = int(sys.stdin.readline())
    target, source1, source2, robot1, robot2 = [list(map(int, sys.stdin.readline().split())) for _ in range(5)]
    k = int(sys.stdin.readline())
    obstacles = [list(map(int, sys.stdin.readline().split())) for _ in range(k)]
    return n, target, source1, source2, robot1, robot2, obstacles

def is_valid(x, y, n):
    return 0 <= x < n and 0 <= y < n

def get_max_wall_length(obstacles, n):
    max_length = 0
    for i in range(n):
        horizontal_length = 0
        vertical_length = 0
        for j in range(n):
            if (i, j) in obstacles:
                horizontal_length = 0
            else:
                horizontal_length += 1
            if (j, i) in obstacles:
                vertical_length = 0
            else:
                vertical_length += 1
            max_length = max(max_length, horizontal_length, vertical_length)
    return max_length

def get_min_time(target, source1, source2, robot1, robot2, obstacles, n):
    def bfs(start, end):
        queue = deque([(start, 0)])
        visited = set()
        while queue:
            node, time = queue.popleft()
            if node == end:
                return time
            if node not in visited:
                visited.add(node)
                for neighbor in [(node[0] + 1, node[1]), (node[0] - 1, node[1]), (node[0], node[1] + 1), (node[0], node[1] - 1)]:
                    if is_valid(neighbor[0], neighbor[1], n) and neighbor not in obstacles:
                        queue.append((neighbor, time + 1))
        return -1

    time1 = bfs(robot1, target)
    time2 = bfs(robot2, target)
    if time1 == -1 or time2 == -1:
        return -1
    return max(time1, time2)

def main():
    n, target, source1, source2, robot1, robot2, obstacles = read_input()
    max_wall_length = get_max_wall_length(obstacles, n)
    min_time = get_min_time(target, source1, source2, robot1, robot2, obstacles, n)
    with open('ai.out', 'w') as f:
        f.write(str(max_wall_length) + '\n')
        if min_time != -1:
            f.write(str(min_time))

if __name__ == '__main__':
    main()
