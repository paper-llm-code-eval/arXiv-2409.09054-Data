import re

with open("ecuatii.in", "r") as f:
    n = int(f.readline())
    ecuatii = [f.readline().strip() for _ in range(n)]

with open("ecuatii.out", "w") as f:
    for ecuatie in ecuatii:
        # Extragem coeficientul lui x si termenul liber
        coef_x, termen_liber = map(int, re.findall(r"(\d+x?)|(\d+)", ecuatie))

        # Verificam daca ecuatia are solutii
        if coef_x == 0:
            if termen_liber == 0:
                f.write("infinit\n")
            else:
                f.write("imposibil\n")
        else:
            # Calculam solutia
            solutie = termen_liber / coef_x
            f.write(f"{solutie:.4f}\n")
```
