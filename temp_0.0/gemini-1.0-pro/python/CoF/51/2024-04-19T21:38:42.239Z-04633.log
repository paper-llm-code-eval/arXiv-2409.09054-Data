import sys

def read_input():
    with open('lant.in', 'r') as f:
        k = int(f.readline())
        text = f.read().split()
    return k, text

def write_output(result):
    with open('lant.out', 'w') as f:
        f.write(str(result))

def get_chains(k, text):
    chains = []
    for i in range(len(text)):
        chain = [text[i]]
        for j in range(i+1, len(text)):
            if is_similar(text[i], text[j], k):
                chain.append(text[j])
        if len(chain) > 1:
            chains.append(chain)
    return chains

def is_similar(word1, word2, k):
    if abs(len(word1) - len(word2)) > k:
        return False
    
    i = 0
    j = 0
    mismatches = 0
    while i < len(word1) and j < len(word2):
        if word1[i] != word2[j]:
            mismatches += 1
            if mismatches > k:
                return False
        i += 1
        j += 1
    
    return mismatches <= k

def main():
    k, text = read_input()
    chains = get_chains(k, text)
    result = len(chains)
    write_output(result)

if __name__ == '__main__':
    main()
