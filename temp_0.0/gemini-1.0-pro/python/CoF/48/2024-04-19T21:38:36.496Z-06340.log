import sys

def main():
    n, k = map(int, sys.stdin.readline().split())
    graph = [[] for _ in range(n + 1)]
    for _ in range(n - 1):
        a, b = map(int, sys.stdin.readline().split())
        graph[a].append(b)
        graph[b].append(a)

    # Find the center of the graph
    dist = [0] * (n + 1)
    visited = [False] * (n + 1)
    queue = [1]
    visited[1] = True
    while queue:
        u = queue.pop(0)
        for v in graph[u]:
            if not visited[v]:
                dist[v] = dist[u] + 1
                visited[v] = True
                queue.append(v)

    center = 1
    for i in range(2, n + 1):
        if dist[i] > dist[center]:
            center = i

    # Find the k closest nodes to the center
    dist = [0] * (n + 1)
    visited = [False] * (n + 1)
    queue = [center]
    visited[center] = True
    while queue:
        u = queue.pop(0)
        for v in graph[u]:
            if not visited[v]:
                dist[v] = dist[u] + 1
                visited[v] = True
                queue.append(v)

    closest = []
    for i in range(1, n + 1):
        if dist[i] <= k:
            closest.append(i)

    # Find the minimum cost
    cost = 0
    for i in range(1, n + 1):
        if i in closest:
            cost += dist[i]
        else:
            cost += 2 * dist[i]

    sys.stdout.write(str(cost) + "\n")

if __name__ == "__main__":
    main()
