import sys
import heapq

def dijkstra(graph, start, end):
    dist = [float('inf')] * (len(graph) + 1)
    dist[start] = 0
    pq = [(0, start)]
    while pq:
        d, u = heapq.heappop(pq)
        if u == end:
            return d
        for v, w in graph[u]:
            if d + w < dist[v]:
                dist[v] = d + w
                heapq.heappush(pq, (d + w, v))
    return -1

if __name__ == '__main__':
    with open('zmeu.in', 'r') as f:
        n, p, k = map(int, f.readline().split())
        graph = [[] for _ in range(p + 1)]
        for i in range(1, p + 1):
            d, c = map(int, f.readline().split())
            if n - c >= 0:
                graph[i].append((i + 1, d))
        for _ in range(k):
            a, b = map(int, f.readline().split())
            graph[a].append((b, float('inf')))
    with open('zmeu.out', 'w') as f:
        f.write(str(dijkstra(graph, 1, p)) + '\n')
```
