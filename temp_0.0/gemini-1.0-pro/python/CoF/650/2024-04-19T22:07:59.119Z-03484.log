with open('yinyang.in', 'r') as f:
    n, m = map(int, f.readline().split())
    a = [list(map(int, f.readline().split())) for _ in range(n)]

def check(a):
    for i in range(n):
        for j in range(1, m):
            if a[i][j] < a[i][j - 1]:
                return False
    for j in range(m):
        for i in range(1, n):
            if a[i][j] < a[i - 1][j]:
                return False
    return True

def swap_rows(a, i, j):
    a[i], a[j] = a[j], a[i]

def swap_cols(a, i, j):
    for k in range(n):
        a[k][i], a[k][j] = a[k][j], a[k][i]

def solve(a):
    ans = 0
    for i in range(n):
        for j in range(1, m):
            if a[i][j] < a[i][j - 1]:
                swap_cols(a, j, j - 1)
                ans += 1
    for j in range(m):
        for i in range(1, n):
            if a[i][j] < a[i - 1][j]:
                swap_rows(a, i, i - 1)
                ans += 1
    if check(a):
        return ans
    else:
        return -1

with open('yinyang.out', 'w') as f:
    f.write(str(solve(a)))
