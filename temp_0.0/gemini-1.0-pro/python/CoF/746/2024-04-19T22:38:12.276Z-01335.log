import sys

def main():
    with open('scara.in', 'r') as f:
        H, N, M, p = map(int, f.readline().split())

    # Initialize the DP table
    dp = [[float('inf')] * (M + 1) for _ in range(N + 1)]
    dp[0][0] = 0

    # Iterate over the steps
    for i in range(1, N + 1):
        # Iterate over the possible heights of the current step
        for h in range(1, M + 1):
            # Try all possible ways to reach the current step
            for j in range(i):
                # If the sum of the heights of the previous steps is less than or equal to the maximum height
                if dp[j][h - 1] != float('inf'):
                    # Calculate the effort required to reach the current step
                    effort = (dp[j][h - 1] + h + p) / (i - j)
                    # Update the DP table if the effort is less than the current minimum
                    if effort < dp[i][h]:
                        dp[i][h] = effort

    # Find the minimum effort
    min_effort = float('inf')
    for h in range(1, M + 1):
        if dp[N][h] < min_effort:
            min_effort = dp[N][h]

    # Reconstruct the optimal solution
    steps = []
    i = N
    h = M
    while i > 0:
        # Find the previous step that was used to reach the current step
        for j in range(i - 1, -1, -1):
            if dp[j][h - 1] != float('inf') and (dp[j][h - 1] + h + p) / (i - j) == min_effort:
                # Add the height of the previous step to the list of steps
                steps.append(h - 1)
                # Update the current step and height
                i = j
                h -= 1
                break

    # Reverse the list of steps
    steps.reverse()

    # Write the output to the file
    with open('scara.out', 'w') as f:
        f.write(f'{min_effort:.2f}\n')
        f.write(' '.join(map(str, steps)))

if __name__ == '__main__':
    main()
