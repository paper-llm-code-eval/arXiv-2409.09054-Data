import sys

def main():
    with open('elfi.in', 'r') as f_in, open('elfi.out', 'w') as f_out:
        n = int(f_in.readline())
        times = []
        for _ in range(n):
            c, p = map(int, f_in.readline().split())
            times.append((c, p))

        times.sort(key=lambda x: x[0])
        max_time = 0
        for c, p in times:
            if c > max_time:
                max_time = c
            max_time += p

        f_out.write(str(max_time))

if __name__ == '__main__':
    main()
```
