with open('nr.in') as f:
    n, k, p = map(int, f.readline().split())

def is_prime(n):
    if n < 2:
        return False
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            return False
    return True

def get_term(p):
    if p <= 4:
        return p
    else:
        return get_term(p - get_term(p - get_term(p - 1)))

prime_sum = 0
k_count = 0
for i in range(1, n + 1):
    term = get_term(i)
    if is_prime(term):
        prime_sum += term
    if str(k) in str(term):
        k_count += 1

with open('nr.out', 'w') as f:
    f.write(str(prime_sum) + '\n')
    f.write(str(k_count) + '\n')
    f.write(str(get_term(p)) + '\n')
```
