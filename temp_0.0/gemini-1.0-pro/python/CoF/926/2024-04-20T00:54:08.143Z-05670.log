import sys

with open('triunghi.in') as f:
    n = int(f.readline())
    A = [list(map(int, f.readline().split())) for _ in range(n)]
    Q = int(f.readline())
    zones = [list(map(int, f.readline().split())) for _ in range(Q)]

sums = [[0] * n for _ in range(n)]
for i in range(n):
    for j in range(n):
        sums[i][j] = A[i][j]
        if i > 0:
            sums[i][j] += sums[i - 1][j]
        if j > 0:
            sums[i][j] += sums[i][j - 1]
        if i > 0 and j > 0:
            sums[i][j] -= sums[i - 1][j - 1]

max_sum = 0
for lin, col, k in zones:
    if k > 0:
        sum = sums[lin + k - 1][col + k - 1]
        if lin > 0:
            sum -= sums[lin - 1][col + k - 1]
        if col > 0:
            sum -= sums[lin + k - 1][col - 1]
        if lin > 0 and col > 0:
            sum += sums[lin - 1][col - 1]
    else:
        sum = sums[lin - 1][col - 1]
        if lin + k >= 0:
            sum -= sums[lin + k][col - 1]
        if col + k >= 0:
            sum -= sums[lin - 1][col + k]
        if lin + k >= 0 and col + k >= 0:
            sum += sums[lin + k][col + k]
    max_sum = max(max_sum, sum)

with open('triunghi.out', 'w') as f:
    f.write(str(max_sum))
```
