dp = [0] * (n + 1)
dp[0] = 0
for i in range(n):
    x, y = notes[i]
    dp[i + 1] = max(dp[i + 1], dp[i])
    for j in range(i + 1, n):
        x2, y2 = notes[j]
        if x2 > x:
            dp[j + 1] = max(dp[j + 1], dp[i] + y * y)

fout.write(str(dp[n]))
