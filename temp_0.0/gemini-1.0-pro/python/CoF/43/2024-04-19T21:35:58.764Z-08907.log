import sys

with open('cerc.in') as f:
    n = int(f.readline())
    cercuri = [tuple(map(int, f.readline().split())) for _ in range(n)]

def dist(p1, p2):
    return ((p1[0] - p2[0]) ** 2 + (p1[1] - p2[1]) ** 2) ** 0.5

def pe_dreapta(c, p1, p2):
    return (p1[0] - c[0]) * (p2[1] - c[1]) == (p2[0] - c[0]) * (p1[1] - c[1])

def exterioare(c1, c2):
    return dist(c1, c2) > c1[2] + c2[2]

m = 0
q = 0
p = 0
for i in range(n):
    drepte = set()
    for j in range(n):
        if i == j:
            continue
        if exterioare(cercuri[i], cercuri[j]):
            drepte.add((cercuri[i][0], cercuri[j][0]))
            drepte.add((cercuri[i][1], cercuri[j][1]))
    if len(drepte) > m:
        m = len(drepte)
        q = 0
        p = 0
    if len(drepte) == m:
        for dreapta in drepte:
            cnt = 0
            for j in range(n):
                if i == j:
                    continue
                if exterioare(cercuri[i], cercuri[j]) and pe_dreapta(cercuri[j], cercuri[i], dreapta):
                    cnt += 1
            if cnt > q:
                q = cnt
                p = 0
            if cnt == q:
                p += 1

with open('cerc.out', 'w') as f:
    f.write(f'{m} {q} {p}')
```
