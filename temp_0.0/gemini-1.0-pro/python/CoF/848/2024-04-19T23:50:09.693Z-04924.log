with open('arrows.in') as f:
    cerinta = int(f.readline())
    n, m = map(int, f.readline().split())
    tabla = [list(map(int, f.readline().split())) for _ in range(n)]
    lin, col = map(int, f.readline().split())

def punctaj(lin, col):
    vizitate = set()
    puncte = 0
    while True:
        vizitate.add((lin, col))
        puncte += 1
        if lin == 1 and tabla[lin - 1][col - 1] == 4:
            break
        elif lin == n and tabla[lin - 1][col - 1] == 2:
            break
        elif col == 1 and tabla[lin - 1][col - 1] == 3:
            break
        elif col == m and tabla[lin - 1][col - 1] == 1:
            break
        if tabla[lin - 1][col - 1] == 1:
            col += 1
        elif tabla[lin - 1][col - 1] == 2:
            lin -= 1
        elif tabla[lin - 1][col - 1] == 3:
            col -= 1
        elif tabla[lin - 1][col - 1] == 4:
            lin += 1
    return puncte * 1000 if (lin, col) in vizitate else puncte

def numar_pozitii_favorabile():
    favorabile = 0
    for i in range(n):
        for j in range(m):
            vizitate = set()
            while True:
                vizitate.add((i, j))
                if i == 0 and tabla[i][j] == 4:
                    break
                elif i == n - 1 and tabla[i][j] == 2:
                    break
                elif j == 0 and tabla[i][j] == 3:
                    break
                elif j == m - 1 and tabla[i][j] == 1:
                    break
                if tabla[i][j] == 1:
                    j += 1
                elif tabla[i][j] == 2:
                    i -= 1
                elif tabla[i][j] == 3:
                    j -= 1
                elif tabla[i][j] == 4:
                    i += 1
            if (i, j) in vizitate:
                favorabile += 1
    return favorabile

def punctaj_maxim():
    max_puncte = 0
    for i in range(n):
        for j in range(m):
            max_puncte = max(max_puncte, punctaj(i, j))
    return max_puncte

if cerinta == 1:
    with open('arrows.out', 'w') as f:
        f.write(str(punctaj(lin - 1, col - 1)))
elif cerinta == 2:
    with open('arrows.out', 'w') as f:
        f.write(str(numar_pozitii_favorabile()))
elif cerinta == 3:
    with open('arrows.out', 'w') as f:
        f.write(str(punctaj_maxim()))
