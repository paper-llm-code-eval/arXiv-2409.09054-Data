#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MOD = 1e9 + 7;
const int MAXN = 16;

int n;
vector<int> p;
vector<vector<int>> adj;
vector<int> ans;
int cnt;

void dfs(int u, int par) {
    for (int v : adj[u]) {
        if (v == par) continue;
        dfs(v, u);
        ans.push_back(v);
    }
}

int main() {
    ifstream fin("triang.in");
    ofstream fout("triang.out");

    fin >> n;
    p.resize(n + 1);
    adj.resize(n + 1);
    for (int i = 1; i <= n; i++) {
        fin >> p[i];
    }
    for (int i = 1; i < n; i++) {
        int u, v;
        fin >> u >> v;
        adj[u].push_back(v);
        adj[v].push_back(u);
    }

    dfs(1, 0);

    sort(ans.begin(), ans.end());
    cnt = unique(ans.begin(), ans.end()) - ans.begin();

    fout << cnt << '\n';
    for (int i = 0; i < cnt; i++) {
        int code = 1;
        for (int j = i; j < cnt; j++) {
            code = (1LL * code * (137LL * min(ans[i], ans[j]) + 1LL * max(ans[i], ans[j]))) % MOD;
        }
        fout << code << '\n';
    }

    return 0;
}
