#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("cetate.in");
ofstream fout("cetate.out");

const int MAXN = 400;
const int MAXK = 400;
const int INF = 1e9;

int n, m, k, c;
int a[MAXN][MAXN];
int dp[MAXN][MAXN][MAXK][MAXK];
int dp2[MAXN][MAXN][MAXK][MAXK];
int ans, ans2;
int ans_i1, ans_j1, ans_i2, ans_j2;
int ans_i1_2, ans_j1_2, ans_i2_2, ans_j2_2;

int main() {
    fin >> c >> n >> m >> k;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            fin >> a[i][j];
        }
    }

    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            dp[i][j][1][1] = a[i][j];
            dp2[i][j][1][1] = a[i][j];
        }
    }

    for (int l = 2; l <= k; l++) {
        for (int i = 1; i <= n - l + 1; i++) {
            for (int j = 1; j <= m - l + 1; j++) {
                dp[i][j][l][1] = max(dp[i][j][l - 1][1], dp[i + 1][j][l - 1][1]) + a[i + l - 1][j];
                dp[i][j][1][l] = max(dp[i][j][1][l - 1], dp[i][j + 1][1][l - 1]) + a[i][j + l - 1];
                dp2[i][j][l][1] = max(dp2[i][j][l - 1][1], dp2[i][j + 1][1][l - 1]) + a[i][j + l - 1];
            }
        }
    }

    for (int l = 2; l <= k; l++) {
        for (int i = 1; i <= n - l + 1; i++) {
            for (int j = 1; j <= m - l + 1; j++) {
                for (int w = 2; w <= l; w++) {
                    dp[i][j][l][w] = max(dp[i][j][l][w - 1], dp[i + w - 1][j][l - w + 1][w - 1]) + a[i + l - 1][j + w - 1];
                    dp[i][j][w][l] = max(dp[i][j][w - 1][l], dp[i][j + w - 1][w - 1][l - w + 1]) + a[i + w - 1][j + l - 1];
                    dp2[i][j][l][w] = max(dp2[i][j][l][w - 1], dp2[i][j + w - 1][w - 1][l - w + 1]) + a[i + w - 1][j + l - 1];
                }
            }
        }
    }

    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            for (int l = 1; l <= k; l++) {
                for (int w = 1; w <= k; w++) {
                    if (dp[i][j][l][w] > ans) {
                        ans = dp[i][j][l][w];
                        ans_i1 = i;
                        ans_j1 = j;
                        ans_i2 = i + l - 1;
                        ans_j2 = j + w - 1;
                    }
                    if (dp2[i][j][l][w] > ans2) {
                        ans2 = dp2[i][j][l][w];
                        ans_i1_2 = i;
                        ans_j1_2 = j;
                        ans_i2_2 = i + l - 1;
                        ans_j2_2 = j + w - 1;
                    }
                }
            }
        }
    }

    if (c == 1) {
        fout << ans << '\n';
        fout << ans_i1 << ' ' << ans_j1 << ' ' << ans_i2 << ' ' << ans_j2 << '\n';
    } else {
        fout << ans2 << '\n';
        fout << ans_i1_2 << ' ' << ans_j1_2 << ' ' << ans_i2_2 << ' ' << ans_j2_2 << '\n';
    }

    return 0;
}
