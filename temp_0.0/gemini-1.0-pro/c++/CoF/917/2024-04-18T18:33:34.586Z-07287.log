                        ans_i2 = i + l - 1;
                        ans_j2 = j + w - 1;
                    }
                    if (dp2[i][j][l][w] > ans2) {
                        ans2 = dp2[i][j][l][w];
                        ans_i1_2 = i;
                        ans_j1_2 = j;
                        ans_i2_2 = i + l - 1;
                        ans_j2_2 = j + w - 1;
                    }
                }
            }
        }
