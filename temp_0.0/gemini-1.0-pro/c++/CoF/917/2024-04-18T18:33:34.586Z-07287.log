                    dp2[i][j][l][w] = max(dp2[i][j][l][w - 1], dp2[i][j + w - 1][w - 1][l - w + 1]) + a[i + w - 1][j + l - 1];
                }
            }
        }
    }

    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            for (int l = 1; l <= k; l++) {
