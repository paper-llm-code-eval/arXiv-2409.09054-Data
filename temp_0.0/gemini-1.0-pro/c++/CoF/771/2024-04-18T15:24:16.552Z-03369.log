#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream in("pluricex.in");
ofstream out("pluricex.out");

const int NMAX = 22;
const int KMAX = 8;
const int DMAX = 10;

int n, k, d;
int a[NMAX][DMAX + 1];
int sol[KMAX];
bool ok[NMAX];
vector<vector<int>> solv;

void back(int p, int cnt) {
    if (cnt == k) {
        vector<int> aux;
        for (int i = 0; i < k; ++i) {
            aux.push_back(sol[i]);
        }
        sort(aux.begin(), aux.end());
        solv.push_back(aux);
        return;
    }
    for (int i = p + 1; i <= n; ++i) {
        if (!ok[i]) {
            bool ok_sol = true;
            for (int j = 1; j <= d; ++j) {
                if (a[i][j] == 0) {
                    ok_sol = false;
                    break;
                }
            }
            if (ok_sol) {
                ok[i] = true;
                sol[cnt] = i;
                back(i, cnt + 1);
                ok[i] = false;
            }
        }
    }
}

int main() {
    in >> n >> k >> d;
    for (int i = 1; i <= n; ++i) {
        int nr;
        in >> nr;
        for (int j = 1; j <= nr; ++j) {
            int x;
            in >> x;
            a[i][x] = 1;
        }
    }
    back(0, 0);
    sort(solv.begin(), solv.end());
    for (auto& v : solv) {
        for (auto& x : v) {
            out << x << ' ';
        }
        out << '\n';
    }
    return 0;
}
