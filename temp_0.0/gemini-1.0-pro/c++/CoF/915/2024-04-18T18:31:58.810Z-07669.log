#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("traseu.in");
    ofstream out("traseu.out");

    int n, m;
    in >> n >> m;

    vector<vector<int>> a(n, vector<int>(m));
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            in >> a[i][j];
        }
    }

    vector<vector<int>> dp(n, vector<int>(m, 0));
    int ans = 0;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (i == 0 && j == 0) {
                dp[i][j] = 1;
            } else {
                if (i > 0 && a[i][j] > a[i - 1][j]) {
                    dp[i][j] = max(dp[i][j], dp[i - 1][j] + 1);
                }
                if (j > 0 && a[i][j] > a[i][j - 1]) {
                    dp[i][j] = max(dp[i][j], dp[i][j - 1] + 1);
                }
            }
            ans = max(ans, dp[i][j]);
        }
    }

    out << ans << endl;

    return 0;
}
```
