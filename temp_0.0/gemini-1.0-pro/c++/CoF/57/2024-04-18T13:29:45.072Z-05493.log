#include <fstream>
#include <vector>
#include <algorithm>
#include <queue>
#include <cstring>

using namespace std;

const int NMAX = 256;
const int MMAX = 32768;
const int KMAX = 256;

struct Edge {
    int node, priority;
};

vector<Edge> graph[NMAX];
int n, m, k;
int parent[NMAX];
int group[NMAX];
int priority[MMAX];
int best_priority[KMAX];
int best_group[NMAX];
int best_k;
int best_c;
int best_edges[MMAX];

void read_input() {
    ifstream fin("urgenta.in");
    fin >> n >> m >> k;
    for (int i = 0; i < m; i++) {
        int a, b, p;
        fin >> a >> b >> p;
        graph[a].push_back({b, p});
        graph[b].push_back({a, p});
        priority[i] = p;
    }
    fin.close();
}

int find_parent(int node) {
    if (parent[node] == node) {
        return node;
    }
    return parent[node] = find_parent(parent[node]);
}

void unite(int a, int b) {
    a = find_parent(a);
    b = find_parent(b);
    if (a != b) {
        parent[a] = b;
    }
}

void dfs(int node, int grp) {
    group[node] = grp;
    for (auto& edge : graph[node]) {
        if (group[edge.node] == -1) {
            dfs(edge.node, grp);
        }
    }
}

void solve() {
    memset(parent, -1, sizeof(parent));
    memset(group, -1, sizeof(group));
    int grp = 0;
    for (int i = 1; i <= n; i++) {
        if (group[i] == -1) {
            dfs(i, grp++);
        }
    }

    for (int i = 0; i < k; i++) {
        best_priority[i] = -1;
    }

    for (int i = 0; i < m; i++) {
        int a = graph[i][0].node;
        int b = graph[i][1].node;
        if (group[a] != group[b]) {
            int p = priority[i];
            if (p > best_priority[group[a]]) {
                best_priority[group[a]] = p;
                best_group[group[a]] = group[b];
                best_edges[group[a]] = i;
            }
            if (p > best_priority[group[b]]) {
                best_priority[group[b]] = p;
                best_group[group[b]] = group[a];
                best_edges[group[b]] = i;
            }
        }
    }

    best_k = 0;
    best_c = 0;
    for (int i = 0; i < k; i++) {
        if (best_priority[i] != -1) {
            best_k++;
            best_c += best_priority[i];
        }
    }
}

void write_output() {
    ofstream fout("urgenta.out");
    fout << best_c << '\n';
    fout << best_c << '\n';
    for (int i = 0; i < best_k; i++) {
        int a = graph[best_edges[i]][0].node;
        int b = graph[best_edges[i]][1].node;
        fout << a << ' ' << b << '\n';
    }
    fout.close();
}

int main() {
    read_input();
    solve();
    write_output();
    return 0;
}
