#include <fstream>
#include <string>
#include <vector>
#include <stack>
#include <algorithm>

using namespace std;

int main() {
  ifstream in("arh.in");
  ofstream out("arh.out");

  string s;
  in >> s;

  int n = s.size();
  vector<int> t(n);
  stack<int> st;
  int cnt = 0;
  for (int i = 0; i < n; i++) {
    if (isdigit(s[i])) {
      t[i] = s[i] - '0';
      st.push(i);
    } else if (s[i] == '(') {
      int j = st.top();
      st.pop();
      int k = i;
      while (s[k] != ')') k++;
      for (int l = 0; l < t[j]; l++) {
        for (int m = j + 1; m < k; m++) {
          t[i++] = t[m];
        }
      }
      i--;
    } else if (s[i] == '[') {
      int j = st.top();
      st.pop();
      int k = i;
      while (s[k] != ']') k++;
      string u = s.substr(j + 1, k - j - 1);
      string v = u;
      reverse(v.begin(), v.end());
      if (s[k + 1] == '*') {
        u += v.substr(1);
      } else {
        u += v;
      }
      for (int l = 0; l < u.size(); l++) {
        t[i++] = u[l];
      }
      i--;
    }
  }

  string r;
  for (int i = 0; i < n; i++) {
    if (t[i] > 0) {
      r += t[i] + 'a';
      cnt++;
    }
  }

  out << cnt << endl;
  out << r << endl;

  return 0;
}
