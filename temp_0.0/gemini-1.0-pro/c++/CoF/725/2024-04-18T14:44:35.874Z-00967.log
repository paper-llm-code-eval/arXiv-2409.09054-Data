#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("cifre.in");
    ofstream out("cifre.out");

    long long a, b;
    in >> a >> b;

    vector<int> v(10, 0);
    while (a > 0) {
        v[a % 10]++;
        a /= 10;
    }
    while (b > 0) {
        v[b % 10]++;
        b /= 10;
    }

    bool ok = false;
    for (int i = 0; i < 10; i++) {
        if (v[i] > 0) {
            if (!ok) {
                ok = true;
            } else {
                out << " ";
            }
            out << i;
        }
    }
    if (!ok) {
        out << "-1";
    }
    out << endl;

    vector<int> digits;
    for (int i = 0; i < 10; i++) {
        while (v[i] > 0) {
            digits.push_back(i);
            v[i]--;
        }
    }
    sort(digits.begin(), digits.end(), greater<int>());
    for (int i = 0; i < digits.size(); i++) {
        out << digits[i];
    }
    out << endl;

    return 0;
}
