const int MAX_M = 200;

int n, m, c;
int a[MAX_N][MAX_N];
int b[MAX_N][MAX_N];
int dx[] = {-1, 0, 1, 0, -1, -1, 1, 1};
int dy[] = {0, 1, 0, -1, -1, 1, -1, 1};

struct Laser {
    int x, y, d;
};

vector<Laser> lasers;

void read_input() {
    ifstream fin("lasere.in");
    fin >> c >> n >> m;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n; j++) {
            fin >> a[i][j];
        }
    }
    for (int i = 1; i <= m; i++) {
        Laser laser;
        fin >> laser.x >> laser.y >> laser.d;
        lasers.push_back(laser);
    }
    fin.close();
}

void shoot_lasers() {
    for (const Laser& laser : lasers) {
        int x = laser.x;
        int y = laser.y;
        int d = laser.d;
        if (d == 1) {
            for (int i = x - 1; i >= 1; i--) {
                a[i][y]--;
            }
        } else if (d == 2) {
            for (int j = y + 1; j <= n; j++) {
                a[x][j]--;
            }
        } else if (d == 3) {
            for (int i = x + 1; i <= n; i++) {
                a[i][y]--;
            }
        } else if (d == 4) {
            for (int j = y - 1; j >= 1; j--) {
                a[x][j]--;
            }
        }
    }
}

int count_pits() {
    int count = 0;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n; j++) {
            bool is_pit = true;
            for (int k = 0; k < 8; k++) {
                int x = i + dx[k];
                int y = j + dy[k];
                if (x >= 1 && x <= n && y >= 1 && y <= n && a[x][y] < a[i][j]) {
                    is_pit = false;
                    break;
                }
            }
            if (is_pit) {
                count++;
            }
        }
    }
    return count;
}

int count_trenches(int n, int a[MAX_N][MAX_N], int dx[], int dy[]) {
    int count = 0;
    for (int i = 1; i <= n; i++) {
        int start = 0;
        int end = 0;
        for (int j = 1; j <= n; j++) {
            bool is_pit = true;
            for (int k = 0; k < 8; k++) {
                int x = i + dx[k];
                int y = j + dy[k];
                if (x >= 1 && x <= n && y >= 1 && y <= n && a[x][y] < a[i][j]) {
                    is_pit = false;
                    break;
                }
            }
            if (is_pit) {
                if (start == 0) {
                    start = j;
                }
                end = j;
            } else {
                if (start != 0) {
                    count++;
                }
                start = 0;
                end = 0;
            }
        }
        if (start != 0) {
            count++;
        }
    }
    return count;
}

void write_output() {
    ofstream fout("lasere.out");
    if (c == 1) {
        fout << count_pits() << endl;
    } else if (c == 2) {
        fout << count_trenches(n, a, dx, dy) << endl;
    }
    fout.close();
}

int main() {
    read_input();
    shoot_lasers();
    write_output();
    return 0;
}
