#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("blis.in");
    ofstream out("blis.out");
    int k, n;
    in >> k >> n;
    vector<int> a(n);
    for (int i = 0; i < n; i++) {
        in >> a[i];
    }
    vector<int> dp(n + 1, 0);
    vector<int> prv(n + 1, -1);
    int mx = 0;
    int lst = -1;
    for (int i = 0; i < n; i++) {
        for (int j = i; j >= 0 && i - j + 1 <= k; j--) {
            if (a[j] > lst) {
                if (dp[j] + 1 > dp[i]) {
                    dp[i] = dp[j] + 1;
                    prv[i] = j;
                }
            }
        }
        lst = a[i];
        mx = max(mx, dp[i]);
    }
    out << mx << '\n';
    vector<int> ans;
    int cur = n - 1;
    while (cur != -1) {
        ans.push_back(cur);
        cur = prv[cur];
    }
    reverse(ans.begin(), ans.end());
    out << ans.size() << '\n';
    return 0;
}
