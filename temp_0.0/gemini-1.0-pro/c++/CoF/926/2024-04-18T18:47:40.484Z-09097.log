#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream in("triunghi.in");
ofstream out("triunghi.out");

const int MAXN = 1005;
const int MAXQ = 100005;

int n, q;
int a[MAXN][MAXN];
int dp[MAXN][MAXN][2];
int sum[MAXN][MAXN];
vector<tuple<int, int, int>> queries;

int main() {
    in >> n;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n; j++) {
            in >> a[i][j];
            sum[i][j] = sum[i - 1][j] + sum[i][j - 1] - sum[i - 1][j - 1] + a[i][j];
        }
    }
    in >> q;
    for (int i = 0; i < q; i++) {
        int lin, col, k;
        in >> lin >> col >> k;
        queries.emplace_back(lin, col, k);
    }
    int ans = 0;
    for (auto& [lin, col, k] : queries) {
        if (k > 0) {
            for (int i = lin; i < lin + k; i++) {
                for (int j = col; j < col + k; j++) {
                    dp[i][j][0] = max(dp[i - 1][j][0], dp[i][j - 1][0]) + a[i][j];
                    dp[i][j][1] = max(dp[i - 1][j][1], dp[i][j - 1][1]) + a[i][j];
                }
            }
            ans = max(ans, dp[lin + k - 1][col + k - 1][0]);
        } else {
            k = -k;
            for (int i = lin; i > lin - k; i--) {
                for (int j = col; j > col - k; j--) {
                    dp[i][j][0] = max(dp[i + 1][j][0], dp[i][j + 1][0]) + a[i][j];
                    dp[i][j][1] = max(dp[i + 1][j][1], dp[i][j + 1][1]) + a[i][j];
                }
            }
            ans = max(ans, dp[lin - k + 1][col - k + 1][1]);
        }
    }
    out << ans << '\n';
    return 0;
}
