            int mx = 0;
            for (int j = 0; j < k; j++) {
                int ex = pos[j].first;
                int ey = pos[j].second;
                mx = max(mx, dist[ex][ey]);
            }
            ans = min(ans, mx * mx);
        }
        fout << ans << '\n';
