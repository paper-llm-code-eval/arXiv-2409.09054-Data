#include <fstream>
#include <queue>
#include <vector>

using namespace std;

const int MAXN = 100;
const int MAXM = 100;
const int INF = 1e9;

int n, m;
char grid[MAXN][MAXM];
int dist[MAXN][MAXM];
bool visited[MAXN][MAXM];

struct Point {
  int x, y;
};

int dx[] = {-1, -1, -1, 0, 0, 1, 1, 1};
int dy[] = {-1, 0, 1, -1, 1, -1, 0, 1};

bool valid(int x, int y) {
  return x >= 1 && x <= n && y >= 1 && y <= m && grid[x][y] != 'X';
}

int bfs(Point start) {
  queue<Point> q;
  q.push(start);
  visited[start.x][start.y] = true;
  dist[start.x][start.y] = 0;

  while (!q.empty()) {
    Point current = q.front();
    q.pop();

    for (int i = 0; i < 8; i++) {
      int nx = current.x + dx[i];
      int ny = current.y + dy[i];

      if (valid(nx, ny) && !visited[nx][ny]) {
        q.push({nx, ny});
        visited[nx][ny] = true;
        dist[nx][ny] = dist[current.x][current.y] + 1;
      }
    }
  }

  int min_dist = INF;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      if (grid[i][j] == 'R' || grid[i][j] == 'J') {
        min_dist = min(min_dist, dist[i][j]);
      }
    }
  }

  return min_dist;
}

int main() {
  ifstream fin("rj.in");
  ofstream fout("rj.out");

  fin >> n >> m;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      fin >> grid[i][j];
    }
  }

  Point start;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      if (grid[i][j] == 'R') {
        start = {i, j};
      }
    }
  }

  int min_time = bfs(start);

  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      if (grid[i][j] == 'J' && dist[i][j] == min_time) {
        fout << i << " " << j << " " << min_time << endl;
        return 0;
      }
    }
  }

  return 0;
}
