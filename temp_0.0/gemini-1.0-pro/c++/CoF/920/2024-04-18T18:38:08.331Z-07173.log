void tai2(int x) {
    for (int i = 1; i <= x / 2; ++i) {
        int a = x / i;
        int b = x % i;
        if (a >= 1 && b >= 1) {
            if (prim(a)) p2.push_back(a);
            if (prim(b)) p2.push_back(b);
        }
    }
}

void tai3(int x) {
    for (int i = 1; i <= x / 2; ++i) {
        for (int j = 1; j <= (x - i) / 2; ++j) {
            int a = x / i;
            int b = (x - i) / j;
            int c = x % j;
            if (a >= 1 && b >= 1 && c >= 1) {
                if (prim(a)) p3.push_back(a);
                if (prim(b)) p3.push_back(b);
                if (prim(c)) p3.push_back(c);
            }
        }
    }
}

int main() {
    in >> c >> n;
    for (int i = 0; i < n; ++i) {
        int x;
        in >> x;
        v.push_back(x);
        if (prim(x)) p2.push_back(x);
        p3.push_back(x);
    }

    int ans = 0;
    if (c == 1) {
        for (int x : v) {
            if (prim(x)) ans = max(ans, x);
        }
    } else if (c == 2) {
        for (int x : v) {
            tai2(x);
        }
        for (int x : p2) {
            ans = max(ans, x);
        }
    } else {
        for (int x : v) {
            tai3(x);
        }
        for (int x : p3) {
            ans = max(ans, x);
        }
    }

    out << ans << '\n';
    return 0;
}
