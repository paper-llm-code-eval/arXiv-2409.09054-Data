        for (int j = x.size() - 1; j >= 0; --j) {
            r = r * 10 + x[j] - '0';
            t = (char)(r / i + '0') + t;
            r %= i;
        }
        if (r != 0) return false;
        x = t;
    }
    return true;
}

void tai2(string x) {
    for (int i = 1; i < x.size(); ++i) {
        string a = x.substr(0, i);
        while (a.size() < x.size()) a = '0' + a;
        string b = x.substr(i);
        while (b.size() < x.size()) b = '0' + b;
        if (a.size() >= 1 && b.size() >= 1) {
            if (prim(a)) p2.push_back(a);
            if (prim(b)) p2.push_back(b);
        }
    }
}

void tai3(string x) {
    for (int i = 1; i < x.size(); ++i) {
        for (int j = i; j < x.size(); ++j) {
            string a = x.substr(0, i);
            while (a.size() < x.size()) a = '0' + a;
            string b = x.substr(i, j - i);
            while (b.size() < x.size()) b = '0' + b;
            string c = x.substr(j);
            while (c.size() < x.size()) c = '0' + c;
            if (a.size() >= 1 && b.size() >= 1 && c.size() >= 1) {
                if (prim(a)) p3.push_back(a);
                if (prim(b)) p3.push_back(b);
                if (prim(c)) p3.push_back(c);
