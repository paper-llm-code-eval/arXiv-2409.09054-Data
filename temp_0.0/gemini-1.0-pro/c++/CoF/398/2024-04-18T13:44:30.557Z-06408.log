    int min_dist = -1;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (grid[i][j] == 0) {
                for (int k = 0; k < 4; k++) {
                    int nx = i + dx[k];
                    int ny = j + dy[k];
                    if (nx >= 0 && nx < n && ny >= 0 && ny < m && grid[nx][ny] == 1) {
                        vis.assign(n, vector<int>(m, 0));
                        dist.assign(n, vector<int>(m, -1));
                        prev.assign(n, vector<pair<int, int>>(m));
                        int dist_to_r = bfs(i, j, nx, ny);
                        if (dist_to_r != -1) {
                            for (int l = 0; l < 4; l++) {
                                int mx = nx + dx[l];
                                int my = ny + dy[l];
                                if (mx >= 0 && mx < n && my >= 0 && my < m && grid[mx][my] == 2) {
                                    vis.assign(n, vector<int>(m, 0));
                                    dist.assign(n, vector<int>(m, -1));
                                    prev.assign(n, vector<pair<int, int>>(m));
                                    int dist_to_g = bfs(nx, ny, mx, my);
                                    if (dist_to_g != -1) {
                                        if (min_dist == -1 || dist_to_r + dist_to_g < min_dist) {
                                            min_dist = dist_to_r + dist_to_g;
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
    fout << nr << " " << ng << " " << nb << " " << min_dist << "\n";
    return 0;
}
