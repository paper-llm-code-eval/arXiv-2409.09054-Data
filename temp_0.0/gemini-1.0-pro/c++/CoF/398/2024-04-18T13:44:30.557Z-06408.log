using namespace std;

const int dx[] = {0, 1, 0, -1};
const int dy[] = {1, 0, -1, 0};

int n, m;
vector<vector<int>> grid;
vector<vector<int>> vis;
vector<vector<int>> dist;
vector<vector<pair<int, int>>> prev;

int bfs(int sx, int sy, int ex, int ey) {
    queue<pair<int, int>> q;
    q.push({sx, sy});
    vis[sx][sy] = 1;
    dist[sx][sy] = 0;
    while (!q.empty()) {
        int x = q.front().first;
