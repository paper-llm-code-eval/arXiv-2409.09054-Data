#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream in("cochilie.in");
  ofstream out("cochilie.out");
  int c;
  in >> c;
  if (c == 1) {
    int n;
    in >> n;
    out << 2 * n - 1 << " " << 2 * n - 1 << "\n";
  } else {
    int n, p;
    in >> n >> p;
    vector<vector<int>> cochilie(2 * n - 1, vector<int>(2 * n - 1, 0));
    int x = n - 1, y = n - 1, val = 1, dir = 0;
    while (val <= n) {
      cochilie[x][y] = val;
      if (dir == 0) {
        y++;
        if (y == 2 * n - 1 || cochilie[x][y] != 0) {
          y--;
          dir = 1;
        }
      } else if (dir == 1) {
        x++;
        if (x == 2 * n - 1 || cochilie[x][y] != 0) {
          x--;
          dir = 2;
        }
      } else if (dir == 2) {
        y--;
        if (y == 0 || cochilie[x][y] != 0) {
          y++;
          dir = 3;
        }
      } else if (dir == 3) {
        x--;
        if (x == 0 || cochilie[x][y] != 0) {
          x++;
          dir = 0;
          val++;
        }
      }
    }
    for (int i = 0; i < 2 * n - 1; i++) {
      out << cochilie[p - 1][i] << " ";
    }
    out << "\n";
  }
  return 0;
}
