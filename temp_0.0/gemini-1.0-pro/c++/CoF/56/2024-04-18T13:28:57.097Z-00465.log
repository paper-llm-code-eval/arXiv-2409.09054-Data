#include <fstream>
#include <vector>
#include <queue>
#include <limits>

using namespace std;

struct Edge {
    int to, weight;
};

struct State {
    int node, heads, time;
    bool operator<(const State& other) const {
        return time > other.time;
    }
};

int main() {
    ifstream in("zmeu.in");
    ofstream out("zmeu.out");

    int n, p, k;
    in >> n >> p >> k;

    vector<vector<Edge>> graph(p + 1);
    vector<pair<int, int>> durations(p + 1);
    for (int i = 1; i <= p; i++) {
        int d, c;
        in >> d >> c;
        durations[i] = {d, c};
    }

    for (int i = 0; i < k; i++) {
        int a, b;
        in >> a >> b;
        graph[a].push_back({b, 0});
    }

    priority_queue<State> pq;
    pq.push({1, n, 0});

    vector<int> dist(p + 1, numeric_limits<int>::max());
    dist[1] = 0;

    while (!pq.empty()) {
        State current = pq.top();
        pq.pop();

        if (current.node == p) {
            out << current.time << endl;
            return 0;
        }

        if (current.time > dist[current.node]) {
            continue;
        }

        for (Edge edge : graph[current.node]) {
            int newTime = current.time + durations[edge.to].first;
            int newHeads = current.heads - durations[edge.to].second;
            if (newHeads >= 1 && newTime < dist[edge.to]) {
                dist[edge.to] = newTime;
                pq.push({edge.to, newHeads, newTime});
            }
        }
    }

    out << -1 << endl;
    return 0;
}
