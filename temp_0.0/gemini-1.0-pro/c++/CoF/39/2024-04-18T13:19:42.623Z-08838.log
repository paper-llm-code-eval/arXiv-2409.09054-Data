#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("suma.in");
ofstream fout("suma.out");

const int MAXN = 63365;
const int MAXM = 320;

int n, m, c[MAXN], dp[MAXN][MAXM], p[MAXN][MAXM];
vector<int> sol;

int main() {
    fin >> n;
    for (int i = 1; i <= n; i++) {
        fin >> c[i];
    }
    m = 1;
    while (m * m < n) {
        m++;
    }
    for (int i = 1; i <= n; i++) {
        dp[i][1] = c[i];
    }
    for (int j = 2; j <= m; j++) {
        for (int i = 1; i <= n; i++) {
            int x = i - (j - 1) * (j - 1);
            if (x > 0) {
                dp[i][j] = dp[x][j - 1] + c[i];
                p[i][j] = x;
            }
            for (int k = 1; k < j; k++) {
                if (dp[i][j] > dp[i - k][j - 1] + c[i]) {
                    dp[i][j] = dp[i - k][j - 1] + c[i];
                    p[i][j] = i - k;
                }
            }
        }
    }
    int min_sum = dp[n][m];
    int pos = n;
    for (int j = m; j >= 1; j--) {
        sol.push_back(pos);
        pos = p[pos][j];
    }
    reverse(sol.begin(), sol.end());
    fout << m << ' ' << min_sum << '\n';
    for (int x : sol) {
        fout << x << ' ';
    }
    fout << '\n';
    return 0;
}
