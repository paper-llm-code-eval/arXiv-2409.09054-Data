            int q = (i - 1) % m + 1;
            if (p > 1) {
                dp[i][j] = min(dp[i][j], dp[i - m][j - 1] + c[i]);
            }
            if (q > 1) {
                dp[i][j] = min(dp[i][j], dp[i - 1][j - 1] + c[i]);
            }
            if (p < j) {
                dp[i][j] = min(dp[i][j], dp[i + m][j - 1] + c[i]);
            }
            if (q < j) {
                dp[i][j] = min(dp[i][j], dp[i + 1][j - 1] + c[i]);
            }
        }
    }
    int min_sum = dp[1][m];
    for (int i = 2; i <= n; i++) {
        if (dp[i][m] < min_sum) {
            min_sum = dp[i][m];
        }
    }
    fout << m << " " << min_sum << endl;
    int i = 1, j = m;
    while (j > 0) {
        int p = (i - 1) / m + 1;
        int q = (i - 1) % m + 1;
        int min_cost = dp[i][j];
        if (p > 1 && dp[i - m][j - 1] + c[i] == min_cost) {
            i -= m;
        } else if (q > 1 && dp[i - 1][j - 1] + c[i] == min_cost) {
            i -= 1;
        } else if (p < j && dp[i + m][j - 1] + c[i] == min_cost) {
            i += m;
        } else if (q < j && dp[i + 1][j - 1] + c[i] == min_cost) {
            i += 1;
        }
        sol.push_back(i);
        j--;
    }
    reverse(sol.begin(), sol.end());
    for (int i = 0; i < sol.size(); i++) {
        fout << sol[i] << " ";
    }
    fout << endl;
    return 0;
}
