#include <fstream>
#include <vector>
#include <algorithm>
#include <cmath>

using namespace std;

struct Point {
    int x, y;
    Point() : x(0), y(0) {}
    Point(int x, int y) : x(x), y(y) {}
    Point operator+(const Point& p) const { return Point(x + p.x, y + p.y); }
    Point operator-(const Point& p) const { return Point(x - p.x, y - p.y); }
    int dot(const Point& p) const { return x * p.x + y * p.y; }
    int cross(const Point& p) const { return x * p.y - y * p.x; }
    double dist(const Point& p) const { return sqrt((x - p.x) * (x - p.x) + (y - p.y) * (y - p.y)); }
};

struct Segment {
    Point p1, p2;
    Segment() : p1(Point()), p2(Point()) {}
    Segment(const Point& p1, const Point& p2) : p1(p1), p2(p2) {}
    double dist(const Point& p) const {
        Point v1 = p2 - p1, v2 = p - p1;
        double d1 = v1.dot(v2), d2 = v1.dot(v1);
        if (d1 < 0) return p.dist(p1);
        if (d1 > d2) return p.dist(p2);
        Point h = p1 + v1 * (d1 / d2);
        return p.dist(h);
    }
};

struct Pole {
    Point p;
    int d;
    Pole() : p(Point()), d(0) {}
    Pole(const Point& p, int d) : p(p), d(d) {}
};

int n;
vector<Pole> poles;
vector<Segment> segments;

double area() {
    double sum = 0;
    for (int i = 0; i < n; i++) {
        sum += segments[i].p1.cross(segments[i].p2);
    }
    return abs(sum) / 2.0;
}

double maxArea() {
    double maxArea = area();
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            if (i == j) continue;
            Point v = poles[j].p - poles[i].p;
            if (v.dist(Point()) <= poles[i].d) {
                segments[i].p2 = poles[j].p;
                double newArea = area();
                if (newArea > maxArea) maxArea = newArea;
                segments[i].p2 = segments[i].p1 + v.dot(segments[i].p2 - segments[i].p1) / v.dot(v) * v;
                newArea = area();
                if (newArea > maxArea) maxArea = newArea;
            }
        }
    }
    return maxArea;
}

int main() {
    ifstream in("mosia.in");
    ofstream out("mosia.out");
    in >> n;
    poles.resize(n);
    segments.resize(n);
    for (int i = 0; i < n; i++) {
        in >> poles[i].p.x >> poles[i].p.y >> poles[i].d;
        segments[i] = Segment(poles[i].p, poles[i].p);
    }
    out << fixed << setprecision(4) << maxArea() << endl;
    return 0;
}
