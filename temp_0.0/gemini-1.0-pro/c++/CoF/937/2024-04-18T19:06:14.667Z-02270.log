#include <fstream>
#include <vector>
#include <queue>

using namespace std;

const int MAXN = 1000;
const int MAXM = 20000;
const int MAXP = 150000;

int N, M, X, C;
vector<vector<int>> G;
vector<int> dist;
vector<bool> viz;

int main() {
  ifstream fin("tunel.in");
  ofstream fout("tunel.out");

  fin >> C >> N >> M >> X;
  G.resize(N + 1);
  dist.resize(N + 1, -1);
  viz.resize(N + 1, false);

  for (int i = 1; i < N; i++) {
    int P;
    fin >> P;
    for (int j = 0; j < P; j++) {
      int x;
      fin >> x;
      G[i].push_back(x);
      G[i + 1].push_back(x);
    }
  }

  queue<int> Q;
  Q.push(X);
  dist[X] = 0;
  viz[X] = true;

  while (!Q.empty()) {
    int u = Q.front();
    Q.pop();

    for (int v : G[u]) {
      if (!viz[v]) {
        Q.push(v);
        dist[v] = dist[u] + 1;
        viz[v] = true;
      }
    }
  }

  if (C == 1) {
    fout << dist[N] << '\n';
  } else {
    fout << dist[N] * M + N - 1 << '\n';
  }

  return 0;
}
