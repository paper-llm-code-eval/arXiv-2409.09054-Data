#include <fstream>
#include <vector>
#include <queue>
#include <utility>

using namespace std;

const int MAX_N = 250;
const int MAX_P = 5000;

int n, p;
int grid[MAX_N][MAX_N];
int dist[MAX_N][MAX_N];
bool visited[MAX_N][MAX_N];
vector<pair<int, int>> start_pos;
vector<pair<int, int>> end_pos;

int dx[] = {0, 0, 1, -1};
int dy[] = {1, -1, 0, 0};

bool valid(int x, int y) {
    return x >= 1 && x <= n && y >= 1 && y <= n;
}

int bfs(int sx, int sy, int ex, int ey) {
    queue<pair<int, int>> q;
    q.push({sx, sy});
    visited[sx][sy] = true;
    dist[sx][sy] = 0;

    while (!q.empty()) {
        int x = q.front().first;
        int y = q.front().second;
        q.pop();

        if (x == ex && y == ey) {
            return dist[x][y];
        }

        for (int i = 0; i < 4; i++) {
            int nx = x + dx[i];
            int ny = y + dy[i];

            if (valid(nx, ny) && !visited[nx][ny] && grid[nx][ny] == 0) {
                q.push({nx, ny});
                visited[nx][ny] = true;
                dist[nx][ny] = dist[x][y] + 1;
            }
        }
    }

    return -1;
}

int main() {
    ifstream fin("poarta.in");
    ofstream fout("poarta.out");

    fin >> p;
    n = 250;

    for (int i = 0; i < p; i++) {
        int sx, sy, ex, ey;
        fin >> sx >> sy >> ex >> ey;
        start_pos.push_back({sx, sy});
        end_pos.push_back({ex, ey});
        grid[sx][sy] = 1;
        grid[ex][ey] = 1;
    }

    int ans = 0;
    for (int i = 0; i < p; i++) {
        int sx = start_pos[i].first;
        int sy = start_pos[i].second;
        int ex = end_pos[i].first;
        int ey = end_pos[i].second;

        if (sx == ex && sy == ey) {
            continue;
        }

        int dist_to_end = bfs(sx, sy, ex, ey);
        if (dist_to_end == -1) {
            fout << -1 << endl;
            return 0;
        }
        ans += dist_to_end;
    }

    fout << ans << endl;

    return 0;
}
