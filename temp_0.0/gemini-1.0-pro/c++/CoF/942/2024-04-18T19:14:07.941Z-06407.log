  for (int i = 0; i < n; i++) {
    in >> v[i];
  }
  if (c == 1) {
    int k = v[0] % 10;
    int sum = 0;
    for (int i = n - k; i < n; i++) {
      sum += v[i];
    }
    out << sum << endl;
  } else {
    int sum = 0;
    int max_sum = 0;
    for (int i = n - 1; i >= 0; i--) {
      sum += v[i];
      max_sum = max(max_sum, sum);
      sum -= v[i - n + i];
    }
    out << max_sum << endl;
  }
  return 0;
}
