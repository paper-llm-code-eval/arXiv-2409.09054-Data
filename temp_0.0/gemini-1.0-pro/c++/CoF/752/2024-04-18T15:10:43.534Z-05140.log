#include <fstream>

using namespace std;

int main() {
  ifstream in("cifre.in");
  ofstream out("cifre.out");
  long long n, c;
  in >> n >> c;
  long long ans = 0;
  for (long long i = 1; i <= n; i++) {
    long long x = i;
    while (x > 0) {
      if (x % 10 == c) ans++;
      x /= 10;
    }
  }
  out << ans << endl;
  return 0;
}
```
