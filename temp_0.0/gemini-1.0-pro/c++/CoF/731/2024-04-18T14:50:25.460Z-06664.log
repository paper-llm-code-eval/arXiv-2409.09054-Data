    vector<vector<int>> trasee(k);
    for (int i = 0; i < k; i++) {
        int r;
        in >> r;
        trasee[i].push_back(0);
        for (int j = 0; j < r; j++) {
            int d;
            in >> d;
            trasee[i].push_back(d);
        }
        trasee[i].push_back(trasee[i].back() + 1);
    }

    int min_popasuri = k + 1;
    int traseu_ales = -1;
    for (int i = 0; i < k; i++) {
        int popasuri = 0;
        int pozitie = 0;
        while (pozitie < trasee[i].size() - 1) {
            int distanta_pana_la_urmatorul_izvor = trasee[i][pozitie + 1] - trasee[i][pozitie];
            if (distanta_pana_la_urmatorul_izvor > u) {
                popasuri++;
                pozitie = lower_bound(trasee[i].begin(), trasee[i].end(), trasee[i][pozitie] + t) - trasee[i].begin();
            } else {
                pozitie++;
            }
