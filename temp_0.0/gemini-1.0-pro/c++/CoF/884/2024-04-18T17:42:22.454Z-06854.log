#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAXN = 100;
const int MAXK = 25;

int n, k, c;
int a[MAXN][MAXN];
int b[MAXN][MAXN];
int v[MAXK][MAXN * MAXN];
int dx[] = {0, 1, 1, 1, 0, -1, -1, -1};
int dy[] = {1, 1, 0, -1, -1, -1, 0, 1};
int t, s, mx;

int main() {
    ifstream fin("joc.in");
    ofstream fout("joc.out");
    fin >> c >> n >> k;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n; j++) {
            a[i][j] = i * n - n + j;
        }
    }
    for (int i = 1; i <= k; i++) {
        for (int j = 1; j <= n * n; j++) {
            fin >> v[i][j];
            b[v[i][j] / n + 1][v[i][j] % n + 1] = (i % 2 == 1 ? 1 : 2);
        }
        for (int j = 1; j <= n; j++) {
            for (int l = 1; l <= n; l++) {
                if (b[j][l] == 0) {
                    continue;
                }
                int cnt = 1;
                for (int d = 0; d < 8; d++) {
                    int x = j + dx[d];
                    int y = l + dy[d];
                    while (x >= 1 && x <= n && y >= 1 && y <= n && b[x][y] == b[j][l]) {
                        cnt++;
                        x += dx[d];
                        y += dy[d];
                    }
                }
                if (cnt >= n) {
                    if (b[j][l] == 1) {
                        t++;
                    } else {
                        s++;
                    }
                    for (int d = 0; d < 8; d++) {
                        int x = j + dx[d];
                        int y = l + dy[d];
                        while (x >= 1 && x <= n && y >= 1 && y <= n && b[x][y] == b[j][l]) {
                            b[x][y] = 0;
                            x += dx[d];
                            y += dy[d];
                        }
                    }
                }
            }
        }
        mx = max(mx, i);
        for (int j = 1; j <= n; j++) {
            for (int l = 1; l <= n; l++) {
                b[j][l] = 0;
            }
        }
    }
    if (c == 1) {
        fout << t << " " << s << "\n";
    } else {
        fout << mx << "\n";
    }
    return 0;
}
