#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("scara.in");
ofstream fout("scara.out");

int main() {
    int H, N, M, p;
    fin >> H >> N >> M >> p;

    vector<int> dp(H + 1, 1e9);
    dp[0] = 0;

    for (int i = 1; i <= H; i++) {
        for (int j = 1; j <= min(i, M); j++) {
            dp[i] = min(dp[i], dp[i - j] + (i - j) / j + p);
        }
    }

    vector<int> ans;
    int i = H;
    while (i > 0) {
        for (int j = 1; j <= min(i, M); j++) {
            if (dp[i] == dp[i - j] + (i - j) / j + p) {
                ans.push_back(i - j);
                i -= j;
                break;
            }
        }
    }

    fout << fixed << setprecision(2) << dp[H] << '\n';
    for (int i = ans.size() - 1; i >= 0; i--) {
        fout << ans[i] << ' ';
    }

    return 0;
}
```
