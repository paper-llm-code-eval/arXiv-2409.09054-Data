#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("caps.in");
ofstream fout("caps.out");

const int MAX_K = 100000;
const int MAX_Q = 50000;
const int MAX_N = 1e18;

int k, q;
string s;
vector<int> p;
vector<int> cnt;

void pre() {
    p.resize(MAX_K * 4);
    cnt.resize(MAX_K * 4);
    int n = s.size();
    for (int i = 0; i < n; i++) {
        p[i] = i;
        cnt[i] = 1;
    }
    for (int i = n; i < n * 4; i++) {
        p[i] = -1;
        cnt[i] = 0;
    }
    for (int i = 1; i < n * 4; i++) {
        int j = p[i - 1];
        while (j >= 0 && s[i % n] != s[j % n]) {
            j = p[j];
        }
        p[i] = j + 1;
        cnt[i] = cnt[j] + 1;
    }
}

int main() {
    fin >> k >> q;
    fin >> s;
    pre();
    for (int i = 0; i < q; i++) {
        long long n;
        fin >> n;
        n--;
        int pos = n % (k * 3);
        if (pos < k) {
            fout << s[pos] << ' ' << n / (k * 3) + 1 << '\n';
        } else if (pos < k * 2) {
            pos -= k;
            int j = p[pos];
            while (j >= 0 && s[pos] != s[j]) {
                j = p[j];
            }
            fout << s[j + 1] << ' ' << n / (k * 3) + cnt[j] + 1 << '\n';
        } else {
            pos -= k * 2;
            fout << s[pos] << ' ' << n / (k * 3) + 1 << '\n';
        }
    }
    return 0;
}
