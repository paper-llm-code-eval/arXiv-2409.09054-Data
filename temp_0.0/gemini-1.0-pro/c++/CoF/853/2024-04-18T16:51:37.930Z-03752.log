            cnt = 0;
            for (j = 1; j <= s; j++) {
                if (a[i][j] == 1) {
                    cnt++;
                }
            }
            if (cnt == 0) {
                out << 0 << " ";
                continue;
            }
            vector<int> b;
            for (j = 1; j <= s; j++) {
                if (a[i][j] == 1) {
                    b.push_back(j);
