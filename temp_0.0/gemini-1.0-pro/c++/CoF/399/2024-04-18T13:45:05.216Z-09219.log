                if (x % j == 0) {
                    int y = x / j;
                    if (y > j) {
                        int aux = y;
                        y = j;
                        j = aux;
                    }
                    int aux = ans;
                    ans = ans / j * (j + 1);
                    j = aux;
                }
            }
            fout << ans << ' ';
        }
        fout << '\n';
    }
    return 0;
}
