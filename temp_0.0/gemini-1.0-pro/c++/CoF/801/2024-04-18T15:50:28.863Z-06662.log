#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

struct Point {
    int x, y;
};

int main() {
    ifstream in("cladiri.in");
    ofstream out("cladiri.out");

    int cx, cy, intensity;
    in >> cx >> cy >> intensity;

    vector<Point> buildings;
    int strength;
    while (in >> cx >> cy >> strength) {
        buildings.push_back({cx, cy});
    }

    int n = buildings.size();
    vector<int> affected(n, 0);

    for (int i = 0; i < n; i++) {
        int dx = abs(buildings[i].x - cx);
        int dy = abs(buildings[i].y - cy);
        int level = max(dx, dy);
        if (level <= intensity) {
            affected[i] = 1;
        }
    }

    int total = 0;
    for (int i = 0; i < n; i++) {
        total += affected[i];
    }

    int max_level = 0;
    int max_count = 0;
    for (int level = 0; level <= intensity; level++) {
        int count = 0;
        for (int i = 0; i < n; i++) {
            if (affected[i] && max(abs(buildings[i].x - cx), abs(buildings[i].y - cy)) == level) {
                count++;
            }
        }
        if (count > max_count) {
            max_count = count;
            max_level = level;
        }
    }

    out << total << endl;
    out << max_count << endl;
    for (int level = 0; level <= intensity; level++) {
        int count = 0;
        for (int i = 0; i < n; i++) {
            if (affected[i] && max(abs(buildings[i].x - cx), abs(buildings[i].y - cy)) == level) {
                count++;
            }
        }
        if (count == max_count) {
            out << level << " ";
        }
    }

    return 0;
}
