#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream in("numere.in");
  ofstream out("numere.out");
  int a, b;
  in >> a >> b;
  vector<int> va, vb;
  while (a) {
    va.push_back(a % 2);
    a /= 2;
  }
  while (b) {
    vb.push_back(b % 2);
    b /= 2;
  }
  int c = 0, d = 0, p = 1;
  while (va.size() && vb.size()) {
    if (va.back() == vb.back()) {
      va.pop_back();
      vb.pop_back();
    } else {
      c += va.back() * p;
      d += vb.back() * p;
      p *= 2;
      va.pop_back();
      vb.pop_back();
    }
  }
  while (va.size()) {
    c += va.back() * p;
    va.pop_back();
    p *= 2;
  }
  while (vb.size()) {
    d += vb.back() * p;
    vb.pop_back();
    p *= 2;
  }
  out << c + d << '\n';
  return 0;
}
