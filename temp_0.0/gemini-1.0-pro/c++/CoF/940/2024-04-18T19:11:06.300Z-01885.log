#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("tort.in");
ofstream fout("tort.out");

const int NMAX = 200000;
const int MOD = 1e9 + 7;

int n, a[NMAX + 5], sum, dp[NMAX + 5][400005];
vector<int> v;

int main() {
    fin >> n;
    for (int i = 1; i <= n; ++i) {
        fin >> a[i];
        sum += a[i];
    }
    if (sum % n) {
        fout << 0;
        return 0;
    }
    int avg = sum / n;
    for (int i = 1; i <= n; ++i) {
        for (int j = 0; j <= sum; ++j) {
            dp[i][j] = dp[i - 1][j];
            if (j >= a[i]) {
                dp[i][j] = (dp[i][j] + dp[i - 1][j - a[i]]) % MOD;
            }
            if (j == avg) {
                v.push_back(dp[i][j]);
            }
        }
    }
    sort(v.begin(), v.end());
    int ans = 0;
    for (int i = 0; i < v.size(); ++i) {
        int pos = lower_bound(v.begin(), v.end(), sum - v[i]) - v.begin();
        if (pos < v.size() && v[pos] == sum - v[i]) {
            ans = (ans + 1LL * v[i] * (v[pos] - (i == pos))) % MOD;
        }
    }
    fout << ans;
    return 0;
}
```
