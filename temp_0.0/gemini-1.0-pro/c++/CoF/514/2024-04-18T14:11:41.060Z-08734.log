#include <fstream>
#include <vector>
#include <algorithm>
#include <cmath>

using namespace std;

const int MAXN = 1e6 + 5;
const int MAXQ = 2e5 + 5;

int n, q, c;
int a[MAXN];
int p[MAXN];
int cost[MAXN];
int dp[MAXN][2];
int ans[MAXQ];

vector<int> primes;

void sieve() {
    for (int i = 2; i < MAXN; i++) {
        if (p[i] == 0) {
            primes.push_back(i);
            for (int j = i + i; j < MAXN; j += i) {
                p[j] = i;
            }
        }
    }
}

int get_cost(int x) {
    int l = 0, r = primes.size() - 1;
    while (l <= r) {
        int mid = (l + r) / 2;
        if (primes[mid] <= x) {
            l = mid + 1;
        } else {
            r = mid - 1;
        }
    }
    if (l == 0) {
        return x - 2;
    }
    int cost1 = x - primes[l - 1];
    int cost2 = primes[l] - x;
    return min(cost1, cost2);
}

void preprocess() {
    for (int i = 1; i <= n; i++) {
        cost[i] = get_cost(a[i]);
    }
    dp[n + 1][0] = dp[n + 1][1] = 0;
    for (int i = n; i >= 1; i--) {
        dp[i][0] = min(dp[i + 1][0], dp[i + 1][1] + cost[i]);
        dp[i][1] = min(dp[i + 1][0] + cost[i], dp[i + 1][1]);
    }
}

int main() {
    ifstream fin("primprim.in");
    ofstream fout("primprim.out");

    fin >> c >> n;
    for (int i = 1; i <= n; i++) {
        fin >> a[i];
    }
    if (c == 1) {
        sieve();
        preprocess();
        fout << dp[1][0] << "\n";
    } else {
        fin >> q;
        sieve();
        preprocess();
        for (int i = 1; i <= q; i++) {
            int x, y, z;
            fin >> x >> y >> z;
            a[x] = y;
            preprocess();
            ans[i] = dp[z][0];
        }
        for (int i = 1; i <= q; i++) {
            fout << ans[i] << "\n";
        }
    }

    return 0;
}
