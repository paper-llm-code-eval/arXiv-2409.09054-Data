#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAXN = 50005;
const int MOD = 666013;

int n, c, t, ans, nr, pos;
char s[MAXN], r[MAXN];
vector<int> v[26];

int main() {
    ifstream fin("circular.in");
    ofstream fout("circular.out");
    fin >> c;
    fin >> s;
    fin >> r;
    n = strlen(s);
    for (int i = 0; i < n; i++) {
        v[s[i] - 'A'].push_back(i);
    }
    for (int i = 0; i < 26; i++) {
        if (v[i].size()) {
            t += min(v[i][0], n - v[i][0]);
            pos = v[i][0];
        }
    }
    fout << t << '\n';
    if (c == 2) {
        for (int i = 0; i < 26; i++) {
            if (v[i].size()) {
                for (int j = 0; j < v[i].size(); j++) {
                    if (v[i][j] > pos) {
                        ans += min(v[i][j] - pos, n - v[i][j] + pos);
                    } else {
                        ans += min(pos - v[i][j], n - pos + v[i][j]);
                    }
                    pos = v[i][j];
                    r[pos] = i + 'A';
                }
            }
        }
        fout << ans << '\n';
        nr = 1;
        for (int i = 1; i < n; i++) {
            if (r[i] != r[i - 1]) {
                nr++;
            }
        }
        fout << nr % MOD << '\n';
        for (int i = 0; i < n; i++) {
            fout << r[i];
        }
        fout << '\n';
    }
    return 0;
}
