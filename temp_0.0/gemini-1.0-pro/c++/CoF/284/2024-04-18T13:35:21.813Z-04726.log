    ifstream fin("circular.in");
    ofstream fout("circular.out");
    fin >> c;
    fin >> s;
    fin >> r;
    n = strlen(s);
    for (int i = 0; i < n; i++) {
        v[s[i] - 'A'].push_back(i);
    }
    for (int i = 0; i < 26; i++) {
        if (v[i].size()) {
            t += min(v[i][0], n - v[i][0]);
            pos = v[i][0];
        }
    }
    fout << t << '\n';
    if (c == 2) {
        dp[0] = 0;
        for (int i = 1; i <= n; i++) {
            dp[i] = dp[i - 1] + 1;
            for (int j = 0; j < 26; j++) {
                if (v[j].size() && v[j][0] < i) {
                    dp[i] = min(dp[i], dp[v[j][0]] + min(i - v[j][0], n - i + v[j][0]));
                }
            }
        }
        ans = dp[n];
        fout << ans << '\n';
        nr = 1;
        for (int i = 1; i < n; i++) {
            if (dp[i] != dp[i - 1]) {
                nr++;
            }
        }
        fout << nr % MOD << '\n';
        pos = n;
        for (int i = n - 1; i >= 0; i--) {
            if (dp[i] != dp[i + 1]) {
                pos = i;
            }
            for (int j = 0; j < 26; j++) {
                if (v[j].size() && v[j][0] < pos && dp[i] == dp[v[j][0]] + min(pos - v[j][0], n - pos + v[j][0])) {
                    r[i] = j + 'A';
                    pos = v[j][0];
                    break;
                }
            }
        }
        for (int i = 0; i < n; i++) {
            fout << r[i];
        }
        fout << '\n';
    }
    return 0;
}
