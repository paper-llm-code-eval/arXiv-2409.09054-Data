#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("divizor.in");
    ofstream out("divizor.out");
    int n, m = 0, nr_div[1000001] = {0}, max_div = 0, max_nr_div = 0, nr_max_div = 0;
    vector<int> v;
    in >> n;
    v.push_back(n);
    while (n > 0) {
        m++;
        n /= 10;
    }
    for (int i = 0; i < m; i++) {
        int nr = 0, p = 1;
        for (int j = m - 1; j >= 0; j--) {
            nr += v[i] % 10 * p;
            p *= 10;
            v[i] /= 10;
        }
        v.push_back(nr);
    }
    for (int i = 0; i < m; i++) {
        for (int j = 1; j * j <= v[i]; j++) {
            if (v[i] % j == 0) {
                nr_div[v[i]]++;
                if (j * j != v[i]) {
                    nr_div[v[i] / j]++;
                }
            }
        }
        if (nr_div[v[i]] > max_nr_div) {
            max_nr_div = nr_div[v[i]];
            max_div = v[i];
            nr_max_div = 1;
        } else if (nr_div[v[i]] == max_nr_div) {
            nr_max_div++;
            if (v[i] > max_div) {
                max_div = v[i];
            }
        }
    }
    if (nr_max_div > 1) {
        for (int i = 0; i < m; i++) {
            if (nr_div[v[i]] == max_nr_div) {
                out << v[i] << " ";
            }
        }
    } else {
        out << max_div << " ";
    }
    out << endl;
    for (int i = 2; i <= 1000000; i++) {
        bool prim = true;
        for (int j = 2; j * j <= i; j++) {
            if (i % j == 0) {
                prim = false;
                break;
            }
        }
        if (prim) {
            for (int j = 0; j < m; j++) {
                if (v[j] % i == 0) {
                    out << i << endl;
                    return 0;
                }
            }
        }
    }
    out << 0 << endl;
    return 0;
}
