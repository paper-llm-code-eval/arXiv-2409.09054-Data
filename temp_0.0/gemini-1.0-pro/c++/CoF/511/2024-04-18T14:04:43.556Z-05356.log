    vector<int> v(n);
    for (int i = 0; i < n; i++) {
        in >> v[i];
    }

    if (c == 1) {
        sort(v.begin(), v.end());
        int freq = 1, max_freq = 1;
        int curr = v[0], ans = v[0];
        for (int i = 1; i < n; i++) {
            if (v[i] == curr) {
                freq++;
            } else {
                if (freq > max_freq) {
                    max_freq = freq;
                    ans = curr;
                } else if (freq == max_freq) {
                    ans = min(ans, curr);
                }
                freq = 1;
                curr = v[i];
            }
        }
        if (freq > max_freq) {
            ans = curr;
        } else if (freq == max_freq) {
            ans = min(ans, curr);
        }
        out << ans << endl;
    } else {
        int sum1 = 0, sum2 = 0;
        int min_diff = 1e9;
        for (int i = 0; i < n; i++) {
            sum1 += v[i];
            sum2 += v[n - i - 1];
            min_diff = min(min_diff, abs(sum1 - sum2));
        }
        out << min_diff << endl;
    }

    return 0;
}
