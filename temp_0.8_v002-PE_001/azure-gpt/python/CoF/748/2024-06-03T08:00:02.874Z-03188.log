import numpy as np
from collections import Counter

def read_identification_numbers(file_path):
    with open(file_path, 'r') as file:
        n = int(file.readline().strip())
        identification_numbers = [file.readline().strip() for _ in range(n)]
    return identification_numbers

def write_results(file_path, num_platoons, max_platoon_size, num_max_platoons, max_platoon_composition):
    with open(file_path, 'w') as file:
        file.write(f"{num_platoons}\n")
        file.write(f"{max_platoon_size}\n")
        file.write(f"{num_max_platoons}\n")
        file.write(" ".join(max_platoon_composition) + "\n")

def create_sorted_key(number):
    return ''.join(sorted(number))

def process_identification_numbers(identification_numbers):
    sorted_keys = [create_sorted_key(num) for num in identification_numbers]
    platoon_counter = Counter(sorted_keys)
    
    num_platoons = len(platoon_counter.keys())
    max_platoon_size = max(platoon_counter.values())
    num_max_platoons = sum(1 for size in platoon_counter.values() if size == max_platoon_size)
    
    # Finding the composition of one of the platoons with max size
    max_key = next(key for key, size in platoon_counter.items() if size == max_platoon_size)
    max_platoon_composition = [num for num in identification_numbers if create_sorted_key(num) == max_key]

    return num_platoons, max_platoon_size, num_max_platoons, max_platoon_composition

# Read data
identification_numbers = read_identification_numbers('pluton.in')

# Process data
num_platoons, max_platoon_size, num_max_platoons, max_platoon_composition = process_identification_numbers(identification_numbers)

# Write results
write_results('pluton.out', num_platoons, max_platoon_size, num_max_platoons, max_platoon_composition)
```

