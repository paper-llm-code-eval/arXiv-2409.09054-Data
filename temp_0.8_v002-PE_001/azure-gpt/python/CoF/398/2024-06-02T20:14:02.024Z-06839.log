def get_neighbors(x, y, max_x, max_y):
    neighbors = []
    if x > 0:
        neighbors.append((x - 1, y))
    if x < max_x - 1:
        neighbors.append((x + 1, y))
    if y > 0:
        neighbors.append((x, y - 1))
    if y < max_y - 1:
        neighbors.append((x, y + 1))
    return neighbors

# Function to perform a BFS and find the minimum bridge length
def find_min_bridge(archipelago, n, m):
    queue = []
    min_length = np.full_like(archipelago, np.inf)
    for i in range(n):
        for j in range(m):
            if archipelago[i, j] == 1:
                for ni, nj in get_neighbors(i, j, n, m):
                    if archipelago[ni, nj] == 0:
