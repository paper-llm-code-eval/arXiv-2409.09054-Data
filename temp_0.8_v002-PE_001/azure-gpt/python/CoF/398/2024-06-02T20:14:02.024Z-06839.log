import numpy as np
from scipy.ndimage import label
from scipy.spatial.distance import cityblock

# Read the data from the input file
with open("insule.in", "r") as file:
    n, m = map(int, file.readline().split())
    archipelago = np.array([list(map(int, list(file.readline().strip()))) for _ in range(n)])

# Function to find the minimum bridge length
def find_min_bridge(archipelago, water, country_R, country_G):
    min_distance = float('inf')
    for r_coord in np.argwhere(archipelago == country_R):
        for g_coord in np.argwhere(archipelago == country_G):
            # Calculate the Manhattan distance and subtract 1 because it counts both ends
            distance = cityblock(r_coord, g_coord) - 1
            if distance < min_distance:
                # Check if the line between the two coordinates crosses only water
                rr, cc = r_coord[0], r_coord[1]
                gr, gc = g_coord[0], g_coord[1]
                if rr == gr:  # Same row
                    if np.all(archipelago[rr, min(cc, gc)+1:max(cc, gc)] == water):
                        min_distance = distance
                elif cc == gc:  # Same column
                    if np.all(archipelago[min(rr, gr)+1:max(rr, gr), cc] == water):
                        min_distance = distance
    return min_distance if min_distance != float('inf') else 0

# Label the islands and find the number of islands for each country
structure = np.ones((3, 3), dtype=int)  # Define structure for 4-connectivity
_, num_islands_R = label(archipelago == 1, structure=structure)
_, num_islands_G = label(archipelago == 2, structure=structure)
_, num_islands_B = label(archipelago == 3, structure=structure)

# Find the minimum bridge length
min_bridge_length = find_min_bridge(archipelago, 0, 1, 2)

# Write the results into the output file
with open("insule.out", "w") as file:
    file.write(f"{num_islands_R} {num_islands_G} {num_islands_B} {min_bridge_length}\n")
