import numpy as np
from scipy.sparse import csr_matrix
from scipy.sparse.csgraph import shortest_path

# Define the function to count islands
def count_islands(matrix, land_value):
    visited = np.zeros_like(matrix, dtype=bool)
    n_islands = 0
    directions = [(0, 1), (1, 0), (-1, 0), (0, -1)]

    def dfs(i, j):
        if visited[i, j] or matrix[i, j] != land_value:
            return
        visited[i, j] = True
        for di, dj in directions:
            ni, nj = i + di, j + dj
            if 0 <= ni < matrix.shape[0] and 0 <= nj < matrix.shape[1]:
                dfs(ni, nj)

    for i in range(matrix.shape[0]):
        for j in range(matrix.shape[1]):
            if matrix[i, j] == land_value and not visited[i, j]:
                n_islands += 1
                dfs(i, j)

    return n_islands

# Define the function to calculate distances from all water positions to land positions
def calculate_distances(matrix, land_value):
    n, m = matrix.shape
    water_positions = np.where(matrix == 0)
    land_positions = np.where(matrix == land_value)
    distances = np.full((n, m), np.inf)
    for i, j in zip(*water_positions):
        for ii, jj in zip(*land_positions):
            distance = abs(i - ii) + abs(j - jj) - 1
            distances[i, j] = min(distances[i, j], distance)
    return distances

# Read input from file
with open('insule.in', 'r') as file:
    n, m = map(int, file.readline().split())
    matrix = np.array([list(map(int, list(file.readline().strip()))) for _ in range(n)])

# Count islands for each country
nr = count_islands(matrix, 1)
ng = count_islands(matrix, 2)
nb = count_islands(matrix, 3)

# Find the minimum bridge length
distances_r = calculate_distances(matrix, 1)
distances_g = calculate_distances(matrix, 2)
min_bridge_length = int(np.min(distances_r + distances_g))

# Write output to file
with open('insule.out', 'w') as file:
    file.write(f"{nr} {ng} {nb} {min_bridge_length}\n")
