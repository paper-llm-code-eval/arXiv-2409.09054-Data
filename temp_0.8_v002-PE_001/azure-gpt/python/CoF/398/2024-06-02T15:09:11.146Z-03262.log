import numpy as np
from scipy.sparse import csr_matrix
from scipy.sparse.csgraph import shortest_path

# Define the function to count islands
def count_islands(matrix, land_value):
    visited = np.zeros_like(matrix, dtype=bool)
    n_islands = 0
    directions = [(0, 1), (1, 0), (-1, 0), (0, -1)]

    def dfs(i, j):
        if visited[i, j] or matrix[i, j] != land_value:
            return
        visited[i, j] = True
        for di, dj in directions:
            ni, nj = i + di, j + dj
            if 0 <= ni < matrix.shape[0] and 0 <= nj < matrix.shape[1]:
                dfs(ni, nj)

    for i in range(matrix.shape[0]):
        for j in range(matrix.shape[1]):
            if matrix[i, j] == land_value and not visited[i, j]:
                n_islands += 1
                dfs(i, j)

    return n_islands

# Define the function to find the minimum bridge length
def min_bridge_length(matrix):
    n, m = matrix.shape
    graph = np.full((n * m, n * m), np.inf)
    for i in range(n):
        for j in range(m):
            if matrix[i, j] == 0:
                for di, dj in [(0, 1), (1, 0), (-1, 0), (0, -1)]:
                    ni, nj = i + di, j + dj
                    if 0 <= ni < n and 0 <= nj < m:
                        if matrix[ni, nj] == 1 or matrix[ni, nj] == 2:
                            u = i * m + j
                            v = ni * m + nj
                            graph[u, v] = 1
                            graph[v, u] = 1

    # Convert graph to CSR format
    graph_csr = csr_matrix(graph)
    # Find the shortest path between R and G islands
    min_length = np.inf
    for i in range(n * m):
        for j in range(n * m):
            if matrix[i // m, i % m] == 1 and matrix[j // m, j % m] == 2:
                distance = shortest_path(graph_csr, indices=[i, j], return_predecessors=False)
                min_length = min(min_length, distance[0, 1])

    return min_length if min_length != np.inf else 0

# Read input from file
with open('insule.in', 'r') as file:
    n, m = map(int, file.readline().split())
    matrix = np.array([list(map(int, list(file.readline().strip()))) for _ in range(n)])

# Count islands for each country
nr = count_islands(matrix, 1)
ng = count_islands(matrix, 2)
nb = count_islands(matrix, 3)

# Find the minimum bridge length
lg = min_bridge_length(matrix)

# Write output to file
with open('insule.out', 'w') as file:
    file.write(f"{nr} {ng} {nb} {lg}\n")
