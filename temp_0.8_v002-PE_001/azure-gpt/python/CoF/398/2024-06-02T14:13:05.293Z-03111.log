import numpy as np
from scipy.ndimage import label
from collections import deque

# Function to read the map from the file
def read_map(filename):
    with open(filename, 'r') as f:
        n, m = map(int, f.readline().split())
        archipelago_map = np.array([list(map(int, list(f.readline().strip()))) for _ in range(n)])
    return archipelago_map

# Function to calculate the number of islands for each country
def count_islands(archipelago_map):
    countries = [1, 2, 3]
    island_counts = []
    for country in countries:
        country_map = (archipelago_map == country).astype(int)
        labeled_map, num_features = label(country_map)
        island_counts.append(num_features)
    return island_counts

# Function to calculate the minimum bridge length
def min_bridge_length(archipelago_map):
    n, m = archipelago_map.shape
    R_coords = np.argwhere(archipelago_map == 1)
    G_coords = np.argwhere(archipelago_map == 2)
    water_map = (archipelago_map == 0)

    # Initialize the bridge length as a large number
    min_length = np.inf

    # Perform BFS from each water cell next to R-land cells and keep the shortest distance to G-land cells
    for r_coord in R_coords:
        for dr, dc in [(-1, 0), (1, 0), (0, -1), (0, 1)]:
            cell = r_coord + np.array([dr, dc])
            if 0 <= cell[0] < n and 0 <= cell[1] < m and water_map[cell[0], cell[1]]:
                visited = np.zeros_like(water_map)
                queue = deque([(cell, 0)])  # (coordinates, distance)
                while queue:
                    curr_coord, dist = queue.popleft()
                    if tuple(curr_coord) in map(tuple, G_coords):
                        min_length = min(min_length, dist)
                        break
                    for dr, dc in [(-1, 0), (1, 0), (0, -1), (0, 1)]:
                        next_cell = curr_coord + np.array([dr, dc])
                        if 0 <= next_cell[0] < n and 0 <= next_cell[1] < m and \
                                water_map[next_cell[0], next_cell[1]] and not visited[next_cell[0], next_cell[1]]:
                            visited[next_cell[0], next_cell[1]] = 1
                            queue.append((next_cell, dist+1))

    return int(min_length) if min_length != np.inf else -1

# Main program
if __name__ == "__main__":
    archipelago_map = read_map('insule.in')
    island_counts = count_islands(archipelago_map)
    bridge_length = min_bridge_length(archipelago_map)
    with open('insule.out', 'w') as f:
        f.write(f"{island_counts[0]} {island_counts[1]} {island_counts[2]} {bridge_length}")
