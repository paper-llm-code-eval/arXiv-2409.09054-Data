import numpy as np
from scipy.ndimage import label
from scipy.ndimage import distance_transform_edt


def read_input(file_path):
    with open(file_path, 'r') as file:
        n, m = map(int, file.readline().split())
        data = np.array([list(map(int, list(file.readline().strip()))) for _ in range(n)])
    return data


def write_output(file_path, counts, min_bridge_length):
    with open(file_path, 'w') as file:
        file.write(f"{counts[1]} {counts[2]} {counts[3]} {min_bridge_length}")


def count_islands(matrix):
    labels_R, num_R = label(matrix == 1)
    labels_G, num_G = label(matrix == 2)
    labels_B, num_B = label(matrix == 3)
    return {1: num_R, 2: num_G, 3: num_B}


def find_min_bridge_length(matrix):
    water = (matrix == 0)
    land_R = (matrix == 1)
    land_G = (matrix == 2)

    # Create a distance transform for islands of country R and G with water as background
    distance_R = distance_transform_edt(~water, return_indices=False, return_distances=True)
    distance_G = distance_transform_edt(~water | land_R, return_indices=False, return_distances=True)

    # Find the minimal distance between R and G, which is the length of the bridge
    min_distance = np.min(distance_R[land_G] + distance_G[land_R] - 1)
    return int(min_distance)


def main():
    matrix = read_input('insule.in')
    island_counts = count_islands(matrix)
    min_bridge_length = find_min_bridge_length(matrix)
    write_output('insule.out', island_counts, min_bridge_length)


main()
```

