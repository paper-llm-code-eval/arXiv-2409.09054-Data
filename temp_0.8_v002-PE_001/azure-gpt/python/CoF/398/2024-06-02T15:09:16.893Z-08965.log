import numpy as np
from scipy.ndimage import label
from collections import deque

# Read the input from 'insule.in'
with open('insule.in', 'r') as infile:
    n, m = map(int, infile.readline().split())
    data = np.array([list(map(int, list(infile.readline().strip()))) for _ in range(n)])

# Count islands for R, G, and B
islands_R, num_R = label(data == 1)
islands_G, num_G = label(data == 2)
islands_B, num_B = label(data == 3)

# Helper function to get neighbors that are water and not visited
def get_neighbors(x, y, visited):
    neighbors = []
    if x > 0 and data[x-1, y] == 0 and not visited[x-1, y]:
        neighbors.append((x-1, y))
    if x < n - 1 and data[x+1, y] == 0 and not visited[x+1, y]:
        neighbors.append((x+1, y))
    if y > 0 and data[x, y-1] == 0 and not visited[x, y-1]:
        neighbors.append((x, y-1))
    if y < m - 1 and data[x, y+1] == 0 and not visited[x, y+1]:
        neighbors.append((x, y+1))
    return neighbors

# BFS to find the shortest bridge
def bfs_shortest_bridge():
    visited = np.zeros_like(data, dtype=bool)
    queue = deque()
    for x in range(n):
        for y in range(m):
            if data[x, y] == 1:
                neighbors = get_neighbors(x, y, visited)
                for neighbor in neighbors:
                    visited[neighbor] = True
                    queue.append((*neighbor, 1))  # Append water tile with bridge length 1

    while queue:
        x, y, length = queue.popleft()
        if data[x, y] == 2:  # Reached country G
            return length
        neighbors = get_neighbors(x, y, visited)
        for neighbor in neighbors:
            visited[neighbor] = True
            queue.append((*neighbor, length + 1))

    return float('inf')  # If no bridge is found

# Find the shortest bridge length
shortest_bridge_length = bfs_shortest_bridge()

# Write the output to 'insule.out'
with open('insule.out', 'w') as outfile:
    outfile.write(f'{num_R} {num_G} {num_B} {shortest_bridge_length}\n')
