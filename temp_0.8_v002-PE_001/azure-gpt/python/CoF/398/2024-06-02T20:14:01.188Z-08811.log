import numpy as np
from scipy.ndimage import label
from scipy.spatial.distance import cdist

# Load the input file into a numpy array
with open('insule.in', 'r') as f:
    n, m = map(int, f.readline().split())
    map_data = np.array([list(map(int, line.strip())) for line in f])

# Label each island for countries R, G, B
islands_r, num_islands_r = label(map_data == 1)
islands_g, num_islands_g = label(map_data == 2)
islands_b, num_islands_b = label(map_data == 3)

# Find all water (0) locations
water_locs = np.column_stack(np.where(map_data == 0))

# Find the shortest bridge length
# Get the edges for islands R and G
edges_r = np.column_stack(np.where(np.isin(map_data, [0, 1]) & (np.roll(map_data, 1, axis=0) == 0) | (np.roll(map_data, -1, axis=0) == 0) | (np.roll(map_data, 1, axis=1) == 0) | (np.roll(map_data, -1, axis=1) == 0)))
edges_g = np.column_stack(np.where(np.isin(map_data, [0, 2]) & (np.roll(map_data, 1, axis=0) == 0) | (np.roll(map_data, -1, axis=0) == 0) | (np.roll(map_data, 1, axis=1) == 0) | (np.roll(map_data, -1, axis=1) == 0)))

# Ensure we only include edges that are adjacent to R and G islands
edges_r = edges_r[np.isin(islands_r[tuple(edges_r.T)], range(1, num_islands_r + 1))]
edges_g = edges_g[np.isin(islands_g[tuple(edges_g.T)], range(1, num_islands_g + 1))]

# Calculate the pairwise Manhattan distance between edges of R and G
distances = cdist(edges_r, edges_g, metric='cityblock') - 1
min_bridge_length = np.min(distances)

# Save the result to the output file
with open('insule.out', 'w') as f:
    f.write(f'{num_islands_r} {num_islands_g} {num_islands_b} {min_bridge_length}')
```

