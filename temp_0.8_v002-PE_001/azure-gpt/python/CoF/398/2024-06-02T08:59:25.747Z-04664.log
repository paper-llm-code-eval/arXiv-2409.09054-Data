import numpy as np
from scipy.ndimage import label
from scipy.spatial.distance import cdist

def parse_input(file_path):
    with open(file_path, 'r') as file:
        n, m = map(int, file.readline().split())
        data = np.array([list(map(int, list(file.readline().strip()))) for _ in range(n)])
    return data

def find_islands_and_distances(map_matrix):
    # Find islands for each country
    islands_R, num_islands_R = label(map_matrix == 1)
    islands_G, num_islands_G = label(map_matrix == 2)
    islands_B, num_islands_B = label(map_matrix == 3)

    # To calculate minimum bridge length, find perimeter coordinates for R and G islands
    perimeter_R = np.argwhere(np.pad(map_matrix == 1, 1, mode='constant') == 1)
    perimeter_G = np.argwhere(np.pad(map_matrix == 2, 1, mode='constant') == 2)
    water = np.pad(map_matrix == 0, 1, mode='constant')

    # Only consider water coordinates that are adjacent to the land
    border_R = perimeter_R[np.any(water[perimeter_R[:, 0], perimeter_R[:, 1] + 1] |
                                  water[perimeter_R[:, 0], perimeter_R[:, 1] - 1] |
                                  water[perimeter_R[:, 0] + 1, perimeter_R[:, 1]] |
                                  water[perimeter_R[:, 0] - 1, perimeter_R[:, 1]], axis=1)] - 1
    border_G = perimeter_G[np.any(water[perimeter_G[:, 0], perimeter_G[:, 1] + 1] |
                                  water[perimeter_G[:, 0], perimeter_G[:, 1] - 1] |
                                  water[perimeter_G[:, 0] + 1, perimeter_G[:, 1]] |
                                  water[perimeter_G[:, 0] - 1, perimeter_G[:, 1]], axis=1)] - 1

    # Calculate the pairwise Manhattan distances and find the minimum distance
    distances = cdist(border_R, border_G, metric='cityblock') - 1
    min_distance = int(distances.min())

    return num_islands_R, num_islands_G, num_islands_B, min_distance

def write_output(file_path, nr, ng, nb, lg):
    with open(file_path, 'w') as file:
        file.write(f"{nr} {ng} {nb} {lg}\n")

# Main logic
if __name__ == "__main__":
    map_matrix = parse_input('insule.in')
    nr, ng, nb, lg = find_islands_and_distances(map_matrix)
    write_output('insule.out', nr, ng, nb, lg)
```

This code uses NumPy for efficient array manipulation and SciPy's `cdist` function to calculate the Manhattan distances between border coordinates. It avoids using unnecessary memory by processing the map in-place and writing the output directly to a file without any intermediate large data structures.
