# Importing numpy for efficient array manipulations
import numpy as np

# Read the input from 'cladiri.in'
with open('cladiri.in', 'r') as file:
    N = int(file.readline().strip())
    buildings = [file.readline().strip() for _ in range(N)]

# Convert the buildings to numpy arrays for efficient processing
buildings = np.array([np.array(list(map(int, list(b))), dtype=np.int8) for b in buildings])

# Task 1: Find the tallest tower and the number of buildings with such a tower
max_height = buildings.max()
count_max_height = (buildings == max_height).sum()

# Task 2: Compute the number of buildings with identical numbers on both sides
# Counting digits for each height in the building from both sides
count_identical_buildings = sum(
    [int(''.join(map(str, building))) == int(''.join(map(str, building[::-1]))) for building in buildings]
)

# Task 3: Calculate the minimum number of cubes to make the numbers on the notes identical
min_cubes_to_add = sum(
    [abs(len(building) - 2 * height) for building in buildings for height in building if height * 2 <= len(building)]
)

# Write the output to 'cladiri.out'
with open('cladiri.out', 'w') as file:
    file.write(f"{max_height} {count_max_height}\n")
    file.write(f"{count_identical_buildings}\n")
    file.write(f"{min_cubes_to_add}\n")
```

