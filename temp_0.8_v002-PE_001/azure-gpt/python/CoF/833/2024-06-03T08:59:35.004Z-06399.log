# Required Libraries
import numpy as np

# Read input from 'cladiri.in'
with open('cladiri.in', 'r') as file:
    N = int(file.readline().strip())
    buildings = [int(file.readline().strip()) for _ in range(N)]

# Find the tallest tower (max digit) and count buildings with such a tower
max_height = max(max(str(building)) for building in buildings)
tallest_tower_count = sum(max_height in str(building) for building in buildings)

# Find buildings with matching numbers when read from both ends
matching_numbers_count = sum(int(str(building)[::-1]) == building for building in buildings)

# Calculate the minimum number of cubes needed to make all buildings symmetrical
def calc_min_cubes_to_symmetry(building):
    building_str = str(building)
    left = 0
    right = len(building_str) - 1
    min_cubes = 0
    while left < right:
        if building_str[left] != building_str[right]:
            min_cubes += abs(int(building_str[left]) - int(building_str[right]))
        left += 1
        right -= 1
    return min_cubes

min_cubes_required = np.sum([calc_min_cubes_to_symmetry(building) for building in buildings])

# Write output to 'cladiri.out'
with open('cladiri.out', 'w') as file:
    file.write(f"{max_height} {tallest_tower_count}\n")
    file.write(f"{matching_numbers_count}\n")
    file.write(f"{min_cubes_required}\n")
```

