import numpy as np

# Function to calculate the sum and count of grains for a given zone
def calc_zone(n, m, x, y, zone, grains):
    if zone == 1:
        return np.sum(grains[:x, :y]), x * y
    elif zone == 2:
        return np.sum(grains[:x, y-1:]), x * (m - y + 1)
    elif zone == 3:
        return np.sum(grains[x-1:, :y]), (n - x + 1) * y
    elif zone == 4:
        return np.sum(grains[x-1:, y-1:]), (n - x + 1) * (m - y + 1)
    return 0, 0

# Read data from input file
with open('cri.in', 'r') as file:
    n, m, x, y = map(int, file.readline().split())
    grains = np.array([list(map(int, file.readline().split())) for _ in range(n)])

# Calculate the best zone
best_zone = 0
max_grains = -1
min_rooms = n * m + 1

for zone in range(1, 5):
    total_grains, num_rooms = calc_zone(n, m, x, y, zone, grains)
    if total_grains > max_grains or (total_grains == max_grains and num_rooms < min_rooms):
        max_grains = total_grains
        min_rooms = num_rooms
        best_zone = zone

# Write the result to output file
with open('cri.out', 'w') as file:
    file.write(f'{best_zone} {max_grains} {min_rooms}\n')
```

