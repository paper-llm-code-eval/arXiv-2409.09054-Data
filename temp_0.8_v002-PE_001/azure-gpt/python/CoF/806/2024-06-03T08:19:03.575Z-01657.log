# Function to sum grains and count rooms in a given zone
def sum_grains_and_count_rooms(zone, grains, X, Y):
    total_grains = 0
    rooms = 0
    for i in range(zone[0], zone[1]):
        for j in range(zone[2], zone[3]):
            total_grains += grains[i][j]
            rooms += 1
    return total_grains, rooms

# Loading data from "cri.in"
