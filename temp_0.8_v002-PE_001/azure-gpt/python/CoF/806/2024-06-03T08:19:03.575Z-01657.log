    (0, X, 0, Y),   # Zone 1 coordinates
    (0, X, Y, M),   # Zone 2 coordinates
    (X, N, 0, Y),   # Zone 3 coordinates
    (X, N, Y, M)    # Zone 4 coordinates
]

# Calculate the outcome for each zone
for z, (i_start, i_end, j_start, j_end) in enumerate(zones, 1):
    total_grains, rooms = sum_grains_and_count_rooms((i_start, i_end, j_start, j_end), grains, X, Y)

    # Check if this zone has more grains or equal grains but fewer rooms
    if (total_grains > max_grains) or (total_grains == max_grains and rooms < min_rooms):
        max_grains = total_grains
        min_rooms = rooms
        chosen_zone = z

# Write the output to "cri.out"
with open("cri.out", "w") as file:
    file.write(f"{chosen_zone} {max_grains} {min_rooms}\n")
