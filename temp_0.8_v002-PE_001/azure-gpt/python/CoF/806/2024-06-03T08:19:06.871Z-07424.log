import numpy as np

# Read the data from the input file
with open('cri.in', 'r') as file:
    N, M, X, Y = map(int, file.readline().split())
    grain_matrix = np.array([list(map(int, file.readline().split())) for _ in range(N)], dtype=np.int32)

# Define the slices for each zone
zone_slices = {
    1: (slice(0, X), slice(0, Y)),  # Top-left
    2: (slice(0, X), slice(Y-1, M)),  # Top-right
    3: (slice(X-1, N), slice(0, Y)),  # Bottom-left
    4: (slice(X-1, N), slice(Y-1, M)),  # Bottom-right
}

# Initialize variables for results
max_grains = 0
min_rooms = float('inf')
chosen_zone = 0

