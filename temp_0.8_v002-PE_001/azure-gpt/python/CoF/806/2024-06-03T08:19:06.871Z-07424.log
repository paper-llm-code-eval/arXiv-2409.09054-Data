import numpy as np

def solve_grain_theft_problem(input_file, output_file):
    with open(input_file, 'r') as file:
        N, M, X, Y = map(int, file.readline().split())
        grain_matrix = np.genfromtxt(file, dtype=np.int32, max_rows=N)

    # Calculate the total grains and the number of rooms for each zone
    zone_data = {
        1: (np.sum(grain_matrix[:X, :Y]), (X) * (Y)),
        2: (np.sum(grain_matrix[:X, Y-1:]), (X) * (M - Y + 1)),
        3: (np.sum(grain_matrix[X-1:, :Y]), (N - X + 1) * (Y)),
        4: (np.sum(grain_matrix[X-1:, Y-1:]), (N - X + 1) * (M - Y + 1)),
    }

    # Find the optimal zone based on the maximum grains and minimum rooms
    Z, T, K = min((zone, data[0], data[1]) for zone, data in zone_data.items())

    # Write the results to the output file
    with open(output_file, 'w') as file:
        file.write(f"{Z} {T} {K}\n")

# Call the function with the specified input and output files
