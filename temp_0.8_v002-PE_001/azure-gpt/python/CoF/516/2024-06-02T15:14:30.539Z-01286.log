import numpy as np

# Read data from file
with open('tema.in', 'r') as f:
    C, N, K = map(int, f.readline().split())
    A = np.array(f.readline().split(), dtype=np.uint32)

# Auxiliary function to check if a number is prime
def is_prime(n):
    if n == 2:
        return True
    if n == 1 or n % 2 == 0:
        return False
    for current in range(3, int(n ** 0.5) + 1, 2):
        if n % current == 0:
            return False
    return True

# Auxiliary function to find prime factorization
def prime_factors(n):
    i = 2
    factors = []
    while i * i <= n:
        if n % i:
            i += 1
        else:
            n //= i
            factors.append(i)
    if n > 1:
        factors.append(n)
    return factors

# Solve the first requirement
def solve_first_requirement(A, K):
    primes = np.array([is_prime(a) for a in A], dtype=np.bool_)
    max_length = 0
    for i in range(N):
        for j in range(i+1, N):
            prime_sum = np.sum(A[i:j+1][primes[i:j+1]])
            composite_sum = np.sum(A[i:j+1][~primes[i:j+1]])
            cost = prime_sum * composite_sum
            if cost <= K:
                max_length = max(max_length, j-i+1)
            else:
                break
    return max_length

# Solve the second requirement
def solve_second_requirement(A):
    # Replace composite numbers with the product of their min and max prime factors
    for i in range(len(A)):
        if not is_prime(A[i]) and A[i] != 1:
            factors = prime_factors(A[i])
            A[i] = factors[0] * factors[-1]
    
    max_len = 0
    max_sequence = (0, 0)
    for i in range(N - 1, -1, -1):
        current_gcd = A[i]
        for j in range(i - 1, -1, -1):
            current_gcd = np.gcd(current_gcd, A[j])
            if current_gcd == 1:
                break
            if i - j + 1 > max_len:
                max_len = i - j + 1
                max_sequence = (j + 1, i + 1)
    return max_sequence

# Main process
result = solve_first_requirement(A, K) if C == 1 else solve_second_requirement(A)

# Write result to file
with open('tema.out', 'w') as f:
    if isinstance(result, int):
        f.write(str(result))
    else:
        f.write(f"{result[0]} {result[1]}")
