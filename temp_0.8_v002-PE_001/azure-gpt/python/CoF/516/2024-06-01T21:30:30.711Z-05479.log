import numpy as np

def is_prime(number):
    if number <= 1:
        return False
    for i in range(2, int(np.sqrt(number)) + 1):
        if number % i == 0:
            return False
    return True

def get_prime_factors(number):
    factors = []
    while number % 2 == 0:
        factors.append(2)
        number //= 2
    for i in range(3, int(np.sqrt(number)) + 1, 2):
        while number % i == 0:
            factors.append(i)
            number //= i
    if number > 2:
        factors.append(number)
    return factors

def task1(sequence, max_cost):
    left = 0
    max_length = 0
    prime_sum = 0
    composite_sum = 0
    
    for right in range(len(sequence)):
        if is_prime(sequence[right]):
            prime_sum += sequence[right]
        elif sequence[right] > 1:
            composite_sum += sequence[right]
        
        while prime_sum * composite_sum > max_cost and left <= right:
            if is_prime(sequence[left]):
                prime_sum -= sequence[left]
            elif sequence[left] > 1:
                composite_sum -= sequence[left]
            left += 1
        max_length = max(max_length, right - left + 1)
    return max_length

def task2(sequence):
    max_length = 0
    max_l_pos = 0
    max_r_pos = 0
    left = 0
    gcd = 0
    
    for right in range(len(sequence)):
        if sequence[right] > 1 and not is_prime(sequence[right]):
            factors = get_prime_factors(sequence[right])
            sequence[right] = factors[0] * factors[-1]

        if right == 0 or gcd == 1:
            gcd = sequence[right]
        else:
            gcd = np.gcd(gcd, sequence[right])

        while gcd == 1 and left <= right:
            left += 1
            gcd = sequence[left]
            for i in range(left + 1, right + 1):
                gcd = np.gcd(gcd, sequence[i])

        if max_length <= right - left + 1:
            max_length = right - left + 1
            max_l_pos = left
            max_r_pos = right
            
    return (max_l_pos + 1, max_r_pos + 1)

with open('tema.in', 'r') as f:
    C, N, K = map(int, f.readline().split())
    A = list(map(int, f.readline().split()))

if C == 1:
    result = task1(A, K)
elif C == 2:
    result = task2(A)

with open('tema.out', 'w') as f:
