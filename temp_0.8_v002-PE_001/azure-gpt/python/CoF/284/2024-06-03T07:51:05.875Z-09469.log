import numpy as np

def distance(a, b):
    return min(abs(ord(a) - ord(b)), 26 - abs(ord(a) - ord(b)))

def compute_time(blues, reds, calculate_min=False):
    total_time = 0
    prev_char = 'A'
    mod = 666013

    # Calculate time to print the blues without inserting reds
    for char in blues:
        total_time += distance(prev_char, char)
        prev_char = char
    
    if not calculate_min:
        return total_time

    min_time = total_time
    combinations = 1
    min_lexi_str = ""
    for i in range(1, len(blues)):
        added_time = {}

        # Calculate additional time if a red is inserted between blues[i-1] and blues[i]
        for red in reds:
            left = distance(blues[i-1], red)
            right = distance(red, blues[i])
            # Subtract the direct distance between two blue letters
            added_time[red] = left + right - distance(blues[i-1], blues[i])

        # Find the min additional time and the corresponding red characters
        min_added_time = min(added_time.values())
        min_red_chars = [red for red, time in added_time.items() if time == min_added_time]

        # Update the min time and combinations count
        min_time += min_added_time
        combinations *= len(min_red_chars)
        combinations %= mod

        # Update the min lexicographic string
        min_lexi_str += blues[i-1] + min(min_red_chars)

    min_lexi_str += blues[-1]

    return min_time, combinations, min_lexi_str

def main():
    with open('circular.in', 'r') as infile:
        task = int(infile.readline().strip())
        blue_string = infile.readline().strip()
        red_string = infile.readline().strip()

    if task == 1:
        time_to_print = compute_time(blue_string, red_string)
        with open('circular.out', 'w') as outfile:
            outfile.write(f'{time_to_print}\n')
    elif task == 2:
        min_time, num_combinations, min_lexi_str = compute_time(blue_string, list(red_string), True)
        with open('circular.out', 'w') as outfile:
            outfile.write(f'{min_time}\n{num_combinations}\n{min_lexi_str}\n')

if __name__ == '__main__':
    main()
