import numpy as np

# Function to calculate the time to move from one character to another
def time_to_move(from_char, to_char):
    distance = abs(ord(to_char) - ord(from_char))
    return min(distance, 26 - distance)

# Function to solve the first requirement
def calculate_printing_time(blue_string):
    time = 0
    current_pos = 'A'
    for char in blue_string:
        time += time_to_move(current_pos, char)
        current_pos = char
    return time

# Function to solve the second requirement
def calculate_with_red_insertion(blue_string, red_set):
    MOD = 666013
    dp = [0] * (len(blue_string) + 1)
    count = [0] * (len(blue_string) + 1)
    dp[0] = 0
    count[0] = 1
    for i in range(1, len(blue_string) + 1):
        dp[i] = dp[i-1] + time_to_move(blue_string[i-1], 'A')
        count[i] = 1
        for red_char in red_set:
            time_with_red = dp[i-1] + time_to_move('A', red_char) + time_to_move(red_char, blue_string[i-1])
            if time_with_red < dp[i]:
                dp[i] = time_with_red
                count[i] = count[i-1]
            elif time_with_red == dp[i]:
                count[i] = (count[i] + count[i-1]) % MOD
                
    # Reconstruct the lexicographically smallest string
    current_pos = 'A'
    min_lex_string = ""
    for i in range(len(blue_string)):
        temp_char = ''
        temp_time = float('inf')
        for red_char in red_set:
            time_with_red = time_to_move(current_pos, red_char) + time_to_move(red_char, blue_string[i])
            if time_with_red < temp_time or (time_with_red == temp_time and red_char < temp_char):
                temp_char = red_char
                temp_time = time_with_red
        
        # Check if inserting the red char results in the same DP value
        if dp[i] + temp_time == dp[i+1]:
            min_lex_string += temp_char
            current_pos = temp_char

        min_lex_string += blue_string[i]
        current_pos = blue_string[i]

    return dp[len(blue_string)], count[len(blue_string)], min_lex_string

# Read input from 'circular.in'
with open('circular.in', 'r') as f:
