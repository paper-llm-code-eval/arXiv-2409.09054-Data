# circular.py
import numpy as np
from collections import Counter
from itertools import product

MOD = 666013

def distance(c1, c2):
    return min(abs(ord(c1) - ord(c2)), 26 - abs(ord(c1) - ord(c2)))

def calculate_time(blue_string, red_letters):
    time = 0
    current_pos = 'A'
    for ch in blue_string:
        time += distance(current_pos, ch)
        current_pos = ch
    return time

def insertion_options(blue_string, red_letters):
    n = len(blue_string)
    m = len(red_letters)
    dp = np.zeros((n+1, m+1), dtype=int)
    lexicographically_min = [''] * (n+1)

    for i in range(n+1):
        for j in range(m+1):
            if i == 0:
                dp[i, j] = 0
                lexicographically_min[i] = ""
            else:
                dp[i, j] = float('inf')

                for k in range(m):
                    red_char = red_letters[k]
                    temp_time = dp[i-1, k] + distance(blue_string[i-1], red_char) + distance(red_char, blue_string[i-1] if i < n else 'A')
                    if temp_time < dp[i, j]:
                        dp[i, j] = temp_time
                        lexicographically_min[i] = lexicographically_min[i-1] + red_char
                    elif temp_time == dp[i, j] and lexicographically_min[i-1] + red_char < lexicographically_min[i]:
                        lexicographically_min[i] = lexicographically_min[i-1] + red_char

    time_min = min(dp[n])
    
    count = 0
    for k in range(m):
        if dp[n, k] == time_min:
            count += 1

    return time_min, count % MOD, lexicographically_min[n]

def main():
    with open('circular.in', 'r') as fin:
        task_type = int(fin.readline().strip())
        blue_string = fin.readline().strip()
        red_letters = fin.readline().strip()
    
    if task_type == 1:
        result = calculate_time(blue_string, red_letters)
        with open('circular.out', 'w') as fout:
            fout.write(f"{result}\n")
    elif task_type == 2:
        time_min, count, lexico_min = insertion_options(blue_string, red_letters)
        with open('circular.out', 'w') as fout:
            fout.write(f"{time_min}\n{count}\n{lexico_min}\n")

if __name__ == "__main__":
    main()
```

