# summax.py
import numpy as np

MAX_VAL = 2000000001

def calculate_max_paths(matrix, n):
    paths = np.zeros((n, n), dtype=np.int64)
    paths[-1] = 1

    for row in range(n-2, -1, -1):
        for col in range(row+1):
            if matrix[row+1][col] > matrix[row+1][col+1]:
                paths[row][col] = paths[row+1][col]
            elif matrix[row+1][col] < matrix[row+1][col+1]:
                paths[row][col] = paths[row+1][col+1]
            else:
                paths[row][col] = paths[row+1][col] + paths[row+1][col+1]
                if paths[row][col] >= MAX_VAL:
                    paths[row][col] = MAX_VAL

    return paths[0][0]

def find_paths(matrix, paths, st, dr, n):
    result = []
    for path_number in range(st, dr + 1):
        path = []
        col = 0
        remaining_paths = path_number
        
        for row in range(n):
            path.append(col + 1)
            if row < n - 1:
                if remaining_paths <= paths[row+1][col]:
                    pass
                else:
                    remaining_paths -= paths[row+1][col]
                    col += 1

        result.append(path)

    return result

def main():
    with open('summax.in', 'r') as file:
        v = int(file.readline().strip())
        n, st, dr = map(int, file.readline().split())
        matrix = np.zeros((n, n), dtype=np.int32)

        for i in range(n):
            line = list(map(int, file.readline().split()))
            matrix[i, :i+1] = line

    cum_sum = np.flip(np.cumsum(np.flip(matrix, axis=0), axis=0), axis=0)

    if v == 1:
        num_max_paths = calculate_max_paths(cum_sum, n)
        with open('summax.out', 'w') as file:
            file.write(f"{num_max_paths}\n")
    elif v == 2:
        paths = calculate_max_paths(cum_sum, n)
        selected_paths = find_paths(cum_sum, paths, st, dr, n)
        with open('summax.out', 'w') as file:
            for path in selected_paths:
                file.write(' '.join(map(str, path)) + '\n')

if __name__ == '__main__':
    main()
```

Please note that this code is intended to be as memory-efficient as possible, given the constraints of the challenge. It avoids unnecessary data structures and utilizes in-place operations when possible. The use of numpy allows for memory-efficient array operations, which is critical given the 8MB memory limit.
