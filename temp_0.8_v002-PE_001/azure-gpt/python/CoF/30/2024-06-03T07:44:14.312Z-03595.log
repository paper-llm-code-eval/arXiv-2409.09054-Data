    v = int(file.readline().strip())
    n, st, dr = map(int, file.readline().strip().split())
    triangle = [list(map(int, file.readline().strip().split())) for _ in range(n)]

# Process the data
max_sums = calculate_max_sums(triangle, n)
paths = calculate_paths(max_sums, n)

# Write the output data to the file
with open('summax.out', 'w') as file:
    if v == 1:
        # Write the number of maximum sum paths
        file.write(str(paths[0][0]) + '\n')
    elif v == 2:
        # Generate and write the specific paths
        specific_paths = generate_specific_paths(max_sums, paths, n, st, dr)
        for path in specific_paths:
            file.write(' '.join(map(str, path)) + '\n')
