# A function to calculate the maximum sum of each position in the triangle
def calculate_max_sums(triangle, n):
    max_sums = np.array(triangle, dtype=np.int64)
    for i in range(n-2, -1, -1):
        for j in range(i+1):
            max_sums[i][j] += max(max_sums[i+1][j], max_sums[i+1][j+1])
    return max_sums

# A function to calculate the number of paths that can lead to the maximum sum
def calculate_paths(max_sums, n):
    paths = np.ones((n, n), dtype=np.int64)
    for i in range(n-2, -1, -1):
        for j in range(i+1):
            if max_sums[i+1][j] > max_sums[i+1][j+1]:
                paths[i][j] = paths[i+1][j]
            elif max_sums[i+1][j] < max_sums[i+1][j+1]:
                paths[i][j] = paths[i+1][j+1]
            else:
                paths[i][j] = paths[i+1][j] + paths[i+1][j+1]
            if paths[i][j] > 2000000000:
                paths[i][j] = 2000000001
    return paths

# A function to generate the specific paths of the given orders
def generate_specific_paths(max_sums, paths, n, st, dr):
    result_paths = []
    for order in range(st, dr+1):
        current_order = order
        current_path = []
        row, col = 0, 0  # Start from the top of the triangle
        while row < n:
            current_path.append(col + 1)  # 1-indexed
            if row == n-1:
                break
            if current_order <= paths[row+1][col]:
                # Go down to the left
                row += 1
            else:
                # Go down to the right
                current_order -= paths[row+1][col]
                row += 1
                col += 1
        result_paths.append(current_path)
    return result_paths

# Read the input data from the file
