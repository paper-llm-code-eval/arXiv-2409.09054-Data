import numpy as np

# Read the input data from the file
with open('summax.in', 'r') as f:
    lines = f.readlines()

# Extract the values for v, n, st, and dr
v, (n, st, dr) = int(lines[0]), map(int, lines[1].split())

# Initialize the triangle matrix with the given values
triangle = []
for i in range(2, 2 + n):
    triangle.append(list(map(int, lines[i].split())))

# Function to calculate the number of max sum paths
def count_max_sum_paths(triangle, n):
    count = np.ones((n, n), dtype=np.int64)
    for i in range(n - 2, -1, -1):
        for j in range(i + 1):
            if triangle[i + 1][j] > triangle[i + 1][j + 1]:
                count[i][j] = count[i + 1][j]
            elif triangle[i + 1][j] < triangle[i + 1][j + 1]:
                count[i][j] = count[i + 1][j + 1]
            else:
                count[i][j] = count[i + 1][j] + count[i + 1][j + 1]
            if count[i][j] > 2000000000:
                count[i][j] = 2000000001
    return count[0][0]

# Function to generate the paths with given order numbers
def generate_paths(triangle, n, st, dr):
    path_counts = count_max_sum_paths(triangle, n)
    paths = []
    for order in range(st, dr + 1):
        path = []
        row, col = 0, 0
        remaining = order - 1
        for row in range(n):
            path.append(col + 1)  # 1-indexed
            if row < n - 1:
                left_count = path_counts[row + 1][col]
                if left_count < remaining:
                    remaining -= left_count
                    col += 1
        paths.append(path)
    return paths

# Output processing based on the value of v
with open('summax.out', 'w') as f:
    if v == 1:
        num_paths = count_max_sum_paths(triangle, n)
        f.write(str(num_paths) + '\n')
    elif v == 2:
        paths = generate_paths(triangle, n, st, dr)
        for path in paths:
            f.write(' '.join(map(str, path)) + '\n')
