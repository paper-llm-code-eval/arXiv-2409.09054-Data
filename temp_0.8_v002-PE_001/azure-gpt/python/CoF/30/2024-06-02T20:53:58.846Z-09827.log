import numpy as np

MAX_VALUE = 2000000001

def read_input(filename):
    with open(filename, 'r') as file:
        v = int(file.readline().strip())
        n, st, dr = map(int, file.readline().strip().split())
        triangle = [list(map(int, file.readline().strip().split())) for _ in range(n)]
    return v, n, st, dr, triangle

def write_output(filename, result):
    with open(filename, 'w') as file:
        if isinstance(result, list):
            for line in result:
                file.write(f'{line}\n')
        else:
            file.write(f'{result}\n')

def compute_max_sums(triangle):
    max_sums = np.copy(triangle)
    for i in range(len(triangle) - 2, -1, -1):
        for j in range(len(triangle[i])):
            max_sums[i][j] += max(max_sums[i+1][j], max_sums[i+1][j+1])
    return max_sums

def count_max_paths(triangle, max_sums):
    path_counts = np.zeros((len(triangle), len(triangle[-1])), dtype=np.int64)
    path_counts[-1] = 1
    for i in range(len(triangle) - 2, -1, -1):
        for j in range(i+1):
            if max_sums[i+1][j] == max_sums[i][j] - triangle[i][j]:
                path_counts[i][j] += path_counts[i+1][j]
            if max_sums[i+1][j+1] == max_sums[i][j] - triangle[i][j]:
                path_counts[i][j] += path_counts[i+1][j+1]
            path_counts[i][j] = min(MAX_VALUE, path_counts[i][j])
    return path_counts[0][0]

def find_paths(triangle, max_sums, path_counts, st, dr, n):
    result = []
    for idx in range(st, dr + 1):
        path, col = [], 0
        for row in range(n):
            path.append(col + 1)
            if row < n - 1:
                left = 0 if col == 0 else path_counts[row + 1][col - 1]
                if idx <= left:
                    continue
                idx -= left
                col += 1
        result.append(' '.join(map(str, path)))
    return result

def solve(v, n, st, dr, triangle):
    triangle = np.array(triangle, dtype=np.int64)
    max_sums = compute_max_sums(triangle)
    path_counts = compute_max_sums(triangle)
    if v == 1:
        result = min(MAX_VALUE, count_max_paths(triangle, max_sums))
    else:
        result = find_paths(triangle, max_sums, path_counts, st, dr, n)
    return result

