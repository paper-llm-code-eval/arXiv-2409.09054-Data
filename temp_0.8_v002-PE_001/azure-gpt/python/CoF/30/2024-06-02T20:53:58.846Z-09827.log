    for path_number in range(st, dr + 1):
        path = []
        count = path_number - 1
        current_count = 0
        for i in range(n):
            if i == 0:
                path.append(1)
            else:
                left_count = path_counts[i, path[-1] - 1] if path[-1] - 1 < len(path_counts[i]) else 0
                if current_count + left_count < count:
                    current_count += left_count
                    path.append(path[-1] + 1)
                else:
                    path.append(path[-1])
        paths.append(path)
    return paths

