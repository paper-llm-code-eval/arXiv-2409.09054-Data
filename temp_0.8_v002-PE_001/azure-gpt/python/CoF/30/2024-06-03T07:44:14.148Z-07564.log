import numpy as np

MAX_INT = 2000000000

def read_data(filename):
    with open(filename, 'r') as file:
        v = int(file.readline().strip())
        n, st, dr = map(int, file.readline().strip().split())
        triangle = []
        for _ in range(n):
            triangle.append(list(map(int, file.readline().strip().split())))
    return v, n, st, dr, triangle

def write_data(filename, data):
    with open(filename, 'w') as file:
        for line in data:
            file.write(' '.join(map(str, line)) + '\n')

def find_max_paths(triangle, n):
    max_paths = np.zeros((n, n), dtype=np.int64)
    max_paths[0][0] = 1
    
    for i in range(1, n):
        for j in range(i + 1):
            if j > 0:
                max_paths[i][j] += max_paths[i - 1][j - 1]
            if j < i:
                max_paths[i][j] += max_paths[i - 1][j]
            max_paths[i][j] = min(max_paths[i][j], MAX_INT + 1)
    return max_paths

def find_max_sum(triangle, n):
    max_sum = np.array(triangle, dtype=np.int64)
    for i in range(1, n):
        for j in range(i + 1):
            if j == 0:
                max_sum[i][j] += max_sum[i - 1][j]
            elif j == i:
                max_sum[i][j] += max_sum[i - 1][j - 1]
            else:
                max_sum[i][j] += max(max_sum[i - 1][j], max_sum[i - 1][j - 1])
    return max_sum

def find_nth_path(max_paths, max_sum, n, nth):
    path = []
    j = np.argmax(max_sum[-1])
    max_sum_val = max_sum[-1][j]
    path.append(j + 1)
    
    for i in range(n - 2, -1, -1):
        if j > 0 and max_sum[i][j - 1] == max_sum_val - triangle[i + 1][j]:
            possible_paths = max_paths[i][j - 1]
            if possible_paths < nth:
                nth -= possible_paths
                j -= 1
        max_sum_val -= triangle[i + 1][j]
        path.insert(0, j + 1)
    return path

def solve(v, n, st, dr, triangle):
    max_paths = find_max_paths(triangle, n)
    max_sum = find_max_sum(triangle, n)
    total_max_paths = max_paths[-1][np.argmax(max_sum[-1])]

    if v == 1:
        return total_max_paths if total_max_paths <= MAX_INT else MAX_INT + 1
    
    result = []
    for nth in range(st, min(dr + 1, total_max_paths + 1)):
        result.append(find_nth_path(max_paths, max_sum, n, nth))
    return result

def main():
    v, n, st, dr, triangle = read_data('summax.in')
    result = solve(v, n, st, dr, triangle)
    if v == 1:
        write_data('summax.out', [result])
    else:
        write_data('summax.out', result)

if __name__ == '__main__':
    main()
