import numpy as np

# Define constants
MAX_VALUE = 2000000001

def main():
    # Load data from file
    with open('summax.in', 'r') as file:
        v = int(file.readline().strip())
        n, st, dr = map(int, file.readline().strip().split())
        triangle = [list(map(int, file.readline().strip().split())) for _ in range(n)]

    # Convert to numpy array for efficient memory usage
    triangle_np = np.array(triangle, dtype=np.int32)

    # Calculate the cumulative sums needed for both cases
    paths_count, max_sum = calculate_cumulative_sums(triangle_np)

    if v == 1:
        # Write the total number of maximum value paths to the output file
        with open('summax.out', 'w') as file:
            file.write(str(min(paths_count[-1][-1], MAX_VALUE)) + '\n')
    elif v == 2:
        # Find and write the specific paths to the output file
        with open('summax.out', 'w') as file:
            for target_path_number in range(st, dr + 1):
                path = find_path(triangle_np, paths_count, max_sum, target_path_number)
                file.write(' '.join(map(str, path)) + '\n')

def calculate_cumulative_sums(triangle):
    n = len(triangle)
    max_sum = triangle.copy()
    paths_count = np.zeros((n, n), dtype=np.int64)

    # Initialize paths count for the bottom row
    paths_count[-1, :] = 1

    # Calculate the cumulative sum and paths count from bottom to top
    for i in range(n - 2, -1, -1):
        for j in range(i + 1):
            left_val = max_sum[i + 1][j]
            right_val = max_sum[i + 1][j + 1]
            if left_val > right_val:
                max_sum[i][j] += left_val
                paths_count[i][j] = paths_count[i + 1][j]
            elif left_val < right_val:
                max_sum[i][j] += right_val
                paths_count[i][j] = paths_count[i + 1][j + 1]
            else: # left_val == right_val
                max_sum[i][j] += left_val
                paths_count[i][j] = paths_count[i + 1][j] + paths_count[i + 1][j + 1]
                if paths_count[i][j] > MAX_VALUE:
                    paths_count[i][j] = MAX_VALUE

    return paths_count, max_sum

def find_path(triangle, paths_count, max_sum, target_path_number):
    n = len(triangle)
    path = []
    current_path_number = target_path_number
    col = 0

    for row in range(n):
        path.append(col + 1)
        if row < n - 1:
            left_count = paths_count[row + 1][col]
            if current_path_number > left_count:
                current_path_number -= left_count
                col += 1

    return path

if __name__ == '__main__':
    main()
