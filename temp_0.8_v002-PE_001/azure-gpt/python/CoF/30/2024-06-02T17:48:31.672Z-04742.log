import numpy as np

def calculate_paths(matrix, n):
    max_sums = np.zeros((n, n), dtype=np.int64)
    path_count = np.zeros((n, n), dtype=np.int64)
    
    # Initialize the bottom row of max_sums and path_count
    max_sums[-1] = matrix[-1]
    path_count[-1] = 1

    # Calculate the max_sums and path_counts from the bottom up
    for i in range(n - 2, -1, -1):
        for j in range(i + 1):
            lower_values = max_sums[i + 1, j:j + 2]
            max_sum = np.max(lower_values)
            max_sums[i, j] = matrix[i, j] + max_sum
            path_count[i, j] = np.sum(path_count[i + 1, j:j + 2][lower_values == max_sum])

    # Handle overflow
    path_count[path_count > 2000000000] = 2000000001

    return max_sums, path_count

def find_route_indices(start, end, path_count, n):
    routes = []
    for index in range(start, end + 1):
        route = []
        current_index = index
        for row in range(n):
            left_count = path_count[row, len(route)] if len(route) < row else 0
            if current_index > left_count:
                route.append(len(route) + 1)
                current_index -= left_count
            else:
                route.append(len(route))
        routes.append(route)
    return routes

def read_input_file(filename):
    with open(filename, 'r') as file:
        v = int(file.readline().strip())
        n, st, dr = map(int, file.readline().strip().split())
        triangle = [list(map(int, file.readline().strip().split())) for _ in range(n)]
    return v, n, st, dr, triangle

def write_output_file(filename, v, routes, total_paths=None):
    with open(filename, 'w') as file:
        if v == 1:
            file.write(str(total_paths) + '\n')
        elif v == 2:
            for route in routes:
                file.write(' '.join(map(str, np.array(route) + 1)) + '\n')

def main():
    # Read the input file
    v, n, st, dr, triangle = read_input_file('summax.in')

    # Convert the triangle to a numpy array
    matrix = np.zeros((n, n), dtype=np.int32)
    for i in range(n):
        matrix[i, :i + 1] = triangle[i]

    # Calculate the paths
    max_sums, path_count = calculate_paths(matrix, n)

    if v == 1:
        # Write the total number of paths to the output file
        write_output_file('summax.out', v, None, int(path_count[0, 0]))
    elif v == 2:
        # Find the requested routes
        routes = find_route_indices(st, dr, path_count, n)
        # Write the routes to the output file
        write_output_file('summax.out', v, routes)

if __name__ == "__main__":
    main()
```

