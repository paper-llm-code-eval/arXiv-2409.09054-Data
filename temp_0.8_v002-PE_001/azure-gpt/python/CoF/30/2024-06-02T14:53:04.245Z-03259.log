import numpy as np

MAX_INT = 2000000001


def read_triangle(file_path):
    with open(file_path, 'r') as file:
        lines = file.readlines()

    v = int(lines[0].strip())
    n, st, dr = map(int, lines[1].strip().split())
    triangle = []
    for line in lines[2:]:
        triangle.append(np.array(list(map(int, line.strip().split())), dtype=np.int64))
    return v, n, st, dr, triangle


def write_output(file_path, result):
    with open(file_path, 'w') as file:
        if isinstance(result, list):
            for path in result:
                file.write(' '.join(map(str, path)) + '\n')
        else:
            file.write(str(result) + '\n')


def calculate_max_paths(triangle, n):
    max_sum = np.copy(triangle)
    paths = np.zeros_like(max_sum)

    # Initialize the bottom row with 1s as there is only one path to each element.
    paths[-1] = 1

    # Start from the second-to-last row and go upwards.
    for i in range(n - 2, -1, -1):
        for j in range(i + 1):
            left = max_sum[i + 1][j]
            right = max_sum[i + 1][j + 1]
            max_sum[i][j] += max(left, right)
            if left == right:
                paths[i][j] = min(MAX_INT, paths[i + 1][j] + paths[i + 1][j + 1])
            elif max_sum[i][j] == max_sum[i][j] + left:
                paths[i][j] = paths[i + 1][j]
            else:
                paths[i][j] = paths[i + 1][j + 1]

    return max_sum[0][0], paths


def generate_paths(n, st, dr, paths):
    result = []
    for path_num in range(st, dr + 1):
        path = []
        col = 0
        remaining = path_num - 1
        for row in range(n):
            path.append(col + 1)
            if row < n - 1:
                left_paths = paths[row + 1][col]
                if remaining >= left_paths:
                    col += 1
                    remaining -= left_paths
        result.append(path)
    return result


def solve(v, n, st, dr, triangle):
    max_val, paths = calculate_max_paths(triangle, n)
    if v == 1:
        return np.sum(paths[-1])
    elif v == 2:
        return generate_paths(n, st, dr, paths)


def main():
    v, n, st, dr, triangle = read_triangle('summax.in')
    result = solve(v, n, st, dr, triangle)
    write_output('summax.out', result)


if __name__ == "__main__":
    main()
