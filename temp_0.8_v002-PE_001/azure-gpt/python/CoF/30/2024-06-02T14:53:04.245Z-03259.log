                file.write(' '.join(map(str, path)) + '\n')
        else:
            file.write(str(result) + '\n')


def calculate_max_paths(triangle, n):
    # Bottom-up calculation of max path sum at each position
    max_sum = np.copy(triangle)
    for i in range(n-2, -1, -1):
        for j in range(i+1):
            max_sum[i][j] += max(max_sum[i+1][j], max_sum[i+1][j+1])
    
    # Calculate the number of max paths that pass through each position
    paths = np.zeros((n, n), dtype=np.int64)
    paths[0][0] = 1
    for i in range(1, n):
        for j in range(i+1):
            if j > 0:
                paths[i][j] += paths[i-1][j-1] if max_sum[i][j] == triangle[i][j] + max_sum[i-1][j-1] else 0
            if j < i:
                paths[i][j] += paths[i-1][j] if max_sum[i][j] == triangle[i][j] + max_sum[i-1][j] else 0
            if paths[i][j] > MAX_INT:
                paths[i][j] = MAX_INT + 1  # Limit to MAX_INT + 1 as specified
    return max_sum[0][0], paths


def find_paths(triangle, n, st, dr, max_val, paths):
    result_paths = []
    for path_num in range(st, dr+1):
        current_path = []
        current_index = (0, 0)
        current_path_num = path_num
        for i in range(n):
            current_path.append(current_index[1] + 1)
            if i < n - 1:
                left_paths = paths[i+1][current_index[1]]
                if current_path_num <= left_paths or left_paths == 0:
                    current_index = (i+1, current_index[1])
                else:
                    current_index = (i+1, current_index[1] + 1)
                    current_path_num -= left_paths
        result_paths.append(current_path)
    return result_paths


def solve(v, n, st, dr, triangle):
