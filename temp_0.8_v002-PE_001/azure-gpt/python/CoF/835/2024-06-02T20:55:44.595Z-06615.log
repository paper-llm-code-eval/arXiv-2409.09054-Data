# Importing numpy to use its array for efficient memory and execution
import numpy as np

# Function to check for common digits and perform the unification
def unify(ai, aj):
    set_ai = set(str(ai))
    set_aj = set(str(aj))
    common_digits = set_ai & set_aj
    if common_digits:
        # Remove common digits and combine the remaining parts
        new_ai = ''.join(digit for digit in str(ai) if digit not in common_digits).lstrip('0')
        new_aj = ''.join(digit for digit in str(aj) if digit not in common_digits).lstrip('0')
        unified = int(new_ai + new_aj) if new_ai or new_aj else 0
        return unified
    return None

# Function to find the most frequent digit
def most_frequent_digit(digit_counts):
    max_count = max(digit_counts)
    for digit, count in enumerate(digit_counts):
        if count == max_count:
            return digit

# Function to process the unifications
def process_unifications(numbers):
    i = 0
    while i < len(numbers) - 1:
        unified_number = unify(numbers[i], numbers[i+1])
        if unified_number is not None:
            # Perform unification and update the list of numbers
            numbers[i] = unified_number
            del numbers[i+1]
        else:
            i += 1
    return numbers

# Reading from 'unific.in'
with open('unific.in', 'r') as file_in:
    N = int(file_in.readline().strip())
    numbers = [int(file_in.readline().strip()) for _ in range(N)]

# Initializing the digit count array
digit_counts = np.zeros(10, dtype=int)

# Updating the digit counts
for number in numbers:
    for digit in str(number):
        digit_counts[int(digit)] += 1

# Finding the most frequent digit
most_frequent = most_frequent_digit(digit_counts)

# Processing the unifications
unified_numbers = process_unifications(numbers)

# Writing to 'unific.out'
with open('unific.out', 'w') as file_out:
    file_out.write(f"{most_frequent}\n")
    if unified_numbers:
        file_out.write(f"{len(unified_numbers)}\n")
        file_out.write(' '.join(map(str, unified_numbers)))
    else:
        file_out.write("0\n")
```

