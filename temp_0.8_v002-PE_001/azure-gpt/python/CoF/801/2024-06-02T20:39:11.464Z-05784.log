import numpy as np

# Read data from file
with open('cladiri.in', 'r') as file:
    earthquake_center_intensity = np.array(file.readline().split(), dtype=int)
    buildings_data = np.loadtxt(file, delimiter=' ', dtype=int)

# Unpack earthquake data
quake_lon, quake_lat, quake_intensity = earthquake_center_intensity

# Calculate the level and check if the building is affected
affected_buildings = []
levels = {}

for building in buildings_data:
    lon, lat, resistance = building
    level = max(abs(lon - quake_lon), abs(lat - quake_lat))
    if resistance <= quake_intensity - level:
        affected_buildings.append(building)
        levels[level] = levels.get(level, 0) + 1

# Calculate total affected buildings, max number of buildings on any level, and the levels with max buildings
total_affected = len(affected_buildings)
max_buildings_per_level = max(levels.values()) if levels else 0
max_levels = sorted([level for level, count in levels.items() if count == max_buildings_per_level])

# Write results to file
with open('cladiri.out', 'w') as file:
    file.write(f"{total_affected}\n{max_buildings_per_level}\n")
    file.write(' '.join(map(str, max_levels)))
```
