def calculate_degree(sequence):
    sorted_sequence = np.sort(sequence)
    return np.sum(sequence == sorted_sequence)

# Function to find the first sequence of length k with maximum degree
def find_max_degree_sequence(xs, k):
    max_degree = 0
    max_degree_position = 0
    for i in range(len(xs) - k + 1):
        degree = calculate_degree(xs[i:i+k])
        if degree > max_degree:
