import numpy as np
from scipy.sparse.csgraph import dijkstra
from scipy.sparse import csr_matrix

# Read data from file
with open('rj.in', 'r') as file:
    n, m = map(int, file.readline().split())
    city_map = np.array([list(file.readline().strip()) for _ in range(n)])

# Find Romeo 'R' and Juliet 'J' positions
romeo_pos = tuple(zip(*np.where(city_map == 'R')))[0]
juliet_pos = tuple(zip(*np.where(city_map == 'J')))[0]

# Create adjacency matrix for the graph
adj_matrix = np.zeros((n * m, n * m), dtype=np.int8)

# Mapping function from 2D coordinates to 1D index
def to_index(x, y):
    return x * m + y

# Populate adjacency matrix with edges
for x in range(n):
    for y in range(m):
        if city_map[x, y] != 'X':
            idx = to_index(x, y)
            for dx in range(-1, 2):
                for dy in range(-1, 2):
                    if dx == 0 and dy == 0:
                        continue
                    nx, ny = x + dx, y + dy
                    if 0 <= nx < n and 0 <= ny < m and city_map[nx, ny] != 'X':
                        adj_matrix[idx, to_index(nx, ny)] = 1

# Convert to CSR format for efficient graph operations
adj_matrix_csr = csr_matrix(adj_matrix)

# Running Dijkstra's algorithm from Romeo and Juliet's positions
romeo_dist = dijkstra(adj_matrix_csr, indices=to_index(*romeo_pos), unweighted=True, return_predecessors=False)
juliet_dist = dijkstra(adj_matrix_csr, indices=to_index(*juliet_pos), unweighted=True, return_predecessors=False)

# Find the optimal meeting point
optimal_time = np.inf
optimal_position = None

for x in range(n):
    for y in range(m):
        if city_map[x, y] == ' ':
            idx = to_index(x, y)
            max_dist = max(romeo_dist[idx], juliet_dist[idx])
            if max_dist < optimal_time:
                optimal_time = max_dist
                optimal_position = (x + 1, y + 1)

# Write the output to file
with open('rj.out', 'w') as file:
    file.write(f'{int(optimal_time)} {optimal_position[0]} {optimal_position[1]}')
