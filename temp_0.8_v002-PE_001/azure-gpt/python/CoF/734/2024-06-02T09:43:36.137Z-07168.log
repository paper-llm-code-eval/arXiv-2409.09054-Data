import numpy as np
from scipy.ndimage import label, generate_binary_structure
from scipy.spatial.distance import cityblock

def read_map(filename):
    with open(filename, 'r') as file:
        n, m = map(int, file.readline().split())
        city_map = [list(file.readline().strip()) for _ in range(n)]
    return np.array(city_map), n, m

def write_output(filename, t_min, x, y):
    with open(filename, 'w') as file:
        file.write(f"{t_min} {x} {y}\n")

def bfs(start, city_map, n, m):
    queue = [start]
    distances = np.full_like(city_map, np.inf)
    distances[start] = 0
    while queue:
        x, y = queue.pop(0)
        for dx in [-1, 0, 1]:
            for dy in [-1, 0, 1]:
                if dx == 0 and dy == 0:
                    continue
                nx, ny = x + dx, y + dy
                if 0 <= nx < n and 0 <= ny < m and city_map[nx, ny] == ' ' and distances[nx, ny] == np.inf:
                    distances[nx, ny] = distances[x, y] + 1
                    queue.append((nx, ny))
    return distances

def find_meeting_point(city_map, n, m):
    # Find Romeo and Juliet's locations
    R_loc = tuple(map(lambda x: x[0], np.where(city_map == 'R')))
    J_loc = tuple(map(lambda x: x[0], np.where(city_map == 'J')))
    
    # Compute distances from R & J to all points
    R_distances = bfs(R_loc, city_map, n, m)
    J_distances = bfs(J_loc, city_map, n, m)
    
    # Find meeting points, minimum time, and locations
    meeting_points = (R_distances == J_distances) & (R_distances != np.inf)
    if not meeting_points.any():
        return None
    
    t_min = np.min(R_distances[meeting_points])
    meeting_locs = np.array(np.where(meeting_points)).T
    min_loc = meeting_locs[np.argmin(cityblock(meeting_locs, np.array((R_loc, J_loc)).T), axis=0)]

    return t_min, min_loc[0]+1, min_loc[1]+1

def solve_rj_problem():
    city_map, n, m = read_map('rj.in')
    result = find_meeting_point(city_map, n, m)
    if result:
        write_output('rj.out', *result)
    else:
        write_output('rj.out', "No meeting point found")

solve_rj_problem()
