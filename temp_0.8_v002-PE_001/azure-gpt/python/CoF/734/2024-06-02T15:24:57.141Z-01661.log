from collections import deque

def read_input(filename):
    with open(filename, 'r') as file:
        n, m = map(int, file.readline().split())
        matrix = [list(file.readline().strip()) for _ in range(n)]
    return n, m, matrix

def write_output(filename, tmin, x, y):
    with open(filename, 'w') as file:
        file.write(f"{tmin} {x+1} {y+1}\n")

def bfs(matrix, start, n, m):
    queue = deque([start])
    distances = [[None for _ in range(m)] for _ in range(n)]
    distances[start[0]][start[1]] = 0
    while queue:
        x, y = queue.popleft()
        for dx in [-1, 0, 1]:
            for dy in [-1, 0, 1]:
                if dx == 0 and dy == 0:
                    continue
                nx, ny = x + dx, y + dy
                if 0 <= nx < n and 0 <= ny < m and matrix[nx][ny] == ' ' and distances[nx][ny] is None:
                    distances[nx][ny] = distances[x][y] + 1
                    queue.append((nx, ny))
    return distances

def find_meeting_point(n, m, matrix):
    R_loc = J_loc = None
    for i in range(n):
        for j in range(m):
            if matrix[i][j] == 'R':
                R_loc = (i, j)
            elif matrix[i][j] == 'J':
                J_loc = (i, j)
            if R_loc and J_loc:
                break
        if R_loc and J_loc:
            break

    R_distances = bfs(matrix, R_loc, n, m)
    J_distances = bfs(matrix, J_loc, n, m)

    min_time = float('inf')
    meeting_point = (-1, -1)
    for i in range(n):
        for j in range(m):
            if matrix[i][j] == ' ' and R_distances[i][j] is not None and J_distances[i][j] is not None:
                max_distance = max(R_distances[i][j], J_distances[i][j])
                if max_distance < min_time:
                    min_time = max_distance
                    meeting_point = (i, j)

    return min_time, meeting_point[0], meeting_point[1]

n, m, matrix = read_input('rj.in')
tmin, x, y = find_meeting_point(n, m, matrix)
write_output('rj.out', tmin, x, y)
