import numpy as np
from scipy.sparse.csgraph import shortest_path
from scipy.sparse import dok_matrix

# Read the input from the file
with open('rj.in', 'r') as file:
    n, m = map(int, file.readline().split())
    grid = []
    for i in range(n):
        grid.append(file.readline().strip())

# Function to convert a grid position to a graph index
def pos_to_idx(x, y):
    return x * m + y

# Initialize the graph adjacency matrix
graph = dok_matrix((n * m, n * m), dtype=np.int8)

# Variables to store Romeo's and Juliet's positions
romeo_pos = (-1, -1)
juliet_pos = (-1, -1)

# Build the adjacency matrix
for i in range(n):
    for j in range(m):
        if grid[i][j] == ' ' or grid[i][j] == 'R' or grid[i][j] == 'J':
            idx = pos_to_idx(i, j)
            for dx in [-1, 0, 1]:
                for dy in [-1, 0, 1]:
                    if dx == 0 and dy == 0:
                        continue
                    x, y = i + dx, j + dy
                    if 0 <= x < n and 0 <= y < m and (grid[x][y] == ' ' or grid[x][y] == 'R' or grid[x][y] == 'J'):
                        graph[idx, pos_to_idx(x, y)] = 1
        if grid[i][j] == 'R':
            romeo_pos = (i, j)
        elif grid[i][j] == 'J':
            juliet_pos = (i, j)

# Compute shortest paths from Romeo's and Juliet's locations
romeo_distances, _ = shortest_path(csgraph=graph, directed=False, indices=pos_to_idx(*romeo_pos), return_predecessors=True)
juliet_distances, _ = shortest_path(csgraph=graph, directed=False, indices=pos_to_idx(*juliet_pos), return_predecessors=True)

# Find the optimal meeting point
optimal_time = np.inf
optimal_point = (-1, -1)
for i in range(n):
    for j in range(m):
        if grid[i][j] == ' ':
            idx = pos_to_idx(i, j)
            max_dist = max(romeo_distances[idx], juliet_distances[idx])
            if max_dist < optimal_time:
                optimal_time = max_dist
                optimal_point = (i + 1, j + 1)  # Convert to 1-indexed

# Write the output to the file
with open('rj.out', 'w') as file:
    file.write(f"{int(optimal_time)} {optimal_point[0]} {optimal_point[1]}")
