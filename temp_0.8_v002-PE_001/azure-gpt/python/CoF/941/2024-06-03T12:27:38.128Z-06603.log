import numpy as np

def count_digit_occurrences(values, digit):
    return str(values).count(str(digit))

def count_cuts(values):
    cuts = 0
    for value in values:
        while value > 0:
            if value % 100 <= 12:
                value //= 100
            else:
                value //= 10
            cuts += 1
    return cuts - len(values)

def main():
    with open('ceas.in', 'r') as fin:
        C, X, N = map(int, fin.readline().split())
        numbers = np.fromstring(fin.readline(), dtype=int, sep=' ')

    if C == 1:
        result = count_digit_occurrences(numbers, X)
    else:
        result = count_cuts(numbers)

    with open('ceas.out', 'w') as fout:
        fout.write(f'{result}\n')

if __name__ == "__main__":
    main()
