import numpy as np

# Define a function to check if a number is prime
def is_prime(n):
    if n <= 1:
        return False
    if n <= 3:
        return True
    if n % 2 == 0 or n % 3 == 0:
        return False
    i = 5
    while i * i <= n:
        if n % i == 0 or n % (i + 2) == 0:
            return False
        i += 6
    return True

# Define a function to calculate the sum of digits of a number
def sum_of_digits(n):
    return sum(int(digit) for digit in str(n))

# Define a function to calculate the flight duration in minutes
def flight_duration(h1, m1, h2, m2, reverse=False):
    departure = h1 * 60 + m1
    arrival = h2 * 60 + m2
    if reverse:
        departure, arrival = arrival, departure
    if arrival < departure:
        arrival += 1440  # Add 24 hours in minutes
    return arrival - departure

# Read input data
with open('aeriana.in', 'r') as file:
    C = int(file.readline().strip())
    N = int(file.readline().strip())
    flights = [list(map(int, file.readline().split())) for _ in range(N)]

max_duration_pre_virus = 0
max_duration_post_virus = 0

# Process each flight
for flight in flights:
    A1, A2, H1, M1, H2, M2 = flight
    # Calculate the flight duration before the virus's intervention
    duration_pre_virus = flight_duration(H1, M1, H2, M2)
    max_duration_pre_virus = max(max_duration_pre_virus, duration_pre_virus)
    
    # Check if the flight is special
    if is_prime(A1) and A2 % sum_of_digits(A1) == 0:
        # Calculate the flight duration after the virus's intervention (reversed)
        duration_post_virus = flight_duration(H1, M1, H2, M2, reverse=True)
    else:
        duration_post_virus = duration_pre_virus
    
    max_duration_post_virus = max(max_duration_post_virus, duration_post_virus)

# Write the output data
with open('aeriana.out', 'w') as file:
    if C == 1:
        max_hours, max_minutes = divmod(max_duration_pre_virus, 60)
        file.write(f"{max_hours} {max_minutes}\n")
    elif C == 2:
        max_hours, max_minutes = divmod(max_duration_post_virus, 60)
        file.write(f"{max_hours} {max_minutes}\n")
```

