import numpy as np

def is_prime(num):
    if num <= 1:
        return False
    if num in (2, 3):
        return True
    if num % 2 == 0 or num % 3 == 0:
        return False
    i = 5
    while i * i <= num:
        if num % i == 0 or num % (i + 2) == 0:
            return False
        i += 6
    return True

def sum_of_digits(num):
    return sum(int(digit) for digit in str(num))

def calculate_duration(h1, m1, h2, m2):
    departure = h1 * 60 + m1
    arrival = h2 * 60 + m2
    if arrival < departure:
        arrival += 24 * 60
    return arrival - departure

def max_flight_duration(data, reverse_special):
    max_duration = -1
    for flight in data:
        a1, a2, h1, m1, h2, m2 = flight
        if reverse_special and is_prime(a1) and a2 % sum_of_digits(a1) == 0:
            duration = calculate_duration(h2, m2, h1, m1)
        else:
            duration = calculate_duration(h1, m1, h2, m2)
        if duration > max_duration:
            max_duration = duration
    return max_duration

def main():
    with open('aeriana.in', 'r') as infile:
        c = int(infile.readline().strip())
        n = int(infile.readline().strip())
        flights_data = np.loadtxt(infile, dtype=int, ndmin=2)

    reverse_special_flights = c == 2
    max_duration = max_flight_duration(flights_data, reverse_special_flights)
    hours = max_duration // 60
    minutes = max_duration % 60

    with open('aeriana.out', 'w') as outfile:
        outfile.write(f'{hours} {minutes}\n')

if __name__ == "__main__":
    main()
