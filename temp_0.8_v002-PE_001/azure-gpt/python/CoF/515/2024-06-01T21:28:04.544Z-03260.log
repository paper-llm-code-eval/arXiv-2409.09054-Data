# hibrid_solver.py
import numpy as np

def read_data(filename):
    with open(filename, 'r') as file:
        C, P, N = map(int, file.readline().split())
        toll_sections = [list(map(int, file.readline().split())) for _ in range(P)]
        checkpoints = list(map(int, file.readline().split()))
    return C, P, N, toll_sections, checkpoints

def write_output(filename, output):
    with open(filename, 'w') as file:
        file.write(str(output) + '\n')

def calculate_toll_passes(P, N, toll_sections, checkpoints):
    toll_passes = np.zeros(P, dtype=np.int32)
    toll_costs = np.zeros(P, dtype=np.int32)
    for i, (start, end, cost) in enumerate(toll_sections):
        toll_costs[i] = cost
        for j in range(N - 1):
            if (checkpoints[j] < start and checkpoints[j + 1] > end) or (checkpoints[j] > end and checkpoints[j + 1] < start):
                toll_passes[i] += 1

    return toll_passes, toll_costs

def solve_problem(C, P, N, toll_sections, checkpoints):
    toll_passes, toll_costs = calculate_toll_passes(P, N, toll_sections, checkpoints)

    if C == 1:
        most_crossed = np.argwhere(toll_passes == np.amax(toll_passes))
        if most_crossed.size == 0:
            return -1
        return most_crossed[0][0] + 1
    else:
        total_cost = np.dot(toll_passes, toll_costs)
        return total_cost

def main():
    C, P, N, toll_sections, checkpoints = read_data('hibrid.in')
    output = solve_problem(C, P, N, toll_sections, checkpoints)
    write_output('hibrid.out', output)

if __name__ == "__main__":
    main()
```

