import numpy as np

# Constants
VOWELS = set("AEIOU")
CONSONANTS = set("BCDFGHJKLMNPQRSTVWXYZ")

# Read input data
with open("litere.in", "r") as fin:
    n = int(fin.readline().strip())
    word = fin.readline().strip()
    m = int(fin.readline().strip())
    codes = map(int, fin.readline().split())
    letter_to_code = dict(zip(set(word), codes))

# Calculate number of vowel-consonant pairs
pairs = sum((letter in VOWELS and word[i + 1] in CONSONANTS) or
            (letter in CONSONANTS and word[i + 1] in VOWELS)
            for i, letter in enumerate(word[:-1]))

# Prepare array for k values
k_values = np.zeros(n, dtype=np.int64)

# Map letters to codes
word_codes = np.array([letter_to_code[letter] for letter in word], dtype=np.int64)

# Calculate k1 and sum of all k values
k1 = int(''.join(map(str, word_codes)))
k_values[0] = k1
multiplier = 1
for i in range(1, n):
    multiplier = (multiplier * 10) % (10**9 + 7)  # Modulo to prevent overflow
    k_values[i] = k_values[i - 1] - word_codes[i - 1] * multiplier

k_sum = np.sum(k_values)
    
# Write output data
with open("litere.out", "w") as fout:
    fout.write(f"{pairs}\n{k1}\n{k_sum}\n")
```
