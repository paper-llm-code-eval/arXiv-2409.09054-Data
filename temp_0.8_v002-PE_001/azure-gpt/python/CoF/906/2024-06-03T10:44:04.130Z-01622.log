import numpy as np

# Function to calculate the total number of digits
def count_digits(numbers):
    return np.sum(np.floor(np.log10(numbers)) + 1).astype(int)

# Function to find the K-th digit
def find_kth_digit(numbers, lengths, k):
    for i, (num, length) in enumerate(zip(numbers, lengths)):
        if k <= length:
            return int(str(num)[k - 1])
        k -= length
    return -1  # Should not happen, as we are guaranteed to have at least K digits

# Function to find the largest number with P consecutive digits
def find_largest_number(numbers, lengths, p):
    num_str = ''.join(map(str, numbers))
    max_num = -1
    for i in range(len(num_str) - p + 1):
        current_num = int(num_str[i:i + p])
        max_num = max(max_num, current_num)
    return max_num

# Read input
with open('aur.in', 'r') as file:
    c = int(file.readline().strip())
    args = list(map(int, file.readline().strip().split()))
    numbers = np.array(file.readline().strip().split(), dtype=np.int64)

# Calculate lengths of numbers
lengths = np.floor(np.log10(numbers)) + 1

# Solve the problem based on the value of C
if c == 1:
    # Count the total number of digits
    result = count_digits(numbers)
elif c == 2:
    # Find the K-th digit
    n, k = args
    result = find_kth_digit(numbers, lengths, k)
elif c == 3:
    # Find the largest number with P consecutive digits
    n, p = args
    result = find_largest_number(numbers, lengths, p)

# Write output
with open('aur.out', 'w') as file:
    file.write(str(result) + '\n')
```

