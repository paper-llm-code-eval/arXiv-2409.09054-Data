import numpy as np

def read_data(filename):
    with open(filename, 'r') as file:
        lines = file.readlines()
    return [line.strip() for line in lines]

def write_result(filename, result):
    with open(filename, 'w') as file:
        file.write(str(result) + '\n')

def count_digits(numbers):
    return np.sum([int(np.log10(num)) + 1 for num in numbers])

def find_kth_digit(numbers, digit_lengths, k):
    idx = 0
    while k > digit_lengths[idx]:
        k -= digit_lengths[idx]
        idx += 1
    return int(str(numbers[idx])[k - 1])

def find_largest_number(numbers, p):
    joined_str = ''.join(map(str, numbers))
    max_num = ''
    for i in range(len(joined_str) - p + 1):
        if joined_str[i:i+p] > max_num:
            max_num = joined_str[i:i+p]
    return int(max_num)

def process_file(input_file, output_file):
    data = read_data(input_file)
    task_type, n = map(int, data[1].split()[:2])
    numbers = list(map(int, data[2].split()))

    if task_type == 1:
        result = count_digits(numbers)
    elif task_type == 2:
        k = int(data[1].split()[1])
        digit_lengths = [int(np.log10(num)) + 1 for num in numbers]
        result = find_kth_digit(numbers, digit_lengths, k)
    elif task_type == 3:
        p = int(data[1].split()[1])
        result = find_largest_number(numbers, p)

    write_result(output_file, result)

process_file('aur.in', 'aur.out')
