    # Read the points for each test
    test_points = np.array(fin.readline().strip().split(), dtype=int)

    # Read the bonus points
    B = int(fin.readline().strip())

    # Read the number of solutions submitted
    M = int(fin.readline().strip())

    # Initialize the maximum score to a negative value
    max_score = float('-inf')

    # Iterate over each submitted solution
    for attempt in range(M):
        # Read the results of the current submission
        results = np.array(fin.readline().strip().split(), dtype=int)

        # Calculate the score of the current submission
        score = np.sum(test_points * results)

        # Check if the solution passed all tests
        if np.all(results == 1):
            score += B  # Add bonus points

        # Subtract penalty points (2 points per each additional submission)
        score -= 2 * attempt

        # Update the maximum score if needed
        if score > max_score:
            max_score = score

# Open the output file
