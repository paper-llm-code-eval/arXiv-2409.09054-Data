import numpy as np

# Read the input data from submit.in
with open('submit.in', 'r') as f_in:
    N = int(f_in.readline().strip())
    P = np.array(f_in.readline().strip().split(), dtype=np.int32)
    B = int(f_in.readline().strip())
    M = int(f_in.readline().strip())
    attempts = np.zeros((M, N), dtype=np.int8)
    for i in range(M):
        attempts[i] = np.array(f_in.readline().strip().split(), dtype=np.int8)

# Calculate penalty for each attempt
penalty = np.arange(0, 2*M, 2)

# Calculate the scores for each attempt, including bonus and penalty
scores = np.dot(attempts, P) + B*(np.all(attempts, axis=1)) - penalty

# Determine the maximum score obtained
max_score = scores.max()

# Write the output data to submit.out
with open('submit.out', 'w') as f_out:
    f_out.write(str(max_score))
