import numpy as np

def read_input(file_path):
    with open(file_path, 'r') as file:
        N = int(file.readline().strip())
        test_scores = np.array(file.readline().strip().split(), dtype=int)
        bonus = int(file.readline().strip())
        M = int(file.readline().strip())
        submissions = [list(map(int, file.readline().strip().split())) for _ in range(M)]
    return N, test_scores, bonus, M, submissions

def calculate_score(submission, test_scores, penalty, bonus):
    score = np.dot(submission, test_scores) - penalty
    if all(submission):
        score += bonus
    return score

def write_output(file_path, score):
    with open(file_path, 'w') as file:
        file.write(f"{score}\n")

def main():
    N, test_scores, bonus, M, submissions = read_input('submit.in')
    max_score = 0
    penalty = 0
    for submission in submissions:
        submission_array = np.array(submission)
        score = calculate_score(submission_array, test_scores, penalty, bonus)
        max_score = max(max_score, score)
        penalty += 2
    write_output('submit.out', max_score)

if __name__ == "__main__":
    main()
