import numpy as np

def read_input(filename):
    with open(filename, 'r') as f:
        C = int(f.readline().strip())
        N, K = map(int, f.readline().strip().split())
        courses = [tuple(map(int, f.readline().strip().split())) for _ in range(N)]
    return C, N, K, courses

def write_output(filename, result):
    with open(filename, 'w') as f:
        f.write(str(result) + '\n')

def max_courses_scheduled(courses, K):
    # Sort courses by end time
    sorted_courses = sorted(courses, key=lambda x: x[1])

    # Initialize priority queue (min heap)
    rooms = []

    count = 0
    for course in sorted_courses:
        # Remove rooms that are now free
        while rooms and rooms[0] <= course[0]:
            heapq.heappop(rooms)

        # Add current course's end time
        heapq.heappush(rooms, course[1])
        if len(rooms) <= K:
            count += 1
        else:
            heapq.heappop(rooms)

    return count

def max_course_duration(courses, N, K):
    # Find the maximum duration that can be assigned to each course
    max_duration = 0
    for duration in range(1, max(courses, key=lambda x: x[1])[1] + 1):
        sorted_courses = sorted([(start, start + duration) for start, end in courses], key=lambda x: x[1])
        count = max_courses_scheduled(sorted_courses, K)
        if count == N:
            max_duration = duration
        else:
            break
    return max_duration

# Read input
C, N, K, courses = read_input('cursuri.in')

# Solve the problem
