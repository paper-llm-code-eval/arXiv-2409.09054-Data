import numpy as np
import heapq

# Read the input data from lanterna.in
with open('lanterna.in', 'r') as f:
    N, K = map(int, f.readline().split())
    recharging_stations = np.array(list(map(int, f.readline().split())), dtype=np.int8)
    M = int(f.readline())
    roads = [list(map(int, f.readline().split())) for _ in range(M)]

# Initialize the graph with no edges and infinite weights
graph = {i: [] for i in range(1, N + 1)}
for road in roads:
    a, b, T, W = road
    graph[a].append((b, T, W))
    graph[b].append((a, T, W))

def dijkstra_with_recharging(graph, N, recharging_stations, K):
    # Distance array, with time and minimum lantern type required
    # Format: dist[node][lantern_type] = (time, min_lantern_type)
    dist = np.full((N + 1, K + 1, 2), np.inf)
    dist[1, 1:] = (0, np.arange(1, K + 1))  # Starting point with all lantern types

    # Priority queue: (time, node, remaining_watt, lantern_type)
    queue = [(0, 1, w, w) for w in range(1, K + 1)]
    heapq.heapify(queue)

    while queue:
        time, node, remaining_watt, lantern_type = heapq.heappop(queue)

        # If this path is already worse than the best known, skip it
        if time > dist[node, lantern_type, 0]:
            continue

        for neighbor, T, W in graph[node]:
            # If the remaining watts are enough for the road
            if remaining_watt >= W:
                next_remaining_watt = remaining_watt - W if not recharging_stations[neighbor - 1] else K
                next_time = time + T
                # If we found a faster way or equal time with less watts
                if next_time < dist[neighbor, lantern_type, 0] or \
                   (next_time == dist[neighbor, lantern_type, 0] and lantern_type < dist[neighbor, lantern_type, 1]):
                    dist[neighbor, lantern_type] = (next_time, lantern_type)
                    heapq.heappush(queue, (next_time, neighbor, next_remaining_watt, lantern_type))

    # Find the minimum time and corresponding minimum lantern type
    min_time, min_lantern_type = min((dist[N, w, 0], w) for w in range(1, K + 1)
                                      if dist[N, w, 0] != np.inf)
    return min_time, min_lantern_type

# Run dijkstra algorithm and get the minimum time and lantern type
T_min, W_min = dijkstra_with_recharging(graph, N, recharging_stations, K)

# Write the output data to lanterna.out
with open('lanterna.out', 'w') as f:
    f.write(f'{T_min} {W_min}\n')
