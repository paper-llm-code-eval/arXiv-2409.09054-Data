import numpy as np
import heapq

# Read input data
with open('lanterna.in', 'r') as file:
    N, K = map(int, file.readline().split())
    recharge_stations = np.array(file.readline().split(), dtype=int)
    M = int(file.readline())
    edges = [list(map(int, file.readline().split())) for _ in range(M)]

# Adjacency list for the graph (roads between military objectives)
adj_list = {i: [] for i in range(1, N+1)}
for a, b, T, W in edges:
    adj_list[a].append((b, T, W))
    adj_list[b].append((a, T, W))

# Dijkstra's algorithm with a slight modification to find the minimum lantern type
def dijkstra():
    # Initialize distances
    distances = {i: [float('inf'), K+1] for i in range(1, N+1)}
    distances[1] = [0, 0]
    priority_queue = [(0, 0, 1)]  # (time_spent, lantern_type, current_position)

    while priority_queue:
        time_spent, lantern_type, current_position = heapq.heappop(priority_queue)

        # If we reached our destination with a better time or equal time but with a smaller lantern type, update it
        if current_position == N and (time_spent < distances[N][0] or (time_spent == distances[N][0] and lantern_type < distances[N][1])):
            distances[N] = [time_spent, lantern_type]

        for neighbor, T, W in adj_list[current_position]:
            # Calculate new distance and determine the lantern type needed
            new_time = time_spent + T
            new_lantern_type = max(lantern_type, W)

            # We proceed only if we improve the time or maintain the time with a smaller lantern type
            if new_time < distances[neighbor][0] or (new_time == distances[neighbor][0] and new_lantern_type < distances[neighbor][1]):
                distances[neighbor] = [new_time, new_lantern_type]
                # If we reach a recharge station, battery consumed is reset to 0
                if recharge_stations[neighbor-1] == 1:
                    heapq.heappush(priority_queue, (new_time, 0, neighbor))
                else:
                    heapq.heappush(priority_queue, (new_time, new_lantern_type, neighbor))

    return distances[N]

T_min, W_min = dijkstra()

# Write output data
with open('lanterna.out', 'w') as file:
    file.write(f"{T_min} {W_min}\n")
```

