        self.to_node = to_node
        self.time = time
        self.wattage = wattage

def dijkstra(nodes, K):
    queue = []
    heappush(queue, (0, 1, 0))  # time, node_id, wattage_used
    times = np.full((K + 1, len(nodes) + 1), np.inf)
