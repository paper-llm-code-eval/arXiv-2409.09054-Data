import numpy as np
from queue import PriorityQueue

# Function to read the input data from the file
def read_input(filename):
    with open(filename, 'r') as f:
        N, K = map(int, f.readline().split())
        bases = np.array(f.readline().split(), dtype=int)
        M = int(f.readline())
        roads = [list(map(int, f.readline().split())) for _ in range(M)]
    return N, K, bases, M, roads

# Function to write the output data to the file
def write_output(filename, result):
    with open(filename, 'w') as f:
        f.write(f"{result[0]} {result[1]}")

# Function to find the minimum battery type for the shortest path
def find_min_battery_for_shortest_path(N, K, bases, M, roads):
    # Create an adjacency matrix with np.inf for the roads that don't exist
    graph = {i: {} for i in range(1, N + 1)}
    for road in roads:
        a, b, T, W = road
        if b not in graph[a]:
            graph[a][b] = (T, W)
        if a not in graph[b]:
            graph[b][a] = (T, W)
    
    # Use Dijkstra's algorithm to find the shortest path
    pq = PriorityQueue()
    pq.put((0, 1, 0))  # Time, node, battery_consumption
    min_time = np.inf
    battery_consumption = np.inf
    distance = {i: np.inf for i in range(1, N + 1)}
    distance[1] = 0

    while not pq.empty():
        time, node, battery = pq.get()
        if node == N and time < min_time:
            min_time = time
            battery_consumption = battery

        for neighbour in graph[node]:
            neighbour_time, neighbour_battery = graph[node][neighbour]
            if bases[node]:
                # Recharge battery if on a friendly base
                total_time = time + neighbour_time
                total_battery = neighbour_battery
            else:
                # Check if the battery has enough charge
                if battery < neighbour_battery:
                    continue
                total_time = time + neighbour_time
                total_battery = battery + neighbour_battery

            if total_time < distance[neighbour]:
                distance[neighbour] = total_time
                pq.put((total_time, neighbour, total_battery))

    return min_time, battery_consumption

# Read the input
N, K, bases, M, roads = read_input('lanterna.in')

# Find the minimum battery type for the shortest path
result = find_min_battery_for_shortest_path(N, K, bases, M, roads)

# Write the output
write_output('lanterna.out', result)
```

