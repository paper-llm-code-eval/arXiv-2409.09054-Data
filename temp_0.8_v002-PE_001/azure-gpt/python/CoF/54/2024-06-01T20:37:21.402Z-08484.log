import numpy as np
import heapq

def dijkstra(nodes, graph, recharging_stations, max_watts):
    q = [(0, 1, max_watts)]  # (time, position, remaining watts)
    visited = {(1, max_watts): 0}  # using a dictionary to store visited states with their times

    while q:
        time, node, watts = heapq.heappop(q)
        if node == nodes:
            return time
        for neighbour, t, w in graph[node]:
            if neighbour in recharging_stations:
                new_watts = max_watts
            else:
                new_watts = watts - w

            if new_watts >= 0:
                if (neighbour, new_watts) not in visited or visited[(neighbour, new_watts)] > time + t:
                    visited[(neighbour, new_watts)] = time + t
                    heapq.heappush(q, (time + t, neighbour, new_watts))
    return float('inf')

def find_optimal_lantern(file_in, file_out):
    with open(file_in, 'r') as fin:
        n, k = map(int, fin.readline().split())
        recharging_stations = np.nonzero(np.array(fin.readline().split(), dtype=int))[0] + 1
        m = int(fin.readline().strip())
        
        graph = {i: [] for i in range(1, n+1)}
        for _ in range(m):
            a, b, t, w = map(int, fin.readline().split())
            graph[a].append((b, t, w))
            graph[b].append((a, t, w))

    optimal_time = float('inf')
    optimal_watts = 0

    for watts in range(1, k + 1):
        time = dijkstra(n, graph, recharging_stations, watts)
        if time < optimal_time:
            optimal_time = time
            optimal_watts = watts
        elif time == optimal_time:
            optimal_watts = min(optimal_watts, watts)

    with open(file_out, 'w') as fout:
        fout.write(f"{optimal_time} {optimal_watts}\n")

# Now call the function with the input and output file names
find_optimal_lantern('lanterna.in', 'lanterna.out')
