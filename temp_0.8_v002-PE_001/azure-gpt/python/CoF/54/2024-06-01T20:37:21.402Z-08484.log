import heapq

# Read the input data from `lanterna.in`.
with open('lanterna.in', 'r') as file:
    N, K = map(int, file.readline().split())
    recharging_bases = list(map(int, file.readline().split()))
    M = int(file.readline().strip())
    edges = [list(map(int, file.readline().split())) for _ in range(M)]

# Prepare the adjacency list for the graph.
graph = {i: [] for i in range(N)}
for a, b, T, W in edges:
    graph[a - 1].append((b - 1, T, W))
    graph[b - 1].append((a - 1, T, W))

# Define Dijkstra's algorithm for finding the shortest path.
def dijkstra(start, end, graph, recharging_bases, initial_watts):
    pq = [(0, start, initial_watts)]  # Priority queue: (time, node, remaining_watts)
    visited = set()
    while pq:
        time, node, watts = heapq.heappop(pq)
        if (node, watts) in visited:
            continue
        visited.add((node, watts))
        if node == end:
            return time
        for neigh, t, w in graph[node]:
            if recharging_bases[neigh]:
                next_watts = initial_watts
            else:
                next_watts = watts - w
            if next_watts >= 0 and (neigh, next_watts) not in visited:
                heapq.heappush(pq, (time + t, neigh, next_watts))
    return float('inf')

# Find the smallest type of lantern for which the travel time is minimal.
min_time = float('inf')
min_watts = 0

# Iterate over all possible lantern types.
for initial_watts in range(1, K + 1):
    time = dijkstra(0, N - 1, graph, recharging_bases, initial_watts)
    if time < min_time:
        min_time, min_watts = time, initial_watts
    elif time == min_time and initial_watts < min_watts:
        min_watts = initial_watts

# Write the output to `lanterna.out`.
with open('lanterna.out', 'w') as file:
    file.write(f"{min_time} {min_watts}\n")
