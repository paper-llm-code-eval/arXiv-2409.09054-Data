# lanterna.py
import heapq
import numpy as np

def read_input(filename):
    with open(filename, 'r') as file:
        N, K = map(int, file.readline().split())
        recharge_stations = list(map(int, file.readline().split()))
        M = int(file.readline())
        roads = []
        for _ in range(M):
            a, b, T, W = map(int, file.readline().split())
            roads.append((a, b, T, W))
    return N, K, recharge_stations, roads

def write_output(filename, min_time, min_watt):
    with open(filename, 'w') as file:
        file.write(f"{min_time} {min_watt}\n")

def dijkstra(N, M, roads, recharge_stations, K):
    graph = {i: [] for i in range(1, N+1)}
    for a, b, T, W in roads:
        graph[a].append((b, T, W))
        graph[b].append((a, T, W))

    pq = []
    # (time, current_node, current_watt, watt_type)
    heapq.heappush(pq, (0, 1, K, K))
    visited = np.full((N+1, K+1), np.inf)
    visited[1, K] = 0

    while pq:
        time, node, watt, watt_type = heapq.heappop(pq)
        if visited[node, watt] < time:
            continue
        if node == N:
            return time, watt_type
        for neighbor, t, w in graph[node]:
            if watt >= w:
                new_watt = watt - w if recharge_stations[node-1] == 0 else watt_type
                if visited[neighbor, new_watt] > time + t:
                    visited[neighbor, new_watt] = time + t
                    heapq.heappush(pq, (time + t, neighbor, new_watt, watt_type))

    return -1, -1

def solve_lanterna(N, K, recharge_stations, roads):
    min_time, min_watt = float('inf'), K
    for watt in range(K, 0, -1):
        time, watt_type = dijkstra(N, len(roads), roads, recharge_stations, watt)
        if time != -1:
            if time < min_time:
                min_time, min_watt = time, watt_type
            elif time == min_time and watt_type < min_watt:
                min_watt = watt_type
    return min_time, min_watt

# Main execution
if __name__ == '__main__':
    N, K, recharge_stations, roads = read_input('lanterna.in')
    min_time, min_watt = solve_lanterna(N, K, recharge_stations, roads)
    write_output('lanterna.out', min_time, min_watt)
```

To run this code, place it in a file named `lanterna.py`. Make sure that the `lanterna.in` file is in the same directory from where the script is executed. The solution will create or overwrite a file named `lanterna.out` with the required output.
