# lanterna.py
import numpy as np
import heapq

class Node:
    def __init__(self, id, recharge):
        self.id = id
        self.recharge = recharge
        self.adjacent = []

    def add_edge(self, neighbor, time, watt):
        self.adjacent.append((neighbor, time, watt))

def dijkstra(nodes, K):
    min_time = np.full((K + 1, len(nodes)), np.inf)
    min_time[:, 1] = 0

    min_lantern = K + 1
    heap = [(0, 1, min_lantern)]  # time, node, lantern

    while heap:
        time, current_node, lantern = heapq.heappop(heap)

        if current_node == len(nodes) - 1 and lantern < min_lantern:
            min_lantern = lantern

        for neighbor, t, w in nodes[current_node].adjacent:
            new_time = time + t
            new_lantern = min(lantern, w) if nodes[neighbor].recharge else lantern

            if new_time < min_time[new_lantern, neighbor]:
                min_time[new_lantern, neighbor] = new_time
                heapq.heappush(heap, (new_time, neighbor, new_lantern))

    return min_time[1:K + 1, -1]

def read_input(filename):
    with open(filename, 'r') as file:
        N, K = map(int, file.readline().split())
        recharge_stations = list(map(int, file.readline().split()))
        M = int(file.readline())

        nodes = [Node(i, recharge) for i, recharge in enumerate(recharge_stations)]

        for _ in range(M):
            a, b, T, W = map(int, file.readline().split())
            nodes[a].add_edge(b, T, W)
            nodes[b].add_edge(a, T, W)

    return nodes, K

def write_output(filename, time, lantern):
    with open(filename, 'w') as file:
        file.write(f"{time} {lantern}\n")

def main():
    nodes, K = read_input('lanterna.in')

    min_times = dijkstra(nodes, K)
    min_time = np.min(min_times)
    min_lantern = np.argmin(min_times) + 1

    write_output('lanterna.out', int(min_time), min_lantern)

if __name__ == '__main__':
    main()
```

