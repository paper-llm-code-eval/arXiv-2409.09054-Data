import numpy as np

def main():
    with open('album.in', 'r') as file:
        lines = file.readlines()
        c = int(lines[0].strip())
        n = int(lines[1].strip())
        stickers = np.array(list(map(int, lines[2].strip().split())), dtype=np.int32)

    if c == 1:
        with open('album.out', 'w') as file:
            # Get the last sticker
            last_sticker = stickers[-1]
            # Extract the digits and sort them
            digits = sorted(set(int(digit) for digit in str(last_sticker)), reverse=True)
            # Write the top two digits to the output file
            file.write("{} {}".format(digits[1], digits[0]))
    elif c == 2:
        victor_count, radu_count = 0, 0
        turn = 'V'  # Victor starts
        i = 0
        while i < n:
            # First sticker taken by the current turn
            first_sticker = stickers[i]
            # Find the two largest digits
            digits = sorted(set(int(digit) for digit in str(first_sticker)), reverse=True)
            max_digit, second_max_digit = digits[0], digits[0] if len(digits) == 1 else digits[1]
            # Increment sticker count for the current turn
            if turn == 'V':
                victor_count += 1
            else:
                radu_count += 1
            # Skip stickers with the same two largest digits
            i += 1
            while i < n and (max_digit in map(int, str(stickers[i])) or \
                             second_max_digit in map(int, str(stickers[i]))):
                if turn == 'V':
                    victor_count += 1
                else:
                    radu_count += 1
                i += 1
            # Switch turn
            turn = 'R' if turn == 'V' else 'V'
        # Write the result to the output file
        with open('album.out', 'w') as file:
            if victor_count > radu_count:
                file.write("V\n{}".format(victor_count))
            elif radu_count > victor_count:
                file.write("R\n{}".format(radu_count))
            else:
                file.write("V R\n{}".format(victor_count))  # They have the same number

main()
