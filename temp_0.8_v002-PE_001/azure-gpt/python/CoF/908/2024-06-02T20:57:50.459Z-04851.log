import numpy as np

def get_two_largest_digits(number):
    digits = [int(digit) for digit in str(number)]
    return sorted(set(digits))[-2:]

def main():
    with open('album.in', 'r') as fin:
        c = int(fin.readline().strip())
        n = int(fin.readline().strip())
        stickers = np.fromstring(fin.readline().strip(), dtype=int, sep=' ')

    if c == 1:
        last_sticker_digits = get_two_largest_digits(stickers[-1])
        with open('album.out', 'w') as fout:
            fout.write(f"{last_sticker_digits[0]} {last_sticker_digits[1]}\n")
    elif c == 2:
        victor_stickers = 0
        radu_stickers = 0
        turn = 0 # 0 for Victor, 1 for Radu

        while stickers.size > 0:
            first_sticker = stickers[0]
            max_digits = get_two_largest_digits(first_sticker)
            turn_stickers = np.logical_or(stickers >= 10**5 * max_digits[1],
                                          np.logical_or(stickers % 10 == max_digits[0],
                                                        np.logical_or(stickers % 10 == max_digits[1],
                                                                      stickers // 10 % 10 == max_digits[0],
                                                                      stickers // 10 % 10 == max_digits[1])))
            count = np.sum(turn_stickers)

            if turn == 0:
                victor_stickers += count
            else:
                radu_stickers += count
            
            stickers = stickers[~turn_stickers]
            turn = 1 - turn

        with open('album.out', 'w') as fout:
            if victor_stickers > radu_stickers:
                fout.write(f"V\n{victor_stickers}\n")
            elif radu_stickers > victor_stickers:
                fout.write(f"R\n{radu_stickers}\n")
            else:
                fout.write(f"V R\n{victor_stickers}\n")

if __name__ == '__main__':
    main()
