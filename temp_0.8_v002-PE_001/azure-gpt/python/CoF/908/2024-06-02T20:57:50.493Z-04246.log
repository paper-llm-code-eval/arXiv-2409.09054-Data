import numpy as np

def read_data(file_name):
    with open(file_name, 'r') as file:
        c = int(file.readline().strip())
        n = int(file.readline().strip())
        stickers = np.array(file.readline().split(), dtype=np.int32)
    return c, stickers

def write_data(file_name, result):
    with open(file_name, 'w') as file:
        if isinstance(result, tuple):
            file.write(f"{result[0]} {result[1]}\n")
        else:
            file.write(f"{result}\n")

def get_digits(number):
    return sorted(set(str(number)))[-2:]

def task1(stickers):
    return get_digits(stickers[-1])

def task2(stickers):
    victor_count, radu_count = 0, 0
    turn_victor = True
    while stickers.size > 0:
        first_sticker = stickers[0]
        big_digits = get_digits(first_sticker)
        to_take = np.array([any(digit in str(sticker) for digit in big_digits) 
                            for sticker in stickers])
        count = np.sum(to_take)
        if turn_victor:
            victor_count += count
        else:
            radu_count += count
        stickers = stickers[~to_take]
        turn_victor = not turn_victor

    if victor_count > radu_count:
        return 'V', victor_count
    if radu_count > victor_count:
        return 'R', radu_count
    return 'V R', victor_count

def solve():
    c, stickers = read_data('album.in')
    if c == 1:
        result = task1(stickers)
    else:
        result = task2(stickers)
    write_data('album.out', result)

solve()
