import numpy as np

def read_input(filename):
    with open(filename, 'r') as f:
        c = int(f.readline().strip())
        n = int(f.readline().strip())
        stickers = np.fromstring(f.readline().strip(), dtype=int, sep=' ')
    return c, n, stickers

def most_significant_digits(number):
    digits = [int(d) for d in str(number)]
    return sorted(digits)[-2:]

def task1(stickers):
    last_sticker_digits = most_significant_digits(stickers[-1])
    with open('album.out', 'w') as f:
        f.write(f"{last_sticker_digits[0]} {last_sticker_digits[1]}\n")

def task2(n, stickers):
    victor_count = 0
    radu_count = 0
    turn = 0 # 0 for Victor, 1 for Radu

    while stickers.size > 0:
        first_sticker = stickers[0]
        # Find the most significant digits of the first sticker
        significant_digits = set(most_significant_digits(first_sticker))
        # Indices where the stickers have the most significant digits
        indices = [i for i, sticker in enumerate(stickers) if set(most_significant_digits(sticker)) & significant_digits]
        # Update counts and remove the stickers
        if turn == 0:
            victor_count += len(indices)
        else:
            radu_count += len(indices)
        stickers = np.delete(stickers, indices)
        turn = 1 - turn

    with open('album.out', 'w') as f:
        if victor_count > radu_count:
            f.write(f"V\n{victor_count}\n")
        elif radu_count > victor_count:
            f.write(f"R\n{radu_count}\n")
        else:
            f.write(f"V R\n{victor_count}\n")

def main():
    c, n, stickers = read_input('album.in')
    
    if c == 1:
        task1(stickers)
    elif c == 2:
        task2(n, stickers)

if __name__ == "__main__":
    main()
```
