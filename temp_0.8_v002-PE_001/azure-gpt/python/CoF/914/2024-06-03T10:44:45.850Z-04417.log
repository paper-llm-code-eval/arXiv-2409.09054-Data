import numpy as np

MODULO = 1234567

def calculate_good_deeds(n, k, zv, zt):
    max_participants = (k + 1) // 2

    # Calculate the number of days needed for each good deed
    elder_days = np.arange(zv, n + 1, zv)
    youth_days = np.arange(zt, n + 1, zt)

    # Determine the number of elder and youth participants
    elder_participants = min(max_participants, len(elder_days))
    youth_participants = min(max_participants, len(youth_days))

    # Calculate the total expected good deeds by elders and youth
    total_elder_deeds = np.sum((n - elder_days[:elder_participants]) // zv + 1)
    total_youth_deeds = np.sum((n - youth_days[:youth_participants]) // zt + 1)
    total_deeds = total_elder_deeds + total_youth_deeds

    # Calculate the remaining good deeds to be done after n days
    remaining_deeds = k - total_deeds % k if total_deeds % k != 0 else 0
    return remaining_deeds % MODULO

# Read from 'pif.in'
with open('pif.in', 'r') as infile:
    n = int(infile.readline().strip())
    k = int(infile.readline().strip())
    zv, zt = map(int, infile.readline().strip().split())

# Calculate the number of good deeds and write to 'pif.out'
fb = calculate_good_deeds(n, k, zv, zt)
with open('pif.out', 'w') as outfile:
    outfile.write(str(fb))
``` 

