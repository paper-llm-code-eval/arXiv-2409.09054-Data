import numpy as np

# Function to decode the wall information from the map integer value.
def decode_walls(value):
    return [value & 1, (value >> 1) & 1, (value >> 2) & 1, (value >> 3) & 1]

# Function to perform DFS and find the area of a room.
def dfs(harta, i, j, N):
    if i < 0 or j < 0 or i >= N or j >= N or harta[i][j] is None:
        return 0
    area = 1
    walls = harta[i][j]  # Save wall info
    harta[i][j] = None  # Mark as visited
    
    # Decode walls information and move to next squares if no wall is present.
    west, south, east, north = decode_walls(walls)
    if not west: area += dfs(harta, i, j - 1, N)
    if not south: area += dfs(harta, i + 1, j, N)
    if not east: area += dfs(harta, i, j + 1, N)
    if not north: area += dfs(harta, i - 1, j, N)
    
    return area

# Read data from file
with open('castel.in', 'r') as file:
    C = int(file.readline().strip())
    N = int(file.readline().strip())
    harta = np.array([list(map(int, line.split())) for line in file.readlines()], dtype=np.object)

room_count = 0
max_area = 0
max_room_coord = None

# Iterate over the map to find rooms and calculate area.
for i in range(N):
    for j in range(N):
        if harta[i][j] is not None:
            room_count += 1
            room_area = dfs(harta, i, j, N)
            if room_area > max_area:
                max_area = room_area
                max_room_coord = (i, j)

# Depending on the value of C, write the appropriate output.
with open('castel.out', 'w') as file:
    if C == 1:
        file.write(f"{room_count}\n")
    elif C == 2:
        file.write(f"{max_area}\n")
    elif C == 3:
        # To find the bottom-right corner of the room, add the size of the room minus 1 to the top-left coordinates.
        bottom_right_coord = (max_room_coord[0] + int(np.sqrt(max_area)) - 1, max_room_coord[1] + int(np.sqrt(max_area)) - 1)
        file.write(f"{max_room_coord[0]} {max_room_coord[1]} {bottom_right_coord[0]} {bottom_right_coord[1]}\n")
```

