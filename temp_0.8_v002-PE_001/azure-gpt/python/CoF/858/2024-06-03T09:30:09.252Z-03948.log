import numpy as np

# Function to check if a number is balanced
def is_balanced(num):
    digits = np.array([int(d) for d in str(num)])
    odd_sum = np.sum(digits[::2])
    even_sum = np.sum(digits[1::2])
    return odd_sum == even_sum

# Function to find the next balanced number greater than N
def next_balanced(N):
    # Start searching from the next number
    N += 1
    while not is_balanced(N):
        N += 1
    return N

# Read N from file
with open('ech.in', 'r') as file:
    N = int(file.read().strip())

# Find the next balanced number
next_balanced_num = next_balanced(N)

# Write the result to the file
with open('ech.out', 'w') as file:
    file.write(str(next_balanced_num))
