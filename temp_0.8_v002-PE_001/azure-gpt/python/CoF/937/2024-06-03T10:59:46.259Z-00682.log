    for i in range(N - 1):
        passage_info = list(map(int, lines[i + 2].split()))
        for passage in passage_info[1:]:
            passages[i, passage - 1] = True
    return challenge_type, N, M, X, passages

def find_exit_tunnel(N, M, X, passages):
    current_tunnel = X - 1
    while current_tunnel < N - 1 and np.any(passages[current_tunnel, :-1]):
        next_tunnel_passage = np.flatnonzero(passages[current_tunnel, :-1])
        if M - 1 in next_tunnel_passage:
            return N
        current_tunnel += 1
    return current_tunnel + 1

def find_path_length(N, M, X, passages):
    path_length = 0
    current_tunnel = X - 1
    while current_tunnel < N - 1:
        if not np.any(passages[current_tunnel, :-1]):
            return path_length + M
        next_tunnel_passage = np.flatnonzero(passages[current_tunnel, :-1])
        if M - 1 in next_tunnel_passage:
            return path_length + M + 1
        path_length += next_tunnel_passage[0] + 1
        current_tunnel += 1
        path_length += 1
    return path_length + M

def main():
    lines = read_input('tunel.in')
    challenge_type, N, M, X, passages = process_input(lines)
    
    if challenge_type == 1:
        result = find_exit_tunnel(N, M, X, passages)
    elif challenge_type == 2:
        result = find_path_length(N, M, X, passages)
    
    write_output('tunel.out', result)

if __name__ == "__main__":
