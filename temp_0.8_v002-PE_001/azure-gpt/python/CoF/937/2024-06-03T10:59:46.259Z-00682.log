    steps = M  # Starting from the first unit of the tunnel
    while tunnel < len(passages):
        path = passages[tunnel]
        if M in path:
            if tunnel == len(passages) - 1:  # Last tunnel
                steps += 1  # Directly to the reward
                break
            # Go to the next tunnel through the passage
            tunnel += 1
            steps += 1  # Crossing the passage
        else:
            # No more passages, exit the current tunnel
            break
    return steps

def main():
    C, N, M, X, passages = read_input('tunel.in')
    if C == 1:
        result = solve_challenge_1(X, passages)
    elif C == 2:
        result = solve_challenge_2(X, M, passages)
    write_output('tunel.out', result)

