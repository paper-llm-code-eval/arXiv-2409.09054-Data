import numpy as np

# Read input data from file
with open('suma.in', 'r') as file:
    n = int(file.readline().strip())
    costs = np.array(list(map(int, file.readline().split())), dtype=np.int32)

# Calculate number of levels in the pyramid
m = int(np.sqrt(1 + 8 * n) - 1) // 2

# Initialize arrays to store minimum costs and paths
min_cost = np.zeros((m + 1, m), dtype=np.int32) + 100 * n
prev_room = np.zeros((m + 1, m), dtype=np.int32)

# The top level has only one room with its cost
min_cost[1, 0] = costs[0]

# Compute minimum cost for each room at each level
for level in range(2, m + 1):
    for room in range(level):
        room_number = (level * (level - 1)) // 2 + room
        upper_level = level - 1
        for prev_room_offset in range(-1, 2):
            prev_room_number = room_number - level - prev_room_offset
            if 0 <= prev_room_number - ((upper_level * (upper_level - 1)) // 2) < upper_level:
                new_cost = min_cost[upper_level, prev_room_number - ((upper_level - 1) * upper_level) // 2] + costs[room_number]
                if new_cost < min_cost[level, room]:
                    min_cost[level, room] = new_cost
                    prev_room[level, room] = prev_room_number

# The minimum cost is the smallest value in the last level
min_total_cost = min(min_cost[m])

# Find the room with the minimum cost in the last level
last_room = np.argmin(min_cost[m])

# Backtrack to find the path
path = [0] * m
current_room = last_room
for level in range(m, 0, -1):
    room_number = (level * (level - 1)) // 2 + current_room
    path[level - 1] = room_number + 1
    current_room = prev_room[level, current_room] - ((level - 1) * (level - 2) // 2)

# Write output data to file
with open('suma.out', 'w') as file:
    file.write(f"{m} {min_total_cost}\n")
    file.write(' '.join(map(str, path)))
```

