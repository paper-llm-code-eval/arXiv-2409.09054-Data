import numpy as np

# Function to find the number of levels m in the pyramid given n rooms
def find_levels(n):
    m = 1
    while m * (m + 1) * (2 * m + 1) // 6 < n:
        m += 1
    return m

# Function to reconstruct the minimum cost path
def reconstruct_path(parents, end, level):
    path = []
    while level > 0:
        path.append(end)
        end = parents[level][end]
        level -= 1
    return path[::-1]

# Read input data
with open('suma.in', 'r') as file:
    n = int(file.readline().strip())
    costs = [0] + list(map(int, file.readline().strip().split()))

# Initialize variables
m = find_levels(n)
current_level = [0] * ((m + 1) ** 2)
parents = [np.zeros((m + 1) ** 2, dtype=int) for _ in range(m + 1)]

# Base case: the cost of the top room
current_level[1] = costs[1]

# Dynamic programming to calculate minimum costs for each level
for level in range(2, m + 1):
    next_level = [0] * ((level + 1) ** 2)
    for room in range(1, level ** 2 + 1):
        base_room = room + (level - 1) ** 2
        for dr, dc in [(0, 0), (1, 0), (1, 1), (0, 1)]:
            next_room = base_room + dr * level + dc
            if next_level[next_room] == 0 or next_level[next_room] > current_level[room] + costs[next_room]:
                next_level[next_room] = current_level[room] + costs[next_room]
                parents[level][next_room] = room
    current_level = next_level

# Find the minimum cost in the last level
min_cost = min(filter(lambda x: x > 0, current_level))
min_room = current_level.index(min_cost)

# Reconstruct the path
path = reconstruct_path(parents, min_room, m)

# Write output data
with open('suma.out', 'w') as file:
    file.write(f'{m} {min_cost}\n')
    file.write(' '.join(map(str, path)) + '\n')
