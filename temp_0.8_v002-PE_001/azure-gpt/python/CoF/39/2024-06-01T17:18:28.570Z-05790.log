import numpy as np

# Function to identify the number of levels (m) given the number of rooms (n)
def find_levels(n):
    m = 0
    current_sum = 0
    while current_sum < n:
        m += 1
        current_sum += m**2
    return m

# Function to reconstruct the path given parent pointers
def reconstruct_path(parents, level, end):
    path = [end]
    while level > 1:
        end = parents[level][end]
        path.append(end)
        level -= 1
    return path[::-1]

# Read input data from file
with open('suma.in', 'r') as file:
    n = int(file.readline().strip())
    costs = np.array([0] + list(map(int, file.readline().strip().split())), dtype=int)

# Initialize DP array and parent pointers
m = find_levels(n)
dp = [np.full((m + 1) ** 2, np.inf, dtype=int) for _ in range(m + 1)]
parents = [np.zeros((m + 1) ** 2, dtype=int) for _ in range(m + 1)]

# Set base case for the top room
dp[1][1] = costs[1]

# Dynamic programming to find the minimum path cost
for level in range(2, m + 1):
    for room in range(1, level**2 + 1):
        base_room = room + (level - 2) ** 2
        for dr, dc in [(0, 0), (1, 0), (1, 1), (0, 1)]:
            next_room = base_room + dr*level + dc
            if next_room <= level**2 + (level - 1)**2:
                if dp[level][next_room] > dp[level-1][room] + costs[next_room]:
                    dp[level][next_room] = dp[level-1][room] + costs[next_room]
                    parents[level][next_room] = room

# Extract the minimum cost from the last level
last_level_costs = dp[m][(m - 1)**2 + 1:m**2 + (m - 1)**2 + 1]
min_cost = np.min(last_level_costs)
min_room = np.argmin(last_level_costs) + (m - 1)**2 + 1

# Reconstruct the path
path = reconstruct_path(parents, m, min_room)

# Write output data to file
with open('suma.out', 'w') as file:
    file.write(f"{m} {min_cost}\n")
    file.write(" ".join(map(str, path)) + "\n")
