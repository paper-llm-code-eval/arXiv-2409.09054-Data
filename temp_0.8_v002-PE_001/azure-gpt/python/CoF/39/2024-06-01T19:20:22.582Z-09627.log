import numpy as np

def read_input(file_name):
    with open(file_name, 'r') as file:
        n = int(file.readline().strip())
        costs = np.fromstring(file.readline().strip(), dtype=int, sep=' ')
    return n, costs

def write_output(file_name, m, s, path):
    with open(file_name, 'w') as file:
        file.write(f'{m} {s}\n')
        file.write(' '.join(map(str, path)) + '\n')

def find_m_levels(n):
    # We find m by solving the equation for the sum of the first m squares (m*(m+1)*(2m+1)/6 = n)
    m = int(((3*(np.sqrt(24*n + 1) - 1)/2) ** (1/3) - 1) // 3 + 1)
    return m

def min_cost_path(n, costs):
    m = find_m_levels(n)
    # Create a 2D array to keep track of min costs and paths
    min_costs = np.full((m, m), np.inf, dtype=int)
    paths = np.empty((m, m), dtype=object)

    # Calculate the starting index of the last level
    start_level = n - m**2
    # Set the costs and paths for the last level
    for i in range(m):
        for j in range(m):
            idx = start_level + i * m + j
            min_costs[i, j] = costs[idx]
            paths[i, j] = [idx + 1]  # +1 to convert to 1-based indexing

    # Calculate the min costs and paths from bottom to top
    for level in range(m - 1, 0, -1):
        for i in range(level):
            for j in range(level):
                idx = start_level - (level**2 - i*level - j)
                min_cost = np.inf
                min_path = []
                for di, dj in [(0, 0), (0, 1), (1, 0), (1, 1)]:
                    ni, nj = i + di, j + dj
                    new_cost = min_costs[ni, nj] + costs[idx]
                    if new_cost < min_cost or (new_cost == min_cost and [idx + 1] + paths[ni, nj] < min_path):
                        min_cost = new_cost
                        min_path = [idx + 1] + paths[ni, nj]
                min_costs[i, j] = min_cost
                paths[i, j] = min_path
        start_level -= level**2

    # Return the min cost and path for the top level (0, 0)
    return m, min_costs[0, 0], paths[0, 0]

# Read the input, solve the problem, and write the output
