# Read data from file
with open('suma.in', 'r') as file:
    n = int(file.readline().strip())
    costs = np.array(file.readline().strip().split(), dtype=np.int32)

# Function to find the m value (levels of the pyramid)
def find_m(n):
    m = 1
    while (m * (m + 1) // 2) ** 2 < n:
        m += 1
    return m

# Function to calculate the minimum cost path
def minimum_cost_path(costs, m):
    # Create an array to store the minimum costs and the paths
    min_costs = np.full((m, m), np.inf)
    paths = np.zeros((m, m), dtype=object)
    
    # Base case: level 1
    min_costs[0, 0] = costs[0]
    paths[0, 0] = [1]

    # Fill the min_costs and paths arrays
    for level in range(1, m):
        for room in range(level + 1):
            base_index = (level * (level + 1) // 2) ** 2 + room * (level + 1)
            for move in [(0, 0), (1, 0), (0, 1), (1, 1)]:  # Possible moves: Down, East, South-East, South
                prev_room = (room + move[0], room + move[1])
                if prev_room[1] <= level - 1 and min_costs[prev_room] != np.inf:
                    new_cost = min_costs[prev_room] + costs[base_index:base_index + level + 1]
                    new_paths = [paths[prev_room] + [base_index + i + 1] for i in range(level + 1)]
                    for i in range(level + 1):
                        if new_cost[i] < min_costs[level, i]:
                            min_costs[level, i] = new_cost[i]
                            paths[level, i] = new_paths[i]

    # Find the minimum cost and corresponding path
    min_index = np.argmin(min_costs[-1])
    min_cost = min_costs[m - 1, min_index]
    min_path = paths[m - 1, min_index]
    
    return min_cost, min_path

# Find m and calculate the minimum cost path
m = find_m(n)
min_cost, min_path = minimum_cost_path(costs, m)

