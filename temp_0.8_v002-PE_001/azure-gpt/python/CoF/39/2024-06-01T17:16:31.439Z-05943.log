    m = 1
    while (m * (m + 1) // 2) ** 2 < n:
        m += 1
    return m

# Function to calculate the minimum cost path
def minimum_cost_path(costs, m):
    # Initialize a list to store minimum cost for each room
    min_costs = [float('inf')] * (m ** 2)
    paths = [None] * (m ** 2)

    # Base case: first room
    min_costs[0] = costs[0]
    paths[0] = [1]

    # Fill min_costs and paths for other rooms
    for level in range(1, m):
        for room in range(level * (level + 1) // 2, (level + 1) * (level + 2) // 2):
            above = room - level
            for i in range(level):
                if above - i >= 0 and min_costs[above - i] + costs[room] < min_costs[room]:
                    min_costs[room] = min_costs[above - i] + costs[room]
                    paths[room] = paths[above - i] + [room + 1]
                if above - i - 1 >= 0 and min_costs[above - i - 1] + costs[room] < min_costs[room]:
                    min_costs[room] = min_costs[above - i - 1] + costs[room]
                    paths[room] = paths[above - i - 1] + [room + 1]
    
    # The last level's rooms start from index (m-1)*m//2 and end at m*(m+1)//2 - 1
    min_cost_index = min(range((m - 1) * m // 2, m * (m + 1) // 2), key=lambda x: min_costs[x])
    min_cost = min_costs[min_cost_index]
    min_path = paths[min_cost_index]

    return min_cost, min_path

