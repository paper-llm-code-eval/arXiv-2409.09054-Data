# Code to solve the "pergament" problem
import numpy as np

def calculate_purple_squares(N, K, Q, A, B, C, D, X1, Y1, vertical_streets):
    # Initialize horizontal streets
    horizontal_streets = np.zeros((N, 2), dtype=int)
    horizontal_streets[0] = [X1, Y1]
    
    # Fill in the horizontal streets using the provided formulas
    for i in range(1, N):
        X_i = 1 + (horizontal_streets[i-1][0] * A + B) % K
        Y_i = 1 + (horizontal_streets[i-1][1] * C + D) % (K - X_i + 1)
        horizontal_streets[i] = [X_i, Y_i]

    # Calculate the number of purple squares
    purple_squares = 0
    for j, r, l in vertical_streets:
        for i in range(N):
            X_i, Y_i = horizontal_streets[i]
            start_horizontal = X_i
            end_horizontal = X_i + Y_i - 1
            start_vertical = r
            end_vertical = r + l - 1
            
            if start_vertical <= i+1 <= end_vertical and start_horizontal <= j <= end_horizontal:
                purple_squares += 1

    return purple_squares

def main():
    # Read input
    with open('pergament.in', 'r') as file:
        N, K, Q = map(int, file.readline().strip().split())
        A, B, C, D = map(int, file.readline().strip().split())
