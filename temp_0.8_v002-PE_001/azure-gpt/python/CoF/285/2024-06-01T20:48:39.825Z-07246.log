# Required Libraries
import numpy as np

def read_input(filename):
    with open(filename, 'r') as file:
        C, N, P = map(int, file.readline().split())
        pulsars = [tuple(map(int, file.readline().split())) for _ in range(P)]
        vulcan = tuple(map(int, file.readline().split()))
        qonos = tuple(map(int, file.readline().split()))
    return C, N, P, pulsars, vulcan, qonos

def write_output(filename, result):
    with open(filename, 'w') as file:
        file.write(str(result))

def max_affected_sectors(N, pulsars):
    galaxy = np.zeros((N, N), dtype=int)
    for x, y, r, t in pulsars:
        for i in range(max(0, x-r), min(N, x+r)):
            for j in range(max(0, y-r), min(N, y+r)):
                if abs(i-x) + abs(j-y) <= r:
                    galaxy[i][j] += 1
    return np.max(galaxy)

def min_time_to_destination(N, pulsars, vulcan, qonos):
    from heapq import heappush, heappop
    def isValid(x, y, current_time):
        return all((abs(x-xi) + abs(y-yi)) > (current_time - ti) % ri for xi, yi, ri, ti in pulsars)
    
    visited = set()
    heap = [(0, vulcan[0]-1, vulcan[1]-1)]
    while heap:
        time, x, y = heappop(heap)
        if (x, y) == (qonos[0]-1, qonos[1]-1):
            return time
        if (x, y) in visited:
            continue
        visited.add((x, y))
        for nx, ny in [(x-1, y), (x+1, y), (x, y-1), (x, y+1)]:
            if 0 <= nx < N and 0 <= ny < N and (nx, ny) not in visited and isValid(nx, ny, time+1):
                heappush(heap, (time+1, nx, ny))
    return -1  # Should not be reached as per problem statement

C, N, P, pulsars, vulcan, qonos = read_input('pulsar.in')
if C == 1:
    result = max_affected_sectors(N, pulsars)
else:
    result = min_time_to_destination(N, pulsars, vulcan, qonos)
write_output('pulsar.out', result)
```

