import numpy as np

def read_input(file_path):
    with open(file_path, 'r') as file:
        C, N, P = map(int, file.readline().split())
        pulsars = [list(map(int, file.readline().split())) for _ in range(P)]
        x_s, y_s = map(int, file.readline().split())
        x_f, y_f = map(int, file.readline().split())
    return C, N, P, pulsars, x_s, y_s, x_f, y_f

def write_output(file_path, result):
    with open(file_path, 'w') as file:
        file.write(str(result))

def simulate_pulsars(N, P, pulsars):
    grid = np.zeros((N + 1, N + 1), dtype=np.int32)
    
    for x, y, r, t in pulsars:
        for dx in range(-r, r + 1):
            for dy in range(-r, r + 1):
                if 0 <= x+dx <= N and 0 <= y+dy <= N and abs(dx) + abs(dy) <= r:
                    grid[x+dx, y+dy] += 1
    
    return np.max(grid)

def bfs(N, x_s, y_s, x_f, y_f, pulsars):
    visited = set()
    queue = [(x_s, y_s, 0)]  # (x, y, time)
    pulsar_states = np.zeros((N + 1, N + 1), dtype=np.int32) - 1
    
    for x, y, r, t in pulsars:
        pulsar_states[x, y] = t
    
    while queue:
        x, y, time = queue.pop(0)
        if (x, y) == (x_f, y_f):
            return time
        for dx, dy in [(-1, 0), (1, 0), (0, -1), (0, 1)]:
            nx, ny = x + dx, y + dy
            if 1 <= nx <= N and 1 <= ny <= N and (nx, ny) not in visited:
                safe = all((abs(nx - px) + abs(ny - py) > (time + 1) % pr) or pulsar_states[px, py] == -1 
                           for px, py, pr, pt in pulsars)
                if safe:
                    visited.add((nx, ny))
                    queue.append((nx, ny, time + 1))
    
    return float('inf')

# Read input
C, N, P, pulsars, x_s, y_s, x_f, y_f = read_input('pulsar.in')

# Solve the problem depending on the requirement C
if C == 1:
    result = simulate_pulsars(N, P, pulsars)
elif C == 2:
    result = bfs(N, x_s, y_s, x_f, y_f, pulsars)

# Write output
write_output('pulsar.out', result)
```

