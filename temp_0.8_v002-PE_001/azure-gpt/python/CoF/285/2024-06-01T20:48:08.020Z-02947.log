import numpy as np
from collections import deque

def read_input(filename):
    with open(filename, 'r') as file:
        C, N, P = map(int, file.readline().split())
        pulsars = [tuple(map(int, file.readline().split())) for _ in range(P)]
        x_s, y_s = map(int, file.readline().split())
        x_f, y_f = map(int, file.readline().split())
    return C, N, P, pulsars, x_s, y_s, x_f, y_f

def write_output(filename, value):
    with open(filename, 'w') as file:
        file.write(str(value))

def calculate_S_max(N, pulsars):
    galaxy_map = np.zeros((N, N), dtype=int)

    for x_i, y_i, r_i, t_i in pulsars:
        for t in range(r_i):
            for dx in range(-t, t + 1):
                for dy in range(-t, t + 1):
                    if abs(dx) + abs(dy) <= t:
                        x, y = x_i - 1 + dx, y_i - 1 + dy
                        if 0 <= x < N and 0 <= y < N:
                            galaxy_map[x, y] += 1

    return np.max(galaxy_map)

def calculate_T_min(N, P, pulsars, x_s, y_s, x_f, y_f):
    time_map = np.full((N, N), np.inf)
    time_map[x_s - 1, y_s - 1] = 0

    queue = deque([(x_s - 1, y_s - 1)])

    while queue:
        x, y = queue.popleft()
        current_time = time_map[x, y]

        for dx, dy in [(-1, 0), (1, 0), (0, -1), (0, 1)]:
            nx, ny = x + dx, y + dy
            if 0 <= nx < N and 0 <= ny < N and time_map[nx, ny] == np.inf:
                safe = True
                for x_i, y_i, r_i, t_i in pulsars:
                    distance = abs(nx - (x_i - 1)) + abs(ny - (y_i - 1))
                    pulsar_time = (current_time + 1 - t_i) % r_i
                    if distance <= pulsar_time:
                        safe = False
                        break
                if safe:
                    time_map[nx, ny] = current_time + 1
                    queue.append((nx, ny))

    return int(time_map[x_f - 1, y_f - 1])

def solve_problem():
    C, N, P, pulsars, x_s, y_s, x_f, y_f = read_input('pulsar.in')

    if C == 1:
        S_max = calculate_S_max(N, pulsars)
        write_output('pulsar.out', S_max)
    elif C == 2:
        T_min = calculate_T_min(N, P, pulsars, x_s, y_s, x_f, y_f)
        write_output('pulsar.out', T_min)

solve_problem()
```
