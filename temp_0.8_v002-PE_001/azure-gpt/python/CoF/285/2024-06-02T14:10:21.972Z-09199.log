import numpy as np

# Function to calculate the next state of each pulsar
def next_state(pulsars, N):
    for pulsar in pulsars:
        pulsar[3] = (pulsar[3] + 1) % pulsar[2]

# Function to calculate the Manhattan distance between two points
def manhattan_distance(x1, y1, x2, y2):
    return abs(x1 - x2) + abs(y1 - y2)

# Function to calculate the number of affected sectors by pulsars at any given time
def calculate_smax(pulsars, N):
    galaxy = np.zeros((N, N), dtype=int)
    for pulsar in pulsars:
        x, y, r, t = pulsar
        for i in range(N):
            for j in range(N):
                if manhattan_distance(x - 1, y - 1, i, j) <= t:
                    galaxy[i, j] += 1
        next_state(pulsars, N)
    return galaxy.max()

# Function to perform BFS and find the shortest path avoiding pulsars
def bfs_shortest_path(pulsars, start, end, N):
    visited = set()
    queue = [(start, 0)]  # (position, time)
    while queue:
        (x, y), time = queue.pop(0)
        if (x, y) == end:
            return time
        if (x, y) in visited:
            continue
        visited.add((x, y))
        for dx, dy in [(-1, 0), (1, 0), (0, -1), (0, 1)]:
            nx, ny = x + dx, y + dy
            if 1 <= nx <= N and 1 <= ny <= N and (nx, ny) not in visited:
                safe = True
                for pulsar in pulsars:
                    px, py, pr, pt = pulsar
                    if manhattan_distance(nx, ny, px, py) <= ((time + 1) % pr):
                        safe = False
                        break
                if safe:
                    queue.append(((nx, ny), time + 1))
    return -1  # No path found

# Read input data
with open('pulsar.in', 'r') as file:
