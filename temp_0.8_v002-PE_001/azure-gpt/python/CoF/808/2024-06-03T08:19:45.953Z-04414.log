# Code to solve the Numerus challenge, optimized for memory and efficiency
import numpy as np

def compute_grid_values(k):
    start = 3*(k-1)*(k-2) + 1
    return np.array([start + i for i in range(5)] + [start + 4])

def find_position(n):
    if n == 1:
        return 1, 'A'
    
    k = int(np.ceil(((1 + 8/3*n)**0.5 - 1)/2))
    prev_line_start = 3*(k-1)*(k-2) + 1
    
    if n >= prev_line_start and n < prev_line_start + 6:
        offset = n - prev_line_start
        if offset in [4, 5]:
            return k, ['E', 'F']
        else:
            return k, [chr(65 + offset)]
    else:
        return k - 1, ['A', 'B']

# Reading from the file 'numerus.in'
with open('numerus.in', 'r') as fin:
    k, n = map(int, fin.readline().split())

# Computing the solutions
line_values = compute_grid_values(k)
line_number, columns = find_position(n)

# Writing to the file 'numerus.out'
with open('numerus.out', 'w') as fout:
    fout.write(' '.join(map(str, line_values)) + '\n')
    fout.write(str(line_number) + '\n')
    fout.write(' '.join(columns) + '\n')
