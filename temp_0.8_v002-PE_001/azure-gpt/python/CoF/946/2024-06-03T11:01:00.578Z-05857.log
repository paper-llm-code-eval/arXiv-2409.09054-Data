# Function to perform the rotation of the ducks
def rotate(direction, nr):
    return (direction + nr - 1) % 4 + 1

# Function to advance the position of the ducks
def advance(position, direction, nr, n):
    dx, dy = [(0, 1), (1, 0), (0, -1), (-1, 0)][direction - 1]
    return ((position[0] + dx * nr) % n, (position[1] + dy * nr) % n)

# Read data from input file
