# Possible directions
directions = {
    1: (-1, 0),  # North
    2: (0, 1),   # East
    3: (1, 0),   # South
    4: (0, -1)   # West
}

def execute_commands(n, positions, commands):
    # Adjust the position of the ducks according to the commands
    for cmd, value in commands:
        if cmd == 'A':  # Advance
            for pos in positions:
                move = directions[pos[2]]
                pos[0] = (pos[0] + move[0] * value) % n
                pos[1] = (pos[1] + move[1] * value) % n
        elif cmd == 'R':  # Rotate
            for pos in positions:
                pos[2] = (pos[2] + value - 1) % 4 + 1
        elif cmd == 'Z':  # Fly
            for pos in positions:
                pos[0] = value // n
                pos[1] = value % n
    return positions

# Read from the input file
