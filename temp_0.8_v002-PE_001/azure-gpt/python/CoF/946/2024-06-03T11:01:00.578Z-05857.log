            ducks[i][2] = rotate(ducks[i][2], value)
    elif command == 'Z':
        for i in range(p):
            ducks[i][:2] = (value // n, value % n)

# Write data to output file
with open('pelican.out', 'w') as file:
    for duck in ducks:
        file.write(f"{duck[0]} {duck[1]}\n")
