from functools import lru_cache

MOD = 1000000007

def count_subdivisions(code):
    return code.count("*")

def min_rectangle_dimensions(code):
    code_stack = []
    min_width, min_height = 0, 0
    for char in code:
        if char == '*':
            if code_stack and code_stack[-1] == 'H':
                min_height += 1
            elif code_stack and code_stack[-1] == 'V':
                min_width += 1
            else:
                min_width += 1
                min_height += 1
        else:
            code_stack.append(char)
            if char == 'H':
                min_width = max(min_width, 1)
            elif char == 'V':
                min_height = max(min_height, 1)
            if len(code_stack) > 1 and (code_stack[-1] != code_stack[-2]):
                code_stack.pop()
                code_stack.pop()
    return min_height, min_width

@lru_cache(maxsize=None)
def count_equivalent_encodings(code):
    if not code:
        return 1
    if code[0] == '*':
        return 1

    direction, k, rest = code[0], int(code[1]), code[2:]
    equivalent_encodings = 0
    # Split rest into all possible two non-empty parts and count for each
    for i in range(1, len(rest)):
        sub_code1, sub_code2 = rest[:i], rest[i:]
        if sub_code1 and sub_code2:
            equivalent_encodings += count_equivalent_encodings(sub_code1) * count_equivalent_encodings(sub_code2)
            equivalent_encodings %= MOD
    return equivalent_encodings

def lexicographically_first_code(code):
    operations = re.findall(r'(H\d+|V\d+|\*)', code)
    operations.sort(key=lambda x: (x[0], int(x[1:]) if x[1:].isdigit() else 0))
    return ''.join(operations)

