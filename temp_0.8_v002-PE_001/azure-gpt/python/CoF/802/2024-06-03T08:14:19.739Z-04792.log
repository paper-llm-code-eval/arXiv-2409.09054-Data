# Function to generate primes using Sieve of Eratosthenes
def generate_primes(limit):
    sieve = [True] * (limit + 1)
    sieve[0] = sieve[1] = False
    for i in range(2, int(limit ** 0.5) + 1):
        if sieve[i]:
            for j in range(i*i, limit + 1, i):
                sieve[j] = False
    return [i for i, prime in enumerate(sieve) if prime]

# Function to check if a number is a p-prime
def is_p_prime(num, p, primes):
    if num == p or num in primes:
        return True
    if num % p != 0:
        return False
    while num % p == 0:
        num //= p
    return num == 1

# Read from secvente.in and process each dataset
with open('secvente.in', 'r') as fin:
    D = int(fin.readline().strip())
    primes = set(generate_primes(30000))
    results = []

    for _ in range(D):
        n, p, k = map(int, fin.readline().strip().split())
        sequence = [int(fin.readline().strip()) for _ in range(n)]

        p_prime_count = 0
        start = 0
        valid_sequences = []

        for i in range(n):
            if is_p_prime(sequence[i], p, primes):
                p_prime_count += 1
                if p_prime_count == k:
                    end = i + 1
                    while not is_p_prime(sequence[start], p, primes):
                        start += 1
                    valid_sequences.append((start + 1, end))
                    start += 1
                    p_prime_count -= 1
            elif p_prime_count > 0:
                start = i + 1
                p_prime_count = 0

        results.append(valid_sequences)

# Write to secvente.out
with open('secvente.out', 'w') as fout:
    for valid_sequences in results:
        fout.write(f"{len(valid_sequences)}\n")
        for seq in valid_sequences:
            fout.write(f"{seq[0]} {seq[1]}\n")
```

