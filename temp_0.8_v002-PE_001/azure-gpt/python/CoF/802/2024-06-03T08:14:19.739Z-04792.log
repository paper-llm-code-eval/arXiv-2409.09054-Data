import numpy as np

# Function to check if a number is p-prime
def is_p_prime(num, p, prime_set):
    if num == p:
        return True
    if num % p != 0:
        return num in prime_set
    while num % p == 0 and num != 0:
        num //= p
    return num in prime_set

# Function to load the prime set within a range (to optimize using Sieve of Eratosthenes)
def generate_prime_set(limit):
    sieve = np.ones(limit + 1, dtype=bool)
    sieve[:2] = False
    for index in range(2, int(limit ** 0.5) + 1):
        if sieve[index]:
            sieve[index*2::index] = False
    return set(np.nonzero(sieve)[0])

# Read from secvente.in and process each dataset
with open('secvente.in', 'r') as fin:
    D = int(fin.readline().strip())
    prime_set = generate_prime_set(30000)
