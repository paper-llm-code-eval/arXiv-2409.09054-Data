# parc.py
import numpy as np

# Read input data
with open('parc.in', 'r') as file:
    Xparc, Yparc = map(int, file.readline().split())
    xG, yG, xpr, ypr = map(int, file.readline().split())
    m = int(file.readline().strip())
    vertical_tracks = [list(map(int, file.readline().split())) for _ in range(m)]
    n = int(file.readline().strip())
    horizontal_tracks = [list(map(int, file.readline().split())) for _ in range(n)]

# Process vertical and horizontal tracks to get the ranges
vertical_ranges = np.zeros((Xparc + 1,), dtype=np.int8)
horizontal_ranges = np.zeros((Yparc + 1,), dtype=np.int8)

for x1, x2 in vertical_tracks:
    left, right = min(x1, x2), max(x1, x2)
    vertical_ranges[left:right+1] = 1

for y1, y2 in horizontal_tracks:
    bottom, top = min(y1, y2), max(y1, y2)
    horizontal_ranges[bottom:top+1] = 1

# Calculate the minimum path length using Manhattan distance and corrections
# for the bike tracks
min_path_length = abs(xpr - xG) + abs(ypr - yG)
min_path_length -= np.sum(vertical_ranges[min(xG, xpr):max(xG, xpr)+1])
min_path_length -= np.sum(horizontal_ranges[min(yG, ypr):max(yG, ypr)+1])
min_path_length += (np.sum(vertical_ranges[min(xG, xpr):max(xG, xpr)+1]) > 0) * 0.5
min_path_length += (np.sum(horizontal_ranges[min(yG, ypr):max(yG, ypr)+1]) > 0) * 0.5

# Count the number of distinct paths
def count_paths(x1, y1, x2, y2, vertical, horizontal):
    if x1 == x2 and y1 == y2:
        return 1
    if x1 > x2 or y1 > y2:
        return 0

    paths = 0
    if vertical[x1] == 0:
        paths += count_paths(x1 + 1, y1, x2, y2, vertical, horizontal)
    if horizontal[y1] == 0:
        paths += count_paths(x1, y1 + 1, x2, y2, vertical, horizontal)

    return paths

number_of_paths = count_paths(xG, yG, xpr, ypr, vertical_ranges, horizontal_ranges)

# Write output data
with open('parc.out', 'w') as file:
    file.write(f'{min_path_length:.9f}\n')
    file.write(f'{number_of_paths}\n')
