import numpy as np

# Define helper function to compute path length and number of paths
def calc_paths(Xparc, Yparc, xG, yG, xpr, ypr, vertical, horizontal):
    # Create a grid to store the number of ways to reach each cell
    grid = np.zeros((Xparc+1, Yparc+1), dtype=np.int64)
    # Set starting point
    grid[xG, yG] = 1

    # Sort the bike paths for efficient traversal
    vertical.sort(key=lambda x: x[0])
    horizontal.sort(key=lambda x: x[0])

    # Function to check if a cell is blocked by a bike path
    def is_blocked(x, y):
        return any(v[0] <= x <= v[1] for v in vertical if y == v[2]) or \
               any(h[0] <= y <= h[1] for h in horizontal if x == h[2])

    # Dynamic programming to calculate number of paths to each cell
    for x in range(Xparc + 1):
        for y in range(Yparc + 1):
            if is_blocked(x, y):
                continue

            # Sum paths from the left and above cells if not blocked
            if x > 0 and not is_blocked(x-1, y):
                grid[x, y] += grid[x-1, y]
            if y > 0 and not is_blocked(x, y-1):
                grid[x, y] += grid[x, y-1]

    # Compute minimum path length using Euclidean distance
    min_path_length = np.hypot(xpr - xG, ypr - yG)

    # Number of minimum paths is the value in the destination cell
    num_min_paths = grid[xpr, ypr]

    return min_path_length, num_min_paths

# Read input from parc.in
with open('parc.in', 'r') as f:
    Xparc, Yparc = map(int, f.readline().split())
    xG, yG, xpr, ypr = map(int, f.readline().split())
    m = int(f.readline().strip())
    vertical = [list(map(int, f.readline().split())) + [i] for i in range(m)]
    n = int(f.readline().strip())
    horizontal = [list(map(int, f.readline().split())) + [i] for i in range(n)]

# Calculate the minimum path length and number of distinct minimum paths
min_path_length, num_min_paths = calc_paths(Xparc, Yparc, xG, yG, xpr, ypr, vertical, horizontal)

# Write output to parc.out
with open('parc.out', 'w') as f:
    f.write(f'{min_path_length:.9f}\n')
    f.write(f'{num_min_paths}\n')
```

