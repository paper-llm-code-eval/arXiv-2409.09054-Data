# Solution starts here
import numpy as np

# Read input from parc.in
with open('parc.in', 'r') as file:
    Xparc, Yparc = map(int, file.readline().split())
    xG, yG, xpr, ypr = map(int, file.readline().split())
    m = int(file.readline())
    vertical_tracks = [list(map(int, file.readline().split())) for _ in range(m)]
    n = int(file.readline())
    horizontal_tracks = [list(map(int, file.readline().split())) for _ in range(n)]

# Preprocess tracks for continuous representation
vertical_tracks.sort(key=lambda x: x[0])
horizontal_tracks.sort(key=lambda y: y[0])

# Determine the discrete grid points for Gigel's and friend's positions
# and tracks, to calculate the shortest path and number of distinct paths
x_points = sorted(set([xG, xpr] + [min(x) for x in vertical_tracks] + [max(x) for x in vertical_tracks]))
y_points = sorted(set([yG, ypr] + [min(y) for y in horizontal_tracks] + [max(y) for y in horizontal_tracks]))

# Map the coordinates to the reduced grid
x_idx = {x: i for i, x in enumerate(x_points)}
y_idx = {y: i for i, y in enumerate(y_points)}

# Create the reduced grid for dynamic programming
grid = np.ones((len(x_points), len(y_points)), dtype=int) * np.inf
grid[x_idx[xG], y_idx[yG]] = 0  # Start point

# Calculate the shortest path using dynamic programming
for x in range(len(x_points)):
    for y in range(len(y_points)):
        if x > 0:
            grid[x, y] = min(grid[x, y], grid[x - 1, y] + x_points[x] - x_points[x - 1])
        if y > 0:
            grid[x, y] = min(grid[x, y], grid[x, y - 1] + y_points[y] - y_points[y - 1])

# Calculate the number of distinct paths using dynamic programming
paths = np.zeros((len(x_points), len(y_points)), dtype=int)
paths[x_idx[xG], y_idx[yG]] = 1

for x in range(len(x_points)):
    for y in range(len(y_points)):
        if x > 0 and grid[x, y] == grid[x - 1, y] + x_points[x] - x_points[x - 1]:
            paths[x, y] += paths[x - 1, y]
        if y > 0 and grid[x, y] == grid[x, y - 1] + y_points[y] - y_points[y - 1]:
            paths[x, y] += paths[x, y - 1]

# Ensure the number of paths does not exceed the limit
paths %= 1000000000

# The results
min_distance = grid[x_idx[xpr], y_idx[ypr]]
num_distinct_paths = paths[x_idx[xpr], y_idx[ypr]]

# Write output to parc.out
with open('parc.out', 'w') as file:
    file.write(f"{min_distance:.9f}\n")
    file.write(f"{num_distinct_paths}\n")
