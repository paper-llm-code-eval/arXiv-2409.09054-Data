import numpy as np

# Function to calculate the maximum sum of weapon powers after replacements
def maximize_weapon_power(n, m, belt_powers, room_powers):
    # Sort both arrays in ascending order
    sorted_belt = np.sort(belt_powers)
    sorted_room = np.sort(room_powers)

    # Replace the weakest weapons from the belt with the strongest from the room
    for i in range(min(n, m)):
        if sorted_belt[i] < sorted_room[m - 1 - i]:
            sorted_belt[i] = sorted_room[m - 1 - i]
        else:
            break

    # Return the sum of the updated belt powers
    return np.sum(sorted_belt)

# Read from arme.in
with open('arme.in', 'r') as file:
    n, m = map(int, file.readline().split())
    belt_powers = np.array(file.readline().split(), dtype=int)
    room_powers = np.array(file.readline().split(), dtype=int)

# Calculate the result
result = maximize_weapon_power(n, m, belt_powers, room_powers)

# Write to arme.out
with open('arme.out', 'w') as file:
    file.write(str(result))
