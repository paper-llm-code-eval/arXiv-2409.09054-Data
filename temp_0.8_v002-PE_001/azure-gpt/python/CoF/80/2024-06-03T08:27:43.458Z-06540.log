import numpy as np
import sys
from io import StringIO

# Redirect stdin
sys.stdin = StringIO(sys.stdin.read())

def can_be_chessboard(matrix, N):
    for i in range(N):
        for j in range(N - 1):
            if matrix[i, j] == matrix[i, j + 1]:
                return False
            if matrix[j, i] == matrix[j + 1, i]:
                return False
    return True

def count_swaps(pattern, N):
    swap_count = 0
    for i in range(0, N, 2):
        if not np.array_equal(pattern[i:i+2], [0, 1]):
            swap_count += 1
    return swap_count

def get_chessboard_operations(matrix, N):
    operations = []
    # Check rows
    for i in range(0, N, 2):
        if not (matrix[i][0] != matrix[i][1] and matrix[i+1][0] == matrix[i+1][1]):
            for j in range(i + 2, N, 2):
                if matrix[j][0] != matrix[j][1] and matrix[j+1][0] == matrix[j+1][1]:
                    matrix[[i, j]] = matrix[[j, i]]
                    operations.append("L {} {}".format(i + 1, j + 1))
                    break
    # Check columns
    for i in range(0, N, 2):
        if not (matrix[0][i] != matrix[1][i] and matrix[0][i+1] == matrix[1][i+1]):
            for j in range(i + 2, N, 2):
                if matrix[0][j] != matrix[1][j] and matrix[0][j+1] == matrix[1][j+1]:
                    matrix[:, [i, j]] = matrix[:, [j, i]]
                    operations.append("C {} {}".format(i + 1, j + 1))
                    break
    return operations

# Read P and T
P, T = map(int, input().split())

for _ in range(T):
    # Read N and the matrix A
    N = int(input())
    matrix = np.zeros((N, N), dtype=np.int8)
    for i in range(N):
        row = list(map(int, list(input().strip())))
        matrix[i, :] = np.array(row)
    
    if P == 1:
        print(1 if can_be_chessboard(matrix, N) else 0)
        
    elif P == 2:
        row_pattern = np.sum(matrix, axis=1) % 2
        col_pattern = np.sum(matrix, axis=0) % 2
        row_swaps = count_swaps(row_pattern, N)
        col_swaps = count_swaps(col_pattern, N)
        print(row_swaps + col_swaps)
        
    elif P == 3:
        operations = get_chessboard_operations(matrix, N)
        print(len(operations))
        for op in operations:
            print(op)
```

