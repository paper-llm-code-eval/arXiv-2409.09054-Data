import sys

def read_matrix(N):
    return [list(map(int, input().strip())) for _ in range(N)]

def is_chessboard(matrix, N):
    for i in range(N):
        for j in range(N - 1):
            if matrix[i][j] == matrix[i][j + 1] or matrix[j][i] == matrix[j + 1][i]:
                return False
    return True

def min_swaps(arr, target_length):
    even_count = arr.count(0)
    odd_count = arr.count(1)
    if even_count != target_length or odd_count != target_length:
        return sys.maxsize
    return min(even_count, odd_count) // 2

def solve_p1(N, A):
    print(int(is_chessboard(A, N)))

def solve_p2(N, A):
    row_swaps = col_swaps = 0
    for i in range(N):
        if i % 2 == 0:
            row_swaps += min_swaps([row[i] for row in A], N // 2)
            col_swaps += min_swaps(A[i], N // 2)
    print(row_swaps + col_swaps)

def solve_p3(N, A):
    operations = []
    for i in range(0, N, 2):
        for j in range(N):
            if A[i][j] == A[i+1][j]:
                operations.append(f"L {i+1} {i+2}")
                break
    for j in range(0, N, 2):
        for i in range(N):
            if A[i][j] == A[i][j+1]:
                operations.append(f"C {j+1} {j+2}")
                break
    print(len(operations))
    for op in operations:
        print(op)

def main():
    P, T = map(int, input().split())
    for _ in range(T):
        N = int(input())
        A = read_matrix(N)
        if P == 1:
            solve_p1(N, A)
        elif P == 2:
            solve_p2(N, A)
        elif P == 3:
            solve_p3(N, A)

if __name__ == "__main__":
    main()
```

