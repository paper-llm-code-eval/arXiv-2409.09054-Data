def can_be_chessboard(matrix, N):
    for i in range(N):
        for j in range(1, N):
            if matrix[i][j] == matrix[i][j-1]:
                return False
            if matrix[j][i] == matrix[j-1][i]:
                return False
    return True

def swap_rows(matrix, i, j):
    matrix[i], matrix[j] = matrix[j], matrix[i]

def swap_cols(matrix, i, j):
    for row in matrix:
        row[i], row[j] = row[j], row[i]

def find_min_operations(matrix, N):
    operations = []
    for row in range(N):
        if matrix[row][0] == row % 2:
            for row2 in range(row + 1, N):
                if matrix[row2][0] == row % 2:
                    operations.append(f"L {row + 1} {row2 + 1}")
                    swap_rows(matrix, row, row2)
                    break

    for col in range(N):
        if matrix[0][col] == col % 2:
            for col2 in range(col + 1, N):
                if matrix[0][col2] == col % 2:
                    operations.append(f"C {col + 1} {col2 + 1}")
                    swap_cols(matrix, col, col2)
                    break

    return operations

