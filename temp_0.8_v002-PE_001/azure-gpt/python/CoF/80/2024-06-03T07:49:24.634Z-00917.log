import numpy as np

def can_be_chessboard(matrix, N):
    for i in range(N):
        for j in range(1, N):
            if matrix[i, j] == matrix[i, j-1]:
                return False
            if matrix[j, i] == matrix[j-1, i]:
                return False
    return True


def min_operations(matrix, N, operation_type):
    rows_to_swap = np.zeros(N)
    cols_to_swap = np.zeros(N)
    # Rows pattern: 010101... and its complement
    target_row = np.tile([0, 1], N//2)
    target_row_inv = np.tile([1, 0], N//2)
    # Count rows and columns that need to be swapped
    for i in range(N):
        if not np.array_equal(matrix[i], target_row) and not np.array_equal(matrix[i], target_row_inv):
            rows_to_swap[i] = 1
        col = matrix[:,i]
        if not np.array_equal(col, target_row) and not np.array_equal(col, target_row_inv):
            cols_to_swap[i] = 1
    # Rows and columns must be swapped in pairs, so count must be even
    if np.sum(rows_to_swap) % 2 != 0 or np.sum(cols_to_swap) % 2 != 0:
        return -1  # Impossible to convert to chessboard
    
    operations = []
    if operation_type == 3:
        for i in range(N):
            if rows_to_swap[i]:
                for j in range(i+1, N):
                    if rows_to_swap[j] and (matrix[i,0] != matrix[j,0]):
                        operations.append(f"L {i+1} {j+1}")
                        matrix[[i, j]] = matrix[[j, i]]  # Swap rows in matrix
                        rows_to_swap[i], rows_to_swap[j] = rows_to_swap[j], rows_to_swap[i]
                        break
            if cols_to_swap[i]:
                for j in range(i+1, N):
                    if cols_to_swap[j] and (matrix[0,i] != matrix[0,j]):
                        operations.append(f"C {i+1} {j+1}")
                        matrix[:, [i, j]] = matrix[:, [j, i]]  # Swap columns in matrix
                        cols_to_swap[i], cols_to_swap[j] = cols_to_swap[j], cols_to_swap[i]
                        break
    return len(operations) if operation_type == 2 else operations


