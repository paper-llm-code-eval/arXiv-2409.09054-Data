import sys
import numpy as np

# Function to check if a matrix can be converted to a chessboard
def is_convertible(matrix):
    N = len(matrix)
    row_pattern = np.tile([0, 1], (N//2))
    col_pattern = np.tile([0, 1], (N//2)).reshape(N//2, 1)
    for i in range(N):
        if not (np.all(matrix[i] == row_pattern) or np.all(matrix[i] == 1 - row_pattern)):
            return False
        if not (np.all(matrix[:, i] == col_pattern) or np.all(matrix[:, i] == 1 - col_pattern)):
            return False
    return True

# Function to calculate the minimum number of swaps to convert a matrix to a chessboard
def min_swaps(matrix):
    N = len(matrix)
    row_swaps = col_swaps = 0

    # Calculate row swaps needed
    for i in range(0, N, 2):
        if not np.all(matrix[i] == matrix[i+1] ^ 1):
            row_swaps += 1

    # Calculate column swaps needed
    for j in range(0, N, 2):
        if not np.all(matrix[:, j] == matrix[:, j+1] ^ 1):
            col_swaps += 1

    return (row_swaps + col_swaps) // 2

# Function to calculate the swap operations to convert a matrix to a chessboard
def swap_operations(matrix):
    N = len(matrix)
    operations = []
    for i in range(0, N, 2):
        for j in range(i + 2, N, 2):
            if np.all(matrix[i] == matrix[j] ^ 1):
                operations.append(f"L {i+1} {j+1}")
                matrix[[i, j]] = matrix[[j, i]]
                
    for i in range(0, N, 2):
        for j in range(i + 2, N, 2):
            if np.all(matrix[:, i] == matrix[:, j] ^ 1):
                operations.append(f"C {i+1} {j+1}")
                matrix[:, [i, j]] = matrix[:, [j, i]]
                
    return operations

# Parse input
P, T = map(int, input().split())
for _ in range(T):
    N = int(input())
    matrix = np.array([list(map(int, list(input().strip()))) for _ in range(N)])

    if P == 1:
        print(1 if is_convertible(matrix) else 0)
    elif P == 2:
        print(min_swaps(matrix))
    elif P == 3:
        ops = swap_operations(matrix)
        print(len(ops))
        for op in ops:
            print(op)
```

