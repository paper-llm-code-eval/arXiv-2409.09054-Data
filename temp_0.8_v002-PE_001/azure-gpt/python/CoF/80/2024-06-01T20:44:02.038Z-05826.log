import sys
import numpy as np

# Read the type of task (P) and the number of instances (T)
P, T = map(int, input().split())

# Helper functions
def check_chessboard(matrix):
    # Check if matrix is already a chessboard
    return all(matrix[i][j] != matrix[i][j+1] and matrix[i][j] != matrix[i+1][j]
               for i in range(N-1) for j in range(N-1))

def min_swaps_to_chessboard(arr):
    target = np.arange(N) % 2
    swaps = sum(arr != target)
    if swaps % 2 == 0:
        return swaps // 2
    else:
        return (swaps + N) // 2

# Process each instance
for _ in range(T):
    # Read the size of the matrix (N)
    N = int(input())
    matrix = np.array([list(map(int, list(input().strip()))) for _ in range(N)])
    
    if P == 1:
        print(int(check_chessboard(matrix)))
    elif P == 2:
        # Swap rows and columns separately and count the number of swaps
        row_swaps = min_swaps_to_chessboard(matrix.sum(axis=1))
        col_swaps = min_swaps_to_chessboard(matrix.sum(axis=0))
        print(row_swaps + col_swaps)
    elif P == 3:
        def find_swaps(pattern):
            swaps = []
            for i in range(0, N, 2):
                if pattern[i] == pattern[i+1]:
                    # Find the next line that can be swapped
                    for j in range(i+2, N, 2):
                        if pattern[j] == pattern[i]:
                            swaps.append((i+1, j+1))
                            pattern[i+1], pattern[j] = pattern[j], pattern[i+1]
                            break
            return swaps

        # Find swaps for rows and columns
        row_swaps = find_swaps(matrix.sum(axis=1))
        col_swaps = find_swaps(matrix.sum(axis=0))
        num_swaps = len(row_swaps) + len(col_swaps)
        print(num_swaps)
        for r in row_swaps:
            print(f'L {r[0]} {r[1]}')
        for c in col_swaps:
            print(f'C {c[0]} {c[1]}')

# Make sure to flush stdout
sys.stdout.flush()
