import sys

def check_row(row):
    expected = row[0]
    for i in range(1, len(row)):
        expected = 1 - expected
        if row[i] != expected:
            return False
    return True

def check_chessboard(matrix):
    for row in matrix:
        if not check_row(row):
            return False
    transposed_matrix = zip(*matrix)
    for col in transposed_matrix:
        if not check_row(col):
            return False
    return True

def count_swaps(arr):
    count1, count2 = 0, 0
    for i in range(len(arr)):
        if i % 2 == arr[i]:
            count1 += 1
        else:
            count2 += 1
    return min(count1, count2)

def min_swaps(matrix):
    row_swaps = sum(count_swaps(row) for row in matrix)
    col_swaps = sum(count_swaps(col) for col in zip(*matrix))
    return row_swaps + col_swaps

def swaps_sequence(matrix):
    swaps = []
    for i in range(0, len(matrix), 2):
        if not check_row(matrix[i]):
            swaps.append(('L', i + 1, i + 2))
    for j in range(0, len(matrix[0]), 2):
        col = [row[j] for row in matrix]
        if not check_row(col):
            swaps.append(('C', j + 1, j + 2))
    return swaps

