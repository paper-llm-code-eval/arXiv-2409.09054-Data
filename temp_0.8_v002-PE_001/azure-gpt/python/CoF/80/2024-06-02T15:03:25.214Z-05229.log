import sys
import numpy as np

def can_be_chessboard(matrix):
    n = len(matrix)
    pattern1 = np.tile([0, 1], n // 2)
    pattern2 = np.tile([1, 0], n // 2)
    for row in matrix:
        if not (np.array_equal(row, pattern1) or np.array_equal(row, pattern2)):
            return False
    for col in matrix.T:
        if not (np.array_equal(col, pattern1) or np.array_equal(col, pattern2)):
            return False
    return True

def count_swaps(bits):
    swap_count = 0
    for i in range(0, len(bits), 2):
        if bits[i] == bits[i+1]:
            swap_count += 1
    return swap_count

def swap_operations(matrix):
    n = len(matrix)
    row_swaps = count_swaps([row[0] for row in matrix])
    col_swaps = count_swaps(matrix[0])
    
    row_ops = []
    col_ops = []
    
    for i in range(0, n, 2):
        if matrix[i][0] == matrix[i+1][0]:
            row_ops.append((i+1, i+2))
    
    for j in range(0, n, 2):
        if matrix[0][j] == matrix[0][j+1]:
            col_ops.append((j+1, j+2))
    
    return row_ops + col_ops

def solve_problem(p, t, instances):
    results = []
    for n, matrix in instances:
        matrix = np.array(matrix)
        if p == 1:
            result = 1 if can_be_chessboard(matrix) else 0
            results.append(str(result))
        elif p == 2:
