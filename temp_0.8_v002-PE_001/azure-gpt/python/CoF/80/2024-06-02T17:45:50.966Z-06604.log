def can_transform_to_chessboard(matrix):
    # Check each adjacent pair of elements in rows and columns
    for i in range(len(matrix)):
        for j in range(len(matrix) - 1):
            if matrix[i][j] == matrix[i][j + 1] or matrix[j][i] == matrix[j + 1][i]:
                return 0
    return 1

def count_swaps(matrix):
    # Counting swaps needed for rows and columns separately
    def count_line_swaps(line):
        expected = [i % 2 for i in range(len(line))]
        count0 = count1 = 0
        for i in range(len(line)):
            if line[i] != expected[i]:
                if expected[i] == 0:
                    count0 += 1
                else:
                    count1 += 1
        return max(count0, count1)
    
    row_swaps = sum(count_line_swaps(row) for row in matrix) // 2
    col_swaps = sum(count_line_swaps(col) for col in zip(*matrix)) // 2
    return row_swaps + col_swaps

def generate_swap_operations(matrix):
    swaps = []

    # Helper function to generate swap operations for lines (rows/columns)
