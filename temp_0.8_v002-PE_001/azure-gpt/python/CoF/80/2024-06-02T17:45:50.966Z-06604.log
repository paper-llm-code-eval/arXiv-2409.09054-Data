import numpy as np
import sys
from io import StringIO

# Function to check if the matrix is a chessboard
def is_chessboard(matrix):
    N = matrix.shape[0]
    for i in range(N):
        for j in range(N - 1):
            if matrix[i, j] == matrix[i, j + 1] or matrix[j, i] == matrix[j + 1, i]:
                return False
    return True

# Function to find the minimum number of swaps needed
def min_swaps(matrix):
    N = matrix.shape[0]
    row_swaps = col_swaps = 0

    # Count row swaps
    for i in range(0, N, 2):
        if not np.all(matrix[i, 0::2] == matrix[i + 1, 1::2]):
            row_swaps += 1

    # Count column swaps
    for i in range(0, N, 2):
        if not np.all(matrix[0::2, i] == matrix[1::2, i + 1]):
            col_swaps += 1

    return row_swaps + col_swaps

# Function to compute the sequence of swaps
def swap_sequence(matrix):
    N = matrix.shape[0]
    swaps = []

    # Compute row swaps
    for i in range(0, N, 2):
        if not np.all(matrix[i, 0::2] == matrix[i + 1, 1::2]):
            swaps.append(('L', i + 1, i + 2))

    # Compute column swaps
    for i in range(0, N, 2):
        if not np.all(matrix[0::2, i] == matrix[1::2, i + 1]):
            swaps.append(('C', i + 1, i + 2))

    return swaps

