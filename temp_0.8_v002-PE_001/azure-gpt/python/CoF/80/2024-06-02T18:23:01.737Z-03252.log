import numpy as np
from io import StringIO

def check_chessboard(matrix):
    for i in range(0, matrix.shape[0], 2):
        if not ((matrix[i, :] == np.tile([0, 1], matrix.shape[1]//2)).all() or (matrix[i, :] == np.tile([1, 0], matrix.shape[1]//2)).all()):
            return False
        if not ((matrix[i+1, :] == np.tile([0, 1], matrix.shape[1]//2)).all() or (matrix[i+1, :] == np.tile([1, 0], matrix.shape[1]//2)).all()):
            return False
    return True

def transform_to_chessboard(matrix):
    n = matrix.shape[0]
    row_swap = np.array([(row[0::2].sum(), row[1::2].sum()) for row in matrix])
    col_swap = np.array([(col[0::2].sum(), col[1::2].sum()) for col in matrix.T])
    
    # Count the number of swaps needed for rows and columns to alternate 0s and 1s
    row_swaps = min(sum(row_swap[:, 0] > n//4), sum(row_swap[:, 1] > n//4))
    col_swaps = min(sum(col_swap[:, 0] > n//4), sum(col_swap[:, 1] > n//4))
    
    return row_swaps + col_swaps, row_swap, col_swap

def find_swaps(swap_array, n):
    swaps = []
    target = n//4
    indices = np.where(swap_array[:, 0] > target)[0]
    for i in range(0, len(indices), 2):
        swaps.append((indices[i]+1, indices[i+1]+1))
    return swaps

