def is_chessboard(N, board):
    for i in range(N):
        for j in range(N-1):
            if board[i][j] == board[i][j+1]:
                return False
        if i < N-1 and board[i][0] == board[i+1][0]:
            return False
    return True

def count_operations(N, board):
    # Calculate number of rows and columns that need to be swapped
    row_swaps = sum(board[i][0] == i % 2 for i in range(N)) // 2
    col_swaps = sum(board[0][i] == i % 2 for i in range(N)) // 2
    return row_swaps + col_swaps

def generate_operations(N, board):
    row_swaps = []
    col_swaps = []

    # Swap rows
    for i in range(0, N, 2):
        if board[i][0] == board[i+1][0]:
            for j in range(0, N, 2):
                if board[j][0] != board[j+1][0]:
                    board[i], board[j] = board[j], board[i]
                    row_swaps.append(f"L {i+1} {j+1}")
                    break

    # Swap columns
    for i in range(0, N, 2):
        if board[0][i] == board[0][i+1]:
            for j in range(0, N, 2):
                if board[0][j] != board[0][j+1]:
                    for row in board:
                        row[i], row[j] = row[j], row[i]
                    col_swaps.append(f"C {i+1} {j+1}")
                    break

    return row_swaps + col_swaps

