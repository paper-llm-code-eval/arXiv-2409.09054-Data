import numpy as np

def is_chessboard(matrix):
    N = matrix.shape[0]
    for i in range(N):
        for j in range(N - 1):
            if matrix[i, j] == matrix[i, j + 1] or matrix[j, i] == matrix[j + 1, i]:
                return False
    return True

def min_swaps(sequence):
    counter = 0
    for i in range(len(sequence)):
        if sequence[i] != i % 2:
            for j in range(i+1, len(sequence)):
                if sequence[j] == i % 2:
                    sequence[i], sequence[j] = sequence[j], sequence[i]
                    counter += 1
                    break
    return counter

def transform_to_chessboard(matrix):
    N = matrix.shape[0]
    row_swaps = col_swaps = 0
    row_seq = np.zeros(N, dtype=int)
    col_seq = np.zeros(N, dtype=int)
    
