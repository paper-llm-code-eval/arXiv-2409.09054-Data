import sys
from collections import deque
from itertools import accumulate

MOD = 10**9 + 7

def input():
    return sys.stdin.readline().strip()

def find_max_subpackages(days, K):
    last_seen = [-1] * K
    min_last_seen = -1
    packages = 0
    for i in range(len(days)):
        for j, day in enumerate(days[i]):
            if day == '1':
                last_seen[j] = i
        min_last_seen = min(last_seen)
        if min_last_seen != -1:
            packages += (min_last_seen + 1)
    return packages

def count_max_packages(days, K):
    last_seen = [-1] * K
    min_last_seen = -1
    num_ways = 1
    previous_packages = 0
    packages_prefix_sum = [0]
    for i in range(len(days)):
        for j, day in enumerate(days[i]):
            if day == '1':
                last_seen[j] = i
        min_last_seen = min(last_seen)
        if min_last_seen != -1:
            current_packages = previous_packages + (min_last_seen + 1)
            if current_packages > previous_packages:
                num_ways *= (packages_prefix_sum[-1] - packages_prefix_sum[min_last_seen] + 1)
                num_ways %= MOD
            previous_packages = current_packages
        packages_prefix_sum.append(previous_packages)
    return num_ways

def solve():
    P = int(input())   # Problem type
    T = int(input())   # Number of scenarios

    for _ in range(T):
        N, K = map(int, input().split())
        days = [input() for _ in range(N)]

        if P == 1:
            print(find_max_subpackages(days, K))
        elif P == 2:
            print(count_max_packages(days, K))

if __name__ == "__main__":
    solve()
