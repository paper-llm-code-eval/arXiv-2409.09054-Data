import sys
from itertools import accumulate
from operator import mul
MOD = 1000000007

# Helper function to compute the last day on which each type of coffee is produced
def compute_last_produced(coffee_data, N, K):
    last_produced = [-1] * K
    for day in range(N):
        for coffee_type in range(K):
            if coffee_data[day][coffee_type]:
                last_produced[coffee_type] = day
    return last_produced

# Helper function to compute the maximum number of packages that can be formed
def max_packages(coffee_data, N, K):
    last_produced = compute_last_produced(coffee_data, N, K)
    packages = 0
    min_last = N
    for day in range(N - 1, -1, -1):
        min_last = min(min_last, last_produced[coffee_data[day].index(1)])
        if day >= min_last:
            packages += 1
            min_last = N
    return packages

# Helper function to compute the number of ways to divide the days
def num_ways_divide_days(coffee_data, N, K):
    last_produced = compute_last_produced(coffee_data, N, K)
    division_points = [0] * N
    ways = [1]
    min_last = N
    for day in range(N - 1, -1, -1):
        min_last = min(min_last, last_produced[coffee_data[day].index(1)])
        if day >= min_last:
            division_points[day] = 1
            ways.append(ways[-1] * 2 % MOD)
        else:
            ways.append(ways[-1])
    return ways[-1]

# Reading input
P = int(input().strip())
T = int(input().strip())

for _ in range(T):
    N, K = map(int, input().strip().split())
    coffee_data = [list(map(int, list(input().strip()))) for _ in range(N)]
    if P == 1:
        print(max_packages(coffee_data, N, K))
    else:
        print(num_ways_divide_days(coffee_data, N, K))
```

