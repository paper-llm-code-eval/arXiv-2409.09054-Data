import sys
from collections import defaultdict
from itertools import product

MOD = 1000000007

# Function to solve for maximum number of valid packets that can be formed
def max_packets(prod_matrix, N, K):
    # We'll use two pointers, left and right, to find the shortest subsequence that contains all coffee types
    left, right = 0, 0
    current_count = [0] * K
    min_len = float('inf')
    packets = 0

    while right < N:
        # Add the coffee types produced in the day pointed by right into the current_count
        for k in range(K):
            current_count[k] += prod_matrix[right][k]

        # Check if the current sequence contains all coffee types
        while all(current_count):
            # Update the shortest length and count the packet
            min_len = min(min_len, right - left + 1)
            packets += 1

            # Move the left pointer to try to find a shorter subsequence
            for k in range(K):
                current_count[k] -= prod_matrix[left][k]
            left += 1

        # Move the right pointer to include more days
        right += 1

    return packets

# Function to calculate number of ways to partition the days
def ways_to_partition(prod_matrix, N, K):
    last_occurrence = [-1] * (1 << K)
    count_ways = [0] * (N + 1)
    count_ways[0] = 1

    # We can represent the different production profiles by bitmasks
    prod_profile = [0] * N
    for i in range(N):
        for k in range(K):
            if prod_matrix[i][k]:
                prod_profile[i] |= (1 << k)

    # We'll iterate over the days and update the ways to partition them
    for i in range(1, N + 1):
        mask = prod_profile[i - 1]
        last_occurrence[mask] = i

        # Iterate over all the submasks of the current mask
        for submask in range(mask, 0, (submask - 1) & mask):
            if last_occurrence[submask] != -1:
                count_ways[i] = (count_ways[i] + count_ways[last_occurrence[submask] - 1]) % MOD

        # If no coffee is produced, we carry over the previous count
        if mask == 0:
            count_ways[i] = count_ways[i - 1]

    return count_ways[N]

# Read from stdin
def read_input():
    P = int(input().strip())
    T = int(input().strip())
    scenarios = []
    for _ in range(T):
        N, K = map(int, input().strip().split())
        productions = [list(map(int, input().strip())) for _ in range(N)]
        scenarios.append((N, K, productions))
    return P, T, scenarios

# Main function
