from sys import stdin, stdout

MOD = 1000000007

def read_ints():
    return list(map(int, stdin.readline().strip().split()))

def max_valid_packages(coffee_days):
    return min(map(min, zip(*coffee_days)))

def count_splits(coffee_days, max_packages):
    K = len(coffee_days[0])
    day_count = [0] * K
    dp = [1] + [0] * max_packages
    for day in coffee_days:
        for i, coffee in enumerate(day):
            if coffee:
                day_count[i] += 1
        if min(day_count) > 0:
            for i in range(max_packages, 0, -1):
                dp[i] = (dp[i] + dp[i-1]) % MOD
            day_count = [x - 1 for x in day_count]
    return dp[max_packages]

def solve_problems():
    P = int(input())
    T = int(input())
    for _ in range(T):
        N, K = read_ints()
        coffee_days = [read_ints() for _ in range(N)]
        if P == 1:
            print(max_valid_packages(coffee_days), file=stdout)
        else:
            max_packages = max_valid_packages(coffee_days)
            print(count_splits(coffee_days, max_packages), file=stdout)

solve_problems()
```

