import numpy as np
import re
from collections import defaultdict

def parse_expression(expression):
    # Parse the expression using regex to get terms with variables and their coefficients
    terms = re.findall(r'([+-]?\d*)([xyztp]?)', expression)
    return [(int(c) if c else 1, v) for c, v in terms if c or v]

def evaluate_expression_for_1(expression):
    # Evaluate the expression when x, y, z and t are all 1
    terms = parse_expression(expression)
    return sum(c for c, v in terms)

def count_solutions(a, b, E, expression):
    # Count solutions within the interval [a, b] where the expression equals E
    terms = parse_expression(expression)

    # Separate the coefficients by variable
    coeffs = defaultdict(int)
    constant = 0
    for c, v in terms:
        if v:
            coeffs[v] += c
        else:
            constant += c

    # Generate all combinations of x, y, z, t in [a, b]
    count = 0
    for x in range(a, b + 1):
        for y in range(a, b + 1):
            for z in range(a, b + 1):
                for t in range(a, b + 1):
                    if coeffs['x']*x + coeffs['y']*y + coeffs['z']*z + coeffs['t']*t + constant == E:
                        count += 1

    return count

# Read input data
with open('eq4.in', 'r') as file:
    C = int(file.readline().strip())
    expression = file.readline().strip()
    a, b, E = map(int, file.readline().strip().split())

# Process the expression
if C == 1:
    result = evaluate_expression_for_1(expression)
elif C == 2:
    result = count_solutions(a, b, E, expression)

# Write output data
with open('eq4.out', 'w') as file:
    file.write(str(result) + "\n")
