import numpy as np
import re
from itertools import product

# Read the input
with open('eq4.in', 'r') as file:
    task = int(file.readline().strip())
    expression = file.readline().strip()
    a, b, E = map(int, file.readline().split())

# Function to evaluate the expression when x, y, z, t are all 1
def evaluate_expression_at_one(expression):
    # Replace variables with '1' and evaluate
    expr = expression.replace('x', '*1').replace('y', '*1').replace('z', '*1').replace('t', '*1')
    # Add zero to handle cases where the expression starts with a plus
    expr = '0' + expr
    return eval(expr)

# Function to count the number of quartets that satisfy the expression equals E
def count_quartets(expression, a, b, E):
    # Parse the expression and compute the coefficients for x, y, z, t and the constant term
    coeffs = {'x': 0, 'y': 0, 'z': 0, 't': 0, 'constant': 0}
    tokens = re.findall(r'([+-]?\d*)([xyzt]?)', expression)
    for token in tokens:
        coeff, var = token
        if var:
            coeffs[var] += int(coeff + '1' if coeff in '+-' else coeff)
        elif coeff:
            coeffs['constant'] += int(coeff)
    
    # Use numpy for efficient array operations
    range_values = np.arange(a, b + 1)
    count = 0
    for x, y, z, t in product(range_values, repeat=4):
        if coeffs['x']*x + coeffs['y']*y + coeffs['z']*z + coeffs['t']*t + coeffs['constant'] == E:
            count += 1
    return count

# Calculate the result based on the task
result = evaluate_expression_at_one(expression) if task == 1 else count_quartets(expression, a, b, E)

# Write the output
with open('eq4.out', 'w') as file:
    file.write(str(result) + '\n')
