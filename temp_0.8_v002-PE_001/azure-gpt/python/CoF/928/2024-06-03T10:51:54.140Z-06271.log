import numpy as np
import re

def expand(match):
    global transformations
    transformations += 1
    number, content = match.groups()
    return content * int(number)

def palindrome_even(match):
    global transformations
    transformations += 1
    content = match.group(1)
    return content + content[::-1]

def palindrome_odd(match):
    global transformations
    transformations += 1
    content = match.group(1)
    return content + content[-2::-1]

transformations = 0

# Read the input from 'arh.in'
with open('arh.in', 'r') as fin:
    S = fin.read().strip()

# Keep applying transformations until no more can be applied
pattern_expansion = re.compile(r'(\d+)\(([a-z]+)\)')
pattern_palindrome_even = re.compile(r'\[\*([a-z]+)\]')
pattern_palindrome_odd = re.compile(r'\[([a-z]+)\*\]')

while True:
    S, n1 = pattern_expansion.subn(expand, S)
    S, n2 = pattern_palindrome_even.subn(palindrome_even, S)
    S, n3 = pattern_palindrome_odd.subn(palindrome_odd, S)
    
    # If no transformations were made, break the loop
    if n1 + n2 + n3 == 0:
        break

# Write the output to 'arh.out'
with open('arh.out', 'w') as fout:
    fout.write(f'{transformations}\n{S}\n')
```

