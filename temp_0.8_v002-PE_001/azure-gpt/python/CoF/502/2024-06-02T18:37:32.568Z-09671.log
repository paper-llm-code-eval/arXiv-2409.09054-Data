# Utilizing numpy for efficient array manipulation
import numpy as np
from collections import deque

# Function to read the graph from the input file
def read_graph(filepath):
    with open(filepath, 'r') as f:
        content = f.readlines()
        
    c = int(content[0].strip())
    n, m = map(int, content[1].strip().split())
    S, A, B = map(int, content[2].strip().split())
    edges = [tuple(map(int, line.strip().split())) for line in content[3:]]
    
    return c, n, m, S, A, B, edges

# Function to write the output to file
def write_output(filepath, result):
    with open(filepath, 'w') as f:
        if isinstance(result, int):
            f.write(f"{result}\n")
        else:
            for path in result:
                f.write(f"{len(path)}\n{' '.join(map(str, path))}\n")

# Function to perform BFS and find the shortest path
def bfs(graph, start, n):
    distances = np.full(n + 1, np.inf)
    distances[start] = 0
    parents = np.full(n + 1, -1)
    queue = deque([start])
    
    while queue:
        node = queue.popleft()
        for neighbor in graph[node]:
            if distances[neighbor] == np.inf:
                distances[neighbor] = distances[node] + 1
                parents[neighbor] = node
                queue.append(neighbor)
    
    return distances, parents

# Function to reconstruct the path from BFS result
def reconstruct_path(parents, end):
    path = []
    while end != -1:
        path.append(end)
        end = parents[end]
    return path[::-1]

# Function to solve the problem
def solve_problem(c, n, m, S, A, B, edges):
    graph = {i: [] for i in range(1, n + 1)}
    for x, y in edges:
        graph[x].append(y)
        
    distances_from_s, parents_from_s = bfs(graph, S, n)
    distances_from_a, _ = bfs(graph, A, n)
    distances_from_b, _ = bfs(graph, B, n)
    
    min_minutes = np.inf
    Z = -1
    
    # Find the meeting point Z
    for node in range(1, n + 1):
        if distances_from_s[node] < np.inf and distances_from_a[node] < np.inf and distances_from_b[node] < np.inf:
            max_ab = max(distances_from_a[node], distances_from_b[node])
            total_minutes = distances_from_s[node] + max_ab
            if total_minutes < min_minutes:
                min_minutes = total_minutes
                Z = node
    
    if c == 1:
        return min_minutes
    else:
        path_s_to_z = reconstruct_path(parents_from_s, Z)
        path_z_to_a = reconstruct_path(parents_from_s, A)[1:]  # Exclude Z to avoid repetition
        path_z_to_b = reconstruct_path(parents_from_s, B)[1:]  # Exclude Z to avoid repetition
        
        return [path_s_to_z, path_z_to_a, path_z_to_b]

# Read the input, solve the problem, and write the output
c, n, m, S, A, B, edges = read_graph('veri.in')
result = solve_problem(c, n, m, S, A, B, edges)
write_output('veri.out', result)
