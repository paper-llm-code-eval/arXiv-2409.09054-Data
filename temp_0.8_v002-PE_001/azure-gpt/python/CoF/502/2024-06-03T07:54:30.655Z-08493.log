import numpy as np
from scipy.sparse.csgraph import dijkstra
from scipy.sparse import csr_matrix

# Read input data from 'veri.in'
with open('veri.in', 'r') as file:
    c = int(file.readline())
    n, m = map(int, file.readline().split())
    S, A, B = map(int, file.readline().split())
    edges = [tuple(map(int, line.split())) for line in file.readlines()]

# Convert edge list to a sparse adjacency matrix for memory efficiency
adj_matrix = csr_matrix((np.ones(m), zip(*edges)), shape=(n+1, n+1))

# Run Dijkstra's algorithm from S, A, B to find the shortest paths
_, predecessors_s = dijkstra(adj_matrix, directed=True, indices=S, return_predecessors=True)
dist_a, predecessors_a = dijkstra(adj_matrix, directed=True, indices=A, return_predecessors=True)
dist_b, predecessors_b = dijkstra(adj_matrix, directed=True, indices=B, return_predecessors=True)

# Calculate the shortest meeting point Z and the associated times t, t_A, and t_B
min_time = np.inf
for z in range(1, n+1):
    if predecessors_s[z] != -9999 and predecessors_a[z] != -9999 and predecessors_b[z] != -9999:
        t = max(dist_a[z], dist_b[z])
        if t < min_time:
            min_time = t
            Z = z
# Calculate t_A and t_B
t = int(dist_a[Z])
t_A = int(dist_a[A] - dist_a[Z])
t_B = int(dist_b[B] - dist_b[Z])

# Create the function to reconstruct the path using predecessors
def reconstruct_path(predecessors, start, end):
    path = [end]
    while end != start:
        end = predecessors[end]
        path.append(end)
    return path[::-1]

# Write the output to 'veri.out'
with open('veri.out', 'w') as file:
    if c == 1:
        file.write(f'{min_time}\n')
    if c == 2:
        # Reconstruct the paths
        path_s_z = reconstruct_path(predecessors_s, S, Z)
        path_z_a = reconstruct_path(predecessors_a, Z, A) if Z != A else [Z]
        path_z_b = reconstruct_path(predecessors_b, Z, B) if Z != B else [Z]

        # Write paths to the file
        file.write(f'{len(path_s_z)-1}\n{" ".join(map(str, path_s_z))}\n')
        file.write(f'{len(path_z_a)-1}\n{" ".join(map(str, path_z_a))}\n')
        file.write(f'{len(path_z_b)-1}\n{" ".join(map(str, path_z_b))}\n')
