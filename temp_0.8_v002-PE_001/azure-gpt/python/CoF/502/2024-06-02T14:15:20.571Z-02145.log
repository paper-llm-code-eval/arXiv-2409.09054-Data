import numpy as np
from collections import deque

# Efficiently read the input from file
def read_input(file_path):
    with open(file_path, 'r') as file:
        data = file.readlines()
    return [list(map(int, line.strip().split())) for line in data]

# Efficiently write the output to file
def write_output(file_path, data):
    with open(file_path, 'w') as file:
        for line in data:
            file.write(' '.join(map(str, line)) + '\n')

# Use BFS to find the shortest path in a directed graph
def bfs_shortest_path(graph, start, end):
    visited = {start}
    queue = deque([(start, [start])])
    while queue:
        current, path = queue.popleft()
        if current == end:
            return path
        for neighbor in graph[current]:
            if neighbor not in visited:
                visited.add(neighbor)
                queue.append((neighbor, path + [neighbor]))
    return []

# Find the common path and the individual paths to A and B
def find_paths(graph, S, A, B):
    path_to_A = bfs_shortest_path(graph, S, A)
    path_to_B = bfs_shortest_path(graph, S, B)
    common_path, path_A, path_B = [], [], []
    for i in range(min(len(path_to_A), len(path_to_B))):
        if path_to_A[i] != path_to_B[i]:
            break
        common_path.append(path_to_A[i])
    path_A = path_to_A[len(common_path)-1:]
    path_B = path_to_B[len(common_path)-1:]
    return common_path, path_A, path_B

data = read_input('veri.in')
c, n, m = data[0]
S, A, B = data[1]

# Build the adjacency list for the graph with numpy arrays
graph = {i: np.array([], dtype=int) for i in range(1, n+1)}
for edge in data[2:]:
    X, Y = edge
    graph[X] = np.append(graph[X], Y)

common_path, path_A, path_B = find_paths(graph, S, A, B)
max_time = max(len(common_path) + len(path_A), len(common_path) + len(path_B)) - 1

if c == 1:
    output = [[max_time]]
elif c == 2:
    output = [
        [len(common_path)-1] + common_path,
        [len(path_A)] + path_A,
        [len(path_B)] + path_B
    ]

write_output('veri.out', output)
