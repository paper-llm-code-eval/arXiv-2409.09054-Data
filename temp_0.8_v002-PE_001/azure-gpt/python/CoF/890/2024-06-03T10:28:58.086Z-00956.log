import numpy as np

# Function to find k-th prime divisor
def kth_prime_divisor(num, k):
    prime_divisors = []
    # Check for divisibility by 2
    while num % 2 == 0:
        prime_divisors.append(2)
        num = num // 2
    # Check for divisibility by odd numbers
    for i in range(3, int(np.sqrt(num)) + 1, 2):
        while num % i == 0:
            prime_divisors.append(i)
            num = num // i
    # If the remaining number is a prime number greater than 2
    if num > 2:
        prime_divisors.append(num)
    # Return k-th prime divisor
    return prime_divisors[k - 1]

# Read from file
with open('cufar.in', 'r') as file:
    p, n = map(int, file.readline().split())
    if p == 1:
        num, k = map(int, file.readline().split())
        val_magic = kth_prime_divisor(num, k)
        with open('cufar.out', 'w') as output_file:
            output_file.write(str(val_magic) + '\n')
    elif p == 2:
        # Preallocate the numpy array with the max possible size to save memory
        nums = np.zeros((n, 2), dtype=int)
        for i in range(n):
            nums[i] = list(map(int, file.readline().split()))
        # Calculate the sum of k-th prime divisors for each number
        sum_magic_values = sum(kth_prime_divisor(num, k) for num, k in nums)
        with open('cufar.out', 'w') as output_file:
            output_file.write(str(sum_magic_values) + '\n')
