# mosia_solver.py

import numpy as np

def read_input(file_path):
    with open(file_path, 'r') as file:
        n = int(file.readline().strip())
        posts = [tuple(map(int, line.split())) for line in file]
    return posts

def write_output(file_path, area):
    with open(file_path, 'w') as file:
        file.write(f'{area:.4f}\n')

def cross_product(o, a, b):
    return (a[0] - o[0]) * (b[1] - o[1]) - (a[1] - o[1]) * (b[0] - o[0])

def convex_hull(points):
    points.sort()
    lower = []
    for p in points:
        while len(lower) >= 2 and cross_product(lower[-2], lower[-1], p) <= 0:
            lower.pop()
        lower.append(p)
    upper = []
    for p in reversed(points):
        while len(upper) >= 2 and cross_product(upper[-2], upper[-1], p) <= 0:
            upper.pop()
        upper.append(p)
    return lower[:-1] + upper[:-1]

def calculate_area(hull):
    area = 0.0
    for i in range(len(hull)):
        j = (i + 1) % len(hull)
        area += hull[i][0] * hull[j][1]
        area -= hull[j][0] * hull[i][1]
    return abs(area) / 2.0

def main():
    posts = read_input('mosia.in')
    # Create matrix of the initial positions
    points = np.array([[x, y] for x, y, d in posts])
    # Generate all possible moves for each post
    moves = [np.array([[x + dx, y + dy] for dx in range(-d, d+1) for dy in range(-d, d+1) if dx**2 + dy**2 <= d**2]) for x, y, d in posts]
    # Create all combinations of post moves
    combinations = np.array(np.meshgrid(*moves)).T.reshape(-1, len(posts), 2)
    max_area = 0.0
    # Check each combination for the maximal area
    for comb in combinations:
        hull = convex_hull(comb.tolist())
        area = calculate_area(hull)
        max_area = max(max_area, area)
    write_output('mosia.out', max_area)

if __name__ == '__main__':
    main()
```

