from math import sqrt

def read_input(filename):
    with open(filename, 'r') as file:
        n = int(file.readline().strip())
        posts_data = [tuple(map(int, line.strip().split())) for line in file]
    return n, posts_data

def calculate_area(points):
    area = 0.0
    n = len(points)
    for i in range(n):
        x1, y1 = points[i]
        x2, y2 = points[(i + 1) % n]
        area += x1 * y2 - x2 * y1
    return abs(area) / 2.0

def move_posts(n, posts_data):
    posts = [(x, y) for x, y, _ in posts_data]
    distances = [d for _, _, d in posts_data]
    original_area = calculate_area(posts)
    max_area = original_area

    for i in range(n):
        x1, y1 = posts[i]
        x2, y2 = posts[(i + 1) % n]
        distance = distances[i]
        
        # Calculate unit vector for the direction from current post to the next
        dx, dy = x2 - x1, y2 - y1
        length = sqrt(dx**2 + dy**2)
        ux, uy = dx / length, dy / length

        # Move the post i towards the direction of the next post
        new_x, new_y = x1 + distance * ux, y1 + distance * uy
        new_posts = posts[:]
        new_posts[i] = (new_x, new_y)
        
        # Calculate new area
        new_area = calculate_area(new_posts)
        max_area = max(max_area, new_area)
    
    return max_area

def write_output(filename, max_area):
    with open(filename, 'w') as file:
        file.write(f"{max_area:.4f}\n")

# Read input data
n, posts_data = read_input('mosia.in')

# Calculate the maximum extended area
max_area = move_posts(n, posts_data)

# Write output data
write_output('mosia.out', max_area)
```

