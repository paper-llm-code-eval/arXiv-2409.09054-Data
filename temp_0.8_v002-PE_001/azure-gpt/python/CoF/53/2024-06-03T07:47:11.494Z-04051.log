import numpy as np

def read_input(filename):
    with open(filename, 'r') as file:
        lines = file.readlines()
        n = int(lines[0].strip())
        posts_data = [tuple(map(int, line.strip().split())) for line in lines[1:]]
    return n, np.array(posts_data)

def calculate_area(points):
    n = len(points)
    area = 0.5 * np.abs(np.dot(points[:, 0], np.roll(points[:, 1], 1)) - np.dot(points[:, 1], np.roll(points[:, 0], 1)))
    return area

def move_posts(n, posts_data):
    posts = posts_data[:, :2]
    distances = posts_data[:, 2]
    original_area = calculate_area(posts)
    
    # Convert posts to complex numbers for easier manipulation
    complex_posts = posts[:, 0] + 1j * posts[:, 1]
    
    # Create vectors representing the extended posts, then calculate the area
    for i in range(n):
        angle = np.angle(complex_posts[np.mod(i+1, n)] - complex_posts[i])
        displacement = distances[i] * (np.cos(angle) + 1j * np.sin(angle))
        
        # Move the post i towards the direction of the next post
        extended_posts = np.copy(complex_posts)
        extended_posts[i] += displacement
        
        # Check whether the new area is larger
        extended_area = calculate_area(extended_posts.view(float).reshape(-1, 2))
        if extended_area > original_area:
            original_area = extended_area
    
    return original_area

def write_output(filename, max_area):
    with open(filename, 'w') as file:
        file.write(f"{max_area:.4f}\n")

# Read input data
