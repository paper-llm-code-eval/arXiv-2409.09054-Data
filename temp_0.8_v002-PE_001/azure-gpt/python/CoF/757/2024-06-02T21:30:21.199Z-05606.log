import numpy as np

# Read from sudest.in
with open('sudest.in', 'r') as file:
    N = int(file.readline().strip())
    grid = np.array([list(map(int, file.readline().split())) for _ in range(N)], dtype=np.int32)
    K = int(file.readline().strip())
    commands = list(map(int, file.readline().split()))

# Initialize DP array and traceback array
dp = np.zeros((N, N), dtype=np.int32)
trace = np.full((N, N, 2), -1, dtype=np.int32)
dp[0, 0] = grid[0, 0]

# Populate DP array
for step in range(K):
    new_dp = np.zeros_like(dp)
    command = commands[step]
    for i in range(N):
        for j in range(N):
            if dp[i, j] > 0:
                if i + command < N:
                    if new_dp[i + command, j] < dp[i, j] + grid[i + command, j]:
                        new_dp[i + command, j] = dp[i, j] + grid[i + command, j]
                        trace[i + command, j] = [i, j]
                if j + command < N:
                    if new_dp[i, j + command] < dp[i, j] + grid[i, j + command]:
                        new_dp[i, j + command] = dp[i, j] + grid[i, j + command]
                        trace[i, j + command] = [i, j]
    dp = new_dp

# Backtrack to find path
path = []
i, j = N - 1, N - 1
while (i, j) != (-1, -1):
    path.append((i + 1, j + 1))
    i, j = trace[i, j]

# Write to sudest.out
with open('sudest.out', 'w') as file:
    file.write(f"{dp[-1, -1]}\n")
    for i, j in reversed(path):
        file.write(f"{i} {j}\n")
