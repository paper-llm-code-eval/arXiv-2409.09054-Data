#include <iostream>
#include <fstream>
#include <vector>
#include <algorithm>
#include <string>
#include <unordered_set>
#include <unordered_map>

int main() {
    std::ifstream infile("cladiri.in");
    std::ofstream outfile("cladiri.out");

    int N;
    infile >> N;
    std::vector<std::string> buildings(N);
    for (int i = 0; i < N; ++i) {
        infile >> buildings[i];
    }

    // Requirement 1
    int max_height = 0;
    std::unordered_set<int> buildings_with_max_height;
    for (int i = 0; i < N; ++i) {
        for (char ch : buildings[i]) {
            int height = ch - '0';
            if (height > max_height) {
                max_height = height;
                buildings_with_max_height.clear();
            }
            if (height == max_height) {
                buildings_with_max_height.insert(i);
            }
        }
    }
    int max_height_count = buildings_with_max_height.size();

    // Requirement 2
    int identical_labels_count = 0;
    for (const auto& building : buildings) {
        std::string reversed_building = building;
        std::reverse(reversed_building.begin(), reversed_building.end());
        if (building == reversed_building) {
            identical_labels_count++;
        }
    }

    // Requirement 3
    int min_cubes_needed = 0;
    for (const auto& building : buildings) {
        int len = building.size();
        for (int i = 0; i < len / 2; ++i) {
            if (building[i] != building[len - i - 1]) {
                min_cubes_needed += std::abs(building[i] - building[len - i - 1]);
            }
        }
