#include <iostream>
#include <fstream>
#include <vector>
#include <algorithm>
#include <string>
#include <unordered_map>

int main() {
    std::ifstream infile("cladiri.in");
    std::ofstream outfile("cladiri.out");

    int N;
    infile >> N;
    std::vector<std::string> buildings(N);
    for (int i = 0; i < N; ++i) {
        infile >> buildings[i];
    }

    // Requirement 1
    int max_height = 0;
    std::unordered_map<int, int> height_count;
    for (const auto& building : buildings) {
        for (char ch : building) {
            int height = ch - '0';
            max_height = std::max(max_height, height);
            height_count[height]++;
        }
    }
    int max_height_count = height_count[max_height];

    // Requirement 2
    int identical_labels_count = 0;
    for (const auto& building : buildings) {
        std::string reversed_building = building;
        std::reverse(reversed_building.begin(), reversed_building.end());
        if (building == reversed_building) {
            identical_labels_count++;
        }
    }

    // Requirement 3
    int min_cubes_needed = 0;
    for (const auto& building : buildings) {
        std::string reversed_building = building;
        std::reverse(reversed_building.begin(), reversed_building.end());
        int len = building.size();
        for (int i = 0; i < len; ++i) {
            if (building[i] != reversed_building[i]) {
                min_cubes_needed += std::abs(building[i] - reversed_building[i]);
            }
        }
    }

    outfile << max_height << " " << max_height_count << "\n";
    outfile << identical_labels_count << "\n";
    outfile << min_cubes_needed << "\n";

    infile.close();
    outfile.close();
    return 0;
}
```

