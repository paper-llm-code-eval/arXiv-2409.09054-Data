#include <fstream>
#include <vector>
#include <string>
#include <algorithm>
#include <climits>
#include <cmath>

using namespace std;

// Function to count the number of divisors of a number
int countDivisors(long long num) {
    int count = 0;
    for (long long i = 1; i <= sqrt(num); ++i) {
        if (num % i == 0) {
            if (i * i == num) {
                count++;
            } else {
                count += 2;
            }
        }
    }
    return count;
}

// Function to find the number with the minimum divisors and if tie, the smallest number
long long findMinDivisorsNumber(const string &num) {
    long long originalNumber = stoll(num);
    long long minNumber = originalNumber;
    int minDivisors = countDivisors(originalNumber);

    for (size_t i = 1; i < num.size(); ++i) {
        if (num[i] == '0') continue;
        string rotated = num.substr(i) + num.substr(0, i);
        long long rotatedNumber = stoll(rotated);
        int divisors = countDivisors(rotatedNumber);
        if (divisors < minDivisors || (divisors == minDivisors && rotatedNumber < minNumber)) {
            minDivisors = divisors;
            minNumber = rotatedNumber;
        }
    }

    return minNumber;
}

int main() {
    ifstream fin("forus.in");
    ofstream fout("forus.out");

    int C, N;
    fin >> C >> N;
    vector<string> numbers(N);
    for (int i = 0; i < N; ++i) {
        fin >> numbers[i];
    }

    if (C == 1) {
        int count = 0;
        for (const string &num : numbers) {
            bool canCutAnywhere = true;
            for (size_t i = 1; i < num.size(); ++i) {
                if (num[i] == '0') {
                    canCutAnywhere = false;
                    break;
                }
            }
            if (canCutAnywhere) {
                count++;
            }
        }
        fout << count << endl;
    } else if (C == 2) {
        for (const string &num : numbers) {
            fout << findMinDivisorsNumber(num) << " ";
        }
        fout << endl;
    }

    fin.close();
    fout.close();
    return 0;
}
