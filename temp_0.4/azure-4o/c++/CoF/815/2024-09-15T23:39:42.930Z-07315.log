#include <iostream>
#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>
#include <unordered_set>

using namespace std;

const int INF = 1e9;
const int dx[] = {-1, 1, 0, 0};
const int dy[] = {0, 0, -1, 1};

struct Point {
    int x, y;
    bool operator==(const Point& other) const {
        return x == other.x && y == other.y;
    }
    bool operator<(const Point& other) const {
        return x < other.x || (x == other.x && y < other.y);
    }
};

namespace std {
    template <>
    struct hash<Point> {
        size_t operator()(const Point& p) const {
            return hash<int>()(p.x) ^ hash<int>()(p.y);
        }
    };
}

int bfs(const Point& start, const Point& target, const unordered_set<Point>& obstacles, int n) {
    queue<Point> q;
    unordered_set<Point> visited;
    q.push(start);
    visited.insert(start);
    int steps = 0;

    while (!q.empty()) {
        int size = q.size();
        while (size--) {
            Point p = q.front();
            q.pop();

            if (p == target) return steps;

            for (int i = 0; i < 4; ++i) {
                int nx = p.x + dx[i];
                int ny = p.y + dy[i];
                Point np = {nx, ny};

                if (nx > 0 && nx <= n && ny > 0 && ny <= n && obstacles.find(np) == obstacles.end() && visited.find(np) == visited.end()) {
                    q.push(np);
                    visited.insert(np);
                }
            }
        }
        ++steps;
    }

    return INF;
}

int main() {
    ifstream fin("ai.in");
    ofstream fout("ai.out");

    int n;
    fin >> n;

    Point target, source1, source2, robot1, robot2;
    fin >> target.x >> target.y >> source1.x >> source1.y >> source2.x >> source2.y >> robot1.x >> robot1.y >> robot2.x >> robot2.y;

    int k;
    fin >> k;

    unordered_set<Point> obstacles;
    for (int i = 0; i < k; ++i) {
        Point obs;
        fin >> obs.x >> obs.y;
        obstacles.insert(obs);
    }

    // Part 1: Find the maximum length of a wall
    int max_wall_length = 0;

    // Horizontal walls
    for (int i = 1; i <= n; ++i) {
        int current_length = 0;
        for (int j = 1; j <= n; ++j) {
            if (obstacles.find({i, j}) != obstacles.end()) {
                current_length++;
                max_wall_length = max(max_wall_length, current_length);
            } else {
                current_length = 0;
            }
        }
    }

    // Vertical walls
    for (int j = 1; j <= n; ++j) {
        int current_length = 0;
        for (int i = 1; i <= n; ++i) {
            if (obstacles.find({i, j}) != obstacles.end()) {
                current_length++;
                max_wall_length = max(max_wall_length, current_length);
            } else {
                current_length = 0;
            }
        }
    }

    // Part 2: Find the minimum time to protect the target
    int min_time = INF;

    // Calculate the minimum time for each robot to reach the laser paths
    vector<Point> laser_path1, laser_path2;

    if (source1.x == target.x) {
        for (int j = min(source1.y, target.y); j <= max(source1.y, target.y); ++j) {
            laser_path1.push_back({source1.x, j});
        }
    } else if (source1.y == target.y) {
        for (int i = min(source1.x, target.x); i <= max(source1.x, target.x); ++i) {
            laser_path1.push_back({i, source1.y});
        }
    }

    if (source2.x == target.x) {
        for (int j = min(source2.y, target.y); j <= max(source2.y, target.y); ++j) {
            laser_path2.push_back({source2.x, j});
        }
    } else if (source2.y == target.y) {
        for (int i = min(source2.x, target.x); i <= max(source2.x, target.x); ++i) {
            laser_path2.push_back({i, source2.y});
        }
    }

    for (const Point& p1 : laser_path1) {
        for (const Point& p2 : laser_path2) {
            int time1 = bfs(robot1, p1, obstacles, n);
            int time2 = bfs(robot2, p2, obstacles, n);
            if (time1 < INF && time2 < INF) {
                min_time = min(min_time, max(time1, time2));
            }

            time1 = bfs(robot2, p1, obstacles, n);
            time2 = bfs(robot1, p2, obstacles, n);
            if (time1 < INF && time2 < INF) {
                min_time = min(min_time, max(time1, time2));
            }
        }
    }

    fout << max_wall_length << endl;
    fout << min_time << endl;

    return 0;
}
