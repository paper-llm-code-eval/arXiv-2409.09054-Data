#include <fstream>
#include <string>
#include <vector>
#include <cmath>

using namespace std;

int main() {
    ifstream fin("robot.in");
    ofstream fout("robot.out");

    int C;
    fin >> C;

    if (C == 1 || C == 2) {
        string roboprogram;
        fin >> roboprogram;

        int position = 0;
        string screen = "";

        for (size_t i = 0; i < roboprogram.size(); ++i) {
            char cmd = roboprogram[i];
            if (cmd == 'D') {
                int p = roboprogram[++i] - '0';
                position = (position + p) % 10;
            } else if (cmd == 'S') {
                int p = roboprogram[++i] - '0';
                position = (position - p + 10) % 10;
            } else if (cmd == 'A') {
                screen += to_string(position);
            } else if (cmd == 'T') {
                break;
            }
        }

        if (C == 1) {
            fout << screen.size() << endl;
        } else if (C == 2) {
            fout << screen << endl;
        }
    } else if (C == 3) {
        unsigned long long N;
        fin >> N;
        string roboprogram = "";

        string number = to_string(N);
        int position = 0;

        for (char digit : number) {
            int target = digit - '0';
            int right_moves = (target - position + 10) % 10;
            int left_moves = (position - target + 10) % 10;

            if (right_moves <= left_moves) {
                roboprogram += "D" + to_string(right_moves);
                position = target;
            } else {
                roboprogram += "S" + to_string(left_moves);
                position = target;
            }

            roboprogram += "A";
        }

        roboprogram += "T";
        fout << roboprogram << endl;
    }

    fin.close();
    fout.close();

    return 0;
}
```

