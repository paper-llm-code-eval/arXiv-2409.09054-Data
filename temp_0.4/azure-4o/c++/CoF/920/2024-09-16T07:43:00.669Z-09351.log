#include <iostream>
#include <fstream>
#include <vector>
#include <string>
#include <algorithm>

using namespace std;

bool isPrime(int num) {
    if (num <= 1) return false;
    if (num <= 3) return true;
    if (num % 2 == 0 || num % 3 == 0) return false;
    for (int i = 5; i * i <= num; i += 6) {
        if (num % i == 0 || num % (i + 2) == 0) return false;
    }
    return true;
}

int getMaxPrime(const vector<int>& numbers) {
    int maxPrime = 0;
    for (int num : numbers) {
        if (isPrime(num)) {
            maxPrime = max(maxPrime, num);
        }
    }
    return maxPrime;
}

int getMaxPrimeFromCuts(const vector<int>& numbers, int parts) {
    int maxPrime = 0;
    for (int num : numbers) {
        string numStr = to_string(num);
        int len = numStr.size();
        if (parts == 2) {
            for (int i = 1; i < len; ++i) {
                int part1 = stoi(numStr.substr(0, i));
                int part2 = stoi(numStr.substr(i));
                if (isPrime(part1)) maxPrime = max(maxPrime, part1);
                if (isPrime(part2)) maxPrime = max(maxPrime, part2);
            }
        } else if (parts == 3) {
            for (int i = 1; i < len; ++i) {
                for (int j = i + 1; j < len; ++j) {
                    int part1 = stoi(numStr.substr(0, i));
                    int part2 = stoi(numStr.substr(i, j - i));
                    int part3 = stoi(numStr.substr(j));
                    if (isPrime(part1)) maxPrime = max(maxPrime, part1);
                    if (isPrime(part2)) maxPrime = max(maxPrime, part2);
                    if (isPrime(part3)) maxPrime = max(maxPrime, part3);
                }
            }
        }
    }
    return maxPrime;
}

int main() {
    ifstream infile("tai.in");
    ofstream outfile("tai.out");

    int C, N;
    infile >> C >> N;
    vector<int> numbers(N);
    for (int i = 0; i < N; ++i) {
        infile >> numbers[i];
    }

    int result = 0;
    if (C == 1) {
        result = getMaxPrime(numbers);
    } else if (C == 2) {
        result = getMaxPrimeFromCuts(numbers, 2);
    } else if (C == 3) {
        result = getMaxPrimeFromCuts(numbers, 3);
    }

    outfile << result << endl;

    infile.close();
    outfile.close();
    return 0;
}
```

