#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream fin("colier.in");
    ofstream fout("colier.out");

    int T, N;
    fin >> T >> N;
    vector<int> beads(N);
    for (int i = 0; i < N; ++i) {
        fin >> beads[i];
    }

    vector<int> type1, type2;
    for (int i = 0; i < N; ++i) {
        int num = beads[i];
        int minDigit = 9, maxDigit = 1;
        while (num > 0) {
            int digit = num % 10;
            minDigit = min(minDigit, digit);
            maxDigit = max(maxDigit, digit);
            num /= 10;
        }
        if (minDigit < maxDigit) {
            type1.push_back(beads[i]);
        } else {
            type2.push_back(beads[i]);
        }
    }

    if (T == 1) {
        fout << type1.size() << endl;
    } else {
        int maxLength = 0;
        int type1Count = type1.size();
        int type2Count = type2.size();

        if (type1Count > 0 && type2Count > 0) {
            maxLength = 2 * min(type1Count, type2Count);
            if (type1Count != type2Count) {
                maxLength += 1;
            }
        } else {
            maxLength = max(type1Count, type2Count);
        }

        fout << maxLength << endl;
    }

    fin.close();
    fout.close();

    return 0;
}
```

