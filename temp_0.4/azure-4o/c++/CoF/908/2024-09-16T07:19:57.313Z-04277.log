#include <fstream>
#include <vector>
#include <string>
#include <algorithm>
#include <iostream>
#include <set>

using namespace std;

// Function to extract the two largest digits from a number
pair<int, int> getTwoLargestDigits(int number) {
    int max1 = -1, max2 = -1;
    while (number > 0) {
        int digit = number % 10;
        if (digit > max1) {
            max2 = max1;
            max1 = digit;
        } else if (digit > max2) {
            max2 = digit;
        }
        number /= 10;
    }
    return {max1, max2};
}

int main() {
    ifstream infile("album.in");
    ofstream outfile("album.out");

    int c, n;
    infile >> c >> n;

    vector<int> stickers(n);
    for (int i = 0; i < n; ++i) {
        infile >> stickers[i];
    }

    if (c == 1) {
        // Task 1: Find the two largest digits of the last sticker
        auto [max1, max2] = getTwoLargestDigits(stickers.back());
        if (max1 > max2) {
            swap(max1, max2);
        }
        outfile << max1 << " " << max2 << "\n";
    } else if (c == 2) {
        // Task 2: Determine the winner
        vector<int> victor, radu;
        set<int> taken;

        bool victor_turn = true;
        while (taken.size() < n) {
            for (int i = 0; i < n; ++i) {
                if (taken.find(i) != taken.end()) continue;

                int sticker = stickers[i];
                auto [max1, max2] = getTwoLargestDigits(sticker);

                vector<int> to_take;
                for (int j = i; j < n; ++j) {
                    if (taken.find(j) == taken.end()) {
                        auto [d1, d2] = getTwoLargestDigits(stickers[j]);
                        if ((d1 == max1 || d1 == max2) && (d2 == max1 || d2 == max2)) {
                            to_take.push_back(j);
                        }
                    }
                }

                if (victor_turn) {
                    for (int idx : to_take) {
                        victor.push_back(stickers[idx]);
                        taken.insert(idx);
                    }
                } else {
                    for (int idx : to_take) {
                        radu.push_back(stickers[idx]);
                        taken.insert(idx);
                    }
                }
                victor_turn = !victor_turn;
                break;
            }
        }

        if (victor.size() > radu.size()) {
            outfile << "V\n" << victor.size() << "\n";
        } else if (radu.size() > victor.size()) {
            outfile << "R\n" << radu.size() << "\n";
        } else {
            outfile << "V R\n" << victor.size() << "\n";
        }
    }

    infile.close();
    outfile.close();
    return 0;
}
```

### Explanation:

1. **Reading Input**: The input is read from `album.in` file.
2. **Task 1**: If `c == 1`, the program finds the two largest digits of the last sticker and writes them to `album.out`.
3. **Task 2**: If `c == 2`, the program simulates the sticker picking game between Victor and Radu:
   - It uses a set to keep track of taken stickers.
   - It alternates turns between Victor and Radu.
   - For each turn, it identifies the stickers to be taken based on the largest two digits of the current sticker.
   - It writes the result to `album.out` indicating the winner and the number of stickers they have.

This approach ensures that the solution is efficient and meets the problem constraints.
