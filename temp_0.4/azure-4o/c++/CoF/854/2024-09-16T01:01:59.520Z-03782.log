#include <iostream>
#include <fstream>
#include <vector>
#include <string>
#include <algorithm>

using namespace std;

bool isQuartz(int x) {
    int sum = 0, increment = 1;
    while (sum < x) {
        sum += increment;
        increment += 4;
    }
    return sum == x;
}

string removeDigits(const string &num, bool removeEven) {
    string result;
    for (char c : num) {
        int digit = c - '0';
        if ((removeEven && digit % 2 != 0) || (!removeEven && digit % 2 == 0)) {
            result += c;
        }
    }
    return result.empty() || result.find_first_not_of('0') == string::npos ? "" : result;
}

int main() {
    ifstream fin("cuart.in");
    ofstream fout("cuart.out");

    int P, N;
    fin >> P >> N;

    vector<int> cards(2 * N);
    for (int i = 0; i < 2 * N; ++i) {
        fin >> cards[i];
    }

    vector<int> ginaNumbers, mihaiNumbers;
    vector<int> ginaQuartz, mihaiQuartz;
    vector<int> omittedNumbers;

    for (int i = 0; i < N; ++i) {
        string original = to_string(cards[i]);
        string processed = removeDigits(original, true);
        if (!processed.empty()) {
            int num = stoi(processed);
            ginaNumbers.push_back(num);
            if (isQuartz(num)) {
                ginaQuartz.push_back(num);
            }
        } else {
            omittedNumbers.push_back(cards[i]);
        }
    }

    for (int i = N; i < 2 * N; ++i) {
        string original = to_string(cards[i]);
