            fout << mihaiQuartz.size() << endl;
        } else {
            if (ginaNumbers.empty() && mihaiNumbers.empty()) {
                fout << 0 << endl;
            } else {
                int ginaFirst = ginaNumbers.empty() ? 0 : *max_element(ginaNumbers.begin(), ginaNumbers.end());
                int mihaiFirst = mihaiNumbers.empty() ? 0 : *max_element(mihaiNumbers.begin(), mihaiNumbers.end());
                if (ginaFirst > mihaiFirst) {
                    fout << ginaFirst << endl;
                } else if (ginaFirst < mihaiFirst) {
                    fout << mihaiFirst << endl;
                } else {
