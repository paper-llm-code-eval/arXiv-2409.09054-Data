    inFile >> a >> b >> E;

    unordered_map<char, int> coefficients = {{'x', 0}, {'y', 0}, {'z', 0}, {'t', 0}};
    int constantTerm = 0;

    parseExpression(expression, coefficients, constantTerm);

    if (C == 1) {
        long long result = evaluateExpression(coefficients, constantTerm, 1, 1, 1, 1);
