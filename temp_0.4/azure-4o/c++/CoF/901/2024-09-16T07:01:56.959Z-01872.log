#include <iostream>
#include <fstream>
#include <string>
#include <unordered_map>
#include <vector>
#include <cmath>
#include <algorithm>

using namespace std;

void parseExpression(const string &expression, unordered_map<char, int> &coefficients, int &constantTerm) {
    int n = expression.size();
    int sign = 1;
    int num = 0;
    bool hasNum = false;
    char var = '\0';

    for (int i = 0; i <= n; ++i) {
        if (i == n || expression[i] == '+' || expression[i] == '-') {
            if (var != '\0') {
                coefficients[var] += sign * (hasNum ? num : 1);
            } else if (hasNum) {
                constantTerm += sign * num;
            }
            if (i < n) {
                sign = (expression[i] == '+') ? 1 : -1;
            }
            num = 0;
            hasNum = false;
            var = '\0';
        } else if (isdigit(expression[i])) {
            num = num * 10 + (expression[i] - '0');
            hasNum = true;
        } else {
            var = expression[i];
        }
    }
}

long long evaluateExpression(const unordered_map<char, int> &coefficients, int constantTerm, int x, int y, int z, int t) {
    return coefficients.at('x') * x + coefficients.at('y') * y + coefficients.at('z') * z + coefficients.at('t') * t + constantTerm;
}

int countValidQuartets(const unordered_map<char, int> &coefficients, int constantTerm, int a, int b, long long E) {
    int count = 0;
    for (int x = a; x <= b; ++x) {
        for (int y = a; y <= b; ++y) {
            for (int z = a; z <= b; ++z) {
                for (int t = a; t <= b; ++t) {
                    if (evaluateExpression(coefficients, constantTerm, x, y, z, t) == E) {
                        count++;
                    }
                }
            }
        }
    }
    return count;
}

