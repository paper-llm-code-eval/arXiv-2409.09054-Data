#include <iostream>
#include <fstream>
#include <string>
#include <unordered_map>
#include <vector>
#include <cmath>

using namespace std;

// Function to parse the expression and calculate coefficients and constant term
void parseExpression(const string &expr, unordered_map<char, int> &coeff, int &constant) {
    int n = expr.size();
    int i = 0;
    while (i < n) {
        int sign = 1;
        if (expr[i] == '+') {
            sign = 1;
            i++;
        } else if (expr[i] == '-') {
            sign = -1;
            i++;
        }
        
        int num = 0;
        bool hasNum = false;
        while (i < n && isdigit(expr[i])) {
            num = num * 10 + (expr[i] - '0');
            hasNum = true;
            i++;
        }
        
        if (i < n && isalpha(expr[i])) {
            char var = expr[i];
            i++;
            coeff[var] += sign * (hasNum ? num : 1);
        } else {
            constant += sign * num;
        }
    }
}

// Function to evaluate the expression for x=y=z=t=1
int evaluateExpression(const unordered_map<char, int> &coeff, int constant) {
    int result = constant;
    for (auto &pair : coeff) {
        result += pair.second;
    }
    return result;
}

// Function to count the number of distinct quartets (x, y, z, t) in range [a, b] that satisfy the expression = E
int countQuartets(const unordered_map<char, int> &coeff, int constant, int a, int b, int E) {
    int count = 0;
    for (int x = a; x <= b; x++) {
        for (int y = a; y <= b; y++) {
            for (int z = a; z <= b; z++) {
                for (int t = a; t <= b; t++) {
                    int value = constant;
                    if (coeff.find('x') != coeff.end()) value += coeff.at('x') * x;
                    if (coeff.find('y') != coeff.end()) value += coeff.at('y') * y;
                    if (coeff.find('z') != coeff.end()) value += coeff.at('z') * z;
                    if (coeff.find('t') != coeff.end()) value += coeff.at('t') * t;
                    if (value == E) count++;
                }
            }
        }
    }
    return count;
}

int main() {
    ifstream infile("eq4.in");
    ofstream outfile("eq4.out");

    int C;
    infile >> C;
    string expr;
    infile >> expr;
    int a, b, E;
    infile >> a >> b >> E;

    unordered_map<char, int> coeff;
    int constant = 0;
    
    parseExpression(expr, coeff, constant);

    if (C == 1) {
        int result = evaluateExpression(coeff, constant);
        outfile << result << endl;
    } else if (C == 2) {
        int result = countQuartets(coeff, constant, a, b, E);
        outfile << result << endl;
    }

    infile.close();
    outfile.close();

    return 0;
}
