#include <iostream>
#include <fstream>
#include <vector>
#include <queue>
#include <tuple>
#include <algorithm>

using namespace std;

struct Room {
    int area;
    int topLeftRow;
    int topLeftCol;
    int bottomRightRow;
    int bottomRightCol;
};

void floodFill(vector<vector<int>>& grid, vector<vector<bool>>& visited, int startRow, int startCol, Room& room) {
    int n = grid.size();
    queue<pair<int, int>> q;
    q.push({startRow, startCol});
    visited[startRow][startCol] = true;

    int area = 0;
    int minRow = startRow, maxRow = startRow, minCol = startCol, maxCol = startCol;

    vector<int> dRow = {0, 1, 0, -1};
    vector<int> dCol = {-1, 0, 1, 0};

    while (!q.empty()) {
        auto [row, col] = q.front();
        q.pop();
        area++;

        minRow = min(minRow, row);
        maxRow = max(maxRow, row);
        minCol = min(minCol, col);
        maxCol = max(maxCol, col);

        for (int i = 0; i < 4; ++i) {
            int newRow = row + dRow[i];
            int newCol = col + dCol[i];
            int wall = 1 << i;

            if (newRow >= 0 && newRow < n && newCol >= 0 && newCol < n && !visited[newRow][newCol] && !(grid[row][col] & wall)) {
                visited[newRow][newCol] = true;
                q.push({newRow, newCol});
            }
        }
    }

    room.area = area;
    room.topLeftRow = minRow;
    room.topLeftCol = minCol;
    room.bottomRightRow = maxRow;
    room.bottomRightCol = maxCol;
}

int main() {
    ifstream fin("castel.in");
    ofstream fout("castel.out");

    int C, N;
    fin >> C >> N;

    vector<vector<int>> grid(N, vector<int>(N));
    for (int i = 0; i < N; ++i) {
        for (int j = 0; j < N; ++j) {
            fin >> grid[i][j];
        }
    }

    vector<vector<bool>> visited(N, vector<bool>(N, false));
    vector<Room> rooms;

    for (int i = 0; i < N; ++i) {
        for (int j = 0; j < N; ++j) {
            if (!visited[i][j]) {
                Room room;
                floodFill(grid, visited, i, j, room);
                rooms.push_back(room);
            }
        }
    }

    if (C == 1) {
        fout << rooms.size() << endl;
    } else if (C == 2) {
        int maxArea = 0;
        for (const auto& room : rooms) {
            maxArea = max(maxArea, room.area);
        }
        fout << maxArea << endl;
    } else if (C == 3) {
        Room maxRoom = rooms[0];
        for (const auto& room : rooms) {
            if (room.area > maxRoom.area) {
                maxRoom = room;
            } else if (room.area == maxRoom.area) {
                if (room.topLeftRow < maxRoom.topLeftRow ||
                    (room.topLeftRow == maxRoom.topLeftRow && room.topLeftCol < maxRoom.topLeftCol)) {
                    maxRoom = room;
                }
            }
        }
        fout << maxRoom.topLeftRow << " " << maxRoom.topLeftCol << " " << maxRoom.bottomRightRow << " " << maxRoom.bottomRightCol << endl;
    }

    fin.close();
    fout.close();
    return 0;
}
```

This implementation reads the input, processes the grid using a flood fill algorithm to identify rooms and their areas, and then outputs the required results based on the value of `C`. The flood fill algorithm ensures that all connected components (rooms) are identified and their properties are calculated efficiently.
