std::string processSequence(const std::string& seq, int& transformCount) {
    std::string result;
    int n = 0;
    bool isNumber = false;

    for (char ch : seq) {
        if (isdigit(ch)) {
            n = n * 10 + (ch - '0');
            isNumber = true;
        } else if (ch == '(') {
            std::string innerSeq;
            int balance = 1;
            size_t i = 0;
            while (balance > 0 && ++i < seq.size()) {
                if (seq[i] == '(') balance++;
                else if (seq[i] == ')') balance--;
                if (balance > 0) innerSeq += seq[i];
            }
            std::string expanded = processSequence(innerSeq, transformCount);
            for (int j = 0; j < n; ++j) result += expanded;
            transformCount++;
            n = 0;
            isNumber = false;
        } else if (ch == '[') {
            std::string innerSeq;
            int balance = 1;
            size_t i = 0;
            while (balance > 0 && ++i < seq.size()) {
                if (seq[i] == '[') balance++;
                else if (seq[i] == ']') balance--;
                if (balance > 0) innerSeq += seq[i];
            }
            if (innerSeq[0] == '*') {
                std::string palSeq = innerSeq.substr(1);
                std::string mirrored = palSeq;
                std::reverse(mirrored.begin(), mirrored.end());
                result += palSeq + mirrored;
            } else if (innerSeq.back() == '*') {
                std::string palSeq = innerSeq.substr(0, innerSeq.size() - 1);
                std::string mirrored = palSeq;
                std::reverse(mirrored.begin(), mirrored.end());
                result += palSeq + mirrored.substr(1);
            }
            transformCount++;
        } else {
            result += ch;
        }
    }

    return result;
}

