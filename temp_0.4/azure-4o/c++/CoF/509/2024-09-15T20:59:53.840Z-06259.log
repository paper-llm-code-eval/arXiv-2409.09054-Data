#include <fstream>
#include <cmath>
#include <algorithm>

using namespace std;

bool isPrime(int num) {
    if (num <= 1) return false;
    if (num <= 3) return true;
    if (num % 2 == 0 || num % 3 == 0) return false;
    for (int i = 5; i * i <= num; i += 6) {
        if (num % i == 0 || num % (i + 2) == 0) return false;
    }
    return true;
}

int sumOfDigits(int num) {
    int sum = 0;
    while (num > 0) {
        sum += num % 10;
        num /= 10;
    }
    return sum;
}

int calculateDuration(int h1, int m1, int h2, int m2) {
    int startMinutes = h1 * 60 + m1;
    int endMinutes = h2 * 60 + m2;
    if (endMinutes < startMinutes) {
        endMinutes += 24 * 60;
    }
    return endMinutes - startMinutes;
}

int main() {
    ifstream inFile("aeriana.in");
    ofstream outFile("aeriana.out");

    int C, N;
    inFile >> C >> N;

    int maxDurationBefore = 0;
    int maxDurationAfter = 0;

    for (int i = 0; i < N; ++i) {
        int A1, A2, H1, M1, H2, M2;
        inFile >> A1 >> A2 >> H1 >> M1 >> H2 >> M2;

        int durationBefore = calculateDuration(H1, M1, H2, M2);
        maxDurationBefore = max(maxDurationBefore, durationBefore);

        if (isPrime(A1) && A2 % sumOfDigits(A1) == 0) {
            int durationAfter = calculateDuration(H2, M2, H1, M1);
            maxDurationAfter = max(maxDurationAfter, durationAfter);
        } else {
            maxDurationAfter = max(maxDurationAfter, durationBefore);
        }
    }

    int maxHours, maxMinutes;
    if (C == 1) {
        maxHours = maxDurationBefore / 60;
        maxMinutes = maxDurationBefore % 60;
    } else {
        maxHours = maxDurationAfter / 60;
        maxMinutes = maxDurationAfter % 60;
    }

    outFile << maxHours << " " << maxMinutes << endl;

    inFile.close();
    outFile.close();

    return 0;
}
```

