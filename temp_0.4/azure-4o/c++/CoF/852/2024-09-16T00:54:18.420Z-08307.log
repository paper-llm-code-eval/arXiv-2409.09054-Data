using namespace std;

void readInput(int &V, int &n, vector<int> &bile) {
    ifstream fin("arc.in");
    fin >> V >> n;
    bile.resize(n);
    for (int i = 0; i < n; ++i) {
        fin >> bile[i];
    }
    fin.close();
}

int countInitialSequences(const vector<int> &bile) {
    int count = 1;
    for (int i = 1; i < bile.size(); ++i) {
        if (bile[i] != bile[i-1]) {
            ++count;
        }
    }
    return count;
}

void eliminateBalls(vector<int> &bile) {
    bool eliminated;
    do {
        eliminated = false;
        int n = bile.size();
        vector<int> newBile;
        int i = 0;
        while (i < n) {
            int j = i;
            while (j < n && bile[j] == bile[i]) {
                ++j;
            }
            if (j - i < 3) {
                newBile.insert(newBile.end(), bile.begin() + i, bile.begin() + j);
            } else {
                eliminated = true;
            }
            i = j;
        }
        bile = newBile;
    } while (eliminated);
}

void writeOutput(int V, int initialSequences, const vector<int> &bile) {
    ofstream fout("arc.out");
    if (V == 1) {
        fout << initialSequences << endl;
    } else if (V == 2) {
        fout << bile.size() << endl;
        for (int color : bile) {
            fout << color << endl;
        }
    }
    fout.close();
}

int main() {
    int V, n;
    vector<int> bile;

    readInput(V, n, bile);

    int initialSequences = countInitialSequences(bile);

    if (V == 2) {
        eliminateBalls(bile);
    }

    writeOutput(V, initialSequences, bile);

    return 0;
}
