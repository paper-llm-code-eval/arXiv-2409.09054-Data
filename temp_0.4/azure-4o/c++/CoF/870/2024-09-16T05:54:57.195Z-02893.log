#include <fstream>
#include <vector>
#include <algorithm>
#include <string>

using namespace std;

void solve1(const string& X, ofstream& fout) {
    string Y = X;
    prev_permutation(Y.begin(), Y.end());
    fout << Y << endl;
}

int countWays(const string& X, const string& A, int xi, int ai, vector<vector<int>>& dp) {
    if (ai == A.size()) return 1;
    if (xi == X.size()) return 0;
    if (dp[xi][ai] != -1) return dp[xi][ai];

    int count = countWays(X, A, xi + 1, ai, dp);
    if (X[xi] == A[ai]) {
        count += countWays(X, A, xi + 1, ai + 1, dp);
    }

    return dp[xi][ai] = count;
}

void solve2(const string& X, const string& A, ofstream& fout) {
    vector<vector<int>> dp(X.size(), vector<int>(A.size(), -1));
    int Z = countWays(X, A, 0, 0, dp);
    fout << Z << endl;
}

int main() {
    ifstream fin("axyz.in");
    ofstream fout("axyz.out");

    int p;
    fin >> p;

    string A;
    fin >> A;

    int N;
    fin >> N;

    vector<char> digits(N);
    for (int i = 0; i < N; ++i) {
        fin >> digits[i];
    }

    string X(digits.begin(), digits.end());

    if (p == 1) {
        solve1(X, fout);
    } else if (p == 2) {
        solve2(X, A, fout);
    }

    return 0;
}
