#include <iostream>
#include <fstream>
#include <vector>
#include <algorithm>
#include <string>

using namespace std;

void solve1(const string& X) {
    string Y = X;
    prev_permutation(Y.begin(), Y.end());
    ofstream fout("axyz.out");
    fout << Y << endl;
    fout.close();
}

int countWays(const string& A, const string& X) {
    int m = A.size();
    int n = X.size();
    vector<vector<int>> dp(m + 1, vector<int>(n + 1, 0));
    
    for (int j = 0; j <= n; ++j) {
        dp[0][j] = 1;
    }
    
    for (int i = 1; i <= m; ++i) {
        for (int j = 1; j <= n; ++j) {
            dp[i][j] = dp[i][j - 1];
            if (A[i - 1] == X[j - 1]) {
                dp[i][j] += dp[i - 1][j - 1];
            }
        }
    }
    
    return dp[m][n];
}

void solve2(const string& A, const string& X) {
    int Z = countWays(A, X);
    ofstream fout("axyz.out");
    fout << Z << endl;
    fout.close();
}

int main() {
    ifstream fin("axyz.in");
    int p;
    string A;
    int N;
    string X;
    
    fin >> p >> A >> N;
    X.resize(N);
    for (int i = 0; i < N; ++i) {
        fin >> X[i];
    }
    fin.close();
    
    if (p == 1) {
        solve1(X);
    } else if (p == 2) {
        solve2(A, X);
    }
    
    return 0;
}
