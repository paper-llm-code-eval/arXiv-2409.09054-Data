        }
    }

    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= m; ++j) {
            for (int k = 0; k < 4; ++k) {
                dp[i][j][k] = INF;
            }
        }
    }

    dp[1][1][0] = 0;
    dp[1][1][1] = 0;
    dp[1][1][2] = 0;
    dp[1][1][3] = 0;

    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= m; ++j) {
            if (i > 1) {
                dp[i][j][0] = min(dp[i][j][0], dp[i - 1][j][0] + (a[i][j] < a[i - 1][j]));
                dp[i][j][1] = min(dp[i][j][1], dp[i - 1][j][1] + (a[i][j] < a[i - 1][j]));
                dp[i][j][2] = min(dp[i][j][2], dp[i - 1][j][2] + (a[i][j] < a[i - 1][j]));
                dp[i][j][3] = min(dp[i][j][3], dp[i - 1][j][3] + (a[i][j] < a[i - 1][j]));
            }
            if (j > 1) {
                dp[i][j][0] = min(dp[i][j][0], dp[i][j - 1][0] + (a[i][j] < a[i][j - 1]));
                dp[i][j][1] = min(dp[i][j][1], dp[i][j - 1][1] + (a[i][j] > a[i][j - 1]));
                dp[i][j][2] = min(dp[i][j][2], dp[i][j - 1][2] + (a[i][j] < a[i][j - 1]));
                dp[i][j][3] = min(dp[i][j][3], dp[i][j - 1][3] + (a[i][j] > a[i][j - 1]));
            }
        }
    }

    int ans = INF;
    for (int k = 0; k < 4; ++k) {
        ans = min(ans, dp[n][m][k]);
    }

    if (ans == INF) {
        fout << "-1\n";
    } else {
        fout << ans << "\n";
    }

    return 0;
}
