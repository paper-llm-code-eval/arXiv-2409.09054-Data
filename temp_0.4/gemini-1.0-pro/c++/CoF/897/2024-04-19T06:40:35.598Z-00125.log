#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("tbile.in");
ofstream fout("tbile.out");

const int MAX_N = 2e9;
const int MAX_M = 311445015;

int n, m, c, v, x[MAX_N + 5], y[MAX_N + 5], nr_tuburi_rosii, nr_bile_rosii, suma_nivel;
vector<int> rosu;

int main() {
    fin >> c >> n >> m;
    if (c == 2) { fin >> v; }
    x[1] = 1;
    y[1] = 2;
    y[2] = 4;
    int i = 3, j = 3;
    while (x[i - 1] + y[i - 1] <= n) {
        x[i] = x[i - 1] + y[i - 1];
        y[i] = x[i - 1] + 1;
        while (y[i] < x[i] && y[i] <= n) {
            y[++i] = y[i - 1] + 1;
        }
    }
    int k = i;
    while (j <= k) {
        if (y[j] <= n) {
            rosu.push_back(y[j]);
            nr_bile_rosii++;
            if (nr_bile_rosii % m == 0) { nr_tuburi_rosii++; }
        }
        j++;
    }
    while (j <= n) {
        rosu.push_back(j);
        nr_bile_rosii++;
        if (nr_bile_rosii % m == 0) { nr_tuburi_rosii++; }
        j++;
    }
    if (c == 1) {
        fout << nr_tuburi_rosii << " " << nr_bile_rosii << "\n";
    } else if (c == 2) {
        for (int i = 0; i < v; i++) {
            suma_nivel += rosu[i];
        }
        fout << suma_nivel << "\n";
    }
    return 0;
}
