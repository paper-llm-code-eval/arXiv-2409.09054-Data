#include <fstream>
#include <vector>
#include <algorithm>
#include <set>
#include <stack>

using namespace std;

int main() {
  ifstream fin("colaj.in");
  ofstream fout("colaj.out");

  int n, m, p;
  fin >> n >> m >> p;

  vector<pair<int, int>> v[p];

  for (int i = 0; i < n; i++) {
    int a, b, c, d;
    fin >> a >> b >> c >> d;
    v[b].push_back({a, c});
  }

  int ans = 0;
  set<int> covered;
  stack<pair<int, int>> active;
  for (int i = 0; i < p; i++) {
    sort(v[i].begin(), v[i].end());
    int j = 0;
    while (j < v[i].size()) {
      int a = v[i][j].first;
      int b = v[i][j].second;
      j++;
