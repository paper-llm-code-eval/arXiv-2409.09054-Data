#include <fstream>
#include <vector>
#include <queue>
#include <bitset>

using namespace std;

ifstream fin("panda.in");
ofstream fout("panda.out");

const int MAXN = 500;
const int MAXM = 500;
const int MAXS = 8;
const int INF = 1e9;

int n, m, T, p, k, S, L, C;
int dist[MAXN][MAXM];
int cod[MAXN][MAXM];
vector<pair<int, int>> food;
queue<pair<int, int>> q;
bitset<MAXS> k_bin;

int dx[] = {0, 0, 1, -1};
int dy[] = {1, -1, 0, 0};

inline bool valid(int x, int y) {
    return x >= 1 && x <= n && y >= 1 && y <= m;
}

inline bool check(int x) {
    bitset<MAXS> x_bin(x);
    return (x_bin ^ k_bin).count() == S;
}

void bfs() {
    while (!q.empty()) {
        int x = q.front().first;
        int y = q.front().second;
        q.pop();

        for (int i = 0; i < 4; i++) {
            int nx = x + dx[i];
            int ny = y + dy[i];

            if (valid(nx, ny) && dist[nx][ny] == INF && check(cod[nx][ny])) {
                dist[nx][ny] = dist[x][y] + 1;
                q.push({nx, ny});
            }
        }
    }
}

int main() {
    fin >> p;
    fin >> n >> m >> T;
    fin >> L >> C >> k >> S;
    k_bin = k;

    for (int i = 0; i < T; i++) {
        int x, y;
        fin >> x >> y;
        food.push_back({x, y});
    }

    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            fin >> cod[i][j];
        }
    }

    if (p == 1) {
        int ans = 0;
        for (int i = 1; i <= n; i++) {
            for (int j = 1; j <= m; j++) {
                if (i == L && j == C) continue;
                if (check(cod[i][j])) ans++;
            }
        }
        fout << ans << '\n';
    } else {
        for (int i = 1; i <= n; i++) {
            for (int j = 1; j <= m; j++) {
                dist[i][j] = INF;
            }
        }

        dist[L][C] = 0;
        q.push({L, C});
        bfs();

        int ans1 = INF;
        int ans2 = 0;
        for (auto& [x, y] : food) {
            if (dist[x][y] < ans1) {
                ans1 = dist[x][y];
                ans2 = 1;
            } else if (dist[x][y] == ans1) {
                ans2++;
            }
        }

        fout << ans1 << ' ' << ans2 << '\n';
    }

    return 0;
}
