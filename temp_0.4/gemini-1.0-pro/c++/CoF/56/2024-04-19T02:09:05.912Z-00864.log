#include <vector>
#include <queue>
#include <limits>

using namespace std;

ifstream in("zmeu.in");
ofstream out("zmeu.out");

const int INF = numeric_limits<int>::max();

struct Edge {
    int to, cost, cap;
    Edge(int to, int cost, int cap) : to(to), cost(cost), cap(cap) {}
};

vector<vector<Edge>> graph;
vector<int> dist;
vector<int> parent;

void addEdge(int from, int to, int cost, int cap) {
    graph[from].push_back(Edge(to, cost, cap));
    graph[to].push_back(Edge(from, -cost, 0));
}

bool bellmanFord(int source, int sink) {
    dist.assign(graph.size(), INF);
    parent.assign(graph.size(), -1);
    dist[source] = 0;

    for (int i = 0; i < graph.size() - 1; i++) {
        for (int u = 0; u < graph.size(); u++) {
            for (Edge& edge : graph[u]) {
                if (dist[u] + edge.cost < dist[edge.to] && edge.cap > 0) {
                    dist[edge.to] = dist[u] + edge.cost;
                    parent[edge.to] = u;
                }
            }
        }
    }

    for (int u = 0; u < graph.size(); u++) {
        for (Edge& edge : graph[u]) {
            if (dist[u] + edge.cost < dist[edge.to] && edge.cap > 0) {
                return false;
            }
        }
    }

    return true;
}

int main() {
    int n, p, k;
    in >> n >> p >> k;

    graph.resize(p + 2);

    for (int i = 1; i <= p; i++) {
        int d, c;
        in >> d >> c;
        addEdge(i, i + 1, d, n - c);
    }

    for (int i = 0; i < k; i++) {
        int p1, p2;
        in >> p1 >> p2;
        addEdge(p1 + 1, p2 + 1, 0, INF);
    }

    if (!bellmanFord(1, p + 1)) {
        out << -1 << endl;
        return 0;
