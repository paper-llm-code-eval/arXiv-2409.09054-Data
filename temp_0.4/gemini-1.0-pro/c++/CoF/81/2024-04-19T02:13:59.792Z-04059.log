#include <bits/stdc++.h>

using namespace std;

const int NMAX = 200005;
const int KMAX = 25;
const int MOD = 1e9 + 7;

int n, k, p;
int dp[NMAX][KMAX];
int dp2[NMAX][KMAX];
int v[NMAX];

int main() {
    cin >> p;
    cin >> n;
    for (int i = 1; i <= n; ++i) {
        cin >> k;
        for (int j = 1; j <= k; ++j) {
            cin >> v[j];
        }
        for (int mask = 0; mask < (1 << k); ++mask) {
            int cnt = 0;
            for (int j = 1; j <= k; ++j) {
                if (mask & (1 << (j - 1))) {
                    cnt += v[j];
                }
            }
            if (cnt == 0) {
                continue;
            }
            dp[i][mask] = dp[i - 1][mask];
            dp2[i][mask] = dp2[i - 1][mask];
            for (int j = 1; j <= k; ++j) {
                if (mask & (1 << (j - 1))) {
                    dp[i][mask] = max(dp[i][mask], dp[i - 1][mask ^ (1 << (j - 1))] + 1);
                    dp2[i][mask] = (dp2[i][mask] + dp2[i - 1][mask ^ (1 << (j - 1))]) % MOD;
                }
            }
        }
    }
    if (p == 1) {
        int ans = 0;
        for (int mask = 0; mask < (1 << k); ++mask) {
            ans = max(ans, dp[n][mask]);
        }
        cout << ans << "\n";
    } else {
        int ans = 0;
        for (int mask = 0; mask < (1 << k); ++mask) {
            if (dp[n][mask] == dp[n][0]) {
                ans = (ans + dp2[n][mask]) % MOD;
            }
        }
        cout << ans << "\n";
    }
    return 0;
}
