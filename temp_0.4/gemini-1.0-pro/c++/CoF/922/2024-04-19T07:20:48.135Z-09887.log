    for (int i = 0; i < n - 1; i++) {
      time += 3 * v[i].first;
      len += v[i].first;
      if (v[i].second > v[i + 1].second) {
        time += 5 * (v[i].second - v[i + 1].second);
      } else if (v[i].second < v[i + 1].second) {
        time += 2 * (v[i + 1].second - v[i].second);
        len = 0;
      }
      max_len = max(max_len, len);
    }
    time += 3 * v[n - 1].first;
    if (c == 1) {
      out << time << endl;
    } else {
      out << max_len << endl;
    }
  } else {
    in >> t;
    vector<pair<int, int>> v(n);
    for (int i = 0; i < n; i++) {
      in >> v[i].first >> v[i].second;
    }
    int time = 0, pos = 0;
    while (time < t) {
      time += 3 * v[pos].first;
      pos++;
      if (pos == n) {
        pos = 0;
      }
      if (v[pos].second > v[pos - 1].second) {
        time += 5 * (v[pos].second - v[pos - 1].second);
      } else if (v[pos].second < v[pos - 1].second) {
        time += 2 * (v[pos - 1].second - v[pos].second);
      }
    }
    out << pos + 1 << endl;
  }
  return 0;
}
