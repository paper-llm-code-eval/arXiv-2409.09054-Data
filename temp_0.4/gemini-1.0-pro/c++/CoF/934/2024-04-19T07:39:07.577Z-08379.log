    vector<int> puncte(N);
    for (int i = 0; i < N; i++) {
        in >> puncte[i];
    }

    vector<vector<int>> meciuri(N);
    for (int i = 0; i < D; i++) {
        int a, b;
        in >> a >> b;
        a--;
        b--;
        meciuri[a].push_back(b);
        meciuri[b].push_back(a);
    }

    if (T == 1) {
        vector<int> puncte_egalitate(N);
        for (int i = 0; i < N; i++) {
            puncte_egalitate[i] = puncte[i] + meciuri[i].size();
        }

        sort(puncte_egalitate.begin(), puncte_egalitate.end(), greater<int>());
        int max_puncte = puncte_egalitate[0];
        vector<int> echipe_loc_I;
        for (int i = 0; i < N; i++) {
            if (puncte_egalitate[i] == max_puncte) {
                echipe_loc_I.push_back(i + 1);
            }
        }

        for (int i = 0; i < echipe_loc_I.size(); i++) {
            out << echipe_loc_I[i] << " ";
        }
    } else if (T == 2) {
        unordered_map<int, int> puncte_maxime;
        for (int i = 0; i < N; i++) {
            puncte_maxime[i] = puncte[i] + 3 * meciuri[i].size();
        }

        int max_puncte = *max_element(puncte_maxime.begin(), puncte_maxime.end(),
                                       [](const pair<int, int>& a, const pair<int, int>& b) {
                                           return a.second < b.second;
                                       }).second;
        vector<int> echipe_dependente;
        for (auto& it : puncte_maxime) {
            if (it.second == max_puncte) {
                echipe_dependente.push_back(it.first + 1);
            }
        }

        if (echipe_dependente.empty()) {
            out << 0;
        } else {
            for (int i = 0; i < echipe_dependente.size(); i++) {
                out << echipe_dependente[i] << " ";
            }
        }
    }

    return 0;
}
