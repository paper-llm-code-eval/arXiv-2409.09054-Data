#include <fstream>

using namespace std;

ifstream fin("pif.in");
ofstream fout("pif.out");

const int MOD = 1234567;

int main() {
    int n, k, zv, zt;
    fin >> n >> k >> zv >> zt;
    if (zv == zt) {
        fout << (n / zv) % MOD << '\n';
    } else {
        int ans = 0;
        int t = (k + 1) / 2;
        for (int i = 0; i <= t; i++) {
            int j = t - i;
            int days = i * zv + j * zt;
            if (days <= n) {
                ans = max(ans, i + j);
            }
        }
        fout << ans % MOD << '\n';
    }
    return 0;
}
