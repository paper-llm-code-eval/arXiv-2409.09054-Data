int main() {
  ifstream in("pif.in");
  ofstream out("pif.out");

  int n, k, zv, zt;
  in >> n >> k >> zv >> zt;

  long long fb = 0;
  int t = 0;
  int num_young = 0;
  int num_old = 0;
  while (t <= n) {
    int num_new_young = min((k + 1) / 2 - num_young, (n - t) / zt);
    int num_new_old = min((k + 1) / 2 - num_old, (n - t) / zv);
    fb += num_new_young + num_new_old;
    num_young += num_new_young;
    num_old += num_new_old;
    t += zv;
    t += zt;
  }

  out << fb % MOD << '\n';

  return 0;
}
```

