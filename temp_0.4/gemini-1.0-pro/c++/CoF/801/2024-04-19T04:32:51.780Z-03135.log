            }
        }
        if (nr_nivel[i].size() > nr_max) {
            nr_max = nr_nivel[i].size();
            nivel_max = i;
        }
    }

    out << nr_afectate << '\n';
    out << nr_max << '\n';
    for (int i = 0; i <= intensitate; i++) {
        if (nr_nivel[i].size() == nr_max) {
            out << i << ' ';
        }
    }
    out << '\n';

    return 0;
}
```
