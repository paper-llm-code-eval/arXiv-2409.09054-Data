#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream in("cladiri.in");
ofstream out("cladiri.out");

const int MAXN = 100001;

int n, long_c, lat_c, intensitate;
int long_cl[MAXN], lat_cl[MAXN], grad[MAXN];
int nivel_max, nr_max;
vector<int> nr_nivel[MAXN];

int main() {
    in >> long_c >> lat_c >> intensitate;
    in >> n;
    for (int i = 1; i <= n; i++) {
