#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

struct Point {
    int x, y;
};

struct Building {
    Point coord;
    int strength;
};

int main() {
    ifstream in("cladiri.in");
    ofstream out("cladiri.out");

    Point center;
    int intensity;
    in >> center.x >> center.y >> intensity;

    vector<Building> buildings;
    int n;
    in >> n;
    buildings.resize(n);
    for (int i = 0; i < n; i++) {
        in >> buildings[i].coord.x >> buildings[i].coord.y >> buildings[i].strength;
    }

    // Count the number of affected buildings
    int affected = 0;
    for (const auto& building : buildings) {
        int distance = abs(building.coord.x - center.x) + abs(building.coord.y - center.y);
        if (building.strength <= intensity - distance) {
            affected++;
        }
    }

    // Find the maximum number of affected buildings on a level
    int max_level_affected = 0;
    vector<int> level_affected(intensity + 1, 0);
    for (const auto& building : buildings) {
        int distance = abs(building.coord.x - center.x) + abs(building.coord.y - center.y);
        if (building.strength <= intensity - distance) {
            level_affected[distance]++;
            max_level_affected = max(max_level_affected, level_affected[distance]);
        }
    }

    // Find the levels with the maximum number of affected buildings
    vector<int> levels;
    for (int i = 0; i <= intensity; i++) {
        if (level_affected[i] == max_level_affected) {
            levels.push_back(i);
        }
    }

    // Write the output
    out << affected << endl;
    out << max_level_affected << endl;
    for (const auto& level : levels) {
        out << level << " ";
    }
    out << endl;

    return 0;
}
```
