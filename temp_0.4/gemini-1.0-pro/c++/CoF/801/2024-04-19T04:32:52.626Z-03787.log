#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream fin("cladiri.in");
    ofstream fout("cladiri.out");

    int long_c, lat_c, intensitate;
    fin >> long_c >> lat_c >> intensitate;

    int n;
    fin >> n;

    vector<pair<int, int>> cladiri(n);
    vector<int> grad(n);
    for (int i = 0; i < n; i++) {
        int long_i, lat_i, grad_i;
        fin >> long_i >> lat_i >> grad_i;
        cladiri[i] = {long_i, lat_i};
        grad[i] = grad_i;
    }

    int N = 0, M = 0;
    vector<int> nivele;
    for (int i = 0; i < n; i++) {
        int dist = abs(long_c - cladiri[i].first) + abs(lat_c - cladiri[i].second);
        if (dist <= intensitate && grad[i] <= intensitate) {
            N++;
            nivele.push_back(dist);
            M = max(M, nivele.back());
        }
    }

    sort(nivele.begin(), nivele.end());
    nivele.erase(unique(nivele.begin(), nivele.end()), nivele.end());

    fout << N << '\n';
    fout << M << '\n';
    for (int nivel : nivele) {
        if (nivel == M) {
            fout << nivel << ' ';
        }
    }

    return 0;
}
```
