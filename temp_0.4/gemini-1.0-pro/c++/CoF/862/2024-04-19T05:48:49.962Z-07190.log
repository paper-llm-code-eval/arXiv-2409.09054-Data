#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("charlie.in");
    ofstream out("charlie.out");

    int p;
    in >> p;

    string s;
    in >> s;

    int n = s.size();

    if (p == 1) {
        int ans = 0;
        bool alt = false;
        for (int i = 0; i < n; i++) {
            if (alt) {
                if (s[i] > s[i - 1]) {
                    ans++;
                } else {
                    alt = false;
                }
            } else {
                if (s[i] < s[i - 1]) {
                    alt = true;
                    ans++;
                }
            }
        }
        out << ans << endl;
    } else {
        vector<int> f(26);
        for (int i = 0; i < 26; i++) {
            f[s[i] - 'a'] = i + 1;
        }

        vector<int> dp(n);
        dp[0] = 0;
        for (int i = 1; i < n; i++) {
            dp[i] = max(dp[i - 1], max(f[s[i - 1] - 'a'], f[s[i + 1] - 'a']));
        }

        int sum = 0;
        string t;
        for (int i = 0; i < n; i++) {
            if (dp[i] > dp[i - 1]) {
                sum += dp[i];
                t += s[i];
            }
        }

        out << t << endl;
        out << sum << endl;
    }

    return 0;
}
