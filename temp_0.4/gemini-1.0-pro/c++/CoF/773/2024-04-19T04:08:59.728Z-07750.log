#include <fstream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    ifstream fin("schi.in");
    ofstream fout("schi.out");
    int h, m, s, n;
    fin >> h >> m >> s >> n;
    vector<pair<int, int>> times(n);
    for (int i = 0; i < n; i++) {
        int min, sec;
        fin >> min >> sec;
        times[i] = {min, sec};
    }
    sort(times.begin(), times.end());
    int min1 = times[0].first, sec1 = times[0].second;
    int min2 = times[n - 1].first, sec2 = times[n - 1].second;
    int h1 = h + min1, m1 = m + sec1;
    int h2 = h + min2, m2 = m + sec2;
    if (m1 >= 60) {
        m1 -= 60;
        h1++;
    }
    if (m2 >= 60) {
        m2 -= 60;
        h2++;
    }
    fout << h1 << " " << m1 << " " << sec1 << "\n";
    fout << h2 << " " << m2 << " " << sec2 << "\n";
    return 0;
}
