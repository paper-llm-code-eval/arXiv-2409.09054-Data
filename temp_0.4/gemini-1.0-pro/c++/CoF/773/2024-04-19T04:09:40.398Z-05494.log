#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream fin("schi.in");
    ofstream fout("schi.out");

    int h, m, s, n;
    fin >> h >> m >> s >> n;

    vector<pair<int, int>> times;
    for (int i = 0; i < n; i++) {
        int x, y;
        fin >> x >> y;
        times.push_back(make_pair(x, y));
    }

    sort(times.begin(), times.end());

    int min_minutes = times[0].first + m;
    int min_seconds = times[0].second + s;
    if (min_seconds >= 60) {
        min_minutes++;
        min_seconds -= 60;
    }
    int min_hours = h;
    if (min_minutes >= 60) {
        min_hours++;
        min_minutes -= 60;
    }

    int max_minutes = times[n - 1].first + m;
    int max_seconds = times[n - 1].second + s;
    if (max_seconds >= 60) {
        max_minutes++;
        max_seconds -= 60;
    }
    int max_hours = h;
    if (max_minutes >= 60) {
        max_hours++;
        max_minutes -= 60;
    }

    fout << min_hours << " " << min_minutes << " " << min_seconds << endl;
    fout << max_hours << " " << max_minutes << " " << max_seconds << endl;

    return 0;
}
