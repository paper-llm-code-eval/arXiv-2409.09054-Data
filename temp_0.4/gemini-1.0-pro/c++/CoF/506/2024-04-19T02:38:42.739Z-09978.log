#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAXN = 1500;
const int MAXM = 1500;
const int MAX_FIB = 1000000000;

int n, m, c;
int a[MAXN][MAXM];
int fib[MAX_FIB];
int dp[MAXN][MAXM][2];
int sol;

int main() {
    ifstream in("fibosnek.in");
    ofstream out("fibosnek.out");

    in >> c >> n >> m;

    for (int i = 1; i < MAX_FIB; i++) {
        fib[i] = fib[i - 1] + fib[i - 2];
    }

    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            in >> a[i][j];
        }
    }

    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            dp[i][j][0] = dp[i][j][1] = 1;
            if (a[i][j] == 1) {
                dp[i][j][1] = 0;
            } else {
                int pos = lower_bound(fib, fib + MAX_FIB, a[i][j]) - fib;
                if (fib[pos] == a[i][j]) {
                    dp[i][j][1] = dp[i][j][0] + 1;
                }
            }
        }
    }

    for (int i = n; i >= 1; i--) {
        for (int j = m; j >= 1; j--) {
            if (a[i][j] == 1) {
                continue;
            }
            int pos = lower_bound(fib, fib + MAX_FIB, a[i][j]) - fib;
            if (fib[pos] == a[i][j]) {
                dp[i][j][1] = max(dp[i][j][1], dp[i + 1][j][1] + 1);
                dp[i][j][1] = max(dp[i][j][1], dp[i][j + 1][1] + 1);
            } else {
                dp[i][j][1] = max(dp[i][j][1], dp[i + 1][j][0] + 1);
                dp[i][j][1] = max(dp[i][j][1], dp[i][j + 1][0] + 1);
            }
        }
    }

    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            sol = max(sol, dp[i][j][1]);
        }
    }

    if (c == 1) {
        int ans = 0;
        for (int i = 1; i <= n; i++) {
            for (int j = 1; j <= m; j++) {
                ans += (a[i][j] == 1);
            }
        }
        out << ans << endl;
    } else {
        out << sol << endl;
    }

    return 0;
}
```
