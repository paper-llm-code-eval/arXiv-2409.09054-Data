#include <fstream>
#include <vector>
using namespace std;

const int MAXN = 1500;
const int MAXM = 1500;
const int MAXK = 45;
const int MOD = 1e9 + 7;
const int INF = 1e9;

int n, m, c;
int a[MAXN][MAXM];
int fib[MAXK];
bool viz[MAXN][MAXM];
int dp[MAXN][MAXM][MAXK];
int sum[MAXN][MAXM];

void read() {
    ifstream fin("fibosnek.in");
    fin >> c >> n >> m;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            fin >> a[i][j];
        }
    }
    fin.close();
}

void pre() {
    fib[1] = fib[2] = 1;
    for (int i = 3; i < MAXK; i++) {
        fib[i] = (fib[i - 1] + fib[i - 2]) % MOD;
    }
}

int get_fib(int x) {
    int l = 1, r = MAXK - 1;
    while (l <= r) {
        int mid = (l + r) / 2;
        if (fib[mid] <= x) {
            l = mid + 1;
        } else {
            r = mid - 1;
        }
    }
    if (fib[l] - x < x - fib[r]) {
        return fib[l];
    } else {
        return fib[r];
    }
}

void solve1() {
    int ans = 0;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            ans += (a[i][j] == 1 || a[i][j] == 2);
        }
    }
    ofstream fout("fibosnek.out");
    fout << ans << "\n";
    fout.close();
}

void solve2() {
    pre();
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            sum[i][j] = sum[i - 1][j] + sum[i][j - 1] - sum[i - 1][j - 1] + a[i][j];
        }
    }
    int ans = 0;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            dp[i][j][0] = a[i][j];
            for (int k = 1; k < MAXK; k++) {
                dp[i][j][k] = max(dp[i][j][k - 1], dp[i - 1][j][k] + a[i][j]);
                if (i > 1) {
                    dp[i][j][k] = max(dp[i][j][k], dp[i][j - 1][k] + a[i][j]);
                }
                if (a[i][j] != 1 && a[i][j] != 2) {
                    dp[i][j][k] = max(dp[i][j][k], dp[i - 1][j - 1][k - 1] + get_fib(a[i][j]));
                }
            }
            ans = max(ans, dp[i][j][MAXK - 1]);
        }
    }
    ofstream fout("fibosnek.out");
    fout << ans << "\n";
    fout.close();
}

int main() {
    read();
    if (c == 1) {
        solve1();
    } else {
        solve2();
    }
    return 0;
}
