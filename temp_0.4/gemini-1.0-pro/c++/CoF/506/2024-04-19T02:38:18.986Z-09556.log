                    dp[i][j][1] = max(dp[i][j][1], dp[i][j - 1][0] + 1);
                }
            }
        }
    }

    if (c == 1) {
        int ans = 0;
        for (int i = 1; i <= n; ++i) {
            for (int j = 1; j <= m; ++j) {
                if (a[i][j] == 1) {
                    continue;
                }
                int pos = lower_bound(fib, fib + fib_len, a[i][j]) - fib;
                if (pos == fib_len || fib[pos] != a[i][j]) {
                    --pos;
                }
                if (fib[pos] == a[i][j]) {
                    ++ans;
                }
            }
        }
        fout << ans << '\n';
