#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("virus.in");
    ofstream out("virus.out");

    int n;
    in >> n;

    vector<pair<int, int>> intervals(n);
    for (int i = 0; i < n; i++) {
        in >> intervals[i].first >> intervals[i].second;
    }

    sort(intervals.begin(), intervals.end());

    int max_length = 0;
    int max_gap = 0;
    int current_length = 0;
    int current_gap = 0;

    for (int i = 0; i < n; i++) {
        if (i > 0 && intervals[i].first > intervals[i - 1].second) {
            current_gap = intervals[i].first - intervals[i - 1].second;
            max_gap = max(max_gap, current_gap);
        } else {
            current_gap = 0;
        }

        current_length = intervals[i].second - intervals[i].first + 1;
        max_length = max(max_length, current_length);
    }

    out << max_length << " " << max_gap << endl;

    return 0;
}
```
