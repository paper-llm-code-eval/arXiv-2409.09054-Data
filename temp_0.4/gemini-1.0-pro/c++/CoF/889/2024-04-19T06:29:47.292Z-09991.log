#define ll long long

using namespace std;

ll powmod(ll b, ll e) {
  ll ans = 1;
  while (e) {
    if (e & 1) ans = ans * b % MOD;
    b = b * b % MOD;
    e >>= 1;
  }
  return ans;
}

