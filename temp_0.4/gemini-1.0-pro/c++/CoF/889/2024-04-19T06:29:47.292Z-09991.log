    vector<ll> dp(n + 1);
    dp[0] = 1;
    for (int i = 1; i <= n; ++i) {
      for (int j = 1; j <= min(i, x); ++j) {
        dp[i] = (dp[i] + dp[i - j]) % MOD;
      }
    }
    out << dp[n] << '\n';
  }

  return 0;
}
