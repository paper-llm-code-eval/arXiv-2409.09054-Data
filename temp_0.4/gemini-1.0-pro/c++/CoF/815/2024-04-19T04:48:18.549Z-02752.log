#include <bits/stdc++.h>
#define endl '\n'
using namespace std;

const int NMAX = 1005;
const int INF = 0x3f3f3f3f;

int n, k;
int target[2], sources[2][2], robots[2][2], obstacles[150005][2];
vector<pair<int, int>> walls[NMAX];
vector<int> graph[NMAX * NMAX];
int dist[NMAX * NMAX];
int dx[] = {-1, 0, 1, 0};
int dy[] = {0, 1, 0, -1};

int get_node(int x, int y) {
  return (x - 1) * n + y;
}

void add_edge(int u, int v) {
  graph[u].push_back(v);
  graph[v].push_back(u);
}

void add_obstacle(int x, int y) {
  if (x == 1 || x == n)
    walls[x].push_back({y, y});
  else if (y == 1 || y == n)
    walls[n + y].push_back({x, x});
  else {
    walls[x].push_back({y, y});
    walls[n + y].push_back({x, x});
  }
}

int bfs(int start) {
  for (int i = 1; i <= n * n; i++)
    dist[i] = INF;
  queue<int> q;
  q.push(start);
  dist[start] = 0;
  while (!q.empty()) {
    int node = q.front();
    q.pop();
    for (int i = 0; i < graph[node].size(); i++) {
      int next = graph[node][i];
      if (dist[next] == INF) {
        dist[next] = dist[node] + 1;
        q.push(next);
      }
    }
  }
  int ans = 0;
  for (int i = 1; i <= n * n; i++)
    ans = max(ans, dist[i]);
  return ans;
}

int main() {
  ifstream fin("ai.in");
  ofstream fout("ai.out");

  fin >> n;
  fin >> target[0] >> target[1] >> sources[0][0] >> sources[0][1] >> sources[1][0] >>
    sources[1][1] >> robots[0][0] >> robots[0][1] >> robots[1][0] >> robots[1][1];
  fin >> k;
  for (int i = 1; i <= k; i++) {
    fin >> obstacles[i][0] >> obstacles[i][1];
    add_obstacle(obstacles[i][0], obstacles[i][1]);
  }
  fin.close();

  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= n; j++) {
      int node = get_node(i, j);
      if (i != 1 && i != n)
        add_edge(node, get_node(i - 1, j));
      if (j != 1 && j != n)
        add_edge(node, get_node(i, j - 1));
      if (i != 1 && j != 1)
        add_edge(node, get_node(i - 1, j - 1));
      if (i != 1 && j != n)
        add_edge(node, get_node(i - 1, j + 1));
    }
  }

  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= n; j++) {
      int node = get_node(i, j);
      for (int k = 0; k < walls[i].size(); k++) {
        int x1 = walls[i][k].first;
        int x2 = walls[i][k].second;
        if (x1 <= j && j <= x2)
          graph[node].erase(remove(graph[node].begin(), graph[node].end(),
                                   get_node(i, x1)),
                            graph[node].end());
      }
      for (int k = 0; k < walls[n + j].size(); k++) {
        int y1 = walls[n + j][k].first;
        int y2 = walls[n + j][k].second;
        if (y1 <= i && i <= y2)
          graph[node].erase(remove(graph[node].begin(), graph[node].end(),
                                   get_node(y1, j)),
                            graph[node].end());
      }
    }
  }

  int max_wall = 0;
  for (int i = 1; i <= n; i++)
    max_wall = max(max_wall, (int)walls[i].size());
  for (int i = n + 1; i <= n + n; i++)
    max_wall = max(max_wall, (int)walls[i].size());

  fout << max_wall << endl;

  int min_time = INF;
  for (int i = 0; i < 2; i++) {
    for (int j = 0; j < 2; j++) {
      if (i == j)
        continue;
      int start = get_node(robots[i][0], robots[i][1]);
      int end = get_node(sources[j][0], sources[j][1]);
      min_time = min(min_time, bfs(start) + bfs(end));
    }
  }
  fout << min_time << endl;

  fout.close();
  return 0;
}
