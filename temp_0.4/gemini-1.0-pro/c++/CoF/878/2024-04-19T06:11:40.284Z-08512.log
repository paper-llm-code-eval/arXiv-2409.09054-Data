          dp[i] = dp[j] + 1;
          prev[i] = j;
        }
      }
    }
    int max_len = 0;
    int max_idx = -1;
    for (int i = 0; i < n; i++) {
      if (dp[i] > max_len) {
        max_len = dp[i];
        max_idx = i;
      }
    }
    vector<int> ans;
    while (max_idx != -1) {
      ans.push_back(p[max_idx]);
      max_idx = prev[max_idx];
    }
    reverse(ans.begin(), ans.end());
    for (int i = 0; i < n; i++) { out << ans[i] << " "; }
    out << endl;
  }

  return 0;
}
