        lasere.push_back({x, y});
        a[x][y] = 0;
        if (d == 1) {
            for (int j = x - 1; j >= 1; j--) {
                a[j][y]--;
            }
        } else if (d == 2) {
            for (int j = y + 1; j <= n; j++) {
                a[x][j]--;
            }
        } else if (d == 3) {
            for (int j = x + 1; j <= n; j++) {
                a[j][y]--;
            }
        } else if (d == 4) {
            for (int j = y - 1; j >= 1; j--) {
                a[x][j]--;
            }
        }
    }
    int ans = 0;
    for (int i = 1; i <= n; i++) {
