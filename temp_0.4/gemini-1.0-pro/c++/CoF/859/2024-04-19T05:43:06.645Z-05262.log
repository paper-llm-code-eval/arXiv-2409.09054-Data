        for (int i = 0; i < 8; i++) {
            int nx = x + dx[i], ny = y + dy[i];
            if (nx < 1 || nx > n || ny < 1 || ny > n || viz[nx][ny]) continue;
            if (a[nx][ny] < a[x][y]) {
                viz[nx][ny] = 1;
                q.push({nx, ny});
            }
        }
    }
}

int main() {
    fin >> c >> n >> m;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n; j++) {
            fin >> a[i][j];
        }
    }
    for (int i = 0; i < m; i++) {
        int x, y, d;
        fin >> x >> y >> d;
        lasere.push_back({x, y});
        a[x][y] = 0;
        if (d == 1) {
            for (int j = x - 1; j >= 1; j--) {
                a[j][y]--;
            }
        } else if (d == 2) {
            for (int j = y + 1; j <= n; j++) {
                a[x][j]--;
            }
        } else if (d == 3) {
            for (int j = x + 1; j <= n; j++) {
                a[j][y]--;
            }
        } else if (d == 4) {
            for (int j = y - 1; j >= 1; j--) {
                a[x][j]--;
            }
        }
    }
    int ans = 0;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n; j++) {
            if (!viz[i][j]) {
                bfs(i, j);
                ans++;
            }
        }
    }
    fout << ans << "\n";
    return 0;
}
