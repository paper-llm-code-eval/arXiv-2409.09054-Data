#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAX_K = 9;
const int MAX_N = 900000000;

int k, x, a, b;
vector<int> sir;

int main() {
    ifstream in("sir.in");
    in >> k >> x >> a >> b;
    in.close();

    // Generam sirul pana la 9 cifre
    int last = 1;
    while (last < MAX_N) {
        sir.push_back(last);
        int aux = last;
        last = 0;
        while (aux > 0) {
            last = last * 10 + aux % 10;
            aux /= 10;
        }
    }

    // Cerinta a)
    int suma = 0;
    for (int i = 0; i < sir.size(); i++) {
        if (i < (1 << k)) {
            suma += sir[i];
        }
    }
    ofstream out("sir.out");
    out << suma % 10 << endl;

    // Cerinta b)
    int pos = -1;
    for (int i = 0; i < sir.size(); i++) {
        if (sir[i] == x) {
            pos = i;
            break;
        }
    }
    out << sir[pos + 1] << endl;

    // Cerinta c)
    int cnt = 0;
    for (int i = 0; i < sir.size(); i++) {
        int aux = sir[i];
        bool ok = true;
        while (aux > 0) {
            if (aux % 10 == a && aux % 10 != b) {
                ok = false;
            }
            aux /= 10;
        }
        if (ok) {
            cnt++;
        }
    }
    out << cnt << endl;

    out.close();
    return 0;
}
