#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("tcif.in");
    ofstream out("tcif.out");

    int A, B, C, c1, c2, c3;
    in >> A >> B >> C >> c1 >> c2 >> c3;
    string N;
    in >> N;

    // Count the occurrences of each digit in N
    vector<int> freq(10, 0);
    for (char digit : N) {
        freq[digit - '0']++;
    }

    // Adjust the frequencies to match the desired number of occurrences
    freq[c1] = A;
    freq[c2] = B;
    freq[c3] = C;

    // Find the first digit that has a non-zero frequency
    int first_digit = 0;
    while (freq[first_digit] == 0) {
        first_digit++;
    }

    // Construct the new number
    string new_number;
    for (int i = 0; i < 10; i++) {
        for (int j = 0; j < freq[i]; j++) {
            new_number += to_string(i);
        }
    }

    // If the new number is less than or equal to N, increment it by 1
    if (new_number <= N) {
        int carry = 1;
        for (int i = new_number.size() - 1; i >= 0; i--) {
            int digit = new_number[i] - '0';
            digit += carry;
            carry = digit / 10;
            digit %= 10;
            new_number[i] = digit + '0';
        }
        if (carry > 0) {
            new_number = '1' + new_number;
        }
    }

    // Output the new number
    out << new_number << endl;

    return 0;
}
