#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream fin("sss.in");
  ofstream fout("sss.out");

  int c, n, k, l, i, j, sum, max_sum;
  fin >> c >> n;
  vector<int> v(n);
  for (i = 0; i < n; i++) {
    fin >> v[i];
  }
  if (c == 1) {
    k = v[0] % 10;
    sum = 0;
    for (i = n - k; i < n; i++) {
      sum += v[i];
    }
    fout << sum << endl;
  } else {
    l = n;
    max_sum = 0;
    while (l > 0) {
      sum = 0;
      for (i = n - l, j = 0; i < n; i++, j++) {
        sum += v[i];
      }
      max_sum = max(max_sum, sum);
      l--;
    }
    fout << max_sum << endl;
  }
  return 0;
}
```
