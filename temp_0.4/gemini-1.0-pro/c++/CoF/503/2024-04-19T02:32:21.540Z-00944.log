            } else if (s[i][j] == 'n') {
                pos[2] = j;
            } else if (s[i][j] == 'g') {
                pos[3] = j;
            } else if (s[i][j] == 'o') {
                pos[4] = j;
            }
            if (pos[0] != -1 && pos[1] != -1 && pos[2] != -1 && pos[3] != -1 && pos[4] != -1) {
                ans = j;
                break;
