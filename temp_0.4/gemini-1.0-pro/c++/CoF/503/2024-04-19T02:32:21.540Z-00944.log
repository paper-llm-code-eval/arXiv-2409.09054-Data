const int MAXN = 100005;
const int MAXL = 5;

int n;
char s[MAXN][MAXL + 1];

int main() {
    ifstream fin("bingo.in");
    ofstream fout("bingo.out");

    fin >> n;
    for (int i = 1; i <= n; i++) {
        fin >> s[i];
    }

    for (int i = 1; i <= n; i++) {
        int ans = 0;
