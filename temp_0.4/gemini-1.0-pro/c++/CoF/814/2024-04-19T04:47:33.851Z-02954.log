#include <fstream>
#include <iostream>
#include <vector>

using namespace std;

ifstream in("comp.in");
ofstream out("comp.out");

const int MAXN = 1000;
const int MAXS = 250;

int n, cntLess;
char s[MAXN][MAXS + 1];

int main() {
    in >> n;
    for (int i = 0; i < n; i++) {
        in >> s[i];
    }
    in.close();

    for (int i = 0; i < n; i++) {
        int len = strlen(s[i]);
        int pos = 0;
        int nr1 = 0, nr2 = 0;
        bool ok = false;
        while (pos < len) {
            int nr = 0;
            while (pos < len && s[i][pos] >= '0' && s[i][pos] <= '9') {
                nr = nr * 10 + s[i][pos] - '0';
                pos++;
            }
            if (s[i][pos] == 'm')
                nr *= 1000;
            else if (s[i][pos] == 's')
                nr *= 100;
            else if (s[i][pos] == 'z')
                nr *= 10;
            if (s[i][pos] == '+' && !ok) {
                nr1 += nr;
                ok = true;
            } else {
                nr2 += nr;
            }
            pos++;
        }
        if (nr1 < nr2) {
            cntLess++;
            out << "1\n";
        } else {
            out << "0\n";
        }
    }
    out << cntLess << '\n';
    out.close();
    return 0;
}
