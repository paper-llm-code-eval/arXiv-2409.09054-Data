#include <fstream>
#include <iostream>
#include <vector>
#include <cmath>
#include <algorithm>

using namespace std;

int main() {
  ifstream in("expresie.in");
  ofstream out("expresie.out");

  int m, n;
  in >> m >> n;
  vector<int> x(n);
  for (int i = 0; i < n; i++) {
    in >> x[i];
  }
  in.close();

  long long prod = 1;
  for (int i = 0; i < n; i++) {
    prod *= x[i];
  }
  double result = pow(prod, 1.0 / m);
  if (result != floor(result)) {
    out << 0 << endl;
    return 0;
  }

  out << 1 << endl;
  int num = result;
  vector<pair<int, int>> factors;
  for (int i = 2; i * i <= num; i++) {
    if (num % i == 0) {
      int cnt = 0;
      while (num % i == 0) {
        num /= i;
        cnt++;
      }
      factors.push_back({i, cnt});
    }
  }
  if (num > 1) {
    factors.push_back({num, 1});
  }
  sort(factors.begin(), factors.end());
  for (auto &factor : factors) {
    out << factor.first << " " << factor.second << endl;
  }

  return 0;
}
