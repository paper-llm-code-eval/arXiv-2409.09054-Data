#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAXN = 1e6 + 1;
const int MAXQ = 2e5 + 1;

int n, q, c;
vector<int> v;
int prim[MAXN];
vector<int> prim_pos[MAXN];
long long sum_cost;

void preproc() {
  for (int i = 2; i < MAXN; ++i) {
    if (!prim[i]) {
      prim[i] = i;
      for (int j = i * i; j < MAXN; j += i) {
        prim[j] = i;
      }
    }
  }

  for (int i = 1; i < MAXN; ++i) {
    prim_pos[prim[i]].push_back(i);
  }
}

int get_cost(int x) {
  int p = prim[x];
  int l = lower_bound(prim_pos[p].begin(), prim_pos[p].end(), x) - prim_pos[p].begin();
  if (l == prim_pos[p].size()) {
    return p - x;
  } else if (l == 0) {
    return x - p;
  } else {
    return min(x - prim_pos[p][l - 1], prim_pos[p][l] - x);
  }
}

int main() {
  ifstream fin("primprim.in");
  ofstream fout("primprim.out");

  fin >> c;
  fin >> n;
  v.resize(n + 1);
  for (int i = 1; i <= n; ++i) {
    fin >> v[i];
    sum_cost += get_cost(v[i]);
  }

  if (c == 1) {
    fout << sum_cost << '\n';
    return 0;
  }

  fin >> q;
  for (int i = 0; i < q; ++i) {
    int pos, val, p;
    fin >> pos >> val >> p;
    sum_cost -= get_cost(v[pos]);
    v[pos] = val;
    sum_cost += get_cost(v[pos]);
    vector<int> costs;
    for (int j = 0; j < p; ++j) {
      int pos;
      fin >> pos;
      costs.push_back(get_cost(v[pos]));
    }
    sort(costs.begin(), costs.end());
    long long sum = 0;
    for (int j = 0; j < p; ++j) {
      sum += costs[j];
    }
    fout << sum << '\n';
  }

  return 0;
}
