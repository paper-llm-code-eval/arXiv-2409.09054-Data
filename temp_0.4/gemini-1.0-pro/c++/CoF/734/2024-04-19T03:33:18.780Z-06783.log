  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      dist[i][j] = 1e9;
    }
  }
  bfs(rx, ry);
  bfs(jx, jy);
  int min_dist = 1e9;
  int x, y;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      if (dist[i][j] != 1e9 && dist[i][j] < min_dist) {
        min_dist = dist[i][j];
        x = i;
        y = j;
      }
    }
  }
  if (rx == jx && ry == jy) {
    min_dist = 0;
  }
  fout << min_dist << " " << x << " " << y << "\n";
  return 0;
}
