#include <fstream>
#include <cstring>

using namespace std;

const int dx[] = {0, 1, 1, 1, 0, -1, -1, -1};
const int dy[] = {1, 1, 0, -1, -1, -1, 0, 1};

int main() {
    ifstream fin("vecini.in");
    ofstream fout("vecini.out");
    int m, n, k, a[1005][1005], ans = 0, cnt = 0;
    fin >> m >> n >> k;
    memset(a, 0, sizeof(a));
    while (k--) {
        int x, y;
        fin >> x >> y;
        a[x][y] = 1;
    }
    for (int i = 1; i <= m; i++) {
        for (int j = 1; j <= n; j++) {
            int sum = 0;
            if (a[i][j] == 1) {
                for (int dir = 0; dir < 8; dir++) {
                    int x = i + dx[dir];
                    int y = j + dy[dir];
                    if (x >= 1 && x <= m && y >= 1 && y <= n && a[x][y] == 1) {
                        sum++;
                    }
                }
                if (sum > ans) {
                    ans = sum;
                    cnt = 1;
                } else if (sum == ans) {
                    cnt++;
                }
            }
        }
    }
    fout << ans << " " << cnt << '\n';
    return 0;
}
