#include <fstream>
#include <vector>

using namespace std;

ifstream fin("nr.in");
ofstream fout("nr.out");

const int MAXN = 2e9;
const int MOD = 1e9 + 7;

vector<int> v;
vector<bool> is_prime;

void sieve(int n) {
    is_prime.resize(n + 1, true);
    is_prime[0] = is_prime[1] = false;
    for (int i = 2; i * i <= n; ++i) {
        if (is_prime[i]) {
            for (int j = i * i; j <= n; j += i) {
                is_prime[j] = false;
            }
        }
    }
}

int main() {
    int n, k, p;
    fin >> n >> k >> p;
    sieve(MAXN);

    v.push_back(1);
    v.push_back(2);
    v.push_back(3);
    int i = 3;
    while (v.size() < p) {
        ++i;
        int last = v.back();
        if (is_prime[last]) {
            v.push_back(last + 1);
        } else {
            v.push_back(last - 1);
        }
    }

    int sum = 0;
    int cnt = 0;
    for (int i = 0; i < n; ++i) {
        if (is_prime[v[i]]) {
            sum += v[i];
        }
        if (v[i] % 10 == k) {
            ++cnt;
        }
    }

    fout << sum << '\n';
    fout << cnt << '\n';
    fout << v[p - 1] << '\n';

    return 0;
}
