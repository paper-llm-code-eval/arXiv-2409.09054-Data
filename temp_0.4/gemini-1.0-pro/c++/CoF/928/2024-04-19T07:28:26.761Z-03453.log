      int n = 0;
      while (isdigit(s[i])) {
        n = n * 10 + s[i] - '0';
        i++;
      }
      st.push({n, ""});
      i--;
    } else if (s[i] == '(') {
      st.top().second += s[i];
    } else if (s[i] == ')') {
      string tmp = st.top().second;
      st.pop();
      st.top().second += string(st.top().first, tmp);
      cnt++;
    } else if (s[i] == '[') {
      st.push({0, ""});
