#include <fstream>
#include <iostream>
#include <string>
#include <stack>
#include <vector>

using namespace std;

int main() {
  ifstream fin("arh.in");
  ofstream fout("arh.out");
  string s, t;
  fin >> s;
  stack<pair<int, string>> st;
  int cnt = 0;
  for (int i = 0; i < s.size(); i++) {
    if (isdigit(s[i])) {
      int n = 0;
      while (isdigit(s[i])) {
        n = n * 10 + s[i] - '0';
        i++;
      }
      st.push({n, ""});
      i--;
    } else if (s[i] == '(') {
      st.top().second += s[i];
    } else if (s[i] == ')') {
      string tmp = st.top().second;
      st.pop();
      st.top().second += string(st.top().first, tmp);
      cnt++;
    } else if (s[i] == '[') {
      st.push({0, ""});
      st.top().second += s[i];
    } else if (s[i] == '*') {
      string tmp = st.top().second;
      st.pop();
      st.top().second += tmp + string(tmp.rbegin(), tmp.rend());
      cnt++;
    } else if (s[i] == ']') {
      string tmp = st.top().second;
      st.pop();
      st.top().second += tmp + string(tmp.rbegin() + 1, tmp.rend());
      cnt++;
    } else {
      st.top().second += s[i];
    }
  }
  fout << cnt << endl;
  fout << st.top().second << endl;
  return 0;
}
