#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream fin("patrat.in");
  ofstream fout("patrat.out");
  int n, m, a[201][201], b[201][201], c[201][201], mx = 0;
  fin >> n >> m;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      fin >> a[i][j];
      if (a[i][j] > a[i - 1][j] && a[i][j] > a[i][j - 1]) {
        b[i][j] = b[i - 1][j] + 1;
      }
      if (a[i][j] > a[i][j + 1] && a[i][j] > a[i + 1][j]) {
        c[i][j] = c[i][j + 1] + 1;
      }
      mx = max(mx, max(b[i][j], c[i][j]));
    }
  }
  int nr_stele = 0, nr_constelatii = 0;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      if (b[i][j] == mx && c[i][j] == mx) {
        nr_stele++;
        if (mx % 2 == 0) {
          nr_constelatii++;
        }
      }
    }
  }
  fout << nr_stele << '\n' << nr_constelatii << '\n' << mx << '\n';
  return 0;
}
