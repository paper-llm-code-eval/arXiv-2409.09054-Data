#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream in("pluton.in");
ofstream out("pluton.out");

const int NMAX = 4005;

int n, nrPlut, nrMax, nrPlutMax, fr[NMAX];

vector<int> pluton[NMAX], sol;

int main() {
    in >> n;
    for (int i = 1; i <= n; i++) {
        int x;
        in >> x;
        fr[x]++;
        if (fr[x] == 1) {
            nrPlut++;
        }
    }
    for (int i = 0; i < NMAX; i++) {
        if (fr[i] > 0) {
            pluton[fr[i]].push_back(i);
        }
    }
    nrMax = *max_element(fr, fr + NMAX);
    nrPlutMax = pluton[nrMax].size();
    out << nrPlut << '\n' << nrMax << '\n' << nrPlutMax << '\n';
    for (int i = 0; i < nrPlutMax; i++) {
        sol.push_back(pluton[nrMax][i]);
    }
    sort(sol.begin(), sol.end());
    for (int i = 0; i < nrPlutMax; i++) {
        out << sol[i] << ' ';
    }
    return 0;
}
```
