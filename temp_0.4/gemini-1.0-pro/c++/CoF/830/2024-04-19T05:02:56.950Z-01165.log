#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAXN = 1000;
const int MAXK = 1000;

int n, b, k;
int s[MAXK], p[MAXK];
int sus[MAXN], jos[MAXN];

int main() {
    ifstream in("clepsidru.in");
    ofstream out("clepsidru.out");

    in >> n >> b;
    in >> k;
    for (int i = 0; i < k; i++) {
        in >> s[i] >> p[i];
    }

    // Primul experiment
    out << n * b << '\n';

    // Al doilea experiment
    for (int i = 0; i < n; i++) {
        sus[i] = 0;
        jos[i] = 0;
    }
    sus[0] = b;
    int poz = 0;
    for (int i = 0; i < k; i++) {
        int secunde = s[i];
        int pozitie = p[i] - 1;
        if (pozitie != poz) {
            for (int j = 0; j < n; j++) {
                swap(sus[j], jos[j]);
            }
            poz = pozitie;
        }
        for (int j = 0; j < secunde; j++) {
            if (sus[poz] > 0) {
                sus[poz]--;
                jos[poz]++;
            } else if (poz > 0) {
                poz--;
                sus[poz]++;
                jos[poz]--;
            }
        }
    }
    for (int i = 0; i < n; i++) {
        out << sus[i] << ' ' << jos[i] << '\n';
    }

    return 0;
}
