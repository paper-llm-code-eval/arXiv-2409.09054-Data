      if (a[i][j] == 0) {
        q.push({i, j});
      }
    }
  }
  while (!q.empty()) {
    int x = q.front().first;
    int y = q.front().second;
    q.pop();
    for (int i = 0; i < 4; i++) {
      int nx = x + dx[i];
      int ny = y + dy[i];
      if (nx >= 0 && nx < n && ny >= 0 && ny < m && a[nx][ny] != 0 && a[nx][ny] != a[x][y]) {
        lg = 1;
        out << nr << " " << ng << " " << nb << " " << lg << endl;
        return 0;
      } else if (nx >= 0 && nx < n && ny >= 0 && ny < m && a[nx][ny] == 0) {
        a[nx][ny] = a[x][y];
        q.push({nx, ny});
      }
    }
  }
  return 0;
}
