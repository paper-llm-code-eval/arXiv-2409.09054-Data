  ifstream in("insule.in");
  ofstream out("insule.out");
  in >> n >> m;
  a.resize(n, vector<int>(m));
  viz.resize(n, vector<int>(m));
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      in >> a[i][j];
    }
  }
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      if (!viz[i][j]) {
        dfs(i, j);
      }
