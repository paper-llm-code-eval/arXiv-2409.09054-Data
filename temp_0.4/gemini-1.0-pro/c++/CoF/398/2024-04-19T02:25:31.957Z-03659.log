#include <fstream>
#include <vector>
#include <queue>

using namespace std;

const int dx[] = {0, 0, -1, 1};
const int dy[] = {-1, 1, 0, 0};

int n, m;
vector<vector<int>> mat;
vector<vector<int>> viz;
vector<vector<int>> id;

int nr, ng, nb;
int lg = 1e9;

void bfs(int x, int y, int id) {
    queue<pair<int, int>> q;
    q.push({x, y});
    viz[x][y] = 1;
    while (!q.empty()) {
        auto [cx, cy] = q.front();
        q.pop();
        for (int i = 0; i < 4; i++) {
            int nx = cx + dx[i];
            int ny = cy + dy[i];
            if (nx < 0 || nx >= n || ny < 0 || ny >= m || viz[nx][ny] || mat[nx][ny] != mat[x][y])
                continue;
            viz[nx][ny] = 1;
            q.push({nx, ny});
        }
    }
    if (mat[x][y] == 1)
        nr++;
    else if (mat[x][y] == 2)
        ng++;
    else if (mat[x][y] == 3)
        nb++;
}

void find_bridge() {
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (!viz[i][j] && mat[i][j] == 0) {
                int cnt = 0;
                for (int k = 0; k < 4; k++) {
                    int nx = i + dx[k];
                    int ny = j + dy[k];
                    if (nx < 0 || nx >= n || ny < 0 || ny >= m)
                        continue;
                    if (mat[nx][ny] == 1 || mat[nx][ny] == 2)
                        cnt++;
                }
                if (cnt == 1) {
                    queue<pair<int, int>> q;
                    q.push({i, j});
                    viz[i][j] = 1;
                    int l = 0;
                    while (!q.empty()) {
                        auto [cx, cy] = q.front();
                        q.pop();
                        l++;
                        for (int k = 0; k < 4; k++) {
                            int nx = cx + dx[k];
                            int ny = cy + dy[k];
                            if (nx < 0 || nx >= n || ny < 0 || ny >= m || viz[nx][ny] || mat[nx][ny] != 0)
                                continue;
                            viz[nx][ny] = 1;
                            q.push({nx, ny});
                        }
                    }
                    lg = min(lg, l);
                }
            }
        }
    }
}

int main() {
    ifstream fin("insule.in");
    ofstream fout("insule.out");

    fin >> n >> m;
    mat.resize(n, vector<int>(m));
    viz.resize(n, vector<int>(m));
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            fin >> mat[i][j];
        }
    }
    fin.close();

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (!viz[i][j] && mat[i][j] != 0) {
                bfs(i, j, id[i][j]);
            }
        }
    }

    find_bridge();

    fout << nr << ' ' << ng << ' ' << nb << ' ' << lg << '\n';
    fout.close();

    return 0;
}
