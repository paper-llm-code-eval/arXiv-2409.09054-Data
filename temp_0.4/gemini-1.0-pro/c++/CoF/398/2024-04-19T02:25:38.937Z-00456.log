#include <fstream>
#include <vector>

using namespace std;

ifstream fin("insule.in");
ofstream fout("insule.out");

int n, m, a[101][101], nr[4], viz[101][101], lmin = 1000000000;

void dfs(int x, int y, int k) {
  if (x < 1 || x > n || y < 1 || y > m || viz[x][y] || a[x][y] != k) return;
  viz[x][y] = 1;
  nr[k]++;
  dfs(x + 1, y, k);
  dfs(x - 1, y, k);
  dfs(x, y + 1, k);
  dfs(x, y - 1, k);
}

int main() {
  fin >> n >> m;
  for (int i = 1; i <= n; i++)
    for (int j = 1; j <= m; j++) {
      fin >> a[i][j];
      if (a[i][j] == 0) a[i][j] = 4;
    }
  for (int i = 1; i <= n; i++)
    for (int j = 1; j <= m; j++)
      if (!viz[i][j] && a[i][j] != 4) {
        nr[a[i][j]]++;
        dfs(i, j, a[i][j]);
      }
  for (int i = 1; i <= n; i++)
    for (int j = 1; j <= m; j++)
      if (a[i][j] == 4) {
        int l = 0;
        if (a[i][j + 1] == 4) l++;
        if (a[i][j - 1] == 4) l++;
        if (a[i + 1][j] == 4) l++;
        if (a[i - 1][j] == 4) l++;
        if (a[i][j + 1] == 1 || a[i][j - 1] == 2 || a[i + 1][j] == 3 ||
            a[i - 1][j] == 3)
          lmin = min(lmin, l);
      }
  fout << nr[1] << " " << nr[2] << " " << nr[3] << " " << lmin << "\n";
  return 0;
}
