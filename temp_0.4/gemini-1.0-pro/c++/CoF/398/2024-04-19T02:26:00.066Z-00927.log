vector<vector<int>> viz;

void dfs(int x, int y, int& cnt, int& id) {
    viz[x][y] = id;
    cnt++;
    for (int i = 0; i < 4; i++) {
        int nx = x + dx[i], ny = y + dy[i];
        if (nx < 0 || nx >= n || ny < 0 || ny >= m || viz[nx][ny] != -1 || a[nx][ny] != a[x][y]) continue;
        dfs(nx, ny, cnt, id);
    }
}

int main() {
    ifstream fin("insule.in");
    ofstream fout("insule.out");
    fin >> n >> m;
    a.resize(n, vector<int>(m));
    viz.resize(n, vector<int>(m, -1));
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            fin >> a[i][j];
        }
    }
    int nr = 0, ng = 0, nb = 0;
    int id = 0;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (viz[i][j] == -1) {
                int cnt = 0;
                dfs(i, j, cnt, id);
                if (a[i][j] == 1) nr += cnt;
                else if (a[i][j] == 2) ng += cnt;
                else if (a[i][j] == 3) nb += cnt;
                id++;
            }
        }
    }
    int lg = 1e9;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (a[i][j] == 0) {
                queue<pair<int, int>> q;
                q.push({i, j});
                vector<vector<int>> dist(n, vector<int>(m, 1e9));
                dist[i][j] = 0;
                while (!q.empty()) {
                    int x = q.front().first, y = q.front().second;
                    q.pop();
                    for (int k = 0; k < 4; k++) {
                        int nx = x + dx[k], ny = y + dy[k];
                        if (nx < 0 || nx >= n || ny < 0 || ny >= m || a[nx][ny] != 0 || dist[nx][ny] <= dist[x][y] + 1) continue;
                        dist[nx][ny] = dist[x][y] + 1;
                        q.push({nx, ny});
                    }
                }
                for (int k = 0; k < n; k++) {
                    for (int l = 0; l < m; l++) {
                        if (a[k][l] == 1 || a[k][l] == 2) {
                            lg = min(lg, dist[k][l]);
                        }
                    }
                }
            }
        }
    }
    fout << nr << " " << ng << " " << nb << " " << lg << "\n";
    return 0;
}
