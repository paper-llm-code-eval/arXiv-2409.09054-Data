          int yy = j + dc[k];
          if (xx >= 0 && xx < n && yy >= 0 && yy < m && a[xx][yy] == 1) {
            lg = min(lg, dist[i][j] + dist[xx][yy]);
          }
        }
      }
    }
  }
}

