#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("deal.in");
ofstream fout("deal.out");

int main() {
    int n;
    fin >> n;
    vector<int> h(n);
    for (int i = 0; i < n; i++) fin >> h[i];
    sort(h.begin(), h.end());
    int ans = 0;
    for (int i = 0; i < n; i++) {
        if (i == 0 || h[i] != h[i - 1]) ans += h[i];
    }
    fout << ans << '\n';
    return 0;
}
```
