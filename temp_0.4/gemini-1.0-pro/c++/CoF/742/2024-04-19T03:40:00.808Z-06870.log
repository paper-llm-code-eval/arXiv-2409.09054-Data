          table[i][j] = sum;
        } else {
          int sum = 0;
          for (int k = 1; k <= n; k++) {
            sum += table[k][j];
          }
          table[i][j] = sum;
        }
      }
      fout << table[i][j] << " ";
    }
    fout << endl;
  }

  return 0;
}
