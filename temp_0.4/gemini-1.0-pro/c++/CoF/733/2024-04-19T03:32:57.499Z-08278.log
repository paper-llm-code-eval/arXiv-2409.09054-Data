    dp[i][0][3] = 1;
    for (int j = 1; j < 4; ++j) { dp[i][0][j] = 0; }
    for (int j = 1; j < L[i]; ++j) {
      dp[i][j][0] = dp[i][j - 1][0] && (v[i][j] == 1 || v[i][j] == 2 || v[i][j] == 3);
      dp[i][j][1] = dp[i][j - 1][1] && (v[i][j] == 2 || v[i][j] == 3);
      dp[i][j][2] = dp[i][j - 1][2] && (v[i][j] == 2 || v[i][j] == 3);
      dp[i][j][3] = dp[i][j - 1][3] && (v[i][j] == 1 || v[i][j] == 2 || v[i][j] == 3);
      dp[i][j][0] |= dp[i][j - 1][1] && (v[i][j] == 1);
      dp[i][j][1] |= dp[i][j - 1][0] && (v[i][j] == 2);
      dp[i][j][1] |= dp[i][j - 1][2] && (v[i][j] == 2);
      dp[i][j][1] |= dp[i][j - 1][3] && (v[i][j] == 2);
      dp[i][j][2] |= dp[i][j - 1][0] && (v[i][j] == 3);
      dp[i][j][2] |= dp[i][j - 1][1] && (v[i][j] == 3);
      dp[i][j][2] |= dp[i][j - 1][3] && (v[i][j] == 3);
      dp[i][j][3] |= dp[i][j - 1][0] && (v[i][j] == 1);
      dp[i][j][3] |= dp[i][j - 1][1] && (v[i][j] == 1);
      dp[i][j][3] |= dp[i][j - 1][2] && (v[i][j] == 1);
    }
    fout << dp[i][L[i] - 1][0] << '\n';
  }

  return 0;
}
