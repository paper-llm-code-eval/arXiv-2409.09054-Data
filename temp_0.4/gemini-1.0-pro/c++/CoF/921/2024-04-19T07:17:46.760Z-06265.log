#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("forta.in");
    ofstream out("forta.out");
    int c, n;
    in >> c >> n;
    vector<int> v(n);
    for (int i = 0; i < n; i++) in >> v[i];
    sort(v.begin(), v.end());
    vector<int> f(n);
    for (int i = 0; i < n; i++) {
        int x = v[i];
        for (int j = 1; j * j <= x; j++) {
            if (x % j == 0) {
                f[i]++;
                if (x / j != j) f[i]++;
            }
        }
    }
    int maxf = 0, minx = 0;
    for (int i = 0; i < n; i++) {
        if (f[i] > maxf) {
            maxf = f[i];
            minx = v[i];
        }
    }
    if (c == 1) out << minx << endl;
    else {
        int maxl = 1;
        int l = 1;
        for (int i = 1; i < n; i++) {
            if (f[i] == f[i - 1]) l++;
            else {
                maxl = max(maxl, l);
                l = 1;
            }
        }
        maxl = max(maxl, l);
        out << maxl << endl;
    }
    return 0;
}
