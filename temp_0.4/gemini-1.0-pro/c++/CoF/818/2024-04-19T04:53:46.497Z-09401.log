#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("roata.in");
    ofstream out("roata.out");

    int n, p;
    in >> n >> p;
    vector<int> c(p);
    for (int i = 0; i < p; i++) {
        in >> c[i];
    }

    int sum = 0;
    for (int i = 0; i < p; i++) {
        sum += c[i];
    }

    vector<int> order(p);
    for (int i = 0; i < p; i++) {
        order[i] = i + 1;
    }

    sort(order.begin(), order.end(), [&](int a, int b) {
        return c[a - 1] < c[b - 1];
    });

    int last_cabin = 0;
    int last_client = order[p - 1];
    for (int i = 0; i < p; i++) {
        last_cabin = (last_cabin + c[order[i] - 1]) % n;
    }

    out << sum << endl;
    for (int i = 0; i < p; i++) {
        out << order[i] << " ";
    }
    out << endl;
    out << last_cabin + 1 << endl;

    return 0;
}
```
