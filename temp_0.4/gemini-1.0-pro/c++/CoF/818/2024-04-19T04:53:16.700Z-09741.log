#include <fstream>
#include <algorithm>
#include <vector>

using namespace std;

ifstream in("roata.in");
ofstream out("roata.out");

const int MAXN = 360;
const int MAXP = 100000;

int n, p;
int c[MAXP];
int r[MAXP];
int ord[MAXP];
int poz[MAXN];
int sum;

int main() {
    in >> n >> p;
    for (int i = 0; i < p; i++) {
        in >> c[i];
        sum += c[i];
    }
    for (int i = 0; i < p; i++) {
        in >> r[i];
        ord[i] = r[i];
    }
    sort(ord, ord + p);
    int cnt = 0;
    for (int i = 0; i < p; i++) {
        while (cnt < n && poz[cnt] != 0) cnt++;
        poz[cnt] = ord[i];
        cnt = (cnt + c[r[i] - 1]) % n;
        out << ord[i] << " ";
    }
    out << endl;
    out << cnt << endl;
    return 0;
}
```
