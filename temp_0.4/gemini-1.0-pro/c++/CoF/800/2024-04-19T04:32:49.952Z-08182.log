  int n, k, a, b, c;
  fin >> n >> k >> a >> b >> c;

  // Calculate the number of complete cycles of 4 rolls
  int cycles = k / 4;

  // Calculate the number of rolls remaining after the complete cycles
  int remaining_rolls = k % 4;

  // Update the values of a, b, and c after the complete cycles
  a = (a + cycles) % n;
  b = (b + cycles) % n;
  c = (c + cycles) % n;

  // Update the values of a, b, and c for the remaining rolls
