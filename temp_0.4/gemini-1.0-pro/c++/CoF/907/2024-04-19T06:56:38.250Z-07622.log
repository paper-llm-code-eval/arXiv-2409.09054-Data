#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("cartele.in");
    ofstream out("cartele.out");
    int C, N;
    in >> C >> N;
    vector<pair<char, int>> events(N);
    for (int i = 0; i < N; i++) {
        char sex;
        int h, m, s;
        in >> sex >> h >> m >> s;
        events[i] = {sex, h * 3600 + m * 60 + s};
    }
    sort(events.begin(), events.end(), [](const pair<char, int>& a, const pair<char, int>& b) {
        return a.second < b.second;
    });
    int boys = 0, girls = 0, equal = 0, odd_boys = 0, max_odd_boys = 0;
    for (const auto& event : events) {
        if (event.first == 'b') {
            boys++;
            if (boys == girls) {
                equal += event.second - events[equal].second;
                events[equal] = event;
            }
            odd_boys++;
            max_odd_boys = max(max_odd_boys, odd_boys);
        } else {
            girls++;
            if (boys == girls) {
                equal += event.second - events[equal].second;
                events[equal] = event;
            }
            odd_boys--;
        }
    }
    switch (C) {
        case 1:
            out << boys << ' ' << girls;
            break;
        case 2:
            out << equal;
            break;
        case 3:
            out << max_odd_boys;
            break;
    }
    return 0;
}
