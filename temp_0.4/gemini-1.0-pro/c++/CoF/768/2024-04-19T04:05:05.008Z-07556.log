#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>

using namespace std;

#define MAXN 175
#define INF 0x3f3f3f3f

int n, m;
int dx[] = {-1, 0, 1, 0};
int dy[] = {0, 1, 0, -1};
vector<vector<int>> dist;
vector<vector<bool>> viz;
vector<vector<int>> grid;

bool valid(int x, int y) {
    return x >= 1 && x <= n && y >= 1 && y <= n && !viz[x][y] && grid[x][y] == 0;
}

int bfs(int sx, int sy, int ex, int ey) {
    queue<pair<int, int>> q;
    q.push({sx, sy});
    viz[sx][sy] = true;
    dist[sx][sy] = 0;
    while (!q.empty()) {
        int x = q.front().first;
        int y = q.front().second;
        q.pop();
        if (x == ex && y == ey) {
            return dist[x][y];
        }
        for (int i = 0; i < 4; i++) {
            int nx = x + dx[i];
            int ny = y + dy[i];
            if (valid(nx, ny)) {
                q.push({nx, ny});
                viz[nx][ny] = true;
                dist[nx][ny] = dist[x][y] + 1;
            }
        }
    }
    return -1;
}

int main() {
    ifstream fin("alee.in");
    ofstream fout("alee.out");
    fin >> n >> m;
    grid.resize(n + 1, vector<int>(n + 1, 0));
    for (int i = 0; i < m; i++) {
        int x, y;
        fin >> x >> y;
        grid[x][y] = 1;
    }
    int sx, sy, ex, ey;
    fin >> sx >> sy >> ex >> ey;
    dist.resize(n + 1, vector<int>(n + 1, INF));
    viz.resize(n + 1, vector<bool>(n + 1, false));
    int ans = bfs(sx, sy, ex, ey);
    fout << ans << "\n";
    return 0;
}
