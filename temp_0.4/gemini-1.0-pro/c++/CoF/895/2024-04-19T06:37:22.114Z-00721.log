#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

struct Cube
{
    int side;
    char color;
};

int main()
{
    ifstream in("turnuri.in");
    ofstream out("turnuri.out");

    int cerinta;
    in >> cerinta;

    int n;
    in >> n;

    vector<Cube> cubes(n);
    for (int i = 0; i < n; i++)
    {
        in >> cubes[i].side >> cubes[i].color;
    }

    if (cerinta == 1)
    {
        int t = 0, h = 0;
        int last = -1;
        for (int i = 0; i < n; i++)
        {
            if (last == -1 || (cubes[i].color != cubes[last] && cubes[i].side < cubes[last]))
            {
                t++;
                h += cubes[i].side;
                last = i;
            }
        }

        out << t << ' ' << h << '\n';
    }
    else
    {
        sort(cubes.begin(), cubes.end(), [](const Cube& a, const Cube& b) { return a.side < b.side; });

        int n_max = 0;
        int last = -1;
        for (int i = 0; i < n; i++)
        {
            if (last == -1 || (cubes[i].color != cubes[last] && cubes[i].side < cubes[last]))
            {
                n_max++;
                last = i;
            }
        }

        out << n_max << '\n';
    }

    return 0;
}
