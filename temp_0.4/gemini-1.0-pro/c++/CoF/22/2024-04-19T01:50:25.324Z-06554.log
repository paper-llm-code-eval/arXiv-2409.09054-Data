#include <bits/stdc++.h>

using namespace std;

const int mod = 1000000007;

int n, d;
vector<vector<int>> adj;
vector<int> dp;

int dfs(int u, int p) {
  if (dp[u] != -1) return dp[u];
  dp[u] = 1;
  for (int v : adj[u]) {
    if (v == p) continue;
    dp[u] = (dp[u] + dfs(v, u)) % mod;
  }
  if (d == 1) dp[u] = 0;
  return dp[u];
}

int main() {
  ifstream in("tairos.in");
  ofstream out("tairos.out");
  in >> n >> d;
  adj.resize(n + 1);
  dp.resize(n + 1, -1);
  for (int i = 1; i < n; i++) {
    int u, v;
    in >> u >> v;
    adj[u].push_back(v);
    adj[v].push_back(u);
  }
  out << dfs(1, 1) << "\n";
  return 0;
}
```
