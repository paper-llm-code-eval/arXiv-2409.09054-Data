struct Node {
    int x, y, zone, dist;
    Node(int x, int y, int zone, int dist) : x(x), y(y), zone(zone), dist(dist) {}
};

const int MAX_N = 500;
const int MAX_M = 500;
const int MAX_C = 8000;
const int INF = numeric_limits<int>::max();
int N, M, X, Y;
int C[MAX_N][MAX_M];
int dp[MAX_N][MAX_M][4];
int dist[MAX_N][MAX_M][4];

int dx[] = {-1, 0, 1, 0};
int dy[] = {0, 1, 0, -1};

bool valid(int x, int y) {
    return x >= 1 && x <= N && y >= 1 && y <= M;
}

int getZone(int x, int y) {
    if (x <= X && y <= Y) return 1;
    if (x <= X && y > Y) return 2;
    if (x > X && y <= Y) return 3;
    return 4;
}

int dijkstra(int sx, int sy, int sz) {
    priority_queue<Node, vector<Node>, greater<Node>> pq;
    pq.push(Node(sx, sy, sz, 0));
    dist[sx][sy][sz] = 0;

    while (!pq.empty()) {
        Node node = pq.top();
        pq.pop();

        if (node.dist > dist[node.x][node.y][node.zone]) continue;

        for (int i = 0; i < 4; i++) {
            int nx = node.x + dx[i];
            int ny = node.y + dy[i];
            int nz = node.zone;
            if (valid(nx, ny) && (nx != sx || ny != sy)) {
                if (nx <= X && ny <= Y) nz = 1;
                else if (nx <= X && ny > Y) nz = 2;
                else if (nx > X && ny <= Y) nz = 3;
                else nz = 4;
                if (dist[nx][ny][nz] > node.dist + 1) {
                    dist[nx][ny][nz] = node.dist + 1;
                    pq.push(Node(nx, ny, nz, node.dist + 1));
                }
            }
        }
    }

    int maxDist = 0;
    for (int i = 1; i <= N; i++) {
        for (int j = 1; j <= M; j++) {
            maxDist = max(maxDist, dist[i][j][sz]);
        }
    }

    return maxDist;
}

