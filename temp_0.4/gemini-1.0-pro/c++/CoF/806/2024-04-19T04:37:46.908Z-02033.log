        for (int i = 0; i < 4; i++) {
            int nx = node.x + dx[i];
            int ny = node.y + dy[i];
            int nz = node.zone;
            if (valid(nx, ny) && (nx != sx || ny != sy)) {
                if (nx <= X && ny <= Y) nz = 1;
                else if (nx <= X && ny > Y) nz = 2;
                else if (nx > X && ny <= Y) nz = 3;
                else nz = 4;
                if (dist[nx][ny][nz] > node.dist + 1) {
                    dist[nx][ny][nz] = node.dist + 1;
                    pq.push(Node(nx, ny, nz, node.dist + 1));
                }
            }
        }
    }

    int maxDist = 0;
    for (int i = 1; i <= N; i++) {
        for (int j = 1; j <= M; j++) {
            maxDist = max(maxDist, dist[i][j][sz]);
        }
    }

    return maxDist;
}

int main() {
    ifstream fin("cri.in");
    ofstream fout("cri.out");

    fin >> N >> M >> X >> Y;
    for (int i = 1; i <= N; i++) {
        for (int j = 1; j <= M; j++) {
            fin >> C[i][j];
        }
    }

    for (int i = 1; i <= N; i++) {
        for (int j = 1; j <= M; j++) {
            for (int k = 1; k <= 4; k++) {
                dp[i][j][k] = -INF;
                dist[i][j][k] = INF;
            }
        }
    }

    for (int i = 1; i <= N; i++) {
        for (int j = 1; j <= M; j++) {
            int zone = getZone(i, j);
            dp[i][j][zone] = C[i][j];
            dist[i][j][zone] = 0;
        }
    }

    for (int k = 1; k <= 4; k++) {
        for (int i = 1; i <= N; i++) {
            for (int j = 1; j <= M; j++) {
                if (dp[i][j][k] == -INF) continue;
                for (int l = 0; l < 4; l++) {
                    int nx = i + dx[l];
                    int ny = j + dy[l];
                    int nz = k;
                    if (valid(nx, ny) && (nx != i || ny != j)) {
                        if (nx <= X && ny <= Y) nz = 1;
                        else if (nx <= X && ny > Y) nz = 2;
                        else if (nx > X && ny <= Y) nz = 3;
                        else nz = 4;
                        if (dp[nx][ny][nz] < dp[i][j][k] + C[nx][ny]) {
                            dp[nx][ny][nz] = dp[i][j][k] + C[nx][ny];
                            dist[nx][ny][nz] = dist[i][j][k] + 1;
                        }
                    }
                }
            }
        }
    }

    int maxT = -INF;
    int minK = INF;
    int bestZone = -1;
    for (int k = 1; k <= 4; k++) {
        int maxDist = dijkstra(X, Y, k);
        if (dp[N][M][k] > maxT || (dp[N][M][k] == maxT && maxDist < minK)) {
            maxT = dp[N][M][k];
            minK = maxDist;
            bestZone = k;
        }
    }

    fout << bestZone << " " << maxT << " " << minK << endl;

    return 0;
}
