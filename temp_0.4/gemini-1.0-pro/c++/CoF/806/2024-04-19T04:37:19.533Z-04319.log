#include <cstdio>
#include <vector>
#include <algorithm>

using namespace std;

const int NMAX = 505;

int n, m, x, y;
int a[NMAX][NMAX];
int dp[NMAX][NMAX][NMAX][NMAX];
int zone[NMAX][NMAX];
int dx[] = {0, 0, 1, -1};
int dy[] = {1, -1, 0, 0};

int main()
{
    freopen("cri.in", "r", stdin);
    freopen("cri.out", "w", stdout);

    scanf("%d %d %d %d", &n, &m, &x, &y);
    for (int i = 1; i <= n; ++i)
        for (int j = 1; j <= m; ++j)
            scanf("%d", &a[i][j]);

    int z = 0, t = 0, k = 1e9;
    for (int i = 1; i <= n; ++i)
        for (int j = 1; j <= m; ++j)
        {
            dp[i][j][i][j] = a[i][j];
            zone[i][j] = 1;
            if (i == 1 || j == 1)
                zone[i][j] = 2;
            if (i == n || j == m)
                zone[i][j] = 3;
            if (i == n && j == m)
                zone[i][j] = 4;
            if (i == x && j == y)
                zone[i][j] = 5;
        }

    for (int len = 2; len <= n + m; ++len)
        for (int i = 1; i <= n; ++i)
            for (int j = 1; j <= m; ++j)
                for (int ii = i; ii <= n; ++ii)
                    for (int jj = j; jj <= m; ++jj)
                    {
                        if (ii == i && jj == j)
                            continue;
                        for (int k = 0; k < 4; ++k)
                        {
                            int ni = ii + dx[k];
                            int nj = jj + dy[k];
                            if (ni < 1 || ni > n || nj < 1 || nj > m)
                                continue;
                            if (dp[i][j][ii][jj] + a[ni][nj] > dp[i][j][ii][jj])
                            {
                                dp[i][j][ii][jj] = dp[i][j][ii][jj] + a[ni][nj];
                                zone[ii][jj] = zone[ni][nj];
                            }
                        }
                    }

    for (int i = 1; i <= n; ++i)
        for (int j = 1; j <= m; ++j)
        {
            if (zone[i][j] == 5)
                continue;
            int zone_i = zone[i][j];
            int zone_j = zone[i][j];
            while (zone[i][j] == zone_i)
            {
                if (zone[i][j] == 1)
                    ++zone_i;
                if (zone[i][j] == 2)
                    ++zone_j;
                if (zone[i][j] == 3)
                    --zone_i;
                if (zone[i][j] == 4)
                    --zone_j;
                ++j;
            }
            --j;
            int zone_k = zone[i][j];
            int zone_l = zone[i][j];
            while (zone[i][j] == zone_k)
            {
                if (zone[i][j] == 1)
                    ++zone_k;
                if (zone[i][j] == 2)
                    ++zone_l;
                if (zone[i][j] == 3)
                    --zone_k;
                if (zone[i][j] == 4)
                    --zone_l;
                ++i;
            }
            --i;
            if (dp[i][j][zone_i - 1][zone_j - 1] > t ||
                (dp[i][j][zone_i - 1][zone_j - 1] == t &&
                 (zone_i - i - zone_j + j) < k))
            {
                t = dp[i][j][zone_i - 1][zone_j - 1];
                z = zone[i][j];
                k = zone_i - i - zone_j + j;
            }
        }

    printf("%d %d %d\n", z, t, k);

    return 0;
}
