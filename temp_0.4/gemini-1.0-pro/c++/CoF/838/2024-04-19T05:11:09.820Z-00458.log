    int ans2 = 0;
    for (int i = 1; i <= n; ++i) {
        for (int j = 0; j < pos[i].size(); ++j) {
            int idx = pos[i][j];
            ans2 = (ans2 + dp2[idx + 1]) % mod;
        }
    }

    fout << ans1 << '\n' << ans2 << '\n';

    return 0;
}
