#include <fstream>
#include <vector>
using namespace std;

ifstream fin("calcule.in");
ofstream fout("calcule.out");

const int MOD = 20011;

int main() {
    int n, k;
    fin >> n >> k;
    vector<int> v(n);
    for (int i = 0; i < n; i++) fin >> v[i];

    int dp[n][k];
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < k; j++) {
            dp[i][j] = 0;
        }
    }
    dp[0][0] = 1;
    for (int i = 1; i < n; i++) {
        for (int j = 0; j < k; j++) {
            dp[i][j] = dp[i - 1][j];
            if (j >= v[i]) dp[i][j] = (dp[i][j] + dp[i - 1][j - v[i]]) % MOD;
        }
    }

    int ans1 = 0;
    for (int i = 0; i < n; i++) {
        if (v[i] > v[i - 1]) ans1++;
    }
    fout << ans1 << '\n';

    fout << dp[n - 1][k - 1] << '\n';

    return 0;
}
