#include <vector>
#include <algorithm>
#include <fstream>

using namespace std;

const int MAX_N = 10005;
const int INF = 1e9;

int n, k;
vector<vector<int>> adj;
vector<int> dist;
vector<bool> visited;
vector<int> free_streets;
int min_cost = INF;

void dfs(int node, int cost) {
    if (visited[node]) {
        return;
    }
    visited[node] = true;
    dist[node] = cost;
    for (int neighbor : adj[node]) {
        dfs(neighbor, cost + 1);
    }
}

void dfs_free(int node, int cost) {
    if (visited[node]) {
        return;
    }
    visited[node] = true;
    dist[node] = cost;
    for (int neighbor : adj[node]) {
        if (find(free_streets.begin(), free_streets.end(), neighbor) != free_streets.end()) {
            dfs_free(neighbor, cost);
        } else {
            dfs_free(neighbor, cost + 1);
        }
    }
}

int main() {
    ifstream fin("cezar.in");
    ofstream fout("cezar.out");

    fin >> n >> k;
    adj.resize(n + 1);
    dist.resize(n + 1);
    visited.resize(n + 1);

    for (int i = 1; i < n; i++) {
        int u, v;
        fin >> u >> v;
        adj[u].push_back(v);
        adj[v].push_back(u);
