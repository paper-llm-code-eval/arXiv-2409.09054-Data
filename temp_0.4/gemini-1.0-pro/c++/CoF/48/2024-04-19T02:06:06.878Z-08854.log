#include <fstream>
#include <vector>
#include <queue>
#include <limits>

using namespace std;

const int MAX_N = 10005;
const int INF = numeric_limits<int>::max();

int n, k;
vector<vector<int>> adj;
vector<int> dist;

int main() {
  ifstream in("cezar.in");
  ofstream out("cezar.out");

  in >> n >> k;
  adj.resize(n + 1);
  dist.resize(n + 1, INF);

  for (int i = 1; i < n; i++) {
    int u, v;
    in >> u >> v;
    adj[u].push_back(v);
    adj[v].push_back(u);
  }

  // Dijkstra's algorithm to find the shortest path from node 1 to all other nodes
  priority_queue<pair<int, int>> pq;
  pq.push({0, 1});
  dist[1] = 0;

  while (!pq.empty()) {
    int d = -pq.top().first;
    int u = pq.top().second;
    pq.pop();

    if (d > dist[u]) continue;

    for (int v : adj[u]) {
      if (d + 1 < dist[v]) {
        dist[v] = d + 1;
        pq.push({-dist[v], v});
      }
    }
  }

  // Find the minimum cost by selecting the k shortest paths
  int cost = 0;
  for (int i = 1; i <= n; i++) {
    if (i == 1) continue;
    if (dist[i] != INF) {
      cost += dist[i];
      k--;
      if (k == 0) break;
    }
  }

  out << cost << endl;

  return 0;
}
