#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

struct edge {
    int a, b, w;
};

int n, k, t[10001], p[10001], r[10001], ans;
vector<edge> e;
vector<int> g[10001];

bool cmp(const edge &a, const edge &b) {
    return a.w < b.w;
}

int find(int x) {
    if (x == p[x]) return x;
    return p[x] = find(p[x]);
}

void unite(int x, int y) {
    x = find(x);
    y = find(y);
    if (x == y) return;
    if (r[x] < r[y]) p[x] = y;
    else {
        p[y] = x;
        if (r[x] == r[y]) r[x]++;
    }
}

int main() {
    ifstream in("cezar.in");
    ofstream out("cezar.out");
    in >> n >> k;
    for (int i = 1; i <= n; i++) p[i] = i;
    for (int i = 1; i <= n - 1; i++) {
        int a, b;
        in >> a >> b;
        g[a].push_back(b);
        g[b].push_back(a);
        e.push_back({a, b, 1});
    }
    sort(e.begin(), e.end(), cmp);
    for (auto x : e) {
        if (k == 0) break;
        int a = find(x.a), b = find(x.b);
        if (a != b) {
            unite(a, b);
            ans += x.w;
            k--;
        }
    }
    out << ans << '\n';
    return 0;
}
