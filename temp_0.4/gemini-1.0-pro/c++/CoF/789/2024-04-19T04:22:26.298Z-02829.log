#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

struct Proiectil {
    int distanta;
    int viteza;
    double moment_impact;
};

bool compara(const Proiectil& a, const Proiectil& b) {
    return a.moment_impact < b.moment_impact;
}

int main() {
    ifstream fin("startrek.in");
    ofstream fout("startrek.out");

    int n, t;
    fin >> n >> t;

    vector<Proiectil> proiectile(n);
    for (int i = 0; i < n; i++) {
        fin >> proiectile[i].distanta;
    }
    for (int i = 0; i < n; i++) {
        fin >> proiectile[i].viteza;
        proiectile[i].moment_impact = (double)proiectile[i].distanta / proiectile[i].viteza;
    }

    sort(proiectile.begin(), proiectile.end(), compara);

    int numar_proiectile_distruse = 0;
    double moment_curent = 0;
    for (const Proiectil& proiectil : proiectile) {
        if (moment_curent + t <= proiectil.moment_impact) {
            moment_curent += t;
            numar_proiectile_distruse++;
        }
    }

    fout << numar_proiectile_distruse << endl;

    return 0;
}
```
