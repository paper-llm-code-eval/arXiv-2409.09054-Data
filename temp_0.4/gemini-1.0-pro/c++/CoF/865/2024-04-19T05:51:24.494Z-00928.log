    int v;
    fin >> v;
    int n;
    fin >> n;
    vector<int> c(n * (n + 1) / 2);
    for (int i = 0; i < c.size(); i++) {
        fin >> c[i];
    }
    vector<vector<int>> g(n + 1);
    for (int i = 0; i < n; i++) {
        for (int j = 0; j <= i; j++) {
            int u = i * (i + 1) / 2 + j;
            int v = i * (i + 1) / 2 + j + 1;
            if (j > 0) {
                int w = i * (i + 1) / 2 + j - 1;
                g[u].push_back(w);
                g[w].push_back(u);
            }
            g[u].push_back(v);
            g[v].push_back(u);
        }
    }
    vector<int> level(n + 1);
    for (int i = 0; i < n; i++) {
        for (int j = 0; j <= i; j++) {
            int u = i * (i + 1) / 2 + j;
            level[u] = i + 1;
        }
    }
    vector<int> sum(n + 1);
    for (int i = 0; i < n; i++) {
        for (int j = 0; j <= i; j++) {
            int u = i * (i + 1) / 2 + j;
            sum[i + 1] += c[u];
        }
    }
    int max_level = 0;
    int max_sum = 0;
    for (int i = 1; i <= n; i++) {
        if (sum[i] > max_sum) {
            max_sum = sum[i];
            max_level = i;
        }
    }
    if (v == 1) {
        fout << max_level << endl;
    } else {
        vector<int> dist(c.size(), -1);
        dist[0] = 0;
        vector<int> q;
        q.push_back(0);
        while (!q.empty()) {
            int u = q.back();
            q.pop_back();
            for (int v : g[u]) {
                if (dist[v] == -1) {
                    dist[v] = dist[u] + 1;
                    q.push_back(v);
                }
            }
        }
        int max_dist = 0;
        for (int i = 0; i < c.size(); i++) {
            max_dist = max(max_dist, dist[i]);
        }
        fout << max_dist << " " << max_dist * n << endl;
    }
    return 0;
}
