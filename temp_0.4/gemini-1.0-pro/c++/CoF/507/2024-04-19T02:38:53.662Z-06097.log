#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAX_N = 300000;
const int MAX_X = 18;
const int MAX_Y = 10000;

int n;
int x[MAX_N], y[MAX_N];
int dp[MAX_N][MAX_X + 1];

int main() {
    ifstream fin("partitura.in");
    ofstream fout("partitura.out");

    fin >> n;
    for (int i = 0; i < n; i++) {
        fin >> x[i] >> y[i];
    }

    for (int i = n - 1; i >= 0; i--) {
        for (int j = 0; j <= MAX_X; j++) {
            dp[i][j] = dp[i + 1][j];
            if (j + x[i] <= MAX_X) {
                dp[i][j] = max(dp[i][j], dp[i + 1][j + x[i]] + y[i] * y[i]);
            }
        }
    }

    fout << dp[0][0] << endl;

    return 0;
}
```
