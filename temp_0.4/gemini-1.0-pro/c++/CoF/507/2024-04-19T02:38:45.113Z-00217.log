#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream fin("partitura.in");
    ofstream fout("partitura.out");

    int n;
    fin >> n;
    vector<pair<int, int>> notes(n);
    for (auto& [x, y] : notes) {
        fin >> x >> y;
    }
    fin.close();

    sort(notes.begin(), notes.end(), [](const auto& a, const auto& b) {
        return a.first < b.first;
    });

    long long score = 0;
    vector<int> dp(n + 1, 0);
    for (int i = 0; i < n; i++) {
        int j = i;
        long long sum = 0;
        while (j < n && sum + notes[j].first <= 1) {
            sum += notes[j].first;
            dp[j + 1] = max(dp[j + 1], dp[i] + notes[j].second * notes[j].second);
            j++;
        }
    }

    fout << dp[n] << '\n';
    fout.close();

    return 0;
}
```
