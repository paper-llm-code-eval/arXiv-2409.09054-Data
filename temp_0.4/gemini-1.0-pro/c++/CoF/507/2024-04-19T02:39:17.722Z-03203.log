using namespace std;

int main() {
    ifstream in("partitura.in");
    ofstream out("partitura.out");

    int n;
    in >> n;

    vector<pair<int, int>> notes(n);
    for (int i = 0; i < n; i++) {
        in >> notes[i].first >> notes[i].second;
    }

    sort(notes.begin(), notes.end());

    vector<vector<int>> groups;
    int current_duration = 0;
    int current_score = 0;
    for (int i = 0; i < n; i++) {
