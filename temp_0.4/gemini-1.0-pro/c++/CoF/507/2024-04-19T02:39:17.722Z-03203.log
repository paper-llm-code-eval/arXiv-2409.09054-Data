            current_duration += (1 << (notes[i].first - 1));
            current_score += notes[i].second * notes[i].second;
        } else {
            groups.push_back({current_score});
            current_duration = (1 << (notes[i].first - 1));
            current_score = notes[i].second * notes[i].second;
        }
    }
    groups.push_back({current_score});

    int max_score = 0;
    for (int i = 0; i < groups.size(); i++) {
        int group_score = groups[i][0];
        for (int j = i + 1; j < groups.size(); j++) {
            group_score = max(group_score, groups[i][0] + groups[j][0]);
        }
        max_score = max(max_score, group_score);
    }

    out << max_score << "\n";

    return 0;
}
