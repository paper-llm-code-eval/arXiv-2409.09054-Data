#include <fstream>
#include <vector>
#include <algorithm>
#include <cstring>

using namespace std;

const int MAX_N = 700000;
const int MAX_P = 998560000;
const int MAX_M = 100;
const int MAX_VAL = 250000;

int main() {
    ifstream in("livada.in");
    ofstream out("livada.out");

    int m, n, p;
    in >> m >> n >> p;

    vector<int> freq(MAX_P + 1, 0);
    vector<int> max_consecutive(m + 1, 0);
    int max_consecutive_global = 0;
    int rows_with_majority = 0;

    for (int i = 1; i <= m; i++) {
        memset(freq.data(), 0, (p + 1) * sizeof(int));
        int max_freq = 0;
        int soi_majoritar = 0;
        int consecutive = 0;

        for (int j = 1; j <= n; j++) {
            int soi;
            in >> soi;
            freq[soi]++;
            consecutive = (soi == freq[soi] - 1) ? consecutive + 1 : 1;
            max_consecutive = max(max_consecutive, consecutive);
            if (freq[soi] > max_freq) {
                max_freq = freq[soi];
                soi_majoritar = soi;
            }
        }

        if (max_freq >= (n / 2 + 1)) {
            rows_with_majority++;
        }
        max_consecutive_global = max(max_consecutive_global, max_consecutive);
    }

    out << rows_with_majority << endl;
    out << max_consecutive_global << endl;

    in.close();
    out.close();

    return 0;
}
