#include <fstream>
#include <vector>
#include <algorithm>
#include <cstring>

using namespace std;

const int MAX_N = 700000;
const int MAX_P = 998560000;
const int MAX_M = 100;
const int MAX_VAL = 250000;

int main() {
    ifstream in("livada.in");
    ofstream out("livada.out");

    int m, n, p;
    in >> m >> n >> p;

    vector<int> freq(MAX_P + 1, 0);
    vector<int> max_consecutive(m + 1, 0);
    int max_consecutive_global = 0;
    int rows_with_majority = 0;

    for (int i = 1; i <= m; i++) {
        memset(freq.data(), 0, (p + 1) * sizeof(int));
        int max_freq = 0;
        int soi_majoritar = 0;
        int consecutive = 0;

        for (int j = 1; j <= n; j++) {
