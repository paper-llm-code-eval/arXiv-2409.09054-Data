            max_consecutive = max(max_consecutive, consecutive);
            if (freq[soi] > max_freq) {
                max_freq = freq[soi];
                soi_majoritar = soi;
            }
        }

        if (max_freq >= (n / 2 + 1)) {
            rows_with_majority++;
