const int MOD = 30011;
int n, k, dp[101][101];
vector<int> adj[101];

int main() {
  ifstream fin("iepuri.in");
  ofstream fout("iepuri.out");

  fin >> n >> k;
  for (int i = 0; i < n - 1; i++) {
    int a, b;
    fin >> a >> b;
    adj[a].push_back(b);
  }

  dp[0][0] = 1;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= k; j++) {
      for (int sub : adj[i]) {
        if (j > 1) dp[i][j] = (dp[i][j] + dp[sub][j - 1]) % MOD;
        dp[i][j] = (dp[i][j] + dp[sub][j]) % MOD;
      }
    }
  }

  fout << dp[1][k] << '\n';
  return 0;
}
```
