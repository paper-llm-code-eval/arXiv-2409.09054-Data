#include <fstream>
#include <vector>
#include <cmath>
#include <algorithm>

using namespace std;

struct Point {
    double x, y;
    Point() {}
    Point(double x, double y) : x(x), y(y) {}
    Point operator+(const Point& p) const { return Point(x + p.x, y + p.y); }
    Point operator-(const Point& p) const { return Point(x - p.x, y - p.y); }
    double operator*(const Point& p) const { return x * p.y - y * p.x; }
};

struct Segment {
    Point p1, p2;
    Segment() {}
    Segment(const Point& p1, const Point& p2) : p1(p1), p2(p2) {}
    double length() const { return sqrt((p2.x - p1.x) * (p2.x - p1.x) + (p2.y - p1.y) * (p2.y - p1.y)); }
};

struct Pole {
    Point p;
    double d;
    Pole() {}
    Pole(const Point& p, double d) : p(p), d(d) {}
};

bool cmp(const Pole& p1, const Pole& p2) {
    return p1.p.x < p2.p.x;
}

double area(const vector<Point>& points) {
    double sum = 0;
    for (int i = 0; i < points.size(); i++) {
        sum += points[i] * points[(i + 1) % points.size()];
    }
    return abs(sum) / 2;
}

int main() {
    ifstream fin("mosia.in");
    ofstream fout("mosia.out");

    int n;
    fin >> n;

    vector<Pole> poles(n);
    for (int i = 0; i < n; i++) {
        fin >> poles[i].p.x >> poles[i].p.y >> poles[i].d;
    }

    sort(poles.begin(), poles.end(), cmp);

    double maxArea = 0;
    for (int i = 0; i < n; i++) {
        vector<Point> points;
        for (int j = 0; j < n; j++) {
            if (i == j) {
                points.push_back(poles[j].p + Point(poles[j].d, 0));
                points.push_back(poles[j].p + Point(-poles[j].d, 0));
            } else {
                points.push_back(poles[j].p);
            }
        }
        maxArea = max(maxArea, area(points));
    }

    fout << fixed << setprecision(4) << maxArea << endl;

    return 0;
}
