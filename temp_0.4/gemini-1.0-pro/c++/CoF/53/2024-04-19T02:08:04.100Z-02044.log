#include <fstream>
#include <vector>
#include <algorithm>
#include <cmath>

using namespace std;

struct Point {
    int x, y;
    Point() : x(0), y(0) {}
    Point(int x, int y) : x(x), y(y) {}
    Point operator+(const Point& other) const { return Point(x + other.x, y + other.y); }
    Point operator-(const Point& other) const { return Point(x - other.x, y - other.y); }
    int dot(const Point& other) const { return x * other.x + y * other.y; }
    int cross(const Point& other) const { return x * other.y - y * other.x; }
    double dist(const Point& other) const { return sqrt((x - other.x) * (x - other.x) + (y - other.y) * (y - other.y)); }
};

struct Segment {
    Point p1, p2;
    Segment() : p1(Point()), p2(Point()) {}
    Segment(const Point& p1, const Point& p2) : p1(p1), p2(p2) {}
};

struct Line {
    Point p, v;
    Line() : p(Point()), v(Point()) {}
    Line(const Point& p, const Point& v) : p(p), v(v) {}
};

double area(const vector<Point>& points) {
    double sum = 0;
    for (int i = 0; i < points.size(); i++) {
        sum += points[i].cross(points[(i + 1) % points.size()]);
    }
    return abs(sum) / 2.0;
}

Point rotate(const Point& p, const Point& center, double angle) {
    double s = sin(angle);
    double c = cos(angle);
    return Point(center.x + (p.x - center.x) * c - (p.y - center.y) * s, center.y + (p.x - center.x) * s + (p.y - center.y) * c);
}

bool intersect(const Segment& s1, const Segment& s2, Point& intersection) {
    Point p1 = s1.p1, p2 = s1.p2, p3 = s2.p1, p4 = s2.p2;
    int d1 = (p4 - p3).cross(p1 - p3), d2 = (p4 - p3).cross(p2 - p3), d3 = (p2 - p1).cross(p3 - p1), d4 = (p2 - p1).cross(p4 - p1);
    if ((d1 > 0 && d2 < 0) || (d1 < 0 && d2 > 0) || (d3 > 0 && d4 < 0) || (d3 < 0 && d4 > 0)) {
        double t = (double)(d1 * (p2 - p3).dist(p4)) / (d1 - d2);
        intersection = p1 + (p2 - p1) * t;
        return true;
    } else if (d1 == 0 && p1.dist(p3) <= (p2 - p1).dist(p4)) {
        intersection = p1;
        return true;
    } else if (d2 == 0 && p2.dist(p3) <= (p2 - p1).dist(p4)) {
        intersection = p2;
        return true;
    } else if (d3 == 0 && p3.dist(p1) <= (p4 - p3).dist(p2)) {
        intersection = p3;
        return true;
    } else if (d4 == 0 && p4.dist(p1) <= (p4 - p3).dist(p2)) {
        intersection = p4;
        return true;
    } else {
        return false;
    }
}

double extend(const vector<Point>& points, const vector<int>& distances, const vector<Segment>& segments, const Point& p, const int d) {
    double max_area = 0;
    for (int i = 0; i < segments.size(); i++) {
        const Segment& segment = segments[i];
        Point intersection;
        if (intersect(Segment(p, p + Point(distances[i], 0)), segment, intersection)) {
            vector<Point> new_points = points;
            new_points.push_back(intersection);
            sort(new_points.begin(), new_points.end());
            max_area = max(max_area, area(new_points));
        }
        if (intersect(Segment(p, p + Point(0, distances[i])), segment, intersection)) {
            vector<Point> new_points = points;
            new_points.push_back(intersection);
            sort(new_points.begin(), new_points.end());
            max_area = max(max_area, area(new_points));
        }
    }
    return max_area;
}

double solve(const vector<Point>& points, const vector<int>& distances, const vector<Segment>& segments) {
    double max_area = 0;
    for (int i = 0; i < points.size(); i++) {
        for (int j = 0; j < 4; j++) {
            max_area = max(max_area, extend(points, distances, segments, points[i], distances[i]));
            points[i] = rotate(points[i], Point(0, 0), M_PI / 2);
        }
    }
    return max_area;
}

int main() {
    ifstream in("mosia.in");
    ofstream out("mosia.out");
    int n;
    in >> n;
    vector<Point> points(n);
    vector<int> distances(n);
    vector<Segment> segments;
    for (int i = 0; i < n; i++) {
        in >> points[i].x >> points[i].y >> distances[i];
    }
    for (int i = 0; i < n; i++) {
        segments.push_back(Segment(points[i], points[(i + 1) % n]));
    }
    out << fixed << setprecision(4) << solve(points, distances, segments) << endl;
    return 0;
}
