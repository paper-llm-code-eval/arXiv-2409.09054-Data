#include <fstream>
#include <vector>
#include <queue>
#include <limits>

using namespace std;

struct Edge {
    int to, time, watts;
};

struct State {
    int node, lantern, time, watts;
};

const int INF = numeric_limits<int>::max();

int main() {
    ifstream in("lanterna.in");
    ofstream out("lanterna.out");

    int n, k;
    in >> n >> k;

    vector<int> bases(n + 1, 0);
    for (int i = 1; i <= n; ++i) {
        in >> bases[i];
    }

    vector<vector<Edge>> graph(n + 1);
    int m;
    in >> m;
    for (int i = 0; i < m; ++i) {
        int a, b, t, w;
        in >> a >> b >> t >> w;
        graph[a].push_back({b, t, w});
        graph[b].push_back({a, t, w});
    }

    vector<vector<vector<int>>> dp(n + 1, vector<vector<int>>(k + 1, vector<int>(1001, INF)));
    dp[1][1][0] = 0;

    priority_queue<State, vector<State>, greater<State>> pq;
    pq.push({1, 1, 0, 0});

    while (!pq.empty()) {
        State state = pq.top();
        pq.pop();

        if (dp[state.node][state.lantern][state.watts] < state.time) {
            continue;
        }

        if (state.node == n) {
            out << state.time << " " << state.lantern << endl;
            return 0;
        }

        for (Edge edge : graph[state.node]) {
            if (edge.watts <= state.watts) {
                int newTime = state.time + edge.time;
                int newWatts = state.watts - edge.watts;
                if (bases[edge.to]) {
                    newWatts = 0;
                }

                for (int lantern = 1; lantern <= k; ++lantern) {
                    if (dp[edge.to][lantern][newWatts] > newTime) {
                        dp[edge.to][lantern][newWatts] = newTime;
                        pq.push({edge.to, lantern, newTime, newWatts});
                    }
                }
            }
        }
    }

    return 0;
}
```
