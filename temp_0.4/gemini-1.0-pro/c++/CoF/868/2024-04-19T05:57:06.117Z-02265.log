        int num1 = pairs[i].first;
        int num2 = pairs[i].second;

        while (num1 > 0) {
            int digit = num1 % 10;
            if (freq[digit] - 1 < num1 % 10) {
                can_take = false;
                break;
            }
            num1 /= 10;
        }

        while (num2 > 0) {
            int digit = num2 % 10;
            if (freq[digit] - 1 < num2 % 10) {
                can_take = false;
                break;
            }
            num2 /= 10;
        }

        if (can_take) {
            ans1++;
        }
    }

    fout << ans1 << '\n';

    if (p == 2) {
        int ans2 = 0;
        for (int i = 9; i >= 0; i--) {
            while (freq[i] > 0) {
