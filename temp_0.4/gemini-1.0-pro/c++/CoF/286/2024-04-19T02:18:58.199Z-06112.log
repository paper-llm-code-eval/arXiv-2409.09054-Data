                int length = stations[j].first - stations[i].first;
                if (cost <= length * c) {
                    ans = (ans + 1) % MOD;
                }
            }
        }
        cout << ans << '\n';
    } else {
        // Expres
        vector<vector<long long>> dp(n, vector<long long>(n, -1));

        auto solve = [&](int l, int r) -> long long {
            if (l >= r) {
                return 0;
            }
            if (dp[l][r] != -1) {
                return dp[l][r];
            }

            long long ans = 1;
            for (int i = l + 1; i < r; i++) {
                int cost = stations[i].second;
                int length = stations[r].first - stations[l].first;
