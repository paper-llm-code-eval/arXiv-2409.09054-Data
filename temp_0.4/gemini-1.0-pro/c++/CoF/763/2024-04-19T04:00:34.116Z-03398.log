    for (int j = 0; j < n; j++) {
      board[i][j] = (i + j) % 6;
    }
  }

  int x = 0, y = 0;
  int max_count = 0;
  int max_count_square = 0;
  int total_crumbs = 0;
  vector<vector<int>> count(n, vector<int>(n));
  for (int i = 0; i < k; i++) {
    int dir;
    in >> dir;
    switch (dir) {
      case 1:
        x--;
        y--;
        break;
      case 2:
        y--;
        break;
      case 3:
        x++;
        y--;
        break;
      case 4:
        x--;
        break;
      case 6:
        x++;
        break;
      case 7:
        x--;
        y++;
        break;
      case 8:
        y++;
        break;
      case 5:
        x++;
        y++;
        break;
    }
    total_crumbs += board[x][y];
    board[x][y] = 0;
    count[x][y]++;
    if (count[x][y] > max_count) {
      max_count = count[x][y];
      max_count_square = 1;
    } else if (count[x][y] == max_count) {
      max_count_square++;
    }
  }

  out << total_crumbs << " " << max_count_square << endl;

  return 0;
}
