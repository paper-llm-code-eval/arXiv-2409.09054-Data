#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>
using namespace std;

const int MAXN = 100;
int n, t[MAXN], in[MAXN], c[MAXN], d[MAXN];
vector<int> G[MAXN];

int main() {
    ifstream in("pm.in");
    ofstream out("pm.out");
    in >> n;
    for (int i = 0; i < n; i++) in >> t[i];
    for (int i = 1; i < n; i++) {
        int m; in >> m;
        for (int j = 0; j < m; j++) {
            int x; in >> x;
            G[x - 1].push_back(i);
            in[i]++;
        }
    }
    queue<int> Q;
    for (int i = 0; i < n; i++)
        if (in[i] == 0) {
            Q.push(i);
            c[i] = 0;
        }
    while (!Q.empty()) {
        int u = Q.front();
        Q.pop();
        for (int v : G[u]) {
            in[v]--;
            c[v] = max(c[v], c[u] + t[u]);
            if (in[v] == 0) Q.push(v);
        }
    }
    for (int i = n - 1; i >= 0; i--) {
        d[i] = c[i];
        for (int v : G[i]) d[i] = min(d[i], d[v] - t[i]);
    }
    int ans = 0;
    for (int i = 0; i < n; i++) ans = max(ans, c[i] + t[i]);
    out << ans << '\n';
    for (int i = 0; i < n; i++) out << c[i] << ' ' << d[i] << '\n';
    return 0;
}
