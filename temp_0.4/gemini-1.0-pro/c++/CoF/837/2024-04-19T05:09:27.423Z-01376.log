vector<int> v[maxn];

int main() {
    fin >> n;
    for (int i = 1; i <= n; ++i) {
        fin >> x[i] >> y[i];
        ++a[x[i] + maxn];
        ++b[y[i] + maxn];
        ++c[x[i] - y[i] + maxn];
        ++d[x[i] + y[i] + maxn];
    }
    int nrp = 0;
    for (int i = 0; i < maxn * 2; ++i) {
        nrp = max(nrp, a[i]);
        nrp = max(nrp, b[i]);
        nrp = max(nrp, c[i]);
        nrp = max(nrp, d[i]);
    }
    fout << nrp << '\n';
    int nrd = 0;
    for (int i = 0; i < maxn * 2; ++i) {
        if (a[i] >= 2) ++nrd;
        if (b[i] >= 2) ++nrd;
        if (c[i] >= 2) ++nrd;
        if (d[i] >= 2) ++nrd;
    }
    fout << nrd << '\n';
    int xp = 0;
    for (int i = 1; i <= n; ++i) {
        if (i % 3 == 1) v[x[i]].push_back(y[i]);
    }
    for (int i = 0; i < maxn; ++i) {
        if (v[i].size()) {
            sort(v[i].begin(), v[i].end());
            xp = i;
            fout << i << '\n';
            break;
        }
    }
    return 0;
}
