    ofstream fout("puncte.out");

    int n;
    fin >> n;

    vector<pair<int, int>> points(n);
    for (int i = 0; i < n; i++) {
        fin >> points[i].first >> points[i].second;
    }

    // Sort points by x-coordinate
    sort(points.begin(), points.end());

    // Count the number of points on the x-axis
    int nrp_x = 0;
    for (int i = 0; i < n; i++) {
        if (points[i].second == 0) {
            nrp_x++;
        }
    }

    // Count the number of points on the y-axis
    int nrp_y = 0;
    for (int i = 0; i < n; i++) {
        if (points[i].first == 0) {
            nrp_y++;
        }
    }

    // Count the number of points on lines parallel to the x-axis
    int nrp_parallel_x = 0;
    unordered_map<int, int> freq_x;
    for (int i = 0; i < n; i++) {
        freq_x[points[i].second]++;
    }
    for (auto it : freq_x) {
        nrp_parallel_x = max(nrp_parallel_x, it.second);
    }

    // Count the number of points on lines parallel to the y-axis
    int nrp_parallel_y = 0;
    unordered_map<int, int> freq_y;
    for (int i = 0; i < n; i++) {
        freq_y[points[i].first]++;
    }
    for (auto it : freq_y) {
        nrp_parallel_y = max(nrp_parallel_y, it.second);
    }

    // Find the maximum number of points on a line
    int nrp = max({nrp_x, nrp_y, nrp_parallel_x, nrp_parallel_y});

    // Count the number of distinct lines that pass through at least two points
    int nrd = 0;
    unordered_map<int, vector<int>> lines;
    for (int i = 0; i < n; i++) {
        lines[points[i].first + points[i].second].push_back(points[i].first);
    }
    for (auto it : lines) {
        if (it.second.size() >= 2) {
            nrd++;
        }
    }

    // Count the number of points that are deleted in the third level
    int xp = 0;
    int cnt = 0;
    for (int i = 0; i < n; i++) {
        if (cnt % 3 == 0) {
            xp = points[i].first;
        }
        cnt++;
    }

    // Write the output
    fout << nrp << endl;
    fout << nrd << endl;
    fout << xp << endl;

    return 0;
}
