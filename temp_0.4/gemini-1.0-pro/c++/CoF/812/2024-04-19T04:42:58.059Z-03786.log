#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream fin("litere.in");
  ofstream fout("litere.out");

  int n, m;
  fin >> n >> m;
  string word;
  fin >> word;
  vector<int> codes(m);
  for (int i = 0; i < m; i++) {
    fin >> codes[i];
  }

  int count_vowels = 0;
  int count_consonants = 0;
  int k1 = 0;
  int sum = 0;
  for (int i = 0; i < n; i++) {
    if (word[i] == 'A' || word[i] == 'E' || word[i] == 'I' || word[i] == 'O' ||
        word[i] == 'U') {
      count_vowels++;
    } else {
      count_consonants++;
    }
    k1 = k1 * 10 + codes[word[i] - 'A'];
    sum += codes[word[i] - 'A'];
  }

  fout << count_vowels * count_consonants << '\n';
  fout << k1 << '\n';
  fout << sum << '\n';

  return 0;
}
