#include <fstream>
#include <vector>
#include <cmath>
#include <algorithm>
#include <numeric>

using namespace std;

struct Point {
    int x, y;
    Point(int _x = 0, int _y = 0) : x(_x), y(_y) {}
    Point operator-(const Point& other) const { return Point(x - other.x, y - other.y); }
    bool operator<(const Point& other) const { return x < other.x || (x == other.x && y < other.y); }
};

struct Segment {
    Point p1, p2;
    Segment(const Point& _p1, const Point& _p2) : p1(_p1), p2(_p2) {}
    bool isVertical() const { return p1.x == p2.x; }
    bool isHorizontal() const { return p1.y == p2.y; }
};

int main() {
    ifstream fin("arhitect.in");
    ofstream fout("arhitect.out");

    int n;
    fin >> n;
    vector<Segment> segments(n);
    for (int i = 0; i < n; i++) {
        int x1, y1, x2, y2;
        fin >> x1 >> y1 >> x2 >> y2;
        segments[i] = Segment(Point(x1, y1), Point(x2, y2));
    }

    // Count the number of vertical and horizontal segments
    int verticalCount = 0, horizontalCount = 0;
    for (const Segment& segment : segments) {
        if (segment.isVertical()) {
            verticalCount++;
        } else if (segment.isHorizontal()) {
            horizontalCount++;
        }
    }

    // Output the maximum number of segments that can be aligned
    fout << max(verticalCount, horizontalCount) << endl;

    return 0;
}
