#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream fin("fermier.in");
    ofstream fout("fermier.out");

    int n, c;
    fin >> n >> c;

    vector<int> d(n + 1), q(n + 1);
    fin >> d[0] >> d[1];
    for (int i = 2; i <= n; i++) {
        fin >> d[i] >> q[i];
    }

    vector<int> dist(n + 1, 0);
    int pos = 0, rem = c;

    for (int i = 1; i <= n; i++) {
        int d1 = d[pos], d2 = d[n] + d[n - pos];

        if (d1 < d2) {
            dist[i] += d1;
            pos++;
        } else {
            dist[i] += d2;
            pos = n - pos;
        }

        rem -= q[pos];
        while (rem < q[pos]) {
            pos++;
            rem += c;
        }
    }

    int ans = dist[n] + d[0];
    fout << ans << '\n';

    return 0;
}
```
