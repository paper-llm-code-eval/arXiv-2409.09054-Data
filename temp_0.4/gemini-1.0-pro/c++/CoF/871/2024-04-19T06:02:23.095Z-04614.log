#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>
using namespace std;

const int MAXN = 200;
const int MAXM = 200;
const int MAXT = 400;

ifstream fin("galerie.in");
ofstream fout("galerie.out");

int n, m, t;
int a[MAXT];
int dx[] = {0, 1, 0, -1};
int dy[] = {1, 0, -1, 0};
int dist[MAXN][MAXM];
int viz[MAXN][MAXM];
int sol;

struct Cell {
    int x, y;
    int dir;
    int dist;
};

bool operator<(const Cell &a, const Cell &b) {
    return a.dist > b.dist;
}

void bfs(int x, int y, int dir) {
    priority_queue<Cell> q;
    q.push({x, y, dir, 0});
    viz[x][y] = 1;
    while (!q.empty()) {
        Cell u = q.top();
        q.pop();
        if (u.dist > sol) {
            sol = u.dist;
        }
        int nx = u.x + dx[u.dir];
        int ny = u.y + dy[u.dir];
        if (nx < 1 || nx > n || ny < 1 || ny > m || viz[nx][ny]) {
            continue;
        }
        q.push({nx, ny, u.dir, u.dist + 1});
        viz[nx][ny] = 1;
    }
}

int main() {
    int cerinta;
    fin >> cerinta;
    fin >> n >> m >> t;
    for (int i = 0; i < t; i++) {
        fin >> a[i];
    }
    if (cerinta == 1) {
        for (int i = 0; i < t; i++) {
            int x = (a[i] - 1) / m + 1;
            int y = (a[i] - 1) % m + 1;
            int dir;
            if (x == 1) {
                dir = 2;
            } else if (x == n) {
                dir = 0;
            } else if (y == 1) {
                dir = 3;
            } else {
                dir = 1;
            }
            bfs(x, y, dir);
        }
        fout << sol << "\n";
    } else {
        for (int i = 0; i < t; i++) {
            int x = (a[i] - 1) / m + 1;
            int y = (a[i] - 1) % m + 1;
            int dir;
            if (x == 1) {
                dir = 2;
            } else if (x == n) {
                dir = 0;
            } else if (y == 1) {
                dir = 3;
            } else {
                dir = 1;
            }
            bfs(x, y, dir);
            int x1 = x, y1 = y;
            while (viz[x1][y1]) {
                x1 += dx[dir];
                y1 += dy[dir];
            }
            int x2 = x, y2 = y;
            while (viz[x2][y2]) {
                x2 -= dx[dir];
                y2 -= dy[dir];
            }
            sol = max(sol, abs(x1 - x2) + abs(y1 - y2));
        }
        fout << sol << "\n";
    }
    return 0;
}
