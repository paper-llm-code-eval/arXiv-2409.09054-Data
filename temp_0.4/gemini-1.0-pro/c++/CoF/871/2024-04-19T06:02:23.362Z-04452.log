#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>
#include <cstring>
using namespace std;

ifstream fin("galerie.in");
ofstream fout("galerie.out");

const int MAXN = 205;
const int dx[] = {0, 1, 0, -1};
const int dy[] = {1, 0, -1, 0};
int n, m, t, cerinta;
int a[MAXN][MAXN], nr[MAXN][MAXN];
vector<pair<int, int>> poz;
int maxim;

void citire() {
  fin >> cerinta >> n >> m >> t;
  for (int i = 1; i <= t; ++i) {
    int x;
    fin >> x;
    poz.push_back(make_pair((x - 1) / m + 1, x % m));
  }
}

void bfs() {
  queue<pair<int, int>> q;
  for (auto& p : poz) {
    q.push(p);
    nr[p.first][p.second] = 1;
  }
  while (!q.empty()) {
    int x = q.front().first;
    int y = q.front().second;
    q.pop();
    for (int i = 0; i < 4; ++i) {
      int nx = x + dx[i];
      int ny = y + dy[i];
      if (nx >= 1 && nx <= n && ny >= 1 && ny <= m && !nr[nx][ny]) {
        nr[nx][ny] = nr[x][y] + 1;
        q.push(make_pair(nx, ny));
        maxim = max(maxim, nr[nx][ny]);
      }
    }
  }
}

void dfs(int x, int y, int cnt) {
  a[x][y] = 1;
  for (int i = 0; i < 4; ++i) {
    int nx = x + dx[i];
    int ny = y + dy[i];
    if (nx >= 1 && nx <= n && ny >= 1 && ny <= m && !a[nx][ny] && nr[nx][ny] == cnt) {
      dfs(nx, ny, cnt);
    }
  }
}

int main() {
  citire();
  bfs();
  if (cerinta == 1) {
    fout << maxim - 1 << '\n';
  } else {
    int cnt = 0;
    for (int i = 1; i <= n; ++i) {
      for (int j = 1; j <= m; ++j) {
        if (!a[i][j] && nr[i][j] == maxim) {
          ++cnt;
          dfs(i, j, maxim);
        }
      }
    }
    fout << cnt << '\n';
  }
  return 0;
}
