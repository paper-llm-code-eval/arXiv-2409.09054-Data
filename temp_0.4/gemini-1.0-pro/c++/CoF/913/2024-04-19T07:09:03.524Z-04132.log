                    cnt = 0;
                }
            }
            if (cnt > 0) {
                dp2[i][m] = cnt;
            }
        }
        int ans = 0;
        for (int i = 1; i <= n; ++i) {
            for (int j = 1; j <= m; ++j) {
                if (a[i][j] == 1) {
                    ans += dp[i][j - 1] * dp2[i - 1][j];
                }
            }
        }
        out << ans << '\n';
    } else {
        for (int i = 1; i <= n; ++i) {
            for (int j = 1; j <= m; ++j) {
                if (a[i][j] == 1) {
                    dp[i][j] = dp[i][j - 1] + 1;
