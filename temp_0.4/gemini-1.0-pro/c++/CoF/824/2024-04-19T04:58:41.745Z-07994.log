#include <fstream>
#include <vector>
#include <algorithm>
#include <numeric>

using namespace std;

int main() {
    ifstream in("triunghi.in");
    ofstream out("triunghi.out");
    
    int n;
    in >> n;
    
    vector<vector<int>> triunghi(n);
    
    for (int i = 0; i < n; ++i) {
        int p, v;
        in >> p >> v;
        triunghi[i].resize(p);
        triunghi[i][p - 1] = v;
    }
    
    for (int i = n - 2; i >= 0; --i) {
        for (int j = 0; j < triunghi[i].size(); ++j) {
            triunghi[i][j] += triunghi[i + 1][j] + triunghi[i + 1][j + 1];
        }
    }
    
    for (int i : triunghi[0]) {
        out << i << ' ';
    }
    
    return 0;
}
```
