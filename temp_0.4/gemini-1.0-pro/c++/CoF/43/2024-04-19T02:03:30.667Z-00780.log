#include <fstream>
#include <vector>
#include <algorithm>
#include <cmath>

using namespace std;

const int MAXN = 2000;
const int MAXM = 100000;

struct Point {
    int x, y;
    Point() : x(0), y(0) {}
    Point(int _x, int _y) : x(_x), y(_y) {}
    bool operator<(const Point& other) const {
        if (x != other.x) return x < other.x;
        return y < other.y;
    }
};

struct Line {
    Point p1, p2;
    Line() : p1(), p2() {}
    Line(const Point& _p1, const Point& _p2) : p1(_p1), p2(_p2) {}
};

struct Circle {
    Point center;
    int radius;
    Circle() : center(), radius(0) {}
    Circle(const Point& _center, int _radius) : center(_center), radius(_radius) {}
};

int n, m, q, p;
vector<Circle> circles;
vector<Line> lines;
vector<int> linesOnCircle[MAXN];
vector<int> circlesOnLine[MAXM];

int main() {
    ifstream in("cerc.in");
    ofstream out("cerc.out");

    in >> n;
    circles.resize(n);
    for (int i = 0; i < n; i++) {
        int x, y, r;
        in >> x >> y >> r;
        circles[i] = Circle(Point(x, y), r);
    }

    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if (circles[i].center.x == circles[j].center.x) {
                lines.push_back(Line(circles[i].center, circles[j].center));
            } else if (circles[i].center.y == circles[j].center.y) {
                lines.push_back(Line(circles[i].center, circles[j].center));
            } else {
                double slope = (double)(circles[j].center.y - circles[i].center.y) / (double)(circles[j].center.x - circles[i].center.x);
                int intercept = circles[i].center.y - slope * circles[i].center.x;
                lines.push_back(Line(Point(0, intercept), Point(1000, slope * 1000 + intercept)));
            }
        }
    }

    sort(lines.begin(), lines.end());
    lines.erase(unique(lines.begin(), lines.end()), lines.end());
    m = lines.size();

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (lines[j].p1.x <= circles[i].center.x && circles[i].center.x <= lines[j].p2.x) {
                linesOnCircle[i].push_back(j);
                circlesOnLine[j].push_back(i);
            }
        }
    }

    q = 0;
    p = 0;
    for (int i = 0; i < m; i++) {
        if (circlesOnLine[i].size() > q) {
            q = circlesOnLine[i].size();
            p = 1;
        } else if (circlesOnLine[i].size() == q) {
            p++;
        }
    }

    out << m << " " << q << " " << p << "\n";

    return 0;
}
```
