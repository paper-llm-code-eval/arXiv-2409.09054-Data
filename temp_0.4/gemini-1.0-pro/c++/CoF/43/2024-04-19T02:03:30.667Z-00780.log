struct Point {
    int x, y;
    Point() : x(0), y(0) {}
    Point(int _x, int _y) : x(_x), y(_y) {}
    bool operator<(const Point& other) const {
        if (x != other.x) return x < other.x;
        return y < other.y;
    }
};

struct Line {
    Point p1, p2;
    Line() : p1(), p2() {}
    Line(const Point& _p1, const Point& _p2) : p1(_p1), p2(_p2) {}
};

struct Circle {
    Point center;
    int radius;
    Circle() : center(), radius(0) {}
    Circle(const Point& _center, int _radius) : center(_center), radius(_radius) {}
};

int n, m, q, p;
vector<Circle> circles;
vector<Line> lines;
vector<int> linesOnCircle[MAXN];
vector<int> circlesOnLine[MAXM];

