            if (circles[i].center.x == circles[j].center.x) {
                lines.push_back(Line(circles[i].center, circles[j].center));
            } else if (circles[i].center.y == circles[j].center.y) {
                lines.push_back(Line(circles[i].center, circles[j].center));
            } else {
