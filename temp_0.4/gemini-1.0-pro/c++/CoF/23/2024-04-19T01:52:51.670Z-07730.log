#include <fstream>
#include <vector>
#include <queue>

using namespace std;

int main() {
  ifstream fin("galeti.in");
  ofstream fout("galeti.out");

  int n, e;
  fin >> n >> e;

  vector<int> a(n + 1);
  priority_queue<pair<int, int>> pq;
  for (int i = 1; i <= n; i++) {
    a[i] = 1;
    pq.push({-1, i});
  }

  while (e > 0) {
    pair<int, int> top = pq.top();
    pq.pop();
    int i = top.second;
    if (a[i] == 0) continue;
    int j = pq.top().second;
    int min_a = min(a[i], e);
    a[i] -= min_a;
    a[j] += min_a;
    e -= min_a;
    fout << i << ' ' << j << '\n';
    pq.push({-a[i], i});
    pq.push({-a[j], j});
  }

  return 0;
}
```
