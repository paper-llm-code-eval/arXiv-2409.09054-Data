#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream in("pagini.in");
  ofstream out("pagini.out");

  int n;
  in >> n;

  vector<int> pages(n);
  for (int i = 0; i < n; i++) {
    in >> pages[i];
  }

  sort(pages.begin(), pages.end());

  int num_clips = 0;
  int max_pages = 0;
  int current_page = 0;
  for (int i = 0; i < n; i++) {
    if (pages[i] > current_page + 1) {
      num_clips++;
      current_page = pages[i];
    }
    max_pages = max(max_pages, pages[i] - current_page);
  }

  out << num_clips << endl;
  out << max_pages << endl;

  return 0;
}
```
