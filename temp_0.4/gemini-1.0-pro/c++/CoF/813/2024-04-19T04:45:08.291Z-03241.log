#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("adunscad.in");
    ofstream out("adunscad.out");

    int n, m;
    in >> n >> m;

    vector<int> digits(m);
    for (int i = 0; i < m; i++) {
        in >> digits[i];
    }

    sort(digits.begin(), digits.end());

    int sum = 0;
    string expression;
    for (int i = 0; i < m; i++) {
        if (i > 0) {
            if (sum + digits[i] <= n) {
                expression += '+';
                sum += digits[i];
            } else {
                expression += '-';
                sum -= digits[i];
            }
        } else {
            sum += digits[i];
        }
        expression += to_string(digits[i]);
    }

    if (sum == n) {
        out << expression << endl;
    } else {
        out << 0 << endl;
    }

    return 0;
}
```
