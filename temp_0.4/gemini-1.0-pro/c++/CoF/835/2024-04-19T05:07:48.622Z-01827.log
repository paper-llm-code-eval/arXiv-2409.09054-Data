                ok = 1;
                break;
            }
            y /= 10;
        }
        if (ok) {
            continue;
        }
        int l = poz[ord[i - 1]], r = poz[ord[i]];
        if (l > r) {
            swap(l, r);
        }
        int lc = lca(l, r);
        if (lc == l) {
            swap(l, r);
        }
        if (l == lc) {
            ++l;
        }
        if (r == lc) {
