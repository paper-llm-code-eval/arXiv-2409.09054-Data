#include <fstream>
#include <vector>
#include <queue>
#include <cstring>

using namespace std;

const int MAXN = 1001;
const int MAXM = 1001;

int n, m, labirint[MAXN][MAXM], d[MAXN][MAXM], d0;
vector<pair<int, int>> pozitii[MAXN][MAXM];
queue<pair<int, int>> q;

void bfs() {
    q.push({1, 1});
    d[1][1] = 1;
    while (!q.empty()) {
        pair<int, int> u = q.front();
        q.pop();
        if (u.first == n && u.second == m) {
            d0 = d[u.first][u.second];
            return;
        }
        for (auto& v : pozitii[u.first][u.second]) {
            if (d[v.first][v.second] == 0) {
                d[v.first][v.second] = d[u.first][u.second] + 1;
                q.push(v);
            }
        }
    }
}

int main() {
    ifstream fin("labirint.in");
    ofstream fout("labirint.out");

    fin >> n >> m;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            fin >> labirint[i][j];
            if (labirint[i][j] == 0) {
                if (i > 1) pozitii[i][j].push_back({i - 1, j});
                if (i < n) pozitii[i][j].push_back({i + 1, j});
                if (j > 1) pozitii[i][j].push_back({i, j - 1});
                if (j < m) pozitii[i][j].push_back({i, j + 1});
            }
        }
    }

    bfs();

    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            fout << (d[i][j] < d0);
        }
        fout << '\n';
    }

    return 0;
}
