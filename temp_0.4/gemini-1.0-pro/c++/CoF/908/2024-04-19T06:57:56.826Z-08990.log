                if (b[i] == v[i]) {
                    sumV += j - i + 1;
                    sumR += j - i + 1;
                } else {
                    if (i == 1 && b[i] == b[i + 1]) {
                        ++sumV;
                    } else if (i == 1 && v[i] == b[i + 1]) {
                        ++sumV;
                    } else if (i == 1 && v[i] == b[i + 1] && b[i] == b[i + 2]) {
                        ++sumV;
                    } else if (i == 1 && v[i] == b[i + 1] && b[i] == b[i + 2] && v[i] == b[i + 3]) {
                        ++sumV;
                    }
                    sumV += j - i;
                }
                i = j - 1;
            } else {
                int j = i;
                while (j <= n && (b[j] == b[i] || v[j] == b[i])) {
                    ++j;
                }
                if (b[i] == v[i]) {
                    sumV += j - i + 1;
                    sumR += j - i + 1;
                } else {
                    sumR += j - i;
                }
                i = j - 1;
            }
        }
        if (sumV > sumR) {
            out << 'V' << '\n' << sumV;
        } else if (sumV < sumR) {
            out << 'R' << '\n' << sumR;
        } else {
            out << 'V' << ' ' << 'R' << '\n' << sumV;
        }
    }
    return 0;
