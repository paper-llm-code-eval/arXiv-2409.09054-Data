#include <fstream>
#include <vector>
#include <algorithm>
using namespace std;

ifstream in("triunghiuri.in");
ofstream out("triunghiuri.out");

const int MOD = 1000003;
const int MAXN = 100005;

vector<int> v[MAXN];
int fr[MAXN];
int main() {
    int p, n;
    in >> p >> n;
    for (int i = 1; i <= n; i++) {
        int x, y;
        in >> x >> y;
        v[x].push_back(y);
        fr[x]++;
    }
    if (p == 1) {
        int ans = 0;
        for (int i = 0; i < MAXN; i++) {
            ans = max(ans, fr[i]);
        }
        out << ans << '\n';
    } else {
        long long ans = 0;
        for (int i = 0; i < MAXN; i++) {
            if (v[i].size() < 3)
                continue;
            sort(v[i].begin(), v[i].end());
            for (int j = 0; j < v[i].size(); j++) {
                int st = j + 1, dr = v[i].size() - 1;
                while (st < dr) {
                    if (v[i][j] + v[i][st] + v[i][dr] > i) {
                        ans += dr - st;
                        dr--;
                    } else {
                        st++;
                    }
                }
            }
        }
        out << ans % MOD << '\n';
    }
    return 0;
}
