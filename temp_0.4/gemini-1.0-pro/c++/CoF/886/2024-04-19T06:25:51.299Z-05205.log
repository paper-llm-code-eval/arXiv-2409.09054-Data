#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("triunghiuri.in");
ofstream fout("triunghiuri.out");

const int MOD = 1000003;

int main() {
    int p, n;
    fin >> p >> n;

    vector<int> x(n), y(n);
    for (int i = 0; i < n; i++) {
        fin >> x[i] >> y[i];
    }

    if (p == 1) {
        sort(x.begin(), x.end());
        int ans = 1;
        for (int i = 1; i < n; i++) {
            if (x[i] == x[i - 1]) {
                ans++;
            } else {
                break;
            }
        }
        fout << ans << '\n';
    } else {
        sort(x.begin(), x.end());
        sort(y.begin(), y.end());

        int ans = 0;
        for (int i = 0; i < n; i++) {
            for (int j = i + 1; j < n; j++) {
                if (x[j] - x[i] > y[j] - y[i]) {
                    ans += n - j - 1;
                }
            }
        }
        fout << ans % MOD << '\n';
    }

    return 0;
}
