#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream in("triunghiuri.in");
ofstream out("triunghiuri.out");

const int MOD = 1e9 + 3;

int main() {
    int p, n;
    in >> p >> n;

    vector<int> x(n), y(n);
    for (int i = 0; i < n; i++) {
        in >> x[i] >> y[i];
    }

    if (p == 1) {
        sort(x.begin(), x.end());
        int mx = 1;
        int cnt = 1;
        for (int i = 1; i < n; i++) {
            if (x[i] == x[i - 1]) {
                cnt++;
            } else {
                mx = max(mx, cnt);
                cnt = 1;
            }
        }
        mx = max(mx, cnt);
        out << mx << '\n';
    } else {
        sort(y.begin(), y.end());
        long long ans = 0;
        for (int i = 0; i < n; i++) {
            int j = i + 1;
            while (j < n && y[j] == y[i]) {
                j++;
            }
            ans += (long long)(j - i) * (j - i - 1) / 2;
            i = j - 1;
        }
        out << ans % MOD << '\n';
    }

    return 0;
}
