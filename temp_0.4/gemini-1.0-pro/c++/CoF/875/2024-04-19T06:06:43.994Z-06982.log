  q.push({sx, sy});
  dist[sx][sy][0] = 0;

  while (!q.empty()) {
    int x = q.front().first;
    int y = q.front().second;
    int steps = dist[x][y][0];
    q.pop();

    for (int i = 0; i < k; ++i) {
      if (dist[x][y][i] == -1) {
        dist[x][y][i] = steps;
        q.push({x, y});
        break;
      }
    }

    for (int i = 0; i < 4; ++i) {
      int nx = x + dx[i];
      int ny = y + dy[i];

      if (nx < 0 || nx >= n || ny < 0 || ny >= m || grid[nx][ny] == '#') {
        continue;
      }

      if (grid[nx][ny] != '_' && grid[nx][ny] != grid[x][y]) {
        bool ok = false;
        for (int j = 0; j < k; ++j) {
          if (grid[nx][ny] == cuv[j] && dist[nx][ny][j] != -1) {
            ok = true;
            break;
          }
        }
        if (!ok) {
          continue;
        }
      }

      for (int j = 0; j < k; ++j) {
        if (dist[nx][ny][j] == -1 || dist[nx][ny][j] > steps + 1) {
          dist[nx][ny][j] = steps + 1;
          q.push({nx, ny});
        }
      }
    }
  }

  int min_steps = -1;
  for (int i = 0; i < k; ++i) {
    if (dist[sx][sy][i] != -1) {
      if (min_steps == -1 || dist[sx][sy][i] < min_steps) {
        min_steps = dist[sx][sy][i];
      }
    }
  }

  return min_steps;
}

int main() {
  ifstream fin("miting.in");
  ofstream fout("miting.out");

  fin >> p;
  fin >> n >> m;
  fin >> cuv;

  k = cuv.size();

  for (int i = 0; i < n; ++i) {
    for (int j = 0; j < m; ++j) {
      fin >> grid[i][j];
      if (grid[i][j] != '#' && grid[i][j] != '_') {
        start_pos.push_back({i, j});
      }
    }
  }

  int min_area = n * m;
  if (p == 1) {
    for (int i = 0; i < n; ++i) {
      for (int j = 0; j < m; ++j) {
        if (grid[i][j] != '#' && grid[i][j] != '_') {
          int area = 0;
          for (int x = 0; x < n; ++x) {
            for (int y = 0; y < m; ++y) {
              if (grid[x][y] != '#' && grid[x][y] != '_') {
                ++area;
              }
            }
          }

          bool ok = true;
          for (const auto& pos : start_pos) {
            if (i <= pos.first && pos.first < i + area && j <= pos.second && pos.second < j + area) {
              ok = false;
              break;
            }
          }

          if (ok && area < min_area) {
            min_area = area;
          }
        }
      }
    }

    fout << min_area << '\n';
  } else {
    for (int i = 0; i < n; ++i) {
      for (int j = 0; j < m; ++j) {
        for (int l = 0; l < k; ++l) {
          dist[i][j][l] = -1;
        }
      }
    }

    int min_steps = -1;
    for (const auto& pos : start_pos) {
      int steps = bfs(pos.first, pos.second);
      if (steps != -1) {
        if (min_steps == -1 || steps < min_steps) {
          min_steps = steps;
        }
      }
    }

    fout << min_steps << '\n';
  }

  return 0;
}
