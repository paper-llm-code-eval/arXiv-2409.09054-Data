#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>

using namespace std;

const int dx[] = {-1, 0, 1, 0};
const int dy[] = {0, 1, 0, -1};

int n, m, k, p;
string cuv;
vector<vector<char>> mat;
vector<pair<int, int>> poz;
vector<vector<int>> dist;
vector<vector<int>> viz;
vector<bool> in_cuv;

bool valid(int x, int y) {
    return x >= 0 && x < n && y >= 0 && y < m && mat[x][y] != '#';
}

int bfs(int x, int y, int lit) {
    queue<pair<int, int>> q;
    q.push({x, y});
    dist[x][y] = 0;
    viz[x][y] = lit;
    while (!q.empty()) {
        auto [cx, cy] = q.front();
        q.pop();
        for (int i = 0; i < 4; ++i) {
            int nx = cx + dx[i];
            int ny = cy + dy[i];
            if (valid(nx, ny) && viz[nx][ny] != lit) {
                viz[nx][ny] = lit;
                dist[nx][ny] = dist[cx][cy] + 1;
                q.push({nx, ny});
            }
        }
    }
    int mx = 0;
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < m; ++j) {
            if (viz[i][j] == lit) {
                mx = max(mx, dist[i][j]);
            }
        }
    }
    return mx;
}

int main() {
    ifstream in("miting.in");
    ofstream out("miting.out");
    in >> p;
    in >> n >> m;
    in >> cuv;
    k = cuv.size();
    mat.resize(n, vector<char>(m));
    poz.resize(k);
    dist.resize(n, vector<int>(m, -1));
    viz.resize(n, vector<int>(m, -1));
    in_cuv.resize(26, false);
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < m; ++j) {
            in >> mat[i][j];
            if (mat[i][j] != '#' && mat[i][j] != '_') {
                poz[mat[i][j] - 'A'] = {i, j};
                in_cuv[mat[i][j] - 'A'] = true;
            }
        }
    }
    if (p == 1) {
        int mx = 0;
        for (int i = 0; i < k; ++i) {
            if (in_cuv[i]) {
                int x = poz[i].first;
                int y = poz[i].second;
                mx = max(mx, bfs(x, y, i));
            }
        }
        out << (mx + 1) * (mx + 1) << '\n';
    } else {
        vector<int> dist_lit(k, -1);
        for (int i = 0; i < k; ++i) {
            if (in_cuv[i]) {
                int x = poz[i].first;
                int y = poz[i].second;
                dist_lit[i] = bfs(x, y, i);
            }
        }
        int ans = -1;
        for (int i = 0; i < k; ++i) {
            for (int j = i + 1; j < k; ++j) {
                if (dist_lit[i] != -1 && dist_lit[j] != -1) {
                    int x1 = poz[i].first;
                    int y1 = poz[i].second;
                    int x2 = poz[j].first;
                    int y2 = poz[j].second;
                    if (mat[x1][y1] == mat[x2][y2]) {
                        continue;
                    }
                    if (cuv[i] == cuv[j] - 1 || cuv[i] == cuv[j] + 1) {
                        ans = max(ans, dist_lit[i] + dist_lit[j] + 1);
                    }
                }
            }
        }
        out << ans << '\n';
    }
    return 0;
}
