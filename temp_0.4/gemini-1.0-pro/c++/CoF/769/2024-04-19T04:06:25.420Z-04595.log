#include <fstream>
#include <string>
#include <vector>
#include <algorithm>

using namespace std;

ifstream in("dir.in");
ofstream out("dir.out");

struct Folder {
    string name;
    vector<Folder> subfolders;
    vector<string> files;
};

vector<string> paths;

void dfs(Folder& folder, string path) {
    for (auto& subfolder : folder.subfolders) {
        dfs(subfolder, path + folder.name + "\\");
    }
    for (auto& file : folder.files) {
        paths.push_back(path + folder.name + "\\" + file);
    }
}

int main() {
    string line;
    getline(in, line);

    Folder root;
    int pos = 0;
    while (pos < line.size()) {
        if (line[pos] == '(') {
            int start = pos + 1;
            int depth = 1;
            while (depth > 0) {
                pos++;
                if (line[pos] == '(') {
                    depth++;
                } else if (line[pos] == ')') {
                    depth--;
                }
            }
            string subfolderName = line.substr(start, pos - start);
            root.subfolders.push_back({subfolderName});
        } else if (line[pos] == ',') {
            pos++;
        } else {
            int start = pos;
            while (pos < line.size() && line[pos] != ',' && line[pos] != ')') {
                pos++;
            }
            string fileName = line.substr(start, pos - start);
            root.files.push_back(fileName);
        }
        pos++;
    }

    dfs(root, "");

    sort(paths.begin(), paths.end());

    out << paths.size() << endl;
    for (auto& path : paths) {
        out << path << endl;
    }

    return 0;
}
