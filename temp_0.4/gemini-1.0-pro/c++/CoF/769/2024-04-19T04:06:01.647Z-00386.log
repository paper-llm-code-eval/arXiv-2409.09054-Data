                subfolders_str = subfolders_str.substr(subfolders_str.find(',') + 1);
            } else {
                subfolders_str.clear();
            }
        }
    } else {
        folder->name = line;
    }
}

void get_file_paths(Folder* folder, vector<string>& file_paths) {
    for (Folder* subfolder : folder->subfolders) {
        get_file_paths(subfolder, file_paths);
    }
    
    for (string file : folder->files) {
        string file_path = folder->name + "\\" + file;
        file_paths.push_back(file_path);
    }
}

