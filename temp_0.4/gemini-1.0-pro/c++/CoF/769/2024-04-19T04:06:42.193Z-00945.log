                    files.push_back(current_path + file);
                    file = "";
                } else {
                    file += s[j];
                }
                j++;
            }
            files.push_back(current_path + file);
            is_folder[current_path + file] = true;
            current_path += file + "\\";
        } else if (s[i] == ')') {
            while (s[i] == ')') {
                current_path = current_path.substr(0, current_path.size() - 1);
                current_path = current_path.substr(0, current_path.find_last_of('\\') + 1);
                i++;
            }
            i--;
        }
    }
    sort(files.begin(), files.end());
    fout << files.size() << endl;
    for (int i = 0; i < files.size(); i++) {
        if (!is_folder[files[i]]) {
            fout << files[i] << endl;
        }
    }
    return 0;
}
