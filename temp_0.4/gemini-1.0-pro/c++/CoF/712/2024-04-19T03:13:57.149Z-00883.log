#include <fstream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
  ifstream in("numere.in");
  ofstream out("numere.out");
  vector<int> v(10);
  for (int &x : v) in >> x;
  sort(v.begin(), v.end());
  int sum = 0;
  for (int x : v) sum += x;
  int best = 0, best_a = 0, best_b = 0;
  for (int i = 0; i < (1 << 10); i++) {
    int a = 0, b = 0;
    for (int j = 0; j < 10; j++) {
      if (i & (1 << j))
        a += v[j];
      else
        b += v[j];
    }
    if (a == b && a > best) {
      best = a;
      best_a = i;
      best_b = ~i & ((1 << 10) - 1);
    }
  }
  vector<int> va, vb;
  for (int j = 0; j < 10; j++) {
    if (best_a & (1 << j))
      va.push_back(v[j]);
    else
      vb.push_back(v[j]);
  }
  out << (sum - best) / 2 << ' ' << best << '\n';
  for (int x : va) out << x << ' ';
  out << '\n';
  for (int x : vb) out << x << ' ';
  return 0;
}
