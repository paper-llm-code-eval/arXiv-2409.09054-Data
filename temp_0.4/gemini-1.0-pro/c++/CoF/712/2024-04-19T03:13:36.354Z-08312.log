#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream in("numere.in");
  ofstream out("numere.out");
  vector<int> v(10);
  for (int &x : v) in >> x;
  sort(v.begin(), v.end());
  int best_sum = 0, best_a = 0, best_b = 0;
  int cnt = 0;
  for (int i = 0; i < (1 << 10); i++) {
    int sum_a = 0, sum_b = 0, a = 0, b = 0;
    for (int j = 0; j < 10; j++) {
      if ((i >> j) & 1) {
        sum_a += v[j];
        a++;
      } else {
        sum_b += v[j];
        b++;
      }
    }
    if (sum_a == sum_b) {
      cnt++;
      if (sum_a > best_sum) {
        best_sum = sum_a;
        best_a = a;
        best_b = b;
      }
    }
  }
  out << cnt << ' ' << best_sum << '\n';
  for (int i = 0; i < 10; i++) {
    if ((i >> best_a) & 1) out << v[i] << ' ';
  }
  out << '\n';
  for (int i = 0; i < 10; i++) {
    if (!((i >> best_a) & 1)) out << v[i] << ' ';
  }
  out << '\n';
  return 0;
}
```
