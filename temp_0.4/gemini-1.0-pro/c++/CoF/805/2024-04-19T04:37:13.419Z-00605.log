            }
        } else {
            if (v[x] > max(st_sum, dr_sum)) {
                nivmax = st_sum + v[x];
                fout << nivmax - 1 << '\n';
                if (st[x]) {
                    fout << st[x] << ' ' << 'S' << '\n' << nivmax - st[x] - 1 << ' ' << 'D' << '\n';
                } else {
                    fout << 0 << ' ' << 'S' << '\n' << nivmax - 1 << ' ' << 'D' << '\n';
                }
            } else {
                nivmax = st_sum + v[x];
                fout << nivmax << '\n';
                if (st[x]) {
                    fout << st[x] << ' ' << 'S' << '\n' << nivmax - st[x] << ' ' << 'D' << '\n';
                } else {
                    fout << 0 << ' ' << 'S' << '\n' << nivmax << ' ' << 'D' << '\n';
                }
            }
        }
    }
    return 0;
}
