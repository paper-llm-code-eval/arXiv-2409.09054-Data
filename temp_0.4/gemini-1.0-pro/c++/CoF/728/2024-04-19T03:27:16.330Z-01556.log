#include <fstream>
#include <iostream>

using namespace std;

int main() {
  ifstream fin("vanatoare.in");
  ofstream fout("vanatoare.out");

  long long ka, kb, ramuri = 1, sageti_albe = 0, sageti_negre = 0, ramuri_doborate = 0;

  fin >> ka >> kb;
  fin.close();

  while (ramuri <= ka && ramuri <= kb) {
    if (ramuri % 2 == 1) {
      sageti_albe += ramuri;
      if (sageti_albe > ka) break;
    } else {
      sageti_negre += ramuri;
      if (sageti_negre > kb) break;
    }
    ramuri_doborate++;
    ramuri = ramuri * 2 + 1;
  }

  fout << ramuri_doborate << endl;
  fout << ka - sageti_albe << endl;
  fout << kb - sageti_negre << endl;
  fout.close();

  return 0;
}
```
