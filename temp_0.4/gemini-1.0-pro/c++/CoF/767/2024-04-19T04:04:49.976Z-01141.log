#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAXN = 1000;
const int MAXC = 5;

struct Point {
    int x, y, c;
};

int n, maxC;
Point points[MAXN];
vector<int> colors[MAXN][MAXN];

int main() {
    ifstream fin("dreptc.in");
    ofstream fout("dreptc.out");

    fin >> n >> maxC;
    for (int i = 0; i < n; i++) {
        fin >> points[i].x >> points[i].y >> points[i].c;
        colors[points[i].x][points[i].y].push_back(points[i].c);
    }

    int ans = 0;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            sort(colors[i][j].begin(), colors[i][j].end());
            int cnt = 0;
            for (int k = 0; k < colors[i][j].size(); k++) {
                if (k == 0 || colors[i][j][k] != colors[i][j][k - 1]) {
                    cnt++;
                }
            }
            ans += cnt * (cnt - 1) / 2;
        }
    }

    fout << ans << '\n';
    return 0;
}
```
