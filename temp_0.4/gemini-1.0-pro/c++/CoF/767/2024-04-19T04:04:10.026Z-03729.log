#include <fstream>
#include <vector>
#include <algorithm>
using namespace std;

ifstream in("dreptc.in");
ofstream out("dreptc.out");

const int NMAX = 1000;
const int CMAX = 5;

int n, maxc;
vector<pair<int, int>> p[NMAX + 1][CMAX + 1];
int dp[NMAX + 1][NMAX + 1][CMAX + 1];

int main() {
    in >> n >> maxc;
    for (int i = 1; i <= n; ++i) {
        int x, y, c;
        in >> x >> y >> c;
        p[x][c].push_back(make_pair(y, i));
    }
    for (int c = 1; c <= maxc; ++c) {
        for (int i = 0; i <= NMAX; ++i) {
            sort(p[i][c].begin(), p[i][c].end());
        }
