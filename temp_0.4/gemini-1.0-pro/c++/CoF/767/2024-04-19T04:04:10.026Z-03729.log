    for (int c = 1; c <= maxc; ++c) {
        for (int i = 0; i <= NMAX; ++i) {
            for (int j = 0; j < p[i][c].size(); ++j) {
                int y = p[i][c][j].first;
                int id = p[i][c][j].second;
                for (int k = 0; k < j; ++k) {
                    int y2 = p[i][c][k].first;
                    int id2 = p[i][c][k].second;
                    dp[id][id2][c] = max(dp[id][id2][c], dp[id][id2][c - 1]);
                    for (int l = 0; l <= NMAX; ++l) {
                        dp[id][id2][c] = max(dp[id][id2][c], dp[id][l][c - 1] + dp[l][id2][c - 1]);
                    }
                    dp[id][id2][c] += 1;
                }
            }
        }
    }
    int ans = 0;
    for (int c = 1; c <= maxc; ++c) {
        for (int i = 0; i <= n; ++i) {
