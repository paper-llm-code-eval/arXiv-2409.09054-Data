#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAXN = 1000;
const int MAXC = 5;

struct Point {
    int x, y, c;
};

int n, maxc;
vector<Point> points[MAXC + 1];

int main() {
    ifstream in("dreptc.in");
    ofstream out("dreptc.out");

    in >> n >> maxc;
    for (int i = 0; i < n; i++) {
        int x, y, c;
        in >> x >> y >> c;
        points[c].push_back({x, y, c});
    }

    int ans = 0;
    for (int c = 1; c <= maxc; c++) {
        sort(points[c].begin(), points[c].end(), [](const Point& a, const Point& b) {
            return a.x < b.x;
        });

        for (int i = 0; i < (int)points[c].size(); i++) {
            for (int j = i + 1; j < (int)points[c].size(); j++) {
                int x1 = points[c][i].x;
                int y1 = points[c][i].y;
                int x2 = points[c][j].x;
                int y2 = points[c][j].y;

                int cnt = 0;
                for (int k = 0; k < (int)points[c].size(); k++) {
                    if (points[c][k].x == x1 && points[c][k].y == y2) cnt++;
                    if (points[c][k].x == x2 && points[c][k].y == y1) cnt++;
                }

                if (cnt == 2) ans++;
            }
        }
    }

    out << ans << '\n';

    return 0;
}
