#include <fstream>
#include <vector>
#include <algorithm>
#include <utility>

using namespace std;

ifstream fin("dreptc.in");
ofstream fout("dreptc.out");

const int MAXN = 1000;
const int MAXC = 5;

int N, MaxC;
vector<pair<int, int>> puncte[MAXN + 1];
int dp[MAXN + 1][MAXN + 1][MAXC + 1];

int main() {
    fin >> N >> MaxC;
    for (int i = 0; i < N; i++) {
        int x, y, c;
        fin >> x >> y >> c;
        puncte[c].push_back(make_pair(x, y));
    }

    for (int c = 1; c <= MaxC; c++) {
        sort(puncte[c].begin(), puncte[c].end());
        for (int i = 0; i < puncte[c].size(); i++) {
            for (int j = i + 1; j < puncte[c].size(); j++) {
                dp[i][j][c] = 1;
            }
        }
    }

    for (int c = 1; c <= MaxC; c++) {
        for (int i = 0; i < puncte[c].size(); i++) {
            for (int j = i + 1; j < puncte[c].size(); j++) {
                for (int k = 0; k < puncte[c].size(); k++) {
                    if (k == i || k == j) {
                        continue;
                    }
                    if (puncte[c][i].first < puncte[c][k].first && puncte[c][k].first < puncte[c][j].first &&
                        puncte[c][i].second < puncte[c][k].second && puncte[c][k].second < puncte[c][j].second) {
                        dp[i][j][c] = max(dp[i][j][c], dp[i][k][c] + dp[k][j][c]);
                    }
                }
            }
        }
    }

    int ans = 0;
    for (int c = 1; c <= MaxC; c++) {
        for (int i = 0; i < puncte[c].size(); i++) {
            for (int j = i + 1; j < puncte[c].size(); j++) {
                ans = max(ans, dp[i][j][c]);
            }
        }
    }

    fout << ans << '\n';
    return 0;
}
