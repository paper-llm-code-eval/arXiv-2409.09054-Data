#include <fstream>
#include <algorithm>
#include <vector>

using namespace std;

int main() {
  ifstream in("cifre.in");
  ofstream out("cifre.out");

  int a, b;
  in >> a >> b;

  vector<bool> v(10, false);
  while (a > 0) {
    v[a % 10] = true;
    a /= 10;
  }
  while (b > 0) {
    v[b % 10] = true;
    b /= 10;
  }

  vector<int> cifre;
  for (int i = 0; i < 10; i++) {
    if (v[i]) {
      cifre.push_back(i);
    }
  }

  if (cifre.empty()) {
    out << "-1" << endl;
  } else {
    for (int i = 0; i < cifre.size(); i++) {
      out << cifre[i] << " ";
    }
    out << endl;
  }

  vector<int> digits;
  while (a > 0) {
    digits.push_back(a % 10);
    a /= 10;
  }
  while (b > 0) {
    digits.push_back(b % 10);
    b /= 10;
  }

  sort(digits.begin(), digits.end(), greater<int>());

  for (int i = 0; i < digits.size(); i++) {
    out << digits[i];
  }
  out << endl;

  return 0;
}
```
