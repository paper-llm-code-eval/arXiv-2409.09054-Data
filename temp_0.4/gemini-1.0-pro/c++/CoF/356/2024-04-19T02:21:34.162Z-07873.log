#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream fin("cochilie.in");
  ofstream fout("cochilie.out");
  int c;
  fin >> c;
  if (c == 1) {
    int n;
    fin >> n;
    fout << 2 * n - 1 << ' ' << n << '\n';
  } else {
    int n, p;
    fin >> n >> p;
    vector<vector<int>> m(2 * n - 1, vector<int>(n));
    int x = 1, y = 1, val = 1;
    while (val <= n) {
      while (y <= n && m[x - 1][y - 1] == 0) { m[x - 1][y - 1] = val; y++; }
      y--;
      while (x <= 2 * n - 1 && m[x - 1][y - 1] == 0) { m[x - 1][y - 1] = val; x++; }
      x--;
      while (y >= 1 && m[x - 1][y - 1] == 0) { m[x - 1][y - 1] = val; y--; }
      y++;
      while (x >= 1 && m[x - 1][y - 1] == 0) { m[x - 1][y - 1] = val; x--; }
      x++;
      val++;
    }
    for (int i = 0; i < n; i++) { fout << m[p - 1][i] << ' '; }
  }
  return 0;
}
