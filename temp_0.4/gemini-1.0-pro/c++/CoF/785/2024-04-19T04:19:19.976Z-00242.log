#include <fstream>
#include <algorithm>

using namespace std;

ifstream fin("inimioare.in");
ofstream fout("inimioare.out");

const int MAXN = 10005;
int n, v[MAXN][4], w[MAXN][4], dp[MAXN][MAXN][2];

int main() {
    fin >> n;
    for (int i = 1; i <= n; ++i)
        for (int j = 0; j < 4; ++j)
            fin >> v[i][j];
    for (int i = 1; i <= n; ++i)
        for (int j = 0; j < 4; ++j)
            fin >> w[i][j];
    for (int i = 1; i <= n; ++i)
