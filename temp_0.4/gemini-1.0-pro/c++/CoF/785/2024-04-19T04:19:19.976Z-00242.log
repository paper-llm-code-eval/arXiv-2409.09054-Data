            dp[i][j][0] = max(dp[i - 1][j][0], dp[i][j - 1][0]);
            dp[i][j][1] = max(dp[i - 1][j][1], dp[i][j - 1][1]);
            for (int k = 0; k < 4; ++k)
                for (int l = 0; l < 4; ++l) {
                    int sum = v[i][k] * w[j][l];
                    if (k == l)
                        sum /= 2;
                    dp[i][j][0] = max(dp[i][j][0], dp[i - 1][j - 1][1] + sum);
                    dp[i][j][1] = max(dp[i][j][1], dp[i - 1][j - 1][0] + sum);
                }
        }
    fout << dp[n][n][0] << '\n';
    return 0;
}
