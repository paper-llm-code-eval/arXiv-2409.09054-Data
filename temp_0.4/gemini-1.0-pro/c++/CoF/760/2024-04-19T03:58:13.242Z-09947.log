#include <fstream>
#include <vector>
#include <algorithm>

int main() {
    std::ifstream in("telecabina.in");
    std::ofstream out("telecabina.out");

    int n;
    in >> n;

    std::vector<std::pair<int, int>> stations(n);
    for (auto& s : stations) {
        in >> s.first >> s.second;
    }

    std::sort(stations.begin(), stations.end());

    int people = 0;
    int fuel = 0;
    int special = 0;
    bool going_up = true;
    for (auto& s : stations) {
        if (going_up) {
            people += s.second;
            fuel += s.first * 3;
        } else {
            fuel -= s.first;
        }
        if (s.second == 0) {
            going_up = !going_up;
            special++;
        }
    }

    out << people << '\n';
    out << fuel << '\n';
    out << special << '\n';

    return 0;
}
```
