#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAX_K = 100;
const int MAX_R = 20;
const int MAX_D = 360;
const int MAX_T = 10;
const int MAX_U = 5;

int main() {
  ifstream in("popas.in");
  ofstream out("popas.out");

  int k;
  in >> k;

  vector<pair<int, vector<int>>> trasee(k);
  for (int i = 0; i < k; ++i) {
    int r;
    in >> trasee[i].first >> r;
    trasee[i].second.resize(r);
    for (int j = 0; j < r; ++j) {
      in >> trasee[i].second[j];
    }
  }

  int t, u;
  in >> t >> u;

  int min_popasuri = INT_MAX;
  int traseu_ales = -1;
  for (const auto& traseu : trasee) {
    int popasuri = 0;
    int distanta_ramas = traseu.second.back() + 1;
    while (distanta_ramas > 0) {
      int distanta_max = distanta_ramas - u;
      auto it = upper_bound(traseu.second.begin(), traseu.second.end(), distanta_max);
      if (it == traseu.second.begin()) {
        popasuri = -1;
        break;
      }
      --it;
      distanta_ramas = *it - t;
      ++popasuri;
    }
    if (popasuri >= 0 && popasuri < min_popasuri) {
      min_popasuri = popasuri;
      traseu_ales = traseu.first;
    }
  }

  if (traseu_ales == -1) {
    out << 0 << endl;
