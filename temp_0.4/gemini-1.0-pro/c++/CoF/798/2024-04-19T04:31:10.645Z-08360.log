#include <fstream>

using namespace std;

int main() {
  ifstream in("submit.in");
  ofstream out("submit.out");
  int n, p[101], b, m, a[101][101], penalizare = 0;
  in >> n;
  for (int i = 1; i <= n; i++) in >> p[i];
  in >> b;
  in >> m;
  for (int i = 1; i <= m; i++) {
    for (int j = 1; j <= n; j++) {
      in >> a[i][j];
    }
  }
  int maxim = 0;
  for (int i = 1; i <= m; i++) {
    int suma = 0;
    for (int j = 1; j <= n; j++) {
      if (a[i][j] == 1) suma += p[j];
    }
    if (suma == n * p[1]) suma += b;
    suma -= penalizare;
    if (suma > maxim) maxim = suma;
    penalizare += 2;
  }
  out << maxim;
  return 0;
}
