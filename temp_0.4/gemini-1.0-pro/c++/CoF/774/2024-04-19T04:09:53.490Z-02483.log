#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream fin("turnuri.in");
  ofstream fout("turnuri.out");

  int m, n;
  fin >> m >> n;
  vector<vector<int>> mat(m, vector<int>(n));
  vector<int> heights(n);
  for (int i = 0; i < m; i++) {
    for (int j = 0; j < n; j++) {
      fin >> mat[i][j];
