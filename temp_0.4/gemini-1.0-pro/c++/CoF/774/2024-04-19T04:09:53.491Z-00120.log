#include <fstream>
#include <vector>
using namespace std;

ifstream fin("turnuri.in");
ofstream fout("turnuri.out");

const int NMAX = 1000;
int n, m, a[NMAX][NMAX], h[NMAX], hmax, cntmax;

void read() {
    fin >> n >> m;
    for (int i = 1; i <= m; ++i) {
        for (int j = 1; j <= n; ++j) {
            fin >> a[i][j];
            if (a[i][j]) h[j]++;
        }
    }
}

void solve() {
    vector<int> v;
    for (int i = 1; i <= n; ++i) {
        if (h[i]) v.push_back(h[i]);
    }
    for (int i = 0; i < v.size(); ++i) {
        for (int j = i + 1; j < v.size(); ++j) {
            int sum = v[i] + v[j];
            if (sum > m) continue;
            if (sum > hmax) {
                hmax = sum;
                cntmax = 1;
            } else if (sum == hmax) {
                cntmax++;
            }
            v.erase(v.begin() + j);
            v.erase(v.begin() + i);
            i--;
            break;
        }
    }
}

void print() {
    for (int i = 1; i <= n; ++i) {
        fout << h[i] << ' ';
    }
    fout << '\n';
    fout << hmax << '\n';
    fout << cntmax << '\n';
}

int main() {
    read();
    solve();
    print();
    return 0;
}
