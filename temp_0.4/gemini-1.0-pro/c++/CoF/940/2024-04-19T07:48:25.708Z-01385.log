#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream in("tort.in");
ofstream out("tort.out");

const int MOD = 1e9 + 7;
const int MAXN = 2e5 + 1;
const int MAXSUM = 4e5 + 1;

vector<int> a(MAXN);
int dp[MAXN][MAXSUM];
int sum[MAXN];

int main() {
    int n;
    in >> n;

    for (int i = 1; i <= n; i++) {
        in >> a[i];
        sum[i] = sum[i - 1] + a[i];
    }

    dp[0][0] = 1;
    for (int i = 1; i <= n; i++) {
        for (int j = 0; j < MAXSUM; j++) {
            dp[i][j] = dp[i - 1][j];
            if (j >= a[i]) {
                dp[i][j] = (dp[i][j] + dp[i - 1][j - a[i]]) % MOD;
            }
        }
    }

    int ans = 0;
    for (int i = 1; i <= n; i++) {
        ans = (ans + dp[i - 1][sum[i] / 2]) % MOD;
    }

    out << ans << '\n';

    return 0;
}
```
