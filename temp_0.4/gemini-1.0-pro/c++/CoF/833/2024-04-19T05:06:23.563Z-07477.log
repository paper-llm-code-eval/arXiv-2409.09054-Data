#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("cladiri.in");
    ofstream out("cladiri.out");

    int n;
    in >> n;

    vector<vector<int>> cladiri(n);
    for (int i = 0; i < n; i++) {
        int x;
        in >> x;
        while (x > 0) {
            cladiri[i].push_back(x % 10);
            x /= 10;
        }
    }

    int max_turn = 0, nr_max_turn = 0;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < cladiri[i].size(); j++) {
            if (cladiri[i][j] > max_turn) {
                max_turn = cladiri[i][j];
                nr_max_turn = 1;
            } else if (cladiri[i][j] == max_turn) {
                nr_max_turn++;
            }
        }
    }

    int nr_bilete_identice = 0;
    for (int i = 0; i < n; i++) {
        reverse(cladiri[i].begin(), cladiri[i].end());
        if (cladiri[i] == cladiri[n - i - 1]) {
            nr_bilete_identice++;
        }
    }

    int min_cuburi = 0;
    for (int i = 0; i < n; i++) {
        int max_turn_cl = 0;
        for (int j = 0; j < cladiri[i].size(); j++) {
            if (cladiri[i][j] > max_turn_cl) {
                max_turn_cl = cladiri[i][j];
            }
        }
        int max_turn_cl_rev = 0;
        for (int j = 0; j < cladiri[n - i - 1].size(); j++) {
            if (cladiri[n - i - 1][j] > max_turn_cl_rev) {
                max_turn_cl_rev = cladiri[n - i - 1][j];
            }
        }
        if (max_turn_cl > max_turn_cl_rev) {
            min_cuburi += max_turn_cl - max_turn_cl_rev;
        }
    }

    out << max_turn << " " << nr_max_turn << endl;
    out << nr_bilete_identice << endl;
    out << min_cuburi << endl;

    return 0;
}
```
