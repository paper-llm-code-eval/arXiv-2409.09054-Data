            height += crina[i] % 10;
            crina[i] /= 10;
        }
        if (height > max_height) {
            max_height = height;
            count_max_height = 1;
        } else if (height == max_height) {
            count_max_height++;
        }
    }

    int same_count = 0;
    for (int i = 0; i < n; i++) {
        if (crina[i] == rareș[i]) {
            same_count++;
        }
    }

    int min_cubes = 0;
    for (int i = 0; i < n; i++) {
        int height = 0;
        while (crina[i] > 0) {
            height += crina[i] % 10;
            crina[i] /= 10;
        }
        while (rareș[i] > 0) {
            height -= rareș[i] % 10;
            rareș[i] /= 10;
        }
        if (height < 0) {
            min_cubes -= height;
        }
    }

    out << max_height << ' ' << count_max_height << '\n'
        << same_count << '\n'
        << min_cubes << '\n';

    return 0;
}
