int main() {
    std::ifstream in("cladiri.in");
    std::ofstream out("cladiri.out");

    int n;
    in >> n;

    std::vector<int> crina(n), rareș(n);
    for (int i = 0; i < n; i++) {
        in >> crina[i];
        rareș[i] = crina[i];
    }

    std::reverse(rareș.begin(), rareș.end());

    int max_height = 0, count_max_height = 0;
    for (int i = 0; i < n; i++) {
        int height = 0;
