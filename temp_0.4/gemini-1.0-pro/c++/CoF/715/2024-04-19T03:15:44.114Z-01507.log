#include <fstream>
#include <vector>
#include <cmath>

using namespace std;

const int MAX_N = 10;
const int MAX_DIGITS = 10;
const int MAX_PRIME = 100;

bool is_prime[MAX_PRIME + 1];
vector<int> primes;

void sieve() {
    for (int i = 2; i <= MAX_PRIME; i++) {
        if (!is_prime[i]) {
            primes.push_back(i);
            for (int j = i + i; j <= MAX_PRIME; j += i) {
                is_prime[j] = true;
            }
        }
    }
}

bool is_valid(int n) {
    if (n < 10) {
        return true;
    }

    int first_digit = n % 10;
    int num_digits = 0;
    while (n > 0) {
        num_digits++;
        n /= 10;
    }

    if (first_digit != num_digits) {
        return false;
    }

    n = first_digit;
    while (n > 0) {
        int last_digit = n % 10;
        n /= 10;

        if (!is_prime[last_digit] || !is_prime[last_digit + 1]) {
            return false;
        }
    }

    return true;
}

int main() {
    ifstream in("visul.in");
    ofstream out("visul.out");

    int n;
    in >> n;

    sieve();

    int ans = -1;
    for (int i = n; i <= MAX_N; i++) {
        if (is_valid(i)) {
            ans = i;
            break;
        }
    }

    if (ans == -1) {
        out << "Nu exista" << endl;
    } else {
        out << ans << endl;
    }

    return 0;
}
