#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("panglica.in");
ofstream fout("panglica.out");

const int MAXN = 10005;
const int MAXC = 205;

int n, c;
vector<int> a;
vector<int> cnt;
int sol[MAXN];

int main() {
    fin >> n >> c;
    a.resize(n + 1);
    cnt.resize(c + 1);
    for (int i = 1; i <= n; ++i) {
        fin >> a[i];
        ++cnt[a[i]];
    }
    int max_cnt = 0;
    int color = 0;
    for (int i = 1; i <= c; ++i) {
        if (cnt[i] > max_cnt) {
            max_cnt = cnt[i];
            color = i;
        }
    }
    int left = 1;
    int right = n;
    while (a[left] != color) ++left;
    while (a[right] != color) --right;
    int ans = right - left + 1;
    for (int i = 1; i <= left; ++i) {
        if (a[i] == color) {
            sol[i] = 1;
            --ans;
        }
    }
    for (int i = n; i >= right; --i) {
        if (a[i] == color) {
            sol[i] = 1;
            --ans;
        }
    }
    fout << ans << '\n';
    fout << color << '\n';
    int cnt_left = 0;
    for (int i = 1; i <= left; ++i) {
        if (sol[i] == 0) ++cnt_left;
    }
    fout << cnt_left << '\n';
    int cnt_right = 0;
    for (int i = right; i <= n; ++i) {
        if (sol[i] == 0) ++cnt_right;
    }
    fout << cnt_right << '\n';
    return 0;
}
