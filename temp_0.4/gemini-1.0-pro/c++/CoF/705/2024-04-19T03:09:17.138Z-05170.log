    // Count the frequency of each color
    vector<int> freq(c + 1, 0);
    for (int color : colors) {
        freq[color]++;
    }

    // Find the most frequent color
    int max_freq = *max_element(freq.begin(), freq.end());
    int max_color = 0;
    for (int i = 1; i <= c; i++) {
