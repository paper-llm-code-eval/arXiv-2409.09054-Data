            if (first_occurrence == -1) {
                first_occurrence = i;
            }
            last_occurrence = i;
        }
    }

    // Calculate the number of strips to cut from the beginning and end
    int cut_from_beginning = first_occurrence;
    int cut_from_end = n - last_occurrence - 1;

    // Output the results
