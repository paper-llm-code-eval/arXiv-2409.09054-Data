#include <fstream>

using namespace std;

ifstream fin("ferma.in");
ofstream fout("ferma.out");

const int maxn = 400;
int n, m, v, mx, ans_x, ans_y, ans_c;
char a[maxn][maxn], c[maxn][maxn];

int dx[] = {0, 1, 0, -1};
int dy[] = {1, 0, -1, 0};

void dfs(int x, int y, char col) {
  if (x < 0 || x >= n || y < 0 || y >= m || c[x][y] || a[x][y] != col) return;
  c[x][y] = 1;
  mx++;
  for (int i = 0; i < 4; i++) {
    dfs(x + dx[i], y + dy[i], col);
  }
}

int main() {
  fin >> v >> n >> m;
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      fin >> a[i][j];
    }
  }
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      if (!c[i][j]) {
        mx = 0;
        dfs(i, j, a[i][j]);
        if (mx > ans_x) {
          ans_x = mx;
          ans_y = i;
          ans_c = j;
        }
      }
    }
  }
  if (v == 1) {
    fout << ans_x << '\n';
  } else {
    fout << ans_y + 1 << ' ' << ans_c + 1 << '\n';
    for (int i = 0; i < 4; i++) {
      int nx = ans_y + dx[i], ny = ans_c + dy[i];
      if (nx >= 0 && nx < n && ny >= 0 && ny < m && a[nx][ny] != a[ans_y][ans_c]) {
        fout << a[nx][ny] << '\n';
        return 0;
      }
    }
  }
  return 0;
}
