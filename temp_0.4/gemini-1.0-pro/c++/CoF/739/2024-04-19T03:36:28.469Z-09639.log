#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream fin("numere.in");
  ofstream fout("numere.out");

  int a, b;
  fin >> a >> b;

  vector<int> va, vb;

  while (a) {
    va.push_back(a % 2);
    a /= 2;
  }

  while (b) {
    vb.push_back(b % 2);
    b /= 2;
  }

  while (va.size() < vb.size()) va.push_back(0);
  while (vb.size() < va.size()) vb.push_back(0);

  int c = 0, d = 0, p = 1;

  for (int i = va.size() - 1; i >= 0; i--) {
    if (va[i] == vb[i]) {
      va.pop_back();
      vb.pop_back();
    } else
      break;
  }

  for (int i = va.size() - 1; i >= 0; i--) {
    c += va[i] * p;
    p *= 2;
  }

  p = 1;

  for (int i = vb.size() - 1; i >= 0; i--) {
    d += vb[i] * p;
    p *= 2;
  }

  fout << c + d << "\n";

  return 0;
}
