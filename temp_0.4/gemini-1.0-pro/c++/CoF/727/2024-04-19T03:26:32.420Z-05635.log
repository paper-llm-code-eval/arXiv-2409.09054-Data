#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream in("control.in");
  ofstream out("control.out");
  int n, x, cnt = 0, p = 0, im = 0;
  vector<int> v;
  in >> n;
  for (int i = 0; i < n; i++) {
    in >> x;
    if (v.empty() || v.back() != x) {
      v.push_back(x);
      v.push_back(1);
    } else
      v[v.size() - 1]++;
  }
  for (int i = 0; i < v.size(); i += 3) {
    cnt++;
    if (v[i] % 2 == v[i + 1] % 2 && v[i + 1] % 2 == v[i + 2] % 2)
      p++;
    else
      im++;
  }
  out << p * 10 + im << endl;
  if (cnt == 1 || (cnt == 2 && p == 1 && im == 1))
    out << 1 << endl;
  else {
    int nr = p * 10 + im;
    for (int i = 2; i * i <= nr; i++)
      if (nr % i == 0) {
        out << 0 << endl;
        return 0;
      }
    out << 1 << endl;
  }
  return 0;
}
