#include <fstream>
#include <vector>

using namespace std;

const int MAX_N = 70000;
const int MAX_M = 100000;
const int MOD = 666013;

int n, m, c;
vector<int> adj[MAX_N + 1];
int type[MAX_M + 1];
bool visited[MAX_N + 1];

void dfs(int node, int par, int t) {
  visited[node] = true;
  for (int neighbor : adj[node]) {
    if (!visited[neighbor]) {
      if (node < neighbor) {
        type[neighbor - node] = t;
      } else {
        type[node - neighbor] = t;
      }
      dfs(neighbor, node, t);
    }
  }
}

int main() {
  ifstream fin("aquapark.in");
  ofstream fout("aquapark.out");

  fin >> c >> n >> m;

  for (int i = 0; i < m; i++) {
    int x, y;
    fin >> x >> y;
    adj[x].push_back(y);
    adj[y].push_back(x);
  }

  if (c == 1) {
    dfs(1, 1, 1);
    for (int i = 1; i < n; i++) {
      fout << i << " " << i + 1 << " " << type[i] << "\n";
    }
  } else {
    int ans = 1;
    for (int i = 1; i <= n; i++) {
      if (!visited[i]) {
        dfs(i, i, 1);
        ans = (1LL * ans * 2) % MOD;
      }
    }
    fout << ans << "\n";
  }

  return 0;
}
