#include <fstream>
#include <vector>
#include <queue>
#include <tuple>

using namespace std;

const int MAX_M = 100;
const int MAX_N = 100;

int m, n;
int grid[MAX_M][MAX_N];
int dp[MAX_M][MAX_N];
int dx[] = {0, 0, 1, -1};
int dy[] = {1, -1, 0, 0};

int main() {
  ifstream fin("mouse.in");
  ofstream fout("mouse.out");

  fin >> m >> n;
  for (int i = 0; i < m; i++) {
    for (int j = 0; j < n; j++) {
      fin >> grid[i][j];
    }
  }

  dp[0][0] = grid[0][0];
  for (int i = 0; i < m; i++) {
    for (int j = 0; j < n; j++) {
      for (int k = 0; k < 4; k++) {
        int x = i + dx[k];
        int y = j + dy[k];
        if (x >= 0 && x < m && y >= 0 && y < n) {
          dp[i][j] = max(dp[i][j], dp[x][y] + grid[i][j]);
        }
      }
    }
  }

  int max_food = dp[m - 1][n - 1];
  int num_rooms = 0;
  vector<tuple<int, int>> path;

  int i = m - 1;
  int j = n - 1;
  while (i != 0 || j != 0) {
    num_rooms++;
    path.push_back(make_tuple(i + 1, j + 1));

    int max_dp = -1;
    int best_k = -1;
    for (int k = 0; k < 4; k++) {
      int x = i + dx[k];
      int y = j + dy[k];
      if (x >= 0 && x < m && y >= 0 && y < n && dp[x][y] > max_dp) {
        max_dp = dp[x][y];
        best_k = k;
      }
    }

    i = i + dx[best_k];
    j = j + dy[best_k];
  }

  fout << num_rooms << " " << max_food << endl;
  for (int i = path.size() - 1; i >= 0; i--) {
    fout << get<0>(path[i]) << " " << get<1>(path[i]) << endl;
  }

  return 0;
}
