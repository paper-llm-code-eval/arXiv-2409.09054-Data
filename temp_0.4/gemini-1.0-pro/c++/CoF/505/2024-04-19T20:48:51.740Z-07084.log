#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MOD = 1e9 + 9;
const int MAXN = 100000;

struct player {
    int start, end, foot;
    bool operator<(const player& other) const {
        return start < other.start;
    }
};

int main() {
    ifstream fin("fotbal.in");
    ofstream fout("fotbal.out");

    int n, k;
    fin >> n >> k;

    vector<player> players(n);
    for (int i = 0; i < n; i++) {
        fin >> players[i].start >> players[i].end >> players[i].foot;
    }

    sort(players.begin(), players.end());

    vector<int> dp(n + 1, 0);
    dp[0] = 1;

    int left = 0, right = 0;
    int left_foot = 0, right_foot = 0;

    for (int i = 0; i < n; i++) {
        while (left < i && players[i].start - players[left].end > k) {
            if (players[left].foot == 0) {
                left_foot--;
            } else {
                right_foot--;
            }
            left++;
        }

        while (right < i && players[i].start - players[right].start > k) {
            if (players[right].foot == 0) {
                left_foot--;
            } else {
                right_foot--;
            }
            right++;
        }

        if (players[i].foot == 0) {
            left_foot++;
        } else {
            right_foot++;
        }

        dp[i + 1] = (1LL * dp[left] * dp[right] % MOD + 1LL * dp[i] * (left_foot + right_foot) % MOD) % MOD;
    }

    fout << dp[n] << "\n";

    return 0;
}
