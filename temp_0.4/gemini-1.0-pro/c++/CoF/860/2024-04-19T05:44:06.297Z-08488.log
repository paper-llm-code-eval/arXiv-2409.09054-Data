#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream fin("dominant.in");
    ofstream fout("dominant.out");
    int v;
    fin >> v;
    string s;
    fin >> s;
    int n = s.size();
    vector<int> dp(n);
    int ans = 0;
    int cnt = 0;
    for (int i = 0; i < n; ++i) {
        if (s[i] == '0') {
            dp[i] = 0;
        } else {
            if (i == 0 || s[i - 1] == '0') {
                dp[i] = 1;
            } else {
                dp[i] = dp[i - 1] + 1;
            }
            ans = max(ans, dp[i]);
        }
    }
    for (int i = 0; i < n; ++i) {
        if (dp[i] == ans) {
            ++cnt;
        }
    }
    if (v == 1) {
        fout << ans << "\n";
    } else {
        fout << cnt << "\n";
    }
    return 0;
}
