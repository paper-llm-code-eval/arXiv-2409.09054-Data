#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream in("numar.in");
  ofstream out("numar.out");

  int ni, nz;
  in >> ni >> nz;

  vector<int> a(ni), b(nz);
  for (int i = 0; i < ni; i++) in >> a[i];
  for (int i = 0; i < nz; i++) in >> b[i];

  int carry = 0;
  for (int i = nz - 1; i >= 0; i--) {
    int sum = b[i] + carry;
    b[i] = sum % 10;
    carry = sum / 10;
  }

  while (carry) {
    a.push_back(carry % 10);
    carry /= 10;
  }

  reverse(a.begin(), a.end());

  out << a.size() << '\n';
  for (int i : a) out << i;
  out << '\n';

  out << b.size() << '\n';
  for (int i : b) out << i;
  out << '\n';

  return 0;
}
```
