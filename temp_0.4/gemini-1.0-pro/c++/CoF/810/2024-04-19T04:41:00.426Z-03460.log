#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream fin("grad.in");
  ofstream fout("grad.out");
  int n, k;
  fin >> n >> k;
  vector<int> v(n);
  for (int i = 0; i < n; i++) {
    fin >> v[i];
  }
  int grad_tot = 0;
  for (int i = 0; i < n; i++) {
    int cnt = 0;
    for (int j = i; j < i + k && j < n; j++) {
      if (v[j] >= v[i]) cnt++;
    }
    grad_tot = max(grad_tot, cnt);
  }
  int poz_max = -1, grad_max = -1;
  for (int i = 0; i < n - k + 1; i++) {
    int cnt = 0;
    for (int j = i; j < i + k; j++) {
      if (v[j] >= v[i]) cnt++;
    }
    if (cnt > grad_max) {
      grad_max = cnt;
      poz_max = i;
    }
  }
  fout << grad_tot << endl;
  fout << poz_max + 1 << " " << grad_max << endl;
  return 0;
}
