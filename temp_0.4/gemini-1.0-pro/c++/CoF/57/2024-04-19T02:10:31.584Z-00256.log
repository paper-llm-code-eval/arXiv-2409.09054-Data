#include <fstream>
#include <vector>
#include <algorithm>
#include <queue>

using namespace std;

typedef pair<int, int> edge;

const int MAXN = 255;
const int MAXM = 32768;

int N, M, K;
vector<edge> graph[MAXN];
vector<int> groups[MAXN];
int group_size[MAXN];
bool visited[MAXN];

int max_gravity = 0;
vector<edge> max_gravity_edges;

void read_input() {
  ifstream fin("urgenta.in");
  fin >> N >> M >> K;
  for (int i = 0; i < M; i++) {
    int a, b, p;
    fin >> a >> b >> p;
    graph[a].push_back({b, p});
    graph[b].push_back({a, p});
  }
  fin.close();
}

void dfs(int node, int group_id) {
  visited[node] = true;
  groups[group_id].push_back(node);
  group_size[group_id]++;
  for (auto& edge : graph[node]) {
    if (!visited[edge.first]) {
      dfs(edge.first, group_id);
    }
  }
}

void find_groups() {
  int group_id = 0;
  for (int i = 1; i <= N; i++) {
    if (!visited[i]) {
      dfs(i, group_id);
      group_id++;
    }
  }
}

void calculate_gravity(const vector<edge>& edges) {
  int gravity = 0;
  for (auto& edge : edges) {
    gravity += edge.second;
  }
  if (gravity > max_gravity) {
    max_gravity = gravity;
    max_gravity_edges = edges;
  }
}

void find_max_gravity() {
  vector<edge> edges;
  for (int i = 0; i < K - 1; i++) {
    int min_size = MAXN;
    int min_size_group = -1;
    for (int j = 0; j < K; j++) {
      if (group_size[j] < min_size) {
        min_size = group_size[j];
        min_size_group = j;
      }
    }
    for (auto& node : groups[min_size_group]) {
      for (auto& edge : graph[node]) {
        if (groups[edge.first] != groups[min_size_group]) {
          edges.push_back(edge);
        }
      }
    }
    group_size[min_size_group] += min_size;
    for (int j = 0; j < K; j++) {
      if (j != min_size_group) {
        group_size[j] -= min_size;
      }
    }
  }
  calculate_gravity(edges);
}

void write_output() {
  ofstream fout("urgenta.out");
  fout << max_gravity << endl;
  fout << max_gravity_edges.size() << endl;
  for (auto& edge : max_gravity_edges) {
    fout << edge.first << " " << edge.second << endl;
  }
  fout.close();
}

int main() {
  read_input();
  find_groups();
  find_max_gravity();
  write_output();
  return 0;
}
