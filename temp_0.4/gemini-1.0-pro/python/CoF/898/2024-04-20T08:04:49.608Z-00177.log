with open('cruce.in', 'r') as f:
    n, k = map(int, f.readline().split())
    mat = [list(map(int, line.split())) for line in f]

max_sum = -1
max_sum_row = -1
max_sum_col = -1

for i in range(n - k + 1):
    for j in range(n - k + 1):
        sum = 0
        for x in range(k):
            for y in range(k):
                sum += mat[i + x][j + y]
        if sum > max_sum:
            max_sum = sum
            max_sum_row = i + 1
            max_sum_col = j + 1

with open('cruce.out', 'w') as f:
    f.write(f'{max_sum} {max_sum_row} {max_sum_col}')
```
