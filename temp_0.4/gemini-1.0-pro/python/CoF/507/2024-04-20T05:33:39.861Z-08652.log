import sys

fin = open("partitura.in", "r")
fout = open("partitura.out", "w")

n = int(fin.readline())
notes = []
for i in range(n):
    x, y = map(int, fin.readline().split())
    notes.append((x, y))

notes.sort(key=lambda x: x[0])

dp = [0] * (n + 1)

for i in range(n):
    x, y = notes[i]
    dp[i + 1] = max(dp[i + 1], dp[i])
    for j in range(i + 1, n):
        if notes[j][0] == x + 1:
            dp[j + 1] = max(dp[j + 1], dp[i] + notes[j][1] ** 2)

fout.write(str(dp[n]))

fin.close()
fout.close()
```
