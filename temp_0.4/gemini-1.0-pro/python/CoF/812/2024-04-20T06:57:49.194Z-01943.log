with open("litere.in") as f:
    n = int(f.readline())
    cuv = f.readline().strip()
    m = int(f.readline())
    coduri = [int(x) for x in f.readline().split()]

coduri_dict = {}
for i in range(m):
    coduri_dict[cuv[i]] = coduri[i]

vocale = set("AEIOU")
nr_perechi_vocala_consoana = 0
for i in range(n - 1):
    if (cuv[i] in vocale) ^ (cuv[i + 1] in vocale):
        nr_perechi_vocala_consoana += 1

k = 0
for i in range(n):
    k = k * 10 + coduri_dict[cuv[i]]

suma_k = 0
for i in range(n):
    suma_k += k
    k = k // 10

with open("litere.out", "w") as f:
    f.write(f"{nr_perechi_vocala_consoana}\n")
    f.write(f"{k}\n")
    f.write(f"{suma_k}\n")
```
