    cuvant = f.readline()
    m = int(f.readline())
    coduri = list(map(int, f.readline().split()))

coduri_litere = {litera: cod for litera, cod in zip(cuvant, coduri)}
vocale = {'A', 'E', 'I', 'O', 'U'}
nr_perechi_vocala_consoana = 0
for i in range(n - 1):
