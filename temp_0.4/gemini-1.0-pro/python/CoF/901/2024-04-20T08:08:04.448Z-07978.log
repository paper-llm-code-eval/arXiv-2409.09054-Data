import sys

def parse_expression(expression):
    terms = []
    current_term = ""
    for char in expression:
        if char in "+-":
            if current_term != "":
                terms.append(current_term)
            current_term = char
        else:
            current_term += char
    if current_term != "":
        terms.append(current_term)
    return terms

def evaluate_expression(expression, values):
    result = 0
    for term in expression:
