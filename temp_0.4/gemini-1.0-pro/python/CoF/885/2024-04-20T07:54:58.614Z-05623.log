import sys

fin = open("tablou.in", "r")
fout = open("tablou.out", "w")

p = int(fin.readline())
if p == 1:
    n, k = map(int, fin.readline().split())
    grid = [[1] * n for _ in range(n)]
    for _ in range(k):
        op, nr = fin.readline().split()
        if op == "L":
            for i in range(n):
                grid[int(nr) - 1][i] *= -1
        else:
            for i in range(n):
                grid[i][int(nr) - 1] *= -1
    cnt = 0
    for i in range(n):
        for j in range(n):
            if grid[i][j] == 1:
                cnt += 1
    fout.write(str(cnt) + "\n")
else:
    n, z = map(int, fin.readline().split())
    if z % 2 == 1 or z > n * n:
        fout.write("0\n")
    else:
        fout.write(str((z + n * n) // 2) + "\n")
