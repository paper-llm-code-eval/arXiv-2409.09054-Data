import sys

with open('triunghiuri.in') as fin, open('triunghiuri.out', 'w') as fout:
    p = int(fin.readline())
    n = int(fin.readline())
    x = [0] * n
    y = [0] * n
    for i in range(n):
        xi, yi = map(int, fin.readline().split())
        x[i] = xi
        y[i] = yi
    x.sort()
    if p == 1:
        cnt = 1
        mx = 1
        for i in range(1, n):
            if x[i] == x[i - 1]:
                cnt += 1
                mx = max(mx, cnt)
            else:
                cnt = 1
        fout.write(str(mx) + '\n')
    else:
        y.sort()
        cnt = 0
        for i in range(n):
            for j in range(i + 1, n):
                if y[j] - y[i] > 1:
                    cnt += j - i - 1
        fout.write(str(cnt % 1000003) + '\n')
```
