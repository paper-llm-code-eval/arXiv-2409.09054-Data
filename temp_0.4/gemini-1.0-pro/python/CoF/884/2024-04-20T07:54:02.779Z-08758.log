import sys

def read_input():
    C = int(sys.stdin.readline())
    N, K = map(int, sys.stdin.readline().split())
    rounds = []
    for _ in range(K):
        rounds.append(list(map(int, sys.stdin.readline().split())))
    return C, N, K, rounds

def solve1(N, K, rounds):
    t = 0
    s = 0
    for round in rounds:
        board = [0] * (N**2 + 1)
        for cell in round:
            board[cell] = 1 if board[cell] == 0 else -1
        if any(sum(board[i:i+N]) == N or sum(board[i:i+N**2:N]) == N for i in range(1, N**2 + 1)):
            t += 1
        elif any(sum(board[i:i+N]) == -N or sum(board[i:i+N**2:N]) == -N for i in range(1, N**2 + 1)):
            s += 1
    return t, s

def solve2(N, K, rounds):
    max_moves = 0
    for round in rounds:
        board = [0] * (N**2 + 1)
        moves = 0
        for cell in round:
            board[cell] = 1 if board[cell] == 0 else -1
            moves += 1
            if any(sum(board[i:i+N]) == N or sum(board[i:i+N**2:N]) == N for i in range(1, N**2 + 1)):
                max_moves = max(max_moves, moves)
                break
            elif any(sum(board[i:i+N]) == -N or sum(board[i:i+N**2:N]) == -N for i in range(1, N**2 + 1)):
                max_moves = max(max_moves, moves)
                break
    return max_moves

def main():
    C, N, K, rounds = read_input()
    if C == 1:
        t, s = solve1(N, K, rounds)
        sys.stdout.write(f"{t} {s}\n")
    elif C == 2:
