import sys

sys.stdin = open('iepuri.in', 'r')
sys.stdout = open('iepuri.out', 'w')

n, k = map(int, input().split())
adj = [[] for _ in range(n + 1)]
for _ in range(n - 1):
    a, b = map(int, input().split())
    adj[a].append(b)

dp = [[[-1] * (k + 1) for _ in range(k + 1)] for _ in range(n + 1)]

def solve(node, par, carrots):
    if dp[node][par][carrots] != -1:
        return dp[node][par][carrots]

    ans = 1
    for i in range(carrots, k + 1):
        if i > par:
            ans += solve(node, i, i)
            ans %= 30011
        for child in adj[node]:
            if child != par:
                ans += solve(child, i, i + 1)
                ans %= 30011

    dp[node][par][carrots] = ans
    return ans

print(solve(1, 0, 1))
```
