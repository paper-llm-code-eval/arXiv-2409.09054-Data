            case.append((cod, int(c), int(v)))
    return cerinta, a, b, nr0, n, m, l, e, x, case


def arunca_zarul(a, b, nr0, nr_anterior):
    return (a * nr_anterior + b) % nr0 + 1


def simulare(a, b, nr0, n, m, l, e, x, case):
    jucatori = [{"lei": l, "euro": e, "pas": False} for _ in range(m)]
    pozitii = [1 for _ in range(m)]
    nr = nr0
    for i in range(x):
        for j in range(m):
            if jucatori[j]["lei"] <= 0 and jucatori[j]["euro"] <= 0:
                continue
            nr = arunca_zarul(a, b, nr0, nr)
            pozitii[j] = (pozitii[j] + nr - 1) % n + 1
            cod, c, v = case[pozitii[j] - 1]
            if cod == "A":
                continue
            elif cod == "R":
                jucatori[j]["pas"] = True
            elif cod == "G":
                if jucatori[j]["lei"] < c * pozitii[j]:
                    if jucatori[j]["pas"]:
                        jucatori[j]["pas"] = False
                    else:
                        jucatori[j]["lei"] = 0
                        jucatori[j]["euro"] = 0
                else:
                    if not jucatori[j]["pas"]:
                        jucatori[j]["lei"] -= c * pozitii[j]
                        jucatori[j]["euro"] += pozitii[j]
            elif cod == "V":
                if jucatori[j]["euro"] < pozitii[j]:
                    if jucatori[j]["pas"]:
                        jucatori[j]["pas"] = False
                    else:
                        jucatori[j]["lei"] = 0
                        jucatori[j]["euro"] = 0
                else:
                    jucatori[j]["lei"] += v * pozitii[j]
                    jucatori[j]["euro"] -= pozitii[j]
    return jucatori, pozitii


def cerinta1(jucatori):
    return len([j for j in jucatori if j["lei"] > 0 or j["euro"] > 0])


def cerinta2(jucatori, pozitii):
    max_euro = 0
    castigator = 0
    for i in range(len(jucatori)):
        if jucatori[i]["euro"] > max_euro:
            max_euro = jucatori[i]["euro"]
            castigator = i + 1
    return castigator


if __name__ == "__main__":
    cerinta, a, b, nr0, n, m, l, e, x, case = citire()
    jucatori, pozitii = simulare(a, b, nr0, n, m, l, e, x, case)
    if cerinta == 1:
        rezultat = cerinta1(jucatori)
    else:
        rezultat = cerinta2(jucatori, pozitii)
    with open("valutar.out", "w") as f:
        f.write(str(rezultat))
