def solve1(c):
    stack = []
    cnt = 0
    for ch in c:
        if ch == 'H' or ch == 'V':
            stack.append(cnt)
        else:
            cnt += 1
            if stack:
                cnt += stack.pop()
    return cnt

def solve2(c):
    stack = []
    mn = [sys.maxsize, sys.maxsize]
    for ch in c:
        if ch == 'H' or ch == 'V':
            stack.append(mn)
            mn = [sys.maxsize, sys.maxsize]
        else:
            mn[0] = min(mn[0], stack[-1][0] + 1)
            mn[1] = min(mn[1], stack[-1][1] + 1)
            if stack:
                mn[0] = min(mn[0], stack.pop()[0])
                mn[1] = min(mn[1], stack.pop()[1])
    return mn

def solve3(c):
    cnt = defaultdict(int)
    cnt[c] = 1
    stack = []
    for ch in c:
        if ch == 'H' or ch == 'V':
            stack.append(cnt)
        else:
            for k, v in cnt.items():
                cnt[k + ch] = (cnt[k + ch] + v) % mod
            if stack:
                for k, v in stack.pop().items():
                    cnt[k + ch] = (cnt[k + ch] + v) % mod
    return cnt[c]

def solve4(c):
    mn = c
    stack = []
    for ch in c:
        if ch == 'H' or ch == 'V':
            stack.append(mn)
            mn = c
        else:
            mn = min(mn, stack.pop() + ch, key=lambda x: (len(x), x))
    return mn

