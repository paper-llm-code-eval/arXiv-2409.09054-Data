def num_subdiv(code):
    stack = []
    cnt = 0
    for c in code:
        if c == 'H' or c == 'V':
            stack.append(c)
        elif c == '*':
            cnt += 1
        else:
            stack.pop()
    return cnt + len(stack)

def min_rect(code):
    stack = []
    min_h = 1000000000
    min_w = 1000000000
    for c in code:
        if c == 'H' or c == 'V':
            stack.append(c)
        elif c == '*':
