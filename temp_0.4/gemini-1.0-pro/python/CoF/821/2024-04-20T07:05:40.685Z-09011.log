with open('cifru.in') as f:
    n = int(f.readline())
    discuri = [int(f.readline()) for _ in range(n)]

discuri_sortate = sorted(discuri)
cifra_max = discuri_sortate[-1]
cifra_min = discuri_sortate[0]
mutari_minime = 0

for disc in discuri:
    if disc != cifra_max:
        mutari_minime += 1

cifra_min_dupa_mutari = cifra_max
numar_combinatii = 1

for disc in discuri:
    if disc == cifra_max:
        numar_combinatii += 1
    elif disc < cifra_max:
        cifra_min_dupa_mutari = min(cifra_min_dupa_mutari, disc)

with open('cifru.out', 'w') as f:
    f.write(str(cifra_max) + '\n')
    f.write(str(mutari_minime) + '\n')
    f.write(str(cifra_min_dupa_mutari) + '\n')
    f.write(str(numar_combinatii) + '\n')
```
