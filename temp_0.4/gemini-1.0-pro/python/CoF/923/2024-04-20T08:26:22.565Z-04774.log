                elif fulger_curent > 0:
                    nr_fulgere += 1
                    max_inaltime = max(max_inaltime, inaltime_curenta)
                    fulger_curent = 0
                    inaltime_curenta = 0
            if fulger_curent > 0:
                nr_fulgere += 1
                max_inaltime = max(max_inaltime, inaltime_curenta)
                fulger_curent = 0
                inaltime_curenta = 0
        f_out.write(f"{nr_fulgere} {max_inaltime}")
