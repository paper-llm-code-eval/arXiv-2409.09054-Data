with open("foto.in", "r") as f_in, open("foto.out", "w") as f_out:
    cerinta = int(f_in.readline())
    n, m = map(int, f_in.readline().split())
    matrice = [list(map(int, line.split())) for line in f_in]

    if cerinta == 1:
        max_negre = 0
        for linie in matrice:
            cnt = 0
            for patrat in linie:
                if patrat == 0:
                    cnt += 1
                else:
