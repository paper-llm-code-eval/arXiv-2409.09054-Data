with open('foto.in', 'r') as f:
    cerinta = int(f.readline().strip())
    n, m = map(int, f.readline().strip().split())
    foto = [[int(x) for x in f.readline().strip().split()] for _ in range(n)]

if cerinta == 1:
    max_negre = 0
    for linie in foto:
        cnt = 0
        for patrat in linie:
            if not patrat:
                cnt += 1
            else:
                max_negre = max(max_negre, cnt)
                cnt = 0
        max_negre = max(max_negre, cnt)
    with open('foto.out', 'w') as f:
        f.write(str(max_negre))

elif cerinta == 2:
    nr_fulgere = 0
    max_h = 0
    fulger = []
    for i in range(n):
        for j in range(m):
            if foto[i][j]:
                if not fulger:
                    fulger.append((i, j))
                    nr_fulgere += 1
                elif i == fulger[-1][0] + 1 and (j == fulger[-1][1] or j == fulger[-1][1] + 1):
                    fulger.append((i, j))
                else:
                    max_h = max(max_h, len(fulger))
                    fulger = [(i, j)]
    max_h = max(max_h, len(fulger))
    with open('foto.out', 'w') as f:
        f.write(f'{nr_fulgere} {max_h}')
