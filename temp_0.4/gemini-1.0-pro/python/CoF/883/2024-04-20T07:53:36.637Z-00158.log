import sys

with open('cursuri.in') as f:
    C = int(f.readline())
    N, K = map(int, f.readline().split())
    cursuri = [list(map(int, f.readline().split())) for _ in range(N)]

if C == 1:
    cursuri.sort(key=lambda x: x[1])
    k = 0
    cnt = 0
    for a, b in cursuri:
        if k <= a:
            k = b
            cnt += 1
    with open('cursuri.out', 'w') as g:
        g.write(str(cnt))

elif C == 2:
    cursuri.sort(key=lambda x: x[1])
    lungime_max = cursuri[-1][1] - cursuri[-1][0]
    k = 0
    ok = False
    for a, b in cursuri:
        if k <= a:
            k = a + lungime_max
            if k >= b:
                ok = True
    if ok:
        with open('cursuri.out', 'w') as g:
            g.write(str(lungime_max))
    else:
        with open('cursuri.out', 'w') as g:
            g.write(str(lungime_max - 1))
```
