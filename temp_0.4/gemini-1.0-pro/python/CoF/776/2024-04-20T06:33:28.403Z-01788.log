with open('culori.in', 'r') as f:
    n, m, t = map(int, f.readline().split())
    grid = [list(f.readline().strip()) for _ in range(n)]

def get_neighbors(i, j):
    neighbors = []
    if i > 0:
        neighbors.append(grid[i-1][j])
    if i < n-1:
        neighbors.append(grid[i+1][j])
    if j > 0:
        neighbors.append(grid[i][j-1])
    if j < m-1:
        neighbors.append(grid[i][j+1])
    return neighbors

for _ in range(t):
    new_grid = [[' ' for _ in range(m)] for _ in range(n)]
    for i in range(n):
        for j in range(m):
            neighbors = get_neighbors(i, j)
            r_count = neighbors.count('r')
            a_count = neighbors.count('a')
            if r_count > a_count:
                new_grid[i][j] = 'r'
            elif a_count > r_count:
                new_grid[i][j] = 'a'
            else:
                new_grid[i][j] = grid[i][j]
    grid = new_grid

with open('culori.out', 'w') as f:
    for row in grid:
        f.write(''.join(row) + '\n')
```
