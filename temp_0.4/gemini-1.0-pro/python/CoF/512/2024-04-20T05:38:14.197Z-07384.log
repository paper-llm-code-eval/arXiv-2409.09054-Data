import sys

input_file = open("unificare.in", "r")
output_file = open("unificare.out", "w")

C = int(input_file.readline())
n, k = map(int, input_file.readline().split())
a = list(map(int, input_file.readline().split()))

a_digits = [set(str(x)) for x in a]

if C == 1:
    a_digits.sort(key=lambda x: len(x), reverse=True)
    output_file.write("".join(list(a_digits[0])[:k]))
elif C == 2:
    max_unification = 0
    for i in range(n - 1):
        unification = set(str(a[i])) | set(str(a[i + 1]))
        max_unification = max(max_unification, int("".join(sorted(unification, reverse=True))))
    output_file.write(str(max_unification))
elif C == 3:
    max_unification = 0
    for i in range(n - k + 1):
        unification = set()
        for j in range(k):
            unification |= a_digits[i + j]
        max_unification = max(max_unification, int("".join(sorted(unification, reverse=True))))
    output_file.write(str(max_unification))

input_file.close()
output_file.close()
```
