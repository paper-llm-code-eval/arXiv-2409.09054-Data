import sys

def max_k_digits(arr, k):
    arr.sort(reverse=True)
    return int(''.join(map(str, arr[:k])))

def max_adjacent_unification(arr):
    return max(int(''.join(map(str, sorted(str(arr[i] + arr[i+1]))))) for i in range(len(arr) - 1))

def max_consecutive_unification(arr, k):
    return max(int(''.join(map(str, sorted(''.join(map(str, arr[i:i+k])))))) for i in range(len(arr) - k + 1))

with open('unificare.in', 'r') as f_in, open('unificare.out', 'w') as f_out:
    c = int(f_in.readline())
    n, k = map(int, f_in.readline().split())
    arr = list(map(int, f_in.readline().split()))
    if c == 1:
        f_out.write(str(max_k_digits(arr, k)))
    elif c == 2:
        f_out.write(str(max_adjacent_unification(arr)))
    elif c == 3:
        f_out.write(str(max_consecutive_unification(arr, k)))
```
