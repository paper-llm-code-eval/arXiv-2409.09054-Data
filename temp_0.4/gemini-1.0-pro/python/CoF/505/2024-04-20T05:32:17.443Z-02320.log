import sys

fin = open('fotbal.in', 'r')
fout = open('fotbal.out', 'w')

N, K = map(int, fin.readline().split())
st = []
dr = []
for i in range(N):
    a, b, f = map(int, fin.readline().split())
    if f == 0:
        st.append((a, b))
    else:
        dr.append((a, b))

st.sort()
dr.sort()

st_l, st_r = 0, 0
dr_l, dr_r = 0, 0
ans = 0
mod = 10 ** 9 + 9

while st_l < len(st) and dr_l < len(dr):
    if st[st_l][1] < dr[dr_l][0]:
        st_l += 1
    elif dr[dr_l][1] < st[st_l][0]:
        dr_l += 1
    else:
        ans += (min(st[st_l][1], dr[dr_l][1]) - max(st[st_l][0], dr[dr_l][0]) + 1) * (len(st) - st_l) * (len(dr) - dr_l)
        ans %= mod
        st_l += 1
        dr_l += 1

fout.write(str(ans))
fout.close()
```
