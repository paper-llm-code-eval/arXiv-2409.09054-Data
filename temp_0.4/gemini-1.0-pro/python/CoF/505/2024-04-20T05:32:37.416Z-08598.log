import sys

with open('fotbal.in') as f:
    n, k = map(int, f.readline().split())
    players = []
    for i in range(n):
        start, end, foot = map(int, f.readline().split())
        players.append((start, end, foot))

players.sort(key=lambda x: x[1])

dp = [[0] * (k + 1) for _ in range(n + 1)]
dp[0][0] = 1

for i in range(1, n + 1):
    start, end, foot = players[i - 1]
    for j in range(k + 1):
        dp[i][j] = dp[i - 1][j]
        if j > 0 and (foot == 0 or dp[i - 1][j - 1] > 0):
            dp[i][j] += dp[i - 1][j - 1]
        if j > 0 and (foot == 1 or dp[i - 1][j - 1] > 0):
            dp[i][j] += dp[i - 1][j - 1]

print(dp[n][k] % (10 ** 9 + 9))
```
