with open("arrows.in", "r") as f:
    cerinta = int(f.readline())
    n, m = map(int, f.readline().split())
    tabla = [list(map(int, f.readline().split())) for _ in range(n)]
    lin, col = map(int, f.readline().split())

def punctaj_pozitie(lin, col):
    vizitate = set()
    punctaj = 0
    while True:
        vizitate.add((lin, col))
        punctaj += 1
        if lin == 1 and tabla[lin - 1][col - 1] == 4:
            break
        elif lin == n and tabla[lin - 1][col - 1] == 2:
            break
        elif col == 1 and tabla[lin - 1][col - 1] == 3:
            break
        elif col == m and tabla[lin - 1][col - 1] == 1:
            break
        if tabla[lin - 1][col - 1] == 1:
            col += 1
        elif tabla[lin - 1][col - 1] == 2:
            lin -= 1
        elif tabla[lin - 1][col - 1] == 3:
            col -= 1
        else:
            lin += 1
    return punctaj

def numar_pozitii_favorabile():
    numar = 0
    for i in range(1, n + 1):
        for j in range(1, m + 1):
            vizitate = set()
            lin, col = i, j
            while True:
                vizitate.add((lin, col))
                if lin == 1 and tabla[lin - 1][col - 1] == 4:
                    break
                elif lin == n and tabla[lin - 1][col - 1] == 2:
                    break
                elif col == 1 and tabla[lin - 1][col - 1] == 3:
                    break
                elif col == m and tabla[lin - 1][col - 1] == 1:
                    break
                if tabla[lin - 1][col - 1] == 1:
                    col += 1
                elif tabla[lin - 1][col - 1] == 2:
                    lin -= 1
                elif tabla[lin - 1][col - 1] == 3:
                    col -= 1
                else:
                    lin += 1
            if len(vizitate) == n * m:
                numar += 1
    return numar

def punctaj_maxim():
    maxim = 0
    for i in range(1, n + 1):
        for j in range(1, m + 1):
            maxim = max(maxim, punctaj_pozitie(i, j))
    return maxim

if cerinta == 1:
    with open("arrows.out", "w") as f:
        f.write(str(punctaj_pozitie(lin, col)))
