with open('arrows.in') as f:
    cerinta = int(f.readline())
    n, m = map(int, f.readline().split())
    tabla = [list(map(int, f.readline().split())) for _ in range(n)]
    lin, col = map(int, f.readline().split())

def punctaj(lin, col):
    vizitate = set()
    puncte = 0
    while 0 <= lin < n and 0 <= col < m and (lin, col) not in vizitate:
        vizitate.add((lin, col))
        puncte += 1
        if tabla[lin][col] == 1:
            col += 1
        elif tabla[lin][col] == 2:
            lin -= 1
        elif tabla[lin][col] == 3:
            col -= 1
        elif tabla[lin][col] == 4:
            lin += 1
    return puncte

if cerinta == 1:
    print(punctaj(lin - 1, col - 1))
elif cerinta == 2:
    favorabile = 0
    for i in range(n):
        for j in range(m):
            if punctaj(i, j) == -1:
                favorabile += 1
    print(favorabile)
else:
    puncte_max = 0
    for i in range(n):
        for j in range(m):
            puncte_max = max(puncte_max, punctaj(i, j) * 1000)
    print(puncte_max)
```
