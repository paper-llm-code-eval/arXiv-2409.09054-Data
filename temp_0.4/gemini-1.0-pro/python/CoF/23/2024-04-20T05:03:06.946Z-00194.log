import sys

def main():
    with open("galeti.in", "r") as f:
        n, e = map(int, f.readline().split())
    
    if n == 1:
        with open("galeti.out", "w") as f:
            f.write("0\n")
        return
    
    buckets = [1] * n
    
    while e > 0:
        for i in range(1, n):
            if buckets[i] > 0:
                for j in range(i):
                    if buckets[j] == 0:
                        if buckets[i] <= e:
                            buckets[j] += buckets[i]
                            buckets[i] = 0
                            e -= buckets[i]
                            with open("galeti.out", "a") as f:
                                f.write(f"{i+1} {j+1}\n")
                        else:
                            buckets[j] += e
                            buckets[i] -= e
                            e = 0
                            with open("galeti.out", "a") as f:
                                f.write(f"{i+1} {j+1}\n")
                        break
    
    if buckets[0] == 1:
        with open("galeti.out", "a") as f:
            f.write("0\n")

if __name__ == "__main__":
    main()
