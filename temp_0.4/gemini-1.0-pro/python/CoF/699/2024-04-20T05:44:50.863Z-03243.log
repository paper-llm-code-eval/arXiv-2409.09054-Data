with open('poarta.in', 'r') as f:
    p = int(f.readline())
    echipaje = []
    for _ in range(p):
        echipaje.append([int(x) for x in f.readline().split()])

echipaje.sort(key=lambda x: (x[2], x[3]))

dist = [[1000000000] * 251 for _ in range(251)]
for echipaj in echipaje:
    dist[echipaj[0]][echipaj[1]] = 0

coada = [(0, echipaje[0][0], echipaje[0][1])]
while coada:
    cost, x, y = coada.pop(0)
    if x == echipaje[-1][2] and y == echipaje[-1][3]:
        break
    for dx, dy in [(1, 0), (-1, 0), (0, 1), (0, -1)]:
        nx, ny = x + dx, y + dy
        if 1 <= nx <= 250 and 1 <= ny <= 250 and dist[nx][ny] > cost + 1:
            dist[nx][ny] = cost + 1
            coada.append((cost + 1, nx, ny))

print(dist[echipaje[-1][2]][echipaje[-1][3]])
