with open('poarta.in') as f:
    p = int(f.readline())
    echipaje = [tuple(map(int, line.split())) for line in f]

echipaje_poz = {}
for echipaj in echipaje:
    echipaje_poz[echipaj[0], echipaj[1]] = echipaj[2], echipaj[3]

vizitat = set()
dist = 0

while len(vizitat) != p:
    for echipaj in echipaje:
        if echipaj not in vizitat:
            vizitat.add(echipaj)
            echipaj_poz = echipaje_poz[echipaj]
            while echipaj_poz != echipaj:
                dist += 1
                echipaj_poz = echipaje_poz[echipaj_poz]

with open('poarta.out', 'w') as f:
    f.write(str(dist))
```
