with open('sudest.in') as fin, open('sudest.out', 'w') as fout:
    n = int(fin.readline())
    grid = [[int(x) for x in fin.readline().split()] for _ in range(n)]
    k = int(fin.readline())
    steps = [int(x) for x in fin.readline().split()]

    # dp[i][j][k][0/1] = max sum of potatoes collected from (i, j) to (n, n)
    # using k steps, and facing east/south
    dp = [[[[0] * 2 for _ in range(k + 1)] for _ in range(2)] for _ in range(n + 1)]

    # base case: (n, n)
    dp[n - 1][n - 1][0][0] = dp[n - 1][n - 1][0][1] = grid[n - 2][n - 2]

    # iterate in reverse order
    for i in range(n - 1, -1, -1):
        for j in range(n - 1, -1, -1):
            for k in range(k):
                # try all possible moves
                for facing in range(2):
                    # move east
                    if j + steps[k] <= n - 1:
                        dp[i][j][k][facing] = max(dp[i][j][k][facing],
                                                  dp[i][j + steps[k]][k + 1][0] + grid[i][j + steps[k]])
                    # move south
                    if i + steps[k] <= n - 1:
                        dp[i][j][k][facing] = max(dp[i][j][k][facing],
                                                  dp[i + steps[k]][j][k + 1][1] + grid[i + steps[k]][j])

    # reconstruct the path
    path = [(n, n)]
    i, j, k, facing = n, n, 0, 0
    while i != 1 or j != 1:
        # try all possible moves
        for facing in range(2):
            # move east
            if j + steps[k] <= n - 1 and dp[i][j][k][facing] == dp[i][j + steps[k]][k + 1][0] + grid[i][j + steps[k]]:
                i, j, k, facing = i, j + steps[k], k + 1, 0
                path.append((i, j))
                break
            # move south
            if i + steps[k] <= n - 1 and dp[i][j][k][facing] == dp[i + steps[k]][j][k + 1][1] + grid[i + steps[k]][j]:
                i, j, k, facing = i + steps[k], j, k + 1, 1
                path.append((i, j))
                break

    # write the output
    fout.write(str(dp[1][1][0][0]) + '\n')
    for i, j in path:
        fout.write(str(i) + ' ' + str(j) + '\n')
