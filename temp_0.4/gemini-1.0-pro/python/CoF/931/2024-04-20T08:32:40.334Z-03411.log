import sys

with open('sir.in', 'r') as f:
    c = int(f.readline())
    n = int(f.readline())
    a = [int(x) for x in f.readline().split()]

if c == 1:
    for i in range(n - 1, 0, -1):
        if a[i] > a[i - 1]:
            with open('sir.out', 'w') as f:
                f.write(str(i))
            break
elif c == 2:
    ans = []
    for i in range(1, n):
        if a[i] > a[i - 1]:
            ans.append(i)
    with open('sir.out', 'w') as f:
        f.write(' '.join(map(str, ans)))
elif c == 3:
    m = max(a)
    ans = 0
    for i in range(1, n):
        if a[i] != m:
            ans += m - a[i]
    with open('sir.out', 'w') as f:
        f.write(str(ans))
```
