with open('sir.in', 'r') as f:
    k, x, a, b = map(int, f.readline().split())

def suma_cifrelor(n):
    s = 0
    while n > 0:
        s += n % 10
        n //= 10
    return s % 10

def succesor(n):
    s = str(n)
    while s[-1] == '9':
        s = s[:-1]
    if not s:
        return 10 ** (len(str(x)) + 1)
    i = len(s) - 1
    while s[i] >= s[i + 1]:
        i -= 1
    if i == -1:
        return 10 ** (len(str(x)) + 1)
    c = s[i]
    s = s[:i] + s[i + 1:]
    j = len(s) - 1
    while s[j] <= c:
        j -= 1
    s = s[:j] + c + s[j:]
    s = s[:i] + str(int(s[i]) + 1) + s[i + 1:]
    return int(s)

def numar_termeni(a, b):
    cnt = 0
    for i in range(a * 10 ** (k - 1), (a + 1) * 10 ** (k - 1)):
        if str(i).count(str(b)) == 0:
            cnt += 1
    return cnt

with open('sir.out', 'w') as f:
    f.write(str(suma_cifrelor(x)) + '\n')
    f.write(str(succesor(x)) + '\n')
    f.write(str(numar_termeni(a, b)) + '\n')
```
