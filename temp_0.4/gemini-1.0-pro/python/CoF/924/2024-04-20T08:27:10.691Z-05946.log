    for i in range(n):
        s[i + 1] = s[i] + a[i]

    # dp[i][j] = number of ways to group the first i elements into j groups
    dp = [[0] * (n + 1) for _ in range(n + 1)]
    dp[0][0] = 1
    for i in range(1, n + 1):
        for j in range(1, n + 1):
            dp[i][j] = dp[i - 1][j]
            if s[i] - s[i - j] >= 0:
                dp[i][j] += dp[i - j][j - 1]

    return dp[n][2]

def solve2(n, a):
    # prefix sum
    s = [0] * (n + 1)
    for i in range(n):
        s[i + 1] = s[i] + a[i]

    # dp[i][j] = minimum imbalance when grouping the first i elements into j groups
    dp = [[sys.maxsize] * (n + 1) for _ in range(n + 1)]
    dp[0][0] = 0
    for i in range(1, n + 1):
        for j in range(1, n + 1):
            dp[i][j] = min(dp[i][j], dp[i - 1][j])
            if s[i] - s[i - j] >= 0:
                dp[i][j] = min(dp[i][j], dp[i - j][j - 1] + s[i] - s[i - j])

    # find the maximum number of groups with minimum imbalance
    max_groups = 0
    for j in range(1, n + 1):
        if dp[n][j] == dp[n][max_groups]:
            max_groups = j

    # find the group with the maximum sum
    max_sum = -sys.maxsize
    max_sum_group = 0
    for i in range(max_groups, n + 1):
        if s[i] - s[i - max_groups] > max_sum:
            max_sum = s[i] - s[i - max_groups]
            max_sum_group = i - max_groups

    return max_groups, max_sum_group

def main():
