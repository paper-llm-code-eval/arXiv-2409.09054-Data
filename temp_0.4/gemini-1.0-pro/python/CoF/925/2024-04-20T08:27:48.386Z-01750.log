        datorii[creditor] = datorii.get(creditor, 0) + int(suma)
        datorii[debitor] = datorii.get(debitor, 0) - int(suma)

    if cerinta == 1:
        fout.write(str(len(datorii)))
    else:
        for nume in sorted(datorii):
            fout.write(f'{nume} {datorii[nume] if datorii[nume] < 0 else 0} {datorii[nume] if datorii[nume] > 0 else 0}\n')
