with open("ordine.in", "r") as f:
    n = int(f.readline())
    sir = list(map(int, f.readline().split()))
    cerinta = int(f.readline())

# Parcurgem sirul de la final catre inceput
for i in range(n - 1, -1, -1):
    # Daca lungimea sirului este impara, ultima bila luata este cea de la mijloc
    if n % 2 == 1:
        ultima_bila = sir[n // 2]
    # Daca lungimea sirului este para, ultima bila luata este cea de la mijloc + 1
    else:
        ultima_bila = sir[n // 2 - 1]

# Scriem in fisierul de iesire ultima bila luata
with open("ordine.out", "w") as f:
    f.write(str(ultima_bila) + "\n")

    # Daca cerinta este 2, reconstruim ordinea in care au fost luate bilele
    if cerinta == 2:
        ordine = []
        # Pornim de la ultima bila luata
        bila = ultima_bila
        # Cat timp nu am parcurs tot sirul
        while len(ordine) < n:
            # Adaugam bila in ordine
            ordine.append(bila)
            # Daca lungimea sirului este impara, bila anterioara este cea de la mijloc - 1
            if n % 2 == 1:
                bila = sir[n // 2 - 1]
            # Daca lungimea sirului este para, bila anterioara este cea de la mijloc - 2
            else:
                bila = sir[n // 2 - 2]
            # Stergem bila din sir
            sir.remove(bila)
        # Scriem in fisierul de iesire ordinea in care au fost luate bilele
        f.write(" ".join(map(str, ordine)) + "\n")
