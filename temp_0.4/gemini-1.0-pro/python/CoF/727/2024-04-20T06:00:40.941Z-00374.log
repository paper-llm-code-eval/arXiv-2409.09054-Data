    n = int(f_in.readline())
    weights = [int(f_in.readline()) for _ in range(n)]
    weights.sort()

    control_num = 0
    group = []
    for weight in weights:
        if not group or weight != group[-1]:
            group.append(weight)
            group.append(1)
        else:
            group[-1] += 1

