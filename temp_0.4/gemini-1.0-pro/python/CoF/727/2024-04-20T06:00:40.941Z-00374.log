        if group[1] % 2 == group[2] % 2 == group[3] % 2:
            control_num += 10
        else:
            control_num += 1
        group = group[3:]

    f_out.write(f'{control_num}\n')
    f_out.write(f'{1 if is_prime(control_num) else 0}\n')
