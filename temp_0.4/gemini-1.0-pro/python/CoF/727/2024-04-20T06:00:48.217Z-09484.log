import sys

def is_prime(n):
    if n <= 1:
        return False
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            return False
    return True

with open('control.in', 'r') as f_in, open('control.out', 'w') as f_out:
    n = int(f_in.readline())
    weights = [int(f_in.readline()) for _ in range(n)]

    weights.sort()
    control = 0
    i = 0
    while i < len(weights):
        j = i + 2
        while j < len(weights) and weights[j] == weights[j - 1]:
            j += 1
        if (j - i) % 3 == 0:
            control += 1
        i = j

    f_out.write(f'{control}\n')
    f_out.write(f'{1 if is_prime(control) else 0}')
