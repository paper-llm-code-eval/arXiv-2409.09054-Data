with open("placare.in", "r") as f:
    n, m = map(int, f.readline().split())
    a = [[0 for _ in range(m)] for _ in range(n)]
    for i in range(n):
        row = list(map(int, f.readline().split()))
        for j in range(m):
            if row[j] > 0:
                for k in range(j, j + row[j]):
                    a[i][k] = row[j]
            else:
                for k in range(i, i - row[j]):
                    a[k][j] = -row[j]

with open("placare.out", "w") as f:
    for i in range(n):
        for j in range(m):
            f.write(str(abs(a[i][j])) + " ")
        f.write("\n")
```
