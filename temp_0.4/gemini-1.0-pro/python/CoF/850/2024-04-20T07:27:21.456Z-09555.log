import sys

with open("ferma.in", "r") as f:
    v = int(f.readline())
    m, n = map(int, f.readline().split())
    grid = [list(f.readline().strip()) for _ in range(m)]

def bfs(x, y, color):
    q = [(x, y)]
    size = 1
    while q:
        x, y = q.pop(0)
        for dx, dy in [(1, 0), (-1, 0), (0, 1), (0, -1)]:
            nx, ny = x + dx, y + dy
            if 0 <= nx < m and 0 <= ny < n and grid[nx][ny] == color and (nx, ny) not in q:
                q.append((nx, ny))
                size += 1
    return size

max_size = 0
for i in range(m):
    for j in range(n):
        max_size = max(max_size, bfs(i, j, grid[i][j]))

if v == 1:
    with open("ferma.out", "w") as f:
        f.write(str(max_size))
else:
    for i in range(m):
        for j in range(n):
            for c in "abcdefghijklmnopqrstuvwxyz":
                if c != grid[i][j]:
                    grid[i][j] = c
                    new_size = bfs(i, j, c)
                    if new_size > max_size:
                        max_size = new_size
                        x, y, color = i + 1, j + 1, c
    with open("ferma.out", "w") as f:
        f.write(f"{x} {y}\n{color}")
