from collections import defaultdict
from sys import stdin, stdout

def edit_distance(a, b):
    m, n = len(a), len(b)
    t = [[0] * (n + 1) for _ in range(m + 1)]
    for i in range(1, m + 1):
        t[i][0] = i
    for j in range(1, n + 1):
        t[0][j] = j
    for i in range(1, m + 1):
        for j in range(1, n + 1):
            cost = 0 if a[i - 1] == b[j - 1] else 1
            t[i][j] = min(t[i - 1][j] + 1, t[i][j - 1] + 1, t[i - 1][j - 1] + cost)
    return t[m][n]

def main():
    k = int(stdin.readline())
    text = stdin.read().split()
    words = set(text)
    graph = defaultdict(list)
    for i in words:
        for j in words:
            if i != j and edit_distance(i, j) <= k:
                graph[i].append(j)
    visited = set()
    chains = 0
    for word in words:
        if word not in visited:
            visited.add(word)
            stack = [word]
            while stack:
                u = stack.pop()
                for v in graph[u]:
                    if v not in visited:
                        visited.add(v)
                        stack.append(v)
            chains += 1
    stdout.write(str(chains))

if __name__ == "__main__":
    main()
