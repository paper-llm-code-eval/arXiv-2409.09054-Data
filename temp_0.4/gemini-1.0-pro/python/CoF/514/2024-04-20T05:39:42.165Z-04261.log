import sys
from math import sqrt

def is_prime(n):
    if n < 2:
        return False
    for i in range(2, int(sqrt(n)) + 1):
        if n % i == 0:
            return False
    return True

def main():
    with open('primprim.in', 'r') as f_in, open('primprim.out', 'w') as f_out:
        c = int(f_in.readline())
        n = int(f_in.readline())
        arr = list(map(int, f_in.readline().split()))
        if c == 1:
            f_out.write(str(sum(abs(a - min(filter(lambda x: is_prime(x), [a - 1, a, a + 1]))) for a in arr)))
        else:
            q = int(f_in.readline())
            for _ in range(q):
                i, x, p = map(int, f_in.readline().split())
                arr[i - 1] = x
                f_out.write(str(sum(abs(a - min(filter(lambda x: is_prime(x), [a - 1, a, a + 1]))) for a in arr[i - p:i])))

if __name__ == '__main__':
    main()
