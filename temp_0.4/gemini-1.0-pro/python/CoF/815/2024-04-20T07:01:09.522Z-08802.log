import sys
import queue

def read_input():
    n = int(input())
    T = tuple(map(int, input().split()))
    S = [tuple(map(int, input().split())) for _ in range(2)]
    R = [tuple(map(int, input().split())) for _ in range(2)]
    k = int(input())
    O = [tuple(map(int, input().split())) for _ in range(k)]
    return n, T, S, R, O

def write_output(z, s):
    print(z)
    print(s)

def get_max_wall(n, O):
    # Sort obstacles by row and column
    O.sort(key=lambda x: (x[0], x[1]))
    
    # Initialize the length of the longest wall in each row and column
    max_row_wall = [0] * n
    max_col_wall = [0] * n
    
    # Iterate over the obstacles
    for x, y in O:
        # Update the length of the longest wall in the current row and column
        max_row_wall[x - 1] = max(max_row_wall[x - 1], y)
        max_col_wall[y - 1] = max(max_col_wall[y - 1], x)
    
    # Return the maximum of the longest wall in each row and column
    return max(max_row_wall), max(max_col_wall)

def get_min_seconds(n, T, S, R, O):
    # Create a graph to represent the network of segments
    graph = [[[] for _ in range(n)] for _ in range(n)]
    for i in range(n):
        for j in range(n):
            if (i, j) not in O:
                if i > 0:
                    graph[i][j].append((i - 1, j))
                if i < n - 1:
                    graph[i][j].append((i + 1, j))
                if j > 0:
                    graph[i][j].append((i, j - 1))
                if j < n - 1:
                    graph[i][j].append((i, j + 1))
    
    # Create a queue to store the nodes to be visited
    Q = queue.Queue()
    
    # Initialize the distance of each node from the target to infinity
    dist = [[float('inf') for _ in range(n)] for _ in range(n)]
    
    # Set the distance of the target node to 0
    dist[T[0] - 1][T[1] - 1] = 0
    
    # Add the target node to the queue
    Q.put(T)
    
    # While the queue is not empty
    while not Q.empty():
        # Get the next node from the queue
        x, y = Q.get()
        
        # Iterate over the neighbors of the current node
        for u, v in graph[x][y]:
            # If the distance of the current node is less than the distance of the neighbor
            if dist[x][y] + 1 < dist[u][v]:
                # Update the distance of the neighbor
                dist[u][v] = dist[x][y] + 1
                
                # Add the neighbor to the queue
                Q.put((u, v))
    
    # Initialize the minimum number of seconds to infinity
    min_seconds = float('inf')
    
    # Iterate over the source nodes
    for Sx, Sy in S:
        # Iterate over the robot nodes
        for Rx, Ry in R:
            # If the distance from the source to the target is less than the distance from the robot to the target
            if dist[Sx - 1][Sy - 1] < dist[Rx - 1][Ry - 1]:
                # Update the minimum number of seconds
                min_seconds = min(min_seconds, dist[Sx - 1][Sy - 1])
    
    # Return the minimum number of seconds
    return min_seconds

def main():
    n, T, S, R, O = read_input()
    z, s = get_max_wall(n, O)
    write_output(z, s)

if __name__ == "__main__":
    main()
