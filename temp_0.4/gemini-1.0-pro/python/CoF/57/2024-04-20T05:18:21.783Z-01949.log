import sys
from collections import defaultdict

# Read input
N, M, K = map(int, sys.stdin.readline().split())
edges = defaultdict(list)
for _ in range(M):
    a, b, w = map(int, sys.stdin.readline().split())
    edges[a].append((b, w))
    edges[b].append((a, w))

# Find connected components
components = []
visited = [False] * (N + 1)
def dfs(node):
    if visited[node]:
        return
    visited[node] = True
    components[-1].append(node)
    for neighbor, _ in edges[node]:
        dfs(neighbor)

for i in range(1, N + 1):
    if not visited[i]:
        components.append([])
        dfs(i)

# Find maximum gravity
max_gravity = 0
max_components = []
for component in components:
    if len(component) > K:
        continue
    gravity = 0
    for i in range(len(component)):
        for j in range(i + 1, len(component)):
            a, b = component[i], component[j]
            if (a, b) in edges or (b, a) in edges:
                gravity += edges[(a, b)][1] if (a, b) in edges else edges[(b, a)][1]
    if gravity > max_gravity:
        max_gravity = gravity
        max_components = component

# Write output
sys.stdout.write(str(max_gravity) + '\n')
sys.stdout.write(str(len(max_components) - 1) + '\n')
for i in range(len(max_components)):
    for j in range(i + 1, len(max_components)):
        a, b = max_components[i], max_components[j]
        if (a, b) in edges or (b, a) in edges:
            sys.stdout.write(str(a) + ' ' + str(b) + '\n')
            break
