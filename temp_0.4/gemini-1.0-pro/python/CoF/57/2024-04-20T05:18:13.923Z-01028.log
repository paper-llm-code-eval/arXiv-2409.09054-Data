import sys

N, M, K = map(int, sys.stdin.readline().split())
edges = [[] for _ in range(N+1)]
for _ in range(M):
    a, b, w = map(int, sys.stdin.readline().split())
    edges[a].append((b, w))
    edges[b].append((a, w))

vis = [False] * (N+1)
comp = []
def dfs(node, c):
    vis[node] = True
    comp[c].append(node)
    for u, w in edges[node]:
        if not vis[u]:
            dfs(u, c)

max_comp = 0
for i in range(1, N+1):
    if not vis[i]:
        comp.append([])
        dfs(i, max_comp)
        max_comp += 1

comp.sort(key=len, reverse=True)
if len(comp) > K:
    comp = comp[:K]

ans = 0
for i in range(1, len(comp)):
    for u in comp[i]:
        for v, w in edges[u]:
            if v in comp[i-1]:
                ans += w

print(ans)
print(len(comp)-1)
for i in range(1, len(comp)):
    for u in comp[i]:
        for v, w in edges[u]:
            if v in comp[i-1]:
                print(u, v)
