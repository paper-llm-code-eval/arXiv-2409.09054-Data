import sys

sys.setrecursionlimit(1000000)

f = open('urgenta.in', 'r')
g = open('urgenta.out', 'w')

n, m, k = map(int, f.readline().split())
edges = [[] for i in range(n+1)]
for i in range(m):
    a, b, c = map(int, f.readline().split())
    edges[a].append((b, c))
    edges[b].append((a, c))

vis = [False] * (n+1)
comp = []
def dfs(node, c):
    vis[node] = True
    comp[c].append(node)
    for u, w in edges[node]:
        if not vis[u]:
            dfs(u, c)

for i in range(1, n+1):
    if not vis[i]:
        comp.append([])
        dfs(i, len(comp)-1)

comp.sort(key=len, reverse=True)
if len(comp) > k:
    g.write('0\n0')
    exit(0)

ans = 0
broken = []
for i in range(1, len(comp)):
    for a in comp[i]:
        for b, w in edges[a]:
            if b in comp[0]:
                ans += w
                broken.append((a, b))

g.write(str(ans) + '\n')
g.write(str(len(broken)) + '\n')
for a, b in broken:
    g.write(str(a) + ' ' + str(b) + '\n')
```
