import sys

with open('roboti.in') as f:
    v, n = map(int, f.readline().split())
    p = list(map(int, f.readline().split()))

if v == 1:
    max_len = 0
    for i in range(n):
        cur_len = 1
        for j in range(i + 1, n):
            if p[j] > p[j - 1]:
                cur_len += 1
            else:
                break
        max_len = max(max_len, cur_len)
    with open('roboti.out', 'w') as f:
        f.write(str(max_len))

else:
    p.sort()
    max_sum = 0
    min_lex = []
    for i in range(n):
        for j in range(i + 1, n):
            cur_sum = p[i] * p[j]
            for k in range(i + 1, j):
                cur_sum += p[k] * p[k + 1]
            if cur_sum > max_sum:
                max_sum = cur_sum
                min_lex = p[i:] + p[:i]
            elif cur_sum == max_sum:
                new_lex = p[i:] + p[:i]
                if new_lex < min_lex:
                    min_lex = new_lex
    with open('roboti.out', 'w') as f:
        f.write(' '.join(map(str, min_lex)))
