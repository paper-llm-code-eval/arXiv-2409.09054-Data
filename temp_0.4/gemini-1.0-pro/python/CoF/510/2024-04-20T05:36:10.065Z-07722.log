with open('castel.in') as fin, open('castel.out', 'w') as fout:
    c, n = map(int, fin.readline().split())
    galbene = list(map(int, fin.readline().split()))
    if c == 1:
        fout.write(str(sum(map(lambda x: x < 10, galbene))))
    elif c == 2:
        i = 0
        r = 1
        while i < n:
            r += 1
            j = i + 1
            while j < n and galbene[j] < 10:
                j += 1
            i = j
        fout.write(f'{r} {galbene[i]}\n')
    else:
        i = 0
        r = 1
        albastre = []
        while i < n:
            r += 1
            j = i + 1
            while j < n and galbene[j] < 10:
                j += 1
            albastre.append(sum(galbene[i:j + 1]))
            i = j
        fout.write(f'{len(albastre)} {sum(albastre)}\n')
```
