def read_input():
    with open("castel.in", "r") as f:
        c = int(f.readline())
        n = int(f.readline())
        h = []
        for _ in range(n):
            h.append([int(x) for x in f.readline().split()])
    return c, n, h


def count_rooms(h):
    n = len(h)
    vis = [[False for _ in range(n)] for _ in range(n)]
    cnt = 0

    def dfs(i, j):
        if i < 0 or i >= n or j < 0 or j >= n or vis[i][j] or h[i][j] == 0:
            return
        vis[i][j] = True
        cnt[0] += 1
        if h[i][j] & 1 == 0:
            dfs(i, j - 1)
        if h[i][j] & 2 == 0:
            dfs(i + 1, j)
        if h[i][j] & 4 == 0:
            dfs(i, j + 1)
        if h[i][j] & 8 == 0:
            dfs(i - 1, j)

    for i in range(n):
        for j in range(n):
            if not vis[i][j] and h[i][j] != 0:
                cnt = [0]
                dfs(i, j)
                cnt[0] -= 1
    return cnt[0]


def max_room(h):
    n = len(h)
    vis = [[False for _ in range(n)] for _ in range(n)]
    max_area = 0
    max_room = []

    def dfs(i, j, area, room):
        nonlocal max_area
        nonlocal max_room
        if i < 0 or i >= n or j < 0 or j >= n or vis[i][j] or h[i][j] == 0:
            return
        vis[i][j] = True
        room.append((i, j))
        area += 1
        if area > max_area:
            max_area = area
            max_room = room
        if h[i][j] & 1 == 0:
            dfs(i, j - 1, area, room)
        if h[i][j] & 2 == 0:
            dfs(i + 1, j, area, room)
        if h[i][j] & 4 == 0:
            dfs(i, j + 1, area, room)
        if h[i][j] & 8 == 0:
            dfs(i - 1, j, area, room)
        room.pop()
        vis[i][j] = False

    for i in range(n):
        for j in range(n):
            if not vis[i][j] and h[i][j] != 0:
                room = []
                dfs(i, j, 0, room)
    return max_area


def main():
    c, n, h = read_input()
    if c == 1:
        print(count_rooms(h))
    elif c == 2:
        print(max_room(h))
    elif c == 3:
        max_area = max_room(h)
        room = sorted(list(set(max_area)))
        print(*room[0], *room[-1])


if __name__ == "__main__":
    main()
