    dp = [[[-1] * 5 for _ in range(M + 1)] for _ in range(N + 1)]
    
    def solve(i, j, zone):
        if i == X and j == Y:
            return 0, 0, zone
        
        if dp[i][j][zone] != -1:
            return dp[i][j][zone]
        
        ans = (0, 0, 0)
        
        if zone == 0:
            if i + 1 <= N:
                a, b, c = solve(i + 1, j, zone)
                if b + grid[i][j] > ans[1]:
                    ans = (a + 1, b + grid[i][j], c)
            if j + 1 <= M:
                a, b, c = solve(i, j + 1, zone)
                if b + grid[i][j] > ans[1]:
                    ans = (a + 1, b + grid[i][j], c)
        elif zone == 1:
            if i + 1 <= N:
                a, b, c = solve(i + 1, j, zone)
                if b + grid[i][j] > ans[1]:
                    ans = (a + 1, b + grid[i][j], c)
            if j - 1 >= 1:
                a, b, c = solve(i, j - 1, zone)
                if b + grid[i][j] > ans[1]:
                    ans = (a + 1, b + grid[i][j], c)
        elif zone == 2:
            if i - 1 >= 1:
                a, b, c = solve(i - 1, j, zone)
                if b + grid[i][j] > ans[1]:
                    ans = (a + 1, b + grid[i][j], c)
            if j + 1 <= M:
                a, b, c = solve(i, j + 1, zone)
                if b + grid[i][j] > ans[1]:
                    ans = (a + 1, b + grid[i][j], c)
        elif zone == 3:
            if i - 1 >= 1:
                a, b, c = solve(i - 1, j, zone)
                if b + grid[i][j] > ans[1]:
                    ans = (a + 1, b + grid[i][j], c)
            if j - 1 >= 1:
                a, b, c = solve(i, j - 1, zone)
                if b + grid[i][j] > ans[1]:
                    ans = (a + 1, b + grid[i][j], c)
        
        dp[i][j][zone] = ans
        return ans
    
    a, b, c = solve(1, 1, 0)
    fout.write(' '.join(map(str, (c, b, a))))
