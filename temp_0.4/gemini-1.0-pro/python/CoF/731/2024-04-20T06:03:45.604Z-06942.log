import sys

def read_input():
    k = int(sys.stdin.readline())
    trasee = []
    for i in range(k):
        traseu = list(map(int, sys.stdin.readline().split()))
        traseu[0] = i + 1
        trasee.append(traseu)
    t, u = map(int, sys.stdin.readline().split())
    return trasee, t, u

def solve(trasee, t, u):
    min_popasuri = sys.maxsize
    traseu_ales = -1
    for traseu in trasee:
        popasuri = 0
        distanta_ramas = traseu[-1]
        for izvor in traseu[2:]:
            if distanta_ramas <= t:
