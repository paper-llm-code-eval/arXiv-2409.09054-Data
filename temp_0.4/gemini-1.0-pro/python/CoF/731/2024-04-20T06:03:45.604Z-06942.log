        if distanta_ramas <= t:
            if popasuri < min_popasuri:
                min_popasuri = popasuri
                traseu_ales = traseu[0]
    return min_popasuri, traseu_ales

def write_output(min_popasuri, traseu_ales):
    if min_popasuri == sys.maxsize:
        sys.stdout.write("0")
    else:
        sys.stdout.write("{} {}".format(min_popasuri, traseu_ales))

if __name__ == "__main__":
    trasee, t, u = read_input()
