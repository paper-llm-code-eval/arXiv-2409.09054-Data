with open("popas.in", "r") as f:
    k = int(f.readline())
    trasee = []
    for _ in range(k):
        i, r, *d = map(int, f.readline().split())
        trasee.append((i, r, d))
    t, u = map(int, f.readlines())

with open("popas.out", "w") as f:
    min_popasuri = float("inf")
    traseu_ales = 0
    for i, r, d in trasee:
        popasuri = 0
        d = [0] + d + [t]
        for j in range(1, len(d)):
            if d[j] - d[j - 1] > u:
                popasuri += 1
        if popasuri < min_popasuri:
            min_popasuri = popasuri
            traseu_ales = i
    if min_popasuri == float("inf"):
        f.write("0")
    else:
        f.write(f"{min_popasuri} {traseu_ales}")
```
