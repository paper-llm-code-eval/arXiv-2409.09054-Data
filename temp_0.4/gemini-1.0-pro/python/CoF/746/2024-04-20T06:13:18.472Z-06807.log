import sys

# Read input
H, N, M, p = map(int, sys.stdin.readline().split())

# Initialize dp table
dp = [[float('inf') for _ in range(H + 1)] for _ in range(N + 1)]
prev = [[-1 for _ in range(H + 1)] for _ in range(N + 1)]

# Base case
dp[0][0] = 0

# Iterate over the number of steps
for i in range(1, N + 1):
    # Iterate over the current height
    for j in range(1, H + 1):
        # Try all possible ways to reach the current height
        for k in range(1, min(j, M) + 1):
            # Calculate the effort required to reach the current height
            effort = (dp[i - 1][j - k] + sum(range(k)) + p) / k
            # Update the dp table if the current effort is less than the previous effort
            if effort < dp[i][j]:
                dp[i][j] = effort
                prev[i][j] = k

# Reconstruct the solution
solution = []
i = N
j = H
while i > 0:
    k = prev[i][j]
    solution.append(k)
    i -= k
    j -= k

# Write output
sys.stdout.write(f'{dp[N][H]:.2f}\n')
sys.stdout.write(' '.join(map(str, solution)))
```
