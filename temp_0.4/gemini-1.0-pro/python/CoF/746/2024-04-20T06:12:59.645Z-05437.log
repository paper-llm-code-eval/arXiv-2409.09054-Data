import sys

def main():
    with open('scara.in', 'r') as f:
        H, N, M, p = map(int, f.readline().split())

    dp = [[[float('inf') for _ in range(M + 1)] for _ in range(N + 1)] for _ in range(H + 1)]
    dp[0][0][0] = 0
    for h in range(H + 1):
        for n in range(N + 1):
            for m in range(M + 1):
                for i in range(1, min(m + 1, H - h + 1)):
                    dp[h + i][n + 1][m] = min(dp[h + i][n + 1][m], dp[h][n][m] + (h + i) / i + p)
                dp[h][n][m] = min(dp[h][n][m], dp[h][n][m - 1])

    effort = dp[H][N][M]
    if effort == float('inf'):
        sys.exit(1)

    stairs = []
    h = H
    n = N
    m = M
    while n > 0:
        for i in range(1, min(m + 1, h + 1)):
            if dp[h - i][n - 1][m] + (h - i) / i + p == effort:
                stairs.append(h - i)
                h -= i
                n -= 1
                m -= i
                break

    stairs.reverse()
    with open('scara.out', 'w') as f:
        f.write(f'{effort:.2f}\n')
        f.write(' '.join(map(str, stairs)))

if __name__ == '__main__':
    main()
