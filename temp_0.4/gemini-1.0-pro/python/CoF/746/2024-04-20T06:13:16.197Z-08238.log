import sys

# Read input
H, N, M, p = map(int, sys.stdin.readline().split())

# Initialize DP table
dp = [[float('inf') for _ in range(H + 1)] for _ in range(N + 1)]
dp[0][0] = 0

# Iterate over the number of steps
for i in range(1, N + 1):
    # Iterate over the height of the current step
    for h in range(1, H + 1):
        # Try all possible ways to reach the current height
        for j in range(1, min(h, M) + 1):
            dp[i][h] = min(dp[i][h], dp[i - 1][h - j] + (h - j) / j + p)

# Output the minimum effort
print(dp[N][H] if dp[N][H] < float('inf') else -1)

# Output the staircase
if dp[N][H] < float('inf'):
    i, h = N, H
    staircase = []
    while i > 0:
        for j in range(1, min(h, M) + 1):
            if dp[i][h] == dp[i - 1][h - j] + (h - j) / j + p:
                staircase.append(h - j)
                h -= j
                i -= 1
                break
    staircase.reverse()
    print(*staircase)
```
