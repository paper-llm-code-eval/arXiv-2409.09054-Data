        if i == n:
            if taken == 1:
                return 1
            return 0

        ret = dp[i][budget][taken]
        if ret != -1:
            return ret

        ret = 0
        if budget >= d[i]:
            ret = go(i + 1, budget - d[i], taken | 1)
        ret = (ret + go(i + 1, budget, taken)) % mod
        ret = (ret + go(i + 1, budget, taken | 1)) % mod
        dp[i][budget][taken] = ret
        return ret

    return go(1, c, 0)

with open("transport.in", "r") as f:
    t = int(f.readline())
    n, c = map(int, f.readline().split())
    x = [0] + [int(i) for i in f.readline().split()]
    d = [0] + [int(i) for i in f.readline().split()]

if t == 1:
    ans = solve_regio(n, c, x, d)
elif t == 2:
    ans = solve_expres(n, c, x, d)

with open("transport.out", "w") as f:
    f.write(str(ans))
