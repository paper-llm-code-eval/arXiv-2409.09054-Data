import heapq
import sys

sys.stdin = open('lanterna.in', 'r')
sys.stdout = open('lanterna.out', 'w')

N, K = map(int, input().split())
has_base = [int(x) for x in input().split()]
M = int(input())
graph = [[] for i in range(N + 1)]
for i in range(M):
    a, b, t, w = map(int, input().split())
    graph[a].append((b, t, w))
    graph[b].append((a, t, w))

dist = [[10 ** 9] * (K + 1) for i in range(N + 1)]
dist[1][0] = 0
pq = [(0, 1, 0)]  # (dist, node, lantern)
while pq:
    d, node, lantern = heapq.heappop(pq)
    if node == N:
        break
    if d > dist[node][lantern]:
        continue
    for neighbor, t, w in graph[node]:
        if w <= lantern or has_base[neighbor]:
            new_dist = d + t
            if new_dist < dist[neighbor][lantern]:
                dist[neighbor][lantern] = new_dist
                heapq.heappush(pq, (new_dist, neighbor, lantern))
        for i in range(lantern + 1, K + 1):
            if w <= i:
                new_dist = d + t
                if new_dist < dist[neighbor][i]:
                    dist[neighbor][i] = new_dist
                    heapq.heappush(pq, (new_dist, neighbor, i))

min_dist = 10 ** 9
min_lantern = -1
for i in range(K + 1):
    if dist[N][i] < min_dist:
        min_dist = dist[N][i]
        min_lantern = i

print(min_dist, min_lantern)
