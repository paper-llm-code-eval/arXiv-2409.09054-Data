import heapq
from collections import defaultdict

with open('lanterna.in', 'r') as f:
    N, K = map(int, f.readline().split())
    prietene = [int(x) for x in f.readline().split()]
    M = int(f.readline())
    graph = defaultdict(list)
    for _ in range(M):
        a, b, t, w = map(int, f.readline().split())
        graph[a].append((b, t, w))
        graph[b].append((a, t, w))

dist = [[float('inf')] * (K + 1) for _ in range(N + 1)]
dist[1][0] = 0
pq = [(0, 1, 0)]

while pq:
    d, node, lanterna = heapq.heappop(pq)
    if node == N:
        break
    if d > dist[node][lanterna]:
        continue
    for vecin, t, w in graph[node]:
        if lanterna + w <= K and d + t < dist[vecin][lanterna + w]:
            dist[vecin][lanterna + w] = d + t
            heapq.heappush(pq, (d + t, vecin, lanterna + w))
        if prietene[vecin] and d + t < dist[vecin][0]:
            dist[vecin][0] = d + t
            heapq.heappush(pq, (d + t, vecin, 0))

t_min = float('inf')
w_min = float('inf')
for i in range(K + 1):
    if dist[N][i] < t_min:
        t_min = dist[N][i]
        w_min = i

with open('lanterna.out', 'w') as f:
    f.write(f'{t_min} {w_min}')
```
