import sys

fin = open("lasere.in", "r")
fout = open("lasere.out", "w")

c = int(fin.readline())
n, m = map(int, fin.readline().split())
mat = [list(map(int, fin.readline().split())) for _ in range(n)]
lasere = [list(map(int, fin.readline().split())) for _ in range(m)]

for i, j, d in lasere:
    if d == 1:
        for k in range(i - 1, -1, -1):
            mat[k][j] -= 1
    elif d == 2:
        for k in range(j + 1, n):
            mat[i][k] -= 1
    elif d == 3:
        for k in range(i + 1, n):
            mat[k][j] -= 1
    elif d == 4:
        for k in range(j - 1, -1, -1):
            mat[i][k] -= 1

if c == 1:
    cnt = 0
    for i in range(n):
        for j in range(n):
            if all(mat[i][j] <= mat[x][y] for x in range(i - 1, i + 2) for y in range(j - 1, j + 2)):
                cnt += 1
    fout.write(str(cnt) + "\n")
else:
    cnt = 0
    for i in range(n):
        for j in range(n):
            if mat[i][j] <= mat[i][j - 1] and mat[i][j] <= mat[i][j + 1]:
                continue
            if j == 0 or mat[i][j] > mat[i][j - 1]:
                k = j + 1
                while k < n and mat[i][k] >= mat[i][k - 1]:
                    k += 1
                if k - j > 1:
                    cnt += 1
    fout.write(str(cnt) + "\n")
