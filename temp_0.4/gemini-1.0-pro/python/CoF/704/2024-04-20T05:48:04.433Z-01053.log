with open("cuburi.in", "r") as f:
    n = int(f.readline())
    culori = list(map(int, f.readline().split()))

frecventa = [0] * 11
for culoare in culori:
    frecventa[culoare] += 1

numar_culori = 0
for frecventa_culoare in frecventa:
    if frecventa_culoare > 0:
        numar_culori += 1

culoare_maxima = 0
frecventa_maxima = 0
for culoare in range(1, 11):
    if frecventa[culoare] > frecventa_maxima:
        culoare_maxima = culoare
        frecventa_maxima = frecventa[culoare]

pozitii_stergere = []
lungime_maxima = 0
culoare_curenta = 0
pozitie_inceput = 0
for pozitie in range(n):
    culoare = culori[pozitie]
    if culoare != culoare_curenta:
        if pozitie - pozitie_inceput > lungime_maxima:
            lungime_maxima = pozitie - pozitie_inceput
            pozitii_stergere = [pozitie_inceput]
        culoare_curenta = culoare
        pozitie_inceput = pozitie
if n - pozitie_inceput > lungime_maxima:
    pozitii_stergere = [pozitie_inceput]

with open("cuburi.out", "w") as f:
    f.write(str(numar_culori) + "\n")
    f.write(str(culoare_maxima) + "\n")
    f.write(str(lungime_maxima) + "\n")
    for pozitie in pozitii_stergere:
        f.write(str(pozitie + 1) + " ")
```
