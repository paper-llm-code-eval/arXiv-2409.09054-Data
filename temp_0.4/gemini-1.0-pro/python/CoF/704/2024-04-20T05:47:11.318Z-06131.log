with open('cuburi.in', 'r') as f:
    n = int(f.readline())
    v = list(map(int, f.readline().split()))

frecv = [0] * 11
for x in v:
    frecv[x] += 1

max_frecv = max(frecv)
max_color = []
for i in range(1, 11):
    if frec[i] == max_frecv:
        max_color.append(i)

poz = []
for i in range(n):
    if v[i] in max_color:
        poz.append(i)

with open('cuburi.out', 'w') as f:
    f.write(str(len(max_color)) + '\n')
    f.write(' '.join(map(str, max_color)) + '\n')
    f.write(str(len(poz)) + '\n')
    f.write(' '.join(map(str, poz)) + '\n')
