with open('cuburi.in') as f:
    n = int(f.readline())
    culori = list(map(int, f.readline().split()))

frecventa = [0] * 11
for culoare in culori:
    frecventa[culoare] += 1

numar_culori = len([f for f in frecventa if f > 0])

culoare_maxima = max(range(1, 11), key=lambda culoare: frecventa[culoare])

pozitii = []
culoare_curenta = culori[0]
lungime_maxima = 1
for i in range(1, n):
    if culori[i] == culoare_curenta:
        lungime_maxima += 1
    else:
        pozitii.append(i)
        culoare_curenta = culori[i]
        lungime_maxima = 1

if lungime_maxima == n:
    pozitii = []
else:
    pozitii.insert(0, 0)
    pozitii.append(n)

with open('cuburi.out', 'w') as f:
    f.write(str(numar_culori) + '\n')
    f.write(str(culoare_maxima) + '\n')
    f.write(' '.join(map(str, pozitii)) + '\n')
```
