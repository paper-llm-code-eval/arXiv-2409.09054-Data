import sys

def citire():
    f = open('pluricex.in')
    n, k, D = map(int, f.readline().split())
    discipline = [[] for i in range(n)]
    for i in range(n):
        nr, *discipline[i] = map(int, f.readline().split())
    f.close()
    return n, k, D, discipline

def scriere(echipe):
    f = open('pluricex.out', 'w')
    for echipa in echipe:
        f.write(' '.join(map(str, echipa)) + '\n')
    f.close()

def back(k, echipa, discipline_acoperite, discipline, n):
    if k == 0:
        echipe.append(echipa.copy())
        return
    for i in range(n):
        if i not in echipa and all(d in discipline_acoperite for d in discipline[i]):
            echipa.append(i + 1)
            discipline_acoperite.update(discipline[i])
            back(k - 1, echipa, discipline_acoperite, discipline, n)
            discipline_acoperite.difference_update(discipline[i])
            echipa.pop()

n, k, D, discipline = citire()
echipe = []
back(k, [], set(), discipline, n)
echipe.sort()
scriere(echipe)
```
