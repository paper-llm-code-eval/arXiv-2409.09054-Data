import sys

def palindrom(n):
    return n == int(str(n)[::-1])

def main():
    with open('numere.in', 'r') as f:
        c = int(f.readline())
        if c == 1:
            n = int(f.readline())
            nr = 0
            i = 0
            while nr < n:
                i += 1
                if i % 10 != 0 and not palindrom(i):
                    nr += 1
            with open('numere.out', 'w') as g:
                g.write(str(i))
        elif c == 2:
            x = int(f.readline())
            nr1 = 0
            nr2 = 0
            nr3 = 0
            i = 0
            while True:
                i += 1
                if i % 10 != 0 and not palindrom(i):
                    if i // 10 == x:
                        nr1 += 1
                    if i // 100 == x:
                        nr2 += 1
                    if i // 1000 == x:
                        nr3 += 1
                if i == x:
                    break
            with open('numere.out', 'w') as g:
                g.write(f'{nr1} {nr2} {nr3}')
        elif c == 3:
            k = int(f.readline())
            nr = 0
            i = 0
            while True:
                i += 1
                if i % 10 != 0 and not palindrom(i):
                    if len(str(i)) == k:
                        nr += 1
                if len(str(i)) == k + 1:
                    break
            with open('numere.out', 'w') as g:
                g.write(str(nr))

if __name__ == '__main__':
    main()
```
