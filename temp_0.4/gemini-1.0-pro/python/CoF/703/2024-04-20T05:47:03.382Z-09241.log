with open("valori-panta.in", "r") as f_in, open("valori-panta.out", "w") as f_out:
    n = int(f_in.readline())
    v = list(map(int, f_in.readline().split()))
    
    nr_valori_panta = 0
    max_valoare_panta = -1
    poz_max_valoare_panta = []
    min_valoare_panta = 1000000000
    poz_min_valoare_panta = []
    
    for i in range(n):
        cifre = []
        nr = v[i]
        while nr > 0:
            cifre.append(nr % 10)
            nr //= 10
        
        if cifre == sorted(cifre):
            nr_valori_panta += 1
            if v[i] > max_valoare_panta:
                max_valoare_panta = v[i]
                poz_max_valoare_panta = [i + 1]
            elif v[i] == max_valoare_panta:
                poz_max_valoare_panta.append(i + 1)
            if v[i] < min_valoare_panta:
                min_valoare_panta = v[i]
                poz_min_valoare_panta = [i + 1]
            elif v[i] == min_valoare_panta:
                poz_min_valoare_panta.append(i + 1)
    
    if nr_valori_panta == 0:
        f_out.write("NU EXISTA")
    else:
        f_out.write(str(nr_valori_panta) + "\n")
        f_out.write(str(max_valoare_panta) + " " + " ".join(map(str, poz_max_valoare_panta)) + "\n")
        f_out.write(str(min_valoare_panta) + " " + " ".join(map(str, poz_min_valoare_panta)))
