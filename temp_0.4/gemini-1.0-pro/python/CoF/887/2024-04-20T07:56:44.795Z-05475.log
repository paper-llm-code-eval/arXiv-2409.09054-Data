def main():
    with open('caps.in', 'r') as f_in, open('caps.out', 'w') as f_out:
        k, q = map(int, f_in.readline().split())
        s = f_in.readline().strip()
        s_caps = ''.join(map(lambda c: c.swapcase(), s))
        s_next = s + s_caps + s
        s_next_caps = ''.join(map(lambda c: c.swapcase(), s_next))
        s_final = s_next + s_next_caps * ((q + len(s_next) - 1) // len(s_next))
