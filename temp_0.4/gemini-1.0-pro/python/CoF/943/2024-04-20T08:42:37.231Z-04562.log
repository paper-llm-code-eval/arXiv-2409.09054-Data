import sys

with open('vecine.in', 'r') as f:
    p, n = map(int, f.readline().split())
    v = list(map(int, f.readline().split()))

if p == 1:
    ans = 0
    for i in range(1, n):
        if v[i] == v[i - 1] + 1:
            ans += 1
    with open('vecine.out', 'w') as f:
        f.write(str(ans))

else:
    v.append(0)
    dp = [[-1] * 3 for _ in range(n + 1)]
    
    def solve(i, prv):
        if i == n:
            return 0
        if dp[i][prv] != -1:
            return dp[i][prv]
        
        ans = solve(i + 1, prv)
        if prv != 2 or v[i] != 0:
            ans = max(ans, solve(i + 1, 1) * 10 + v[i])
        if i + 1 < n and (prv != 2 or v[i] != 0 or v[i + 1] != 0):
            ans = max(ans, solve(i + 2, 2) * 100 + v[i] * 10 + v[i + 1])
        
        dp[i][prv] = ans
        return ans
    
    with open('vecine.out', 'w') as f:
        f.write(str(solve(0, 0)))
```
