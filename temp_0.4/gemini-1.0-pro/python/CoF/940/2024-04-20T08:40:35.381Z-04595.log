from typing import List, Tuple

def solve(n: int, a: List[int]) -> int:
    """
    Finds the number of ways to split an array into at least two subsequences with equal sums.

    Args:
        n: The length of the array.
        a: The array of elements.

    Returns:
        The number of ways to split the array.
    """

    # Create a prefix sum array.
    prefix_sum = [0] * (n + 1)
    for i in range(n):
        prefix_sum[i + 1] = prefix_sum[i] + a[i]

    # Create a map from prefix sums to the number of occurrences of that prefix sum.
    prefix_sum_counts = {}
    for i in range(n + 1):
        if prefix_sum[i] not in prefix_sum_counts:
            prefix_sum_counts[prefix_sum[i]] = 0
        prefix_sum_counts[prefix_sum[i]] += 1

    # Create a map from prefix sums to the number of ways to split the array at that prefix sum.
    prefix_sum_splits = {}
    for prefix_sum, count in prefix_sum_counts.items():
        if count >= 2:
            prefix_sum_splits[prefix_sum] = count * (count - 1) // 2
        else:
            prefix_sum_splits[prefix_sum] = 0

    # Calculate the total number of ways to split the array.
    total_splits = 0
    for prefix_sum in prefix_sum_counts:
        total_splits += prefix_sum_counts[prefix_sum] * prefix_sum_splits[prefix_sum]

    return total_splits

def main():
    with open('tort.in', 'r') as f:
        n = int(f.readline())
        a = list(map(int, f.readline().split()))

    result = solve(n, a)

    with open('tort.out', 'w') as f:
        f.write(str(result))

if __name__ == '__main__':
    main()
