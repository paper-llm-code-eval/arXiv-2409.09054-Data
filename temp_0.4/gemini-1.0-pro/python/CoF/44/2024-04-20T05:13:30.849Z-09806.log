        earliest[node] = max(earliest[node], earliest[neighbor] + times[neighbor])
        latest[node] = max(latest[node], latest[neighbor] + times[neighbor])
    latest[node] = max(latest[node], earliest[node])

# Write output
sys.stdout.write(str(latest[n - 1]) + "\n")
for i in range(n):
    sys.stdout.write(str(earliest[i]) + " " + str(latest[i]) + "\n")
