with open("turnuri.in", "r") as f:
    m, n = map(int, f.readline().split())
    grid = [list(map(int, f.readline().split())) for _ in range(m)]

heights = [0] * n
for j in range(n):
    for i in range(m - 1, -1, -1):
        if grid[i][j]:
            heights[j] += 1
            break

max_height = 0
max_towers = 0
for i in range(n - 1):
    if heights[i] == heights[i + 1] == 0:
        continue
    new_height = heights[i] + heights[i + 1]
    if new_height <= m:
        heights[i] = heights[i + 1] = 0
        max_height = max(max_height, new_height)
        max_towers += 1

with open("turnuri.out", "w") as f:
    f.write(" ".join(map(str, heights)) + "\n")
    f.write(str(max_height) + "\n")
    f.write(str(max_towers) + "\n")
```
