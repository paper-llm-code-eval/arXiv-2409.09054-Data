with open('turnuri.in') as f:
    m, n = map(int, f.readline().split())
    grid = [[int(x) for x in f.readline().split()] for _ in range(m)]

towers = []
for j in range(n):
    h = 0
    for i in range(m - 1, -1, -1):
        if grid[i][j]:
            h += 1
    towers.append(h)

towers_max = 0
for i in range(n - 1):
    if towers[i] and towers[i + 1]:
        towers_max = max(towers_max, towers[i] + towers[i + 1])

max_towers = 0
for i in range(n - 1):
    if towers[i] and towers[i + 1]:
        if towers[i] + towers[i + 1] == towers_max:
            max_towers += 1
            towers[i] = towers[i + 1] = 0

with open('turnuri.out', 'w') as f:
    f.write(' '.join(map(str, towers)) + '\n')
    f.write(str(towers_max) + '\n')
    f.write(str(max_towers) + '\n')
```
