        # Update seen trains
        seen_trains += 1

        # Update current gap
        if current_trains == 0:
            current_gap += train[3]
        else:
            current_gap = 0

        # Update max gap
        max_gap = max(max_gap, current_gap)

    return max_trains, seen_trains, max_gap

def write_output(filename, result):
    with open(filename, "w") as f:
        f.write(" ".join(map(str, result)))

if __name__ == "__main__":
    trains = read_input("tren.in")
    result = solve(trains)
    write_output("tren.out", result)
