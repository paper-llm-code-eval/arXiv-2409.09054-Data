import sys

def read_input(filename):
    with open(filename, "r") as f:
        T = int(f.readline())
        trains = []
        for _ in range(T):
            L, H, M, S = map(int, f.readline().split())
            trains.append((L, H, M, S))
    return trains

def solve(trains):
    # Sort trains by arrival time
    trains.sort(key=lambda x: (x[1], x[2]))

    # Initialize variables
    max_trains = 0
    current_trains = 0
    seen_trains = 0
    max_gap = 0
    current_gap = 0

