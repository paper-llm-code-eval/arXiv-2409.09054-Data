import sys

def dist(x1, y1, x2, y2):
    return ((x2 - x1) ** 2 + (y2 - y1) ** 2) ** 0.5

def ccw(x1, y1, x2, y2, x3, y3):
    return (x2 - x1) * (y3 - y1) - (y2 - y1) * (x3 - x1)

def intersect(x1, y1, x2, y2, x3, y3, x4, y4):
    return ccw(x1, y1, x3, y3, x4, y4) * ccw(x1, y1, x4, y4, x2, y2) <= 0 and ccw(x3, y3, x1, y1, x2, y2) * ccw(x3, y3, x2, y2, x4, y4) <= 0

def graham_scan(points):
    n = len(points)
    if n < 3:
        return points
    points.sort(key=lambda point: (point[0], point[1]))
    hull = []
    for point in points:
        while len(hull) >= 2 and ccw(hull[-2][0], hull[-2][1], hull[-1][0], hull[-1][1], point[0], point[1]) <= 0:
            hull.pop()
        hull.append(point)
    return hull

def area(points):
    n = len(points)
    if n < 3:
        return 0
    area = 0
    for i in range(n):
        area += (points[i][0] * points[(i + 1) % n][1] - points[(i + 1) % n][0] * points[i][1])
    return abs(area) / 2

def solve(n, points, distances):
    hull = graham_scan(points)
    max_area = 0
    for i in range(n):
        for j in range(i + 1, n):
            x1, y1 = points[i]
            x2, y2 = points[j]
            max_dist = max(distances[i], distances[j])
            for dx in range(-max_dist, max_dist + 1):
                for dy in range(-max_dist, max_dist + 1):
                    x3 = x1 + dx
                    y3 = y1 + dy
                    x4 = x2 + dx
                    y4 = y2 + dy
                    if not any(intersect(x1, y1, x2, y2, x3, y3, x4, y4) for x3, y3, x4, y4 in zip(hull, hull[1:], hull[2:] + [hull[0]])):
                        max_area = max(max_area, area([x1, y1, x2, y2, x3, y3, x4, y4]))
    return max_area

def main():
    with open('mosia.in', 'r') as f:
        n = int(f.readline())
        points = []
        distances = []
        for _ in range(n):
            x, y, d = map(int, f.readline().split())
            points.append((x, y))
            distances.append(d)
    max_area = solve(n, points, distances)
    with open('mosia.out', 'w') as f:
        f.write('{:.4f}\n'.format(max_area))

if __name__ == '__main__':
    main()
