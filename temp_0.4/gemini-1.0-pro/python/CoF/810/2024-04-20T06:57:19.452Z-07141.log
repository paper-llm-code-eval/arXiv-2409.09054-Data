    sys.stdout.write(str(grad_sir) + "\n")
    sys.stdout.write(str(poz_secv) + " " + str(grad_secv) + "\n")

def grad_sir(x):
    n = len(x)
    x_sort = sorted(x)
    grad = 0
    for i in range(n):
        if x[i] == x_sort[i]:
            grad += 1
    return grad

def grad_secv(x, k):
    n = len(x)
    grad_max = 0
    poz_max = 0
    for i in range(n - k + 1):
        x_secv = x[i:i+k]
        x_secv_sort = sorted(x_secv)
        grad = 0
        for j in range(k):
            if x_secv[j] == x_secv_sort[j]:
                grad += 1
        if grad > grad_max:
            grad_max = grad
            poz_max = i
    return poz_max, grad_max

if __name__ == "__main__":
    n, k, x = read_input()
    grad_sir_val = grad_sir(x)
    poz_secv_val, grad_secv_val = grad_secv(x, k)
    write_output(grad_sir_val, poz_secv_val + 1, grad_secv_val)
