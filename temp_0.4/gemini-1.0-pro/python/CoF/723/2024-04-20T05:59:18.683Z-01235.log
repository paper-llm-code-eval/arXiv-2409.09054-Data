import math

with open("expresie.in", "r") as f:
    m = int(f.readline())
    n = int(f.readline())
    x = [int(i) for i in f.readline().split()]

prod = math.prod(x)
if prod == int(math.pow(prod, 1.0 / m)):
    with open("expresie.out", "w") as f:
        f.write("1\n")
        for i in range(2, int(math.sqrt(prod)) + 1):
            cnt = 0
            while prod % i == 0:
                cnt += 1
                prod //= i
            if cnt:
                f.write(f"{i} {cnt}\n")
        if prod > 1:
            f.write(f"{prod} 1\n")
else:
    with open("expresie.out", "w") as f:
        f.write("0\n")
```
