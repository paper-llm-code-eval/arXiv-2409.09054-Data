import sys
from heapq import heappush, heappop

n, p, k = map(int, sys.stdin.readline().split())
adj = [[] for _ in range(p + 1)]
for _ in range(k):
    a, b = map(int, sys.stdin.readline().split())
    adj[a].append(b)

times = [0] * (p + 1)
heads = [n] * (p + 1)
for i in range(1, p + 1):
    d, c = map(int, sys.stdin.readline().split())
    times[i] = d
    heads[i] = n - c

dp = [float('inf')] * (p + 1)
dp[1] = times[1]
pq = [(times[1], 1)]
while pq:
    time, node = heappop(pq)
    if node == p:
        sys.stdout.write(str(time) + '\n')
        sys.exit(0)
    if time > dp[node]:
        continue
    for neighbor in adj[node]:
        if heads[neighbor] >= 1 and time + times[neighbor] < dp[neighbor]:
            dp[neighbor] = time + times[neighbor]
            heappush(pq, (dp[neighbor], neighbor))

sys.stdout.write('-1\n')
```
