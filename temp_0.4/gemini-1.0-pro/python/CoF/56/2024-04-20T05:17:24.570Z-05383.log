dp[1][n] = 0

# Compute dynamic programming table
for i in range(2, p + 1):
    for j in range(n + 1):
        for d, c in stories:
            if j - c >= 0 and i not in dependencies[d]:
                dp[i][j] = min(dp[i][j], dp[i - 1][j - c] + d)

# Output result
if dp[p][n] != float('inf'):
    sys.stdout.write(str(dp[p][n]))
else:
    sys.stdout.write('-1')
