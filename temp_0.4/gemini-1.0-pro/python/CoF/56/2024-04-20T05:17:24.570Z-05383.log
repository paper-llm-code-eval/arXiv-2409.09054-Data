    d, c = map(int, sys.stdin.readline().split())
    stories.append((d, c))
dependencies = [[] for _ in range(p + 1)]
for _ in range(k):
    a, b = map(int, sys.stdin.readline().split())
    dependencies[a].append(b)

# Initialize dynamic programming table
