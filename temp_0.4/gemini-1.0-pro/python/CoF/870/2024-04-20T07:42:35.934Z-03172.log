import sys

f = open('axyz.in', 'r')
p = int(f.readline())
A = int(f.readline())
N = int(f.readline())
X = list(map(int, f.readline().split()))
f.close()

X.sort(reverse=True)

if p == 1:
    for i in range(N - 1, -1, -1):
        if X[i] < X[i - 1]:
            X[i], X[i - 1] = X[i - 1], X[i]
            break
    Y = ''.join(map(str, X))
    open('axyz.out', 'w').write(Y)

elif p == 2:
    cnt = 0
    for i in range(N):
        if X[i] < A % 10:
            cnt += 1
        A //= 10
    open('axyz.out', 'w').write(str(cnt))
```
