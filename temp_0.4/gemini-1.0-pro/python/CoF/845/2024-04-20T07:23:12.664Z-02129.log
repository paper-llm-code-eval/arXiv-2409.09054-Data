with open('munte.in', 'r') as f:
    n = int(f.readline())
    x = [int(i) for i in f.readline().split()]

def munte(x):
    st = []
    dr = []
    for i in range(n):
        while st and x[st[-1]] >= x[i]:
            st.pop()
        if st:
            dr.append(i - st[-1])
        st.append(i)
    st = []
    for i in range(n - 1, -1, -1):
        while st and x[st[-1]] >= x[i]:
            st.pop()
        if st:
            dr[i] = min(dr[i], i - st[-1])
        st.append(i)
    return dr

munte_nr = 0
while True:
    dr = munte(x)
    if not any(dr):
        break
    munte_nr += dr.count(max(dr))
    x = [x[i] for i in range(n) if dr[i] != max(dr)]
    n = len(x)

with open('munte.out', 'w') as f:
    f.write(str(munte_nr) + '\n')
    f.write(str(munte_nr + dr.count(max(dr))) + '\n')
    f.write(str(n) + '\n')
```
