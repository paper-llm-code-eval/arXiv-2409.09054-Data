with open('munte.in') as fin, open('munte.out', 'w') as fout:
    n = int(fin.readline())
    a = list(map(int, fin.readline().split()))
    b = a[:]
    cnt = 0
    while True:
        cnt += 1
        m = []
        for i in range(1, n - 1):
            if a[i - 1] < a[i] > a[i + 1]:
                m.append(i)
        if not m:
            break
        for i in reversed(m):
            a.pop(i)
    fout.write(f'{cnt}\n')
    fout.write(f'{len(b)}\n')
    fout.write(f'{len(a)}\n')
