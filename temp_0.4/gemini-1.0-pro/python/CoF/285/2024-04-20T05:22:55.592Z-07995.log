        x_f, y_f = map(int, sys.stdin.readline().split())
        return C, N, P, pulsars, x_s, y_s, x_f, y_f
    else:
        print("Error: Input file is empty.")
        sys.exit(1)

def get_max_affected_sectors(N, P, pulsars):
    max_affected = 0
    for i in range(N):
        for j in range(N):
            affected = 0
            for x, y, r, t in pulsars:
                if abs(x - i) + abs(y - j) <= t:
                    affected += 1
            max_affected = max(max_affected, affected)
    return max_affected

def get_min_time_to_destination(N, P, pulsars, x_s, y_s, x_f, y_f):
    visited = [[False for _ in range(N)] for _ in range(N)]
    queue = [(x_s, y_s, 0)]
    while queue:
        x, y, time = queue.pop(0)
        if x == x_f and y == y_f:
            return time
        if not visited[x][y]:
            visited[x][y] = True
            for dx, dy in [(1, 0), (-1, 0), (0, 1), (0, -1)]:
                nx, ny = x + dx, y + dy
                if 0 <= nx < N and 0 <= ny < N and not visited[nx][ny]:
                    safe = True
                    for pulsar_x, pulsar_y, r, t in pulsars:
                        if abs(pulsar_x - nx) + abs(pulsar_y - ny) <= t:
                            safe = False
                            break
                    if safe:
                        queue.append((nx, ny, time + 1))
    return -1

def main():
    try:
        C, N, P, pulsars, x_s, y_s, x_f, y_f = read_input()
    except ValueError:
        print("Error: Input file is empty.")
        sys.exit(1)
    if C == 1:
        print(get_max_affected_sectors(N, P, pulsars))
    elif C == 2:
        print(get_min_time_to_destination(N, P, pulsars, x_s, y_s, x_f, y_f))

if __name__ == "__main__":
    main()
