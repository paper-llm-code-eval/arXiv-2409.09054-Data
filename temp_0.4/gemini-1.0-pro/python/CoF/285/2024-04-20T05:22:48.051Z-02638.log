import sys

def max_affected_sectors(galaxy, pulsars):
    max_affected = 0
    for time in range(max(p[3] for p in pulsars)):
        affected = set()
        for pulsar in pulsars:
            x, y, r, t = pulsar
            for dx in range(-t, t + 1):
                for dy in range(-t, t + 1):
                    if abs(dx) + abs(dy) <= t:
                        if 1 <= x + dx <= galaxy and 1 <= y + dy <= galaxy:
                            affected.add((x + dx, y + dy))
            # Check if the pulsar is located at the edge of the galaxy
            if x == 1 or x == galaxy or y == 1 or y == galaxy:
                # Adjust the radius of influence to account for the edge of the galaxy
                r = min(r, galaxy - max(x, y))
                for dx in range(-r, r + 1):
                    for dy in range(-r, r + 1):
                        if abs(dx) + abs(dy) <= r:
                            if 1 <= x + dx <= galaxy and 1 <= y + dy <= galaxy:
                                affected.add((x + dx, y + dy))
            # Check if the pulsar is located at the corner of the galaxy
            if (x == 1 and y == 1) or (x == 1 and y == galaxy) or (x == galaxy and y == 1) or (x == galaxy and y == galaxy):
                # Adjust the radius of influence to account for the corner of the galaxy
                r = min(r, galaxy - 1)
                for dx in range(-r, r + 1):
                    for dy in range(-r, r + 1):
                        if abs(dx) + abs(dy) <= r:
                            if 1 <= x + dx <= galaxy and 1 <= y + dy <= galaxy:
                                affected.add((x + dx, y + dy))
        max_affected = max(max_affected, len(affected))
    return max_affected

def min_time_to_destination(galaxy, pulsars, start, end):
    queue = [(0, start)]
    visited = set()
    while queue:
        time, current = queue.pop(0)
        if current == end:
            return time
        if current in visited:
            continue
        visited.add(current)
        for dx in range(-1, 2):
            for dy in range(-1, 2):
                if abs(dx) + abs(dy) == 1:
                    next = (current[0] + dx, current[1] + dy)
                    if next not in visited and is_safe(next, pulsars, time):
                        queue.append((time + 1, next))
    return -1

def is_safe(sector, pulsars, time):
    for pulsar in pulsars:
        x, y, r, t = pulsar
        if abs(sector[0] - x) + abs(sector[1] - y) <= t + time % r:
            return False
    return True

