with open("robot.in", "r") as f:
    command = f.readline().strip()
    if command == "1":
        program = f.readline().strip()
        pos = 0
        screen = ""
        for instruction in program:
            if instruction == "D":
                pos = (pos + int(program[program.index(instruction) + 1])) % 10
            elif instruction == "S":
                pos = (pos - int(program[program.index(instruction) + 1])) % 10
            elif instruction == "A":
                screen += str(pos)
        with open("robot.out", "w") as g:
            g.write(str(len(screen)) + "\n")
    elif command == "2":
        program = f.readline().strip()
        pos = 0
        screen = ""
        for instruction in program:
