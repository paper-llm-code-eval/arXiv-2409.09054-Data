            elif instruction == "S":
                pos = (pos - int(program[program.index(instruction) + 1])) % 10
            elif instruction == "A":
                screen += str(pos)
        with open("robot.out", "w") as g:
            g.write(screen + "\n")
    elif command == "3":
        n = int(f.readline().strip())
        program = ""
        while n > 0:
            if n % 10 != 0:
                program += "D" + str(n % 10) + "A"
            n //= 10
        program += "T"
        with open("robot.out", "w") as g:
            g.write(program + "\n")
