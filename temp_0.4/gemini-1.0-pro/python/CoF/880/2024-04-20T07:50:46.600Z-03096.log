with open('robot.in', 'r') as f:
    c = int(f.readline())
    if c == 1:
        prog = f.readline().strip()
        pos = 0
        cnt = 0
        for ch in prog:
            if ch == 'A':
                cnt += 1
            elif ch == 'D':
                pos += int(prog[prog.index(ch) + 1])
                pos %= 10
            else:
                pos -= int(prog[prog.index(ch) + 1])
                pos %= 10
        with open('robot.out', 'w') as g:
            g.write(str(cnt))
    elif c == 2:
        prog = f.readline().strip()
        pos = 0
        screen = ''
        for ch in prog:
            if ch == 'A':
                screen += str(pos)
            elif ch == 'D':
                pos += int(prog[prog.index(ch) + 1])
                pos %= 10
            else:
                pos -= int(prog[prog.index(ch) + 1])
                pos %= 10
        with open('robot.out', 'w') as g:
            g.write(screen)
    else:
        n = int(f.readline())
        prog = ''
        pos = 0
        while n > 0:
            if n % 10 != pos:
                if pos < n % 10:
                    prog += 'D' + str(n % 10 - pos)
                else:
                    prog += 'S' + str(pos - n % 10)
                pos = n % 10
            prog += 'A'
            n //= 10
        prog += 'T'
        with open('robot.out', 'w') as g:
            g.write(prog)
