            if (x, y) == (n, n):
                f.write(str(cnt) + '\n')
                break
            if (x, y) not in visited:
                visited.add((x, y))
                if x > 1 and a[x - 1][y] >= g:
                    q.append((x - 1, y, cnt))
                if x < n and a[x + 1][y] >= g:
                    q.append((x + 1, y, cnt))
                if y > 1 and a[x][y - 1] >= g:
                    q.append((x, y - 1, cnt))
                if y < n and a[x][y + 1] >= g:
                    q.append((x, y + 1, cnt + 1))
    else:
        # Binary search to find the maximum weight that can reach (n, n) without crossing dangerous zones
        left, right = 0, 10000
        while left <= right:
            mid = (left + right) // 2
            q = [(1, 1, mid)]
            visited = set()
            can_reach = True
            while q:
                x, y, w = q.pop(0)
                if (x, y) == (n, n):
                    can_reach = True
                    break
                if (x, y) not in visited:
                    visited.add((x, y))
                    if x > 1 and a[x - 1][y] >= w:
                        q.append((x - 1, y, w))
                    if x < n and a[x + 1][y] >= w:
                        q.append((x + 1, y, w))
                    if y > 1 and a[x][y - 1] >= w:
                        q.append((x, y - 1, w))
                    if y < n and a[x][y + 1] >= w:
                        q.append((x, y + 1, w))
            if can_reach:
                left = mid + 1
            else:
                right = mid - 1
        f.write(str(right) + '\n')
