import sys

def is_palindrome(n):
    return str(n) == str(n)[::-1]

def solve(n, s, a):
    if s == 0:
        return 0
    if n == 0:
        return s
    if a[n][s] != -1:
        return a[n][s]
    a[n][s] = min(solve(n - 1, s, a), 1 + solve(n - 1, s - len(str(n)) % 2, a))
    return a[n][s]

def main():
    with open('palindrom.in', 'r') as f:
        c = int(f.readline())
        n = int(f.readline())
        a = [[0 for _ in range(500001)] for _ in range(50001)]
        for i in range(n):
            a[i][0] = 500000
        if c == 1:
            print(solve(n - 1, s, a))
        else:
            s = int(f.readline())
            print(solve(n - 1, s, a))

if __name__ == '__main__':
    main()
```
