import sys

def main():
    n, k = map(int, sys.stdin.readline().split())
    graph = [[] for _ in range(n + 1)]
    for _ in range(n - 1):
        a, b = map(int, sys.stdin.readline().split())
        graph[a].append(b)
        graph[b].append(a)

    # Find the center of the graph
    dist = [0] * (n + 1)
    visited = [False] * (n + 1)
    queue = [1]
    visited[1] = True
    while queue:
        u = queue.pop(0)
        for v in graph[u]:
            if not visited[v]:
                dist[v] = dist[u] + 1
                queue.append(v)
                visited[v] = True
    center = 1
    for i in range(2, n + 1):
        if dist[i] > dist[center]:
            center = i

    # Find the k shortest paths from the center
    paths = []
    for i in range(1, n + 1):
        if i != center:
            path = [i]
            u = i
            while u != center:
                for v in graph[u]:
                    if dist[v] == dist[u] - 1:
                        path.append(v)
                        u = v
                        break
            paths.append(path)
    paths.sort(key=lambda path: len(path))

    # Choose the k shortest paths and calculate the total cost
    total_cost = 0
    for path in paths[:k]:
        for i in range(1, len(path)):
            total_cost += 1

    # Write the output
    sys.stdout.write(str(total_cost) + "\n")

if __name__ == "__main__":
    main()
