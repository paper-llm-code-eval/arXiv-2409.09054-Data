import sys

with open("ocr.in", "r") as f:
    n, m = map(int, f.readline().split())
    img = [list(map(float, line.split())) for line in f]

sums_row = [sum(row) for row in img]
sums_col = [sum(col) for col in zip(*img)]

min_diff_row = min(abs(sums_row[i] - sums_row[n - i - 1]) for i in range(n))
min_diff_col = min(abs(sums_col[j] - sums_col[m - j - 1]) for j in range(m))

for i in range(n):
    if abs(sums_row[i] - sums_row[n - i - 1]) == min_diff_row:
        for j in range(m):
            if abs(sums_col[j] - sums_col[m - j - 1]) == min_diff_col:
                with open("ocr.out", "w") as f:
                    f.write(f"{i + 1} {j + 1}")
                sys.exit(0)
```
