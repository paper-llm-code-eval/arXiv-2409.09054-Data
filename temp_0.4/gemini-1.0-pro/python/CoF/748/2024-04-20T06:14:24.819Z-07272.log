import sys

def read_input(file):
    n = int(file.readline())
    soldiers = [int(file.readline()) for _ in range(n)]
    return n, soldiers

def find_platoons(soldiers):
    platoons = {}
    for soldier in soldiers:
        digits = sorted(str(soldier))
        key = ''.join(digits)
        if key in platoons:
            platoons[key].append(soldier)
        else:
            platoons[key] = [soldier]
    return platoons

def get_max_platoon(platoons):
    max_size = 0
    max_platoon = []
    for platoon in platoons.values():
        size = len(platoon)
        if size > max_size:
            max_size = size
            max_platoon = platoon
    return max_size, max_platoon

def write_output(file, num_platoons, max_size, num_max_platoons, max_platoon):
    file.write(f'{num_platoons}\n')
    file.write(f'{max_size}\n')
    file.write(f'{num_max_platoons}\n')
    file.write(' '.join(map(str, max_platoon)))

def main():
    with open('pluton.in', 'r') as input_file, open('pluton.out', 'w') as output_file:
        n, soldiers = read_input(input_file)
        platoons = find_platoons(soldiers)
        max_size, max_platoon = get_max_platoon(platoons)
        num_platoons = len(platoons)
        num_max_platoons = sum(1 for platoon in platoons.values() if len(platoon) == max_size)
        write_output(output_file, num_platoons, max_size, num_max_platoons, max_platoon)

if __name__ == '__main__':
    main()
