import sys

def main():
    with open('pluton.in', 'r') as f_in, open('pluton.out', 'w') as f_out:
        n = int(f_in.readline())
        nums = [int(f_in.readline()) for _ in range(n)]
        nums.sort()

        plutons = []
        pluton = []
        for num in nums:
            if not pluton or num // 1000000 != pluton[0] // 1000000:
                if pluton:
                    plutons.append(pluton)
                pluton = [num]
            else:
                pluton.append(num)
        if pluton:
            plutons.append(pluton)

        f_out.write(f'{len(plutons)}\n')
        max_pluton = max(plutons, key=len)
        f_out.write(f'{len(max_pluton)}\n')
        f_out.write(f'{plutons.count(max_pluton)}\n')
        f_out.write(' '.join(map(str, max_pluton)))

if __name__ == '__main__':
    main()
```
