import sys

def read_input():
    V = int(input())
    N = int(input())
    C = [int(x) for x in input().split()]
    return V, N, C

def solve_part1(N, C):
    level = 1
    max_sum = 0
    for i in range(N):
        sum = 0
        for j in range(i, N * (N + 1) // 2, N):
            sum += C[j]
        if sum > max_sum:
            max_sum = sum
            level = i // N + 1
    return level

def solve_part2(N, C):
    time = 0
    spilled = 0
    levels = [[] for _ in range(N)]
    for i in range(N):
        for j in range(i, N * (N + 1) // 2, N):
            levels[i // N].append(C[j])
    for level in levels:
        while level:
            time += 1
            for i in range(len(level) - 1, -1, -2):
                level[i] -= 1
                if level[i] == 0:
                    del level[i]
                if i > 0:
                    level[i - 1] += 1
            for i in range(len(level) - 1, -1, -2):
                level[i] -= 1
                if level[i] == 0:
                    del level[i]
                if i + 1 < len(level):
                    level[i + 1] += 1
    for level in levels:
        spilled += sum(level)
    return time, spilled

