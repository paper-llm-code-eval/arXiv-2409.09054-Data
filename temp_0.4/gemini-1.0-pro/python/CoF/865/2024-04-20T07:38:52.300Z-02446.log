    if V == 1:
        result = solve_part1(N, C)
        sys.stdout.write(str(result) + '\n')
    else:
        time, spilled = solve_part2(N, C)
        sys.stdout.write(str(time) + ' ' + str(spilled) + '\n')

if __name__ == '__main__':
