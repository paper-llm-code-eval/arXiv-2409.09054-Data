with open('compresie.in', 'r') as f:
    compresie = f.readline().strip()

nr_impartiri = 0
matrice = []

def decompresie(compresie, x1, y1, x2, y2):
    global nr_impartiri
    global matrice

    if x2 - x1 == 0 and y2 - y1 == 0:
        matrice[x1][y1] = compresie
        return

    if compresie[0] == '*':
        nr_impartiri += 1
        mij_x = (x1 + x2) // 2
        mij_y = (y1 + y2) // 2

        decompresie(compresie[1:], x1, y1, mij_x, mij_y)
        decompresie(compresie[1:], x1, mij_y + 1, mij_x, y2)
        decompresie(compresie[1:], mij_x + 1, y1, x2, mij_y)
        decompresie(compresie[1:], mij_x + 1, mij_y + 1, x2, y2)
    else:
        for i in range(x1, x2 + 1):
            for j in range(y1, y2 + 1):
                matrice[i][j] = compresie[0]

n = int(compresie[1])

for i in range(n):
    matrice.append([' '] * n)

decompresie(compresie, 0, 0, n - 1, n - 1)

with open('compresie.out', 'w') as f:
    f.write(str(nr_impartiri) + '\n')
    for linie in matrice:
        f.write(''.join(linie) + '\n')
```
