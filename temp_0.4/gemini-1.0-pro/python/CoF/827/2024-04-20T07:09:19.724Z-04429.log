import sys

with open("compresie.in", "r") as f:
    comprimat = f.readline()

N = int(comprimat[0])
comprimat = comprimat[1:]

nr_impartiri = 0
matrice = [[' ' for _ in range(N)] for _ in range(N)]

def decompresie(comprimat, x1, y1, x2, y2):
    global nr_impartiri
    if x1 == x2 and y1 == y2:
        matrice[x1][y1] = comprimat
        return
    if comprimat[0] == '*':
        nr_impartiri += 1
        comprimat = comprimat[1:]
        mij_x = (x1 + x2) // 2
        mij_y = (y1 + y2) // 2
        decompresie(comprimat, x1, y1, mij_x, mij_y)
        decompresie(comprimat, x1, mij_y + 1, mij_x, y2)
        decompresie(comprimat, mij_x + 1, y1, x2, mij_y)
        decompresie(comprimat, mij_x + 1, mij_y + 1, x2, y2)
    else:
        nr_caractere = int(comprimat[:2])
        caracter = comprimat[2]
        comprimat = comprimat[3:]
        for i in range(nr_caractere):
            if x1 + i < N:
                matrice[x1 + i][y1] = caracter

decompresie(comprimat, 0, 0, N - 1, N - 1)

with open("compresie.out", "w") as f:
    f.write(str(nr_impartiri) + "\n")
    for linie in matrice:
        f.write(''.join(linie) + "\n")
