    jucator = 3 - jucator

if pozitie[0] == 1 or puncte[1] > puncte[0]:
    jucator_castigator = 2
else:
    jucator_castigator = 1

with open('joc.out', 'w') as f:
    f.write(str(jucator_castigator) + '\n')
    f.write(str(pozitie[0]) + ' ' + str(puncte[0]) + '\n')
    f.write(str(pozitie[1]) + ' ' + str(puncte[1]))
