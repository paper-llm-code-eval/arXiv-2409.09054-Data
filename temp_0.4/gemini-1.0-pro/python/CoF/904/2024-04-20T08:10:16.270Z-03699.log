with open('deminare.in') as f, open('deminare.out', 'w') as g:
    V = int(f.readline())
    L, C = map(int, f.readline().split())
    M = int(f.readline())
    mine = set()
    for _ in range(M):
        x, y = map(int, f.readline().split())
        mine.add((x, y))
    if V == 1:
        max_mine = 0
        max_line = []
        for i in range(1, L + 1):
            cnt = 0
            for j in range(1, C + 1):
                if (i, j) in mine:
                    cnt += 1
            if cnt > max_mine:
                max_mine = cnt
                max_line = [i]
            elif cnt == max_mine:
                max_line.append(i)
        g.write(' '.join(map(str, max_line)))
    else:
        # BFS
        vis = [[False] * (C + 1) for _ in range(L + 1)]
        dist = [[0] * (C + 1) for _ in range(L + 1)]
        q = []
        for x, y in mine:
            vis[x][y] = True
            dist[x][y] = 0
            q.append((x, y))
        min_dist = L * C
        while q:
            x, y = q.pop(0)
            for dx, dy in [(0, 1), (0, -1), (1, 0), (-1, 0)]:
                nx, ny = x + dx, y + dy
                if 1 <= nx <= L and 1 <= ny <= C and not vis[nx][ny]:
                    vis[nx][ny] = True
                    dist[nx][ny] = dist[x][y] + 1
                    q.append((nx, ny))
                    if (nx, ny) in mine:
                        min_dist = min(min_dist, dist[nx][ny])
        if min_dist == L * C:
            g.write('-1')
        else:
            g.write(str(min_dist))
