def is_chessboard(a):
    n = len(a)
    for i in range(n):
        for j in range(n):
            if i + 1 < n and a[i][j] == a[i + 1][j]:
                return False
            if j + 1 < n and a[i][j] == a[i][j + 1]:
                return False
    return True


def count_swaps(a):
    n = len(a)
    swaps = 0
    for i in range(n):
        for j in range(n):
            if a[i][j] != (i + j) % 2:
                swaps += 1
    return swaps // 2


def get_swaps(a):
    n = len(a)
    swaps = []
    for i in range(n):
        for j in range(n):
            if a[i][j] != (i + j) % 2:
                if a[n - 1 - i][n - 1 - j] == (i + j) % 2:
                    swaps.append(('L', i + 1, n - i))
                    a[i][j], a[n - 1 - i][n - 1 - j] = a[n - 1 - i][n - 1 - j], a[i][j]
                else:
                    swaps.append(('C', i + 1, n - j))
                    a[i][j], a[i][n - 1 - j] = a[i][n - 1 - j], a[i][j]
    return swaps


