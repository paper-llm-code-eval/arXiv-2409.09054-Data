import sys

sys.stdin = open('insule.in')
sys.stdout = open('insule.out', 'w')

n, m = map(int, input().split())
grid = [list(map(int, input())) for _ in range(n)]

def dfs(x, y, color, visited):
    if x < 0 or x >= n or y < 0 or y >= m or grid[x][y] != color or (x, y) in visited:
        return 0
    visited.add((x, y))
    return 1 + dfs(x + 1, y, color, visited) + dfs(x - 1, y, color, visited) + dfs(x, y + 1, color, visited) + dfs(x, y - 1, color, visited)

def get_islands():
    visited = set()
    islands = [0, 0, 0]
    for i in range(n):
        for j in range(m):
            if (i, j) not in visited:
                islands[grid[i][j]] += dfs(i, j, grid[i][j], visited)
    return islands

def get_min_bridge():
    for i in range(n):
        for j in range(m):
            if grid[i][j] == 0:
                visited = set()
                visited.add((i, j))
                q = [(i, j, 0)]
                while q:
                    x, y, dist = q.pop(0)
                    if (x, y) not in visited:
                        visited.add((x, y))
                        if grid[x][y] == 1 or grid[x][y] == 2:
                            return dist
                        q.append((x + 1, y, dist + 1))
                        q.append((x - 1, y, dist + 1))
                        q.append((x, y + 1, dist + 1))
                        q.append((x, y - 1, dist + 1))
    return -1

islands = get_islands()
min_bridge = get_min_bridge()
print(*islands, min_bridge)
