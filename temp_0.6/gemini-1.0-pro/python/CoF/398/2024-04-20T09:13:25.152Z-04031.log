                while i + l < n and j + l < m and grid[i + l][j + l] == 0:
                    l += 1
                while i - l >= 0 and j - l >= 0 and grid[i - l][j - l] == 0:
                    l += 1
                while i + l < n and j - l >= 0 and grid[i + l][j - l] == 0:
                    l += 1
                while i - l >= 0 and j + l < m and grid[i - l][j + l] == 0:
                    l += 1
                lg = min(lg, l - 1)
                if l > 1:
                    found_bridge = True
    if not found_bridge:
        lg = 0
    f_out.write(f'{nr} {ng} {nb} {lg}')
