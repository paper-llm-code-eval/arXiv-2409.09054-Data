with open('litere.in') as f:
    n = int(f.readline())
    word = f.readline().strip()
    m = int(f.readline())
    codes = [int(x) for x in f.readline().split()]

vocale = set(['A', 'E', 'I', 'O', 'U'])
k = [0] * n
k[n - 1] = codes[word[n - 1] - ord('A')]
for i in range(n - 2, -1, -1):
    k[i] = k[i + 1] * 10 + codes[word[i] - ord('A')]

perechi = 0
for i in range(n - 1):
    if (word[i] in vocale) ^ (word[i + 1] in vocale):
        perechi += 1

with open('litere.out', 'w') as f:
    f.write(f'{perechi}\n')
    f.write(f'{k[0]}\n')
    f.write(f'{sum(k)}\n')
```
