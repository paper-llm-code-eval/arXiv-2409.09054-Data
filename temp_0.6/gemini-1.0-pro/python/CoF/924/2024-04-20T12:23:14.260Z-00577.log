import sys

def read_data(filename):
    with open(filename, "r") as f:
        c = int(f.readline())
        n = int(f.readline())
        values = [int(x) for x in f.readline().split()]
    return c, n, values

def solve_1(n, values):
    m = 0
    for i in range(2, n + 1):
        if sum(values[0:i]) % i == 0:
            m += 1
    return m

def solve_2(n, values):
