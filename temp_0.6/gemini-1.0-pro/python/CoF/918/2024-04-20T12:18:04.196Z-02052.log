with open('spiralmatrix.in', 'r') as f:
    n = int(f.readline())

def spiral(n):
    matrix = [[0] * n for _ in range(n)]
    x, y = 0, 0
    num = 1
    while num <= n**2:
        while x < n and matrix[x][y] == 0:
            matrix[x][y] = num
            num += 1
            x += 1
        x -= 1
        y += 1
        while y < n and matrix[x][y] == 0:
            matrix[x][y] = num
            num += 1
            y += 1
        y -= 1
        x -= 1
        while x >= 0 and matrix[x][y] == 0:
            matrix[x][y] = num
            num += 1
            x -= 1
        x += 1
        y -= 1
        while y >= 0 and matrix[x][y] == 0:
            matrix[x][y] = num
            num += 1
            y -= 1

    max1, min2 = 0, n**2 + 1
    for i in range(n):
        for j in range(n):
            if matrix[i][j] == n**2 / 2:
                max1 = matrix[i][j]
                x1, y1 = i + 1, j + 1
            if matrix[i][j] == n**2 / 2 + 1:
                min2 = matrix[i][j]
                x2, y2 = i + 1, j + 1

    return x1, y1, x2, y2

x1, y1, x2, y2 = spiral(n)
with open('spiralmatrix.out', 'w') as f:
    f.write(f'{x1} {y1}\n{x2} {y2}')
```
