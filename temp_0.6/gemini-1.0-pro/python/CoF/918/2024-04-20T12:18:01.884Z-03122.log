with open('spiralmatrix.in', 'r') as f:
    n = int(f.readline())

# Calculate the total number of elements in the matrix
n2 = n * n

# Calculate the length of the first subarray
l1 = (n2 // 2) // 2

# Calculate the indices of the first element in each subarray
i1 = 1
j1 = 1
i2 = n
j2 = n

# Iterate over the matrix in a spiral pattern
for k in range(n2):
    # Move right
    while j1 <= n and k < l1:
        if k == l1 - 1:
            i_max = i1
            j_max = j1
        j1 += 1
        k += 1
    
    # Move down
    while i1 <= n and k < l1:
        if k == l1 - 1:
            i_max = i1
            j_max = j1
        i1 += 1
        k += 1
    
    # Move left
    while j1 >= 1 and k < n2:
        if k == l1:
            i_min = i1
            j_min = j1
        j1 -= 1
        k += 1
    
    # Move up
    while i1 >= 1 and k < n2:
        if k == l1:
            i_min = i1
            j_min = j1
        i1 -= 1
        k += 1

# Write the output to the output file
with open('spiralmatrix.out', 'w') as f:
    f.write(f'{i_max} {j_max}\n')
    f.write(f'{i_min} {j_min}\n')
```
