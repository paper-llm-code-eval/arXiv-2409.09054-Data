def decompresie(compresie):
  nr_impartiri = 0
  matrice = []
  i = 0
  while i < len(compresie):
    if compresie[i] == '*':
      nr_impartiri += 1
      i += 1
      matrice_a, i = decompresie(compresie[i:])
      matrice_b, i = decompresie(compresie[i:])
      matrice_c, i = decompresie(compresie[i:])
      matrice_d, i = decompresie(compresie[i:])
      matrice.extend(matrice_a)
      matrice.extend(matrice_b)
      matrice.extend(matrice_c)
      matrice.extend(matrice_d)
