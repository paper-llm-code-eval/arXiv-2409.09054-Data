with open("betasah.in") as fin, open("betasah.out", "w") as fout:
    n, d, k = map(int, fin.readline().split())
    rows = [0] * (n + 1)
    cols = [0] * (n + 1)
    diag1 = [0] * (2 * n + 1)
    diag2 = [0] * (2 * n + 1)
    m = 0
    for i in range(d):
        x, y = map(int, fin.readline().split())
        rows[x] += 1
        cols[y] += 1
        diag1[x + y] += 1
        diag2[x - y + n] += 1
        m = max(m, rows[x])
    for i in range(k):
        x, y = map(int, fin.readline().split())
        rows[x] -= 1
        cols[y] -= 1
        diag1[x + y] -= 1
        diag2[x - y + n] -= 1
    p = 0
    for i in range(1, n + 1):
        p += n - max(rows[i], cols[i], diag1[i], diag2[i])
    fout.write(f"{m}\n{p}\n")
```
