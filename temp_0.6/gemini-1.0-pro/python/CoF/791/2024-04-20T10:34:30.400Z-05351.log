with open('vecini.in', 'r') as f:
    m, n, k = map(int, f.readline().split())
    poz_1 = [list(map(int, f.readline().split())) for _ in range(k)]

mat = [[0] * (n + 2) for _ in range(m + 2)]
for i, j in poz_1:
    mat[i][j] = 1

nr_vecini = [[0] * (n + 2) for _ in range(m + 2)]

for i in range(1, m + 1):
    for j in range(1, n + 1):
        nr_vecini[i][j] = mat[i - 1][j - 1] + mat[i - 1][j] + mat[i - 1][j + 1] + mat[i][j - 1] + mat[i][j + 1] + mat[i + 1][j - 1] + mat[i + 1][j] + mat[i + 1][j + 1]

max_vecini = 0
for i in range(1, m + 1):
    for j in range(1, n + 1):
        if nr_vecini[i][j] > max_vecini:
            max_vecini = nr_vecini[i][j]

nr_max_vecini = 0
for i in range(1, m + 1):
    for j in range(1, n + 1):
        if nr_vecini[i][j] == max_vecini:
            nr_max_vecini += 1

with open('vecini.out', 'w') as f:
    f.write(f'{max_vecini} {nr_max_vecini}')
```
