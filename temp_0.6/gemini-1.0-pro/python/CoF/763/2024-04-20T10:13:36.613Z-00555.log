    path = list(map(int, f.readline().split()))
    
grid = [[(i + j) % 6 for j in range(n)] for i in range(n)]
    
freq = [0] * 9
    
count = 0
    
for move in path:
    count += grid[path[0] - 1][path[0] - 1]
    grid[path[0] - 1][path[0] - 1] = 0
    freq[path[0]] += 1
    path.pop(0)
    path.append(move)
    
max_freq = max(freq)
max_count = freq.count(max_freq)

with open('furnica.out', 'w') as f:
    f.write(f'{count} {max_count}')
