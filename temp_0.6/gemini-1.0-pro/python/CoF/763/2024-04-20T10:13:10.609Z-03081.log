with open('furnica.in', 'r') as f:
    n, k = map(int, f.readline().split())
    path = list(map(int, f.readline().split()))

board = [[(i + j) % 6 for j in range(n)] for i in range(n)]
visited = [[0 for j in range(n)] for i in range(n)]
max_count = 0
max_count_squares = []

x, y = 0, 0
total_crumbs = 0

for direction in path:
    nx, ny = x + [0, 1, 1, 1, 0, -1, -1, -1][direction] - 1, y + [-1, -1, 0, 1, 1, 1, 0, -1][direction] - 1
    total_crumbs += board[nx][ny]
    visited[nx][ny] += 1
    if visited[nx][ny] > max_count:
        max_count = visited[nx][ny]
        max_count_squares = [(nx, ny)]
    elif visited[nx][ny] == max_count:
        max_count_squares.append((nx, ny))
    x, y = nx, ny

with open('furnica.out', 'w') as f:
    f.write(f'{total_crumbs} {len(max_count_squares)}\n')
```
