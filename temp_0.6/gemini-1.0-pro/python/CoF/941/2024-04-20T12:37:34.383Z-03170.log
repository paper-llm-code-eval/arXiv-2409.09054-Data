import sys

def count_digits(N: int, X: int, arr: list) -> int:
    """Count the number of occurrences of the digit X in the given array."""
    count = 0
    for num in arr:
        while num > 0:
            if num % 10 == X:
                count += 1
            num //= 10
    return count

def count_cuts(N: int, arr: list) -> int:
    """Count the number of cuts needed to cut the given array of numbers into groups of at most two digits."""
    cuts = 0
    for num in arr:
        while num > 0:
            if num % 100 > 12:
                cuts += 1
                num //= 100
            elif num % 100 == 10:
                cuts += 1
                num //= 10
            else:
                num //= 100
    return cuts

if __name__ == "__main__":
    with open("ceas.in", "r") as f_in:
        C, X, N = map(int, f_in.readline().split())
        arr = list(map(int, f_in.readline().split()))

    if C == 1:
        result = count_digits(N, X, arr)
    elif C == 2:
        result = count_cuts(N, arr)
    else:
        raise ValueError("Invalid value for C")

    with open("ceas.out", "w") as f_out:
        f_out.write(str(result))
```
