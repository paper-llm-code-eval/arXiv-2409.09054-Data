def is_palindrome(n):
    return str(n) == str(n)[::-1]

def get_next_palindrome(n):
    n += 1
    while not is_palindrome(n):
        n += 1
    return n

def get_magic_sequence(palindromes):
    max_palindrome = max(palindromes)
    magic_sequence = []
    for palindrome in palindromes:
        if palindrome == max_palindrome:
            magic_sequence.append(palindrome)
        else:
            diff = len(str(max_palindrome)) - len(str(palindrome))
            if str(palindrome) == str(max_palindrome)[diff // 2: -diff // 2]:
                magic_sequence.append(palindrome)
    return magic_sequence

def main():
    with open('pal.in', 'r') as f_in:
        p = int(f_in.readline())
        n = int(f_in.readline())
        numbers = [int(x) for x in f_in.readline().split()]

    palindromes = [get_next_palindrome(number) for number in numbers]
    if p == 1:
        with open('pal.out', 'w') as f_out:
            f_out.write(' '.join([str(x) for x in palindromes]))
    elif p == 2:
        with open('pal.out', 'w') as f_out:
            f_out.write(str(len(get_magic_sequence(palindromes))))
    else:
        magic_sequence = get_magic_sequence(palindromes)
        with open('pal.out', 'w') as f_out:
            f_out.write(' '.join([str(x) for x in magic_sequence]))

if __name__ == '__main__':
    main()
