import sys
input = sys.stdin.readline
output = sys.stdout.write

def max_courses(intervals, k):
    intervals.sort(key=lambda x: x[1])
    
    end_times = []
    for i in range(k):
        end_times.append(0)
    
    count = 0
    for start, end in intervals:
        available_room = -1
        for i in range(k):
            if end_times[i] <= start:
                available_room = i
                break
        if available_room != -1:
            end_times[available_room] = end
            count += 1
    return count

def max_duration(intervals, k):
    max_duration = 0
    for start, end in intervals:
        max_duration = max(max_duration, end - start)
    
    intervals.sort(key=lambda x: x[0])
    
    available_rooms = [0] * k
    
    for start, end in intervals:
        room = -1
        for i in range(k):
            if available_rooms[i] <= start:
                room = i
                break
        if room != -1:
            available_rooms[room] = end
            max_duration = min(max_duration, end - start)
    return max_duration

c = int(input())
n, k = map(int, input().split())
intervals = []
for _ in range(n):
    a, b = map(int, input().split())
    intervals.append([a, b])

if c == 1:
    output(str(max_courses(intervals, k)))
else:
    output(str(max_duration(intervals, k)))
