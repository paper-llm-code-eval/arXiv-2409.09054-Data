                fout.write(str(i + 1) + ' ' + str(i) + '\n')
            vol[i] = 0
            vol[i + 1] = 0
        elif vol[i] == mx:
            if i == x:
                fout.write(str(i) + ' ' + str(i - 1) + '\n')
            vol[i] = 0
