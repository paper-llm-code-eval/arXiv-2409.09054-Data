with open('vase.in') as fin, open('vase.out', 'w') as fout:
    n = int(fin.readline())
    vol = [0] * 100001
    for i in range(n):
        v, s = map(str, fin.readline().split())
        vol[int(s)] += int(v)
    x = int(fin.readline())
    mx = max(vol)
    fout.write(str(mx) + '\n')
    for i in range(mx, 0, -1):
        if vol[i] == 0:
            continue
        if vol[i] == mx:
            if i == x:
                fout.write(str(i) + ' ' + '0' + '\n')
            continue
        if vol[i] + vol[i + 1] == mx:
