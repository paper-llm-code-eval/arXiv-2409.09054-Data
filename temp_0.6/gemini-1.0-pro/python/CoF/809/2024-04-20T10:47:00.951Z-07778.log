with open('carte.in') as f:
    n = int(f.readline())
    carte = list(map(int, f.readline().split()))

zi = 1
max_zi = 1
max_pagini = 0
pagini_zi = 0
for i in range(n):
    if carte[i] == zi:
        pagini_zi += 1
        zi += 1
    elif carte[i] > zi:
        if pagini_zi > max_pagini:
            max_pagini = pagini_zi
            max_zi = zi - 1
        zi = carte[i]
        pagini_zi = 1
if pagini_zi > max_pagini:
    max_pagini = pagini_zi
    max_zi = zi - 1

with open('carte.out', 'w') as f:
    f.write(f'{zi - 1} {max_zi} {max_pagini}')
```
