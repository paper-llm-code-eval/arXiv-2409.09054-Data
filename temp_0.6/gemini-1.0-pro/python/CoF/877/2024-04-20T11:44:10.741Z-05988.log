                g.write("R")
                privilegiati_real.pop(0)
            else:
                g.write("U")
                privilegiati_uman.pop(0)
        else:
            if privilegiati_real:
                g.write("R")
                privilegiati_real.pop(0)
            elif privilegiati_uman:
                g.write("U")
                privilegiati_uman.pop(0)
    g.write("\n")

else:
    max_admisi = 0
    rezultat = ""

    for i in range(n):
        r = bisect.bisect_left(real, privilegiati_real[i])
        u = bisect.bisect_left(uman, privilegiati_uman[i])

        if r < m or u < m:
            max_admisi += 1

            if r < m and u < m:
                if real[r] > uman[u]:
                    rezultat += "R"
                else:
                    rezultat += "U"
            elif r < m:
                rezultat += "R"
            else:
                rezultat += "U"

    g.write(str(max_admisi) + "\n")
    g.write(rezultat + "\n")

f.close()
g.close()
