with open('expresie.in') as f:
    expr = f.readline()

expr = expr.replace(' ', '')

numere = 0
for c in expr:
    if c.isdigit():
        numere += 1

stack = []
rez = 0
semn = 1

i = 0
while i < len(expr):
    if expr[i] == '(':
        stack.append((rez, semn))
        rez = 0
        semn = 1
        i += 1
        continue
    if expr[i] == '[':
        stack.append((rez, semn))
        rez = []
        semn = 1
        i += 1
        continue
    if expr[i] == ')':
        nr, semn = stack.pop()
        rez = nr + semn * rez
        i += 1
        continue
    if expr[i] == ']':
        nr, semn = stack.pop()
        rez.sort()
        rez = nr + semn * rez[len(rez) // 2]
        i += 1
        continue
    if expr[i] == ',':
        i += 1
        continue
    if expr[i] == '-':
        semn = -1
        i += 1
        continue
    rez += semn * int(expr[i])
    i += 1

with open('expresie.out', 'w') as f:
    f.write(f'{numere}\n')
    f.write(f'{rez}\n')
```
