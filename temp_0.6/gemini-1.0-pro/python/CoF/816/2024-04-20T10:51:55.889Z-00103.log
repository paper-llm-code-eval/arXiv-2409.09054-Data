expresie = open("expresie.in", "r").read()
cifre = 0
valoare = 0
i = 0
while i < len(expresie):
    if expresie[i].isdigit():
        cifre += 1
        while i < len(expresie) and expresie[i].isdigit():
            i += 1
        i -= 1
    elif expresie[i] == "(":
        start = i + 1
        paranteze = 1
        while paranteze > 0:
            i += 1
            if expresie[i] == "(":
                paranteze += 1
            elif expresie[i] == ")":
                paranteze -= 1
        valoare += sum(map(int, expresie[start:i].split(",")))
    elif expresie[i] == "[":
        start = i + 1
        paranteze = 1
        while paranteze > 0:
            i += 1
            if expresie[i] == "[":
                paranteze += 1
            elif expresie[i] == "]":
                paranteze -= 1
        numere = list(map(int, expresie[start:i].split(",")))
        numere.sort()
        valoare += numere[len(numere) // 2]
    i += 1
open("expresie.out", "w").write(f"{cifre}\n{valoare}")
```
