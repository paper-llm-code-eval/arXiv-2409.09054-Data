import heapq

with open('ubuntzei.in') as fin, open('ubuntzei.out', 'w') as fout:
    N, M = map(int, fin.readline().split())
    K = int(fin.readline())
    C = list(map(int, fin.readline().split()))
    C = C[1:]
    graph = [[] for i in range(N + 1)]
    for i in range(M):
        a, b, c = map(int, fin.readline().split())
        graph[a].append((b, c))
        graph[b].append((a, c))

    dist = [1 << 60] * (N + 1)
    dist[1] = 0
    heap = [(0, 1)]
    while heap:
        d, u = heapq.heappop(heap)
        if u == N:
            fout.write(str(d))
            break
        if d > dist[u]:
            continue
        for v, w in graph[u]:
            if d + w < dist[v]:
                dist[v] = d + w
                heapq.heappush(heap, (d + w, v))
```
