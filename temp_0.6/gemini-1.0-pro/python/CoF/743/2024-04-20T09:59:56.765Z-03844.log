def main():
    with open("muzica.in", "r") as f:
        a, b = map(int, f.readline().split())

    tema = []
    refren = []
    anterior = a
    while True:
        nota = a * 10 // b
        a = a * 10 % b
        if nota == anterior:
            break
        tema.append(nota)

    while True:
        nota = a * 10 // b
        a = a * 10 % b
        if nota in refren:
            break
        refren.append(nota)

    with open("muzica.out", "w") as f:
        f.write("".join(map(str, tema)) + "".join(map(str, refren)) + " " + str(len(refren)))


if __name__ == "__main__":
    main()
