import sys

def read_input(input_file):
    with open(input_file, "r") as f:
        p = int(f.readline())
        n, k = map(int, f.readline().split())
        a = list(map(int, f.readline().split()))
    return p, n, k, a

def solve_part1(n, k, a):
    if k > n:
        return len(set(a[:n]))
    b = sorted(set(a[:k]))
    if len(b) == k and b[-1] == b[0] + k - 1:
        return b[-1]
    return len(set(a[:k]))

def solve_part2(n, k, a):
    max_len = 0
    max_count = 0
    for i in range(n - k + 1):
        b = sorted(set(a[i:i+k]))
        if len(b) == k and b[-1] == b[0] + k - 1:
            if k > max_len:
                max_len = k
                max_count = 1
            elif k == max_len:
                max_count += 1
    return max_len, max_count

def write_output(output_file, result):
    with open(output_file, "w") as f:
        if isinstance(result, int):
            f.write(str(result) + "\n")
        else:
            f.write(str(result[0]) + "\n")
            f.write(str(result[1]) + "\n")

if __name__ == "__main__":
    input_file = sys.argv[1]
    output_file = sys.argv[2]
    p, n, k, a = read_input(input_file)
    if p == 1:
        result = solve_part1(n, k, a)
    else:
        result = solve_part2(n, k, a)
    write_output(output_file, result)
