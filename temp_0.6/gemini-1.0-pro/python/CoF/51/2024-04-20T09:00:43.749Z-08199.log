            adj_list[j].add(i)

visited = [False] * len(words)
cnt = 0

def dfs(node):
    global cnt
    if visited[node]:
        return
    visited[node] = True
    cnt += 1
    for neighbor in adj_list[node]:
        dfs(neighbor)

