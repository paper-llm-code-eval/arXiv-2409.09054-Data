import sys
from collections import defaultdict

with open("lant.in", "r") as f:
    k = int(f.readline())
    words = f.read().split()

sim = [[0] * len(words) for _ in range(len(words))]
for i in range(len(words)):
    for j in range(i + 1, len(words)):
        sim[i][j] = sim[j][i] = min(len(words[i]), len(words[j]))

for l in range(2, k + 1):
    for i in range(len(words)):
        for j in range(i + 1, len(words)):
            for m in range(len(words)):
                if sim[i][m] + sim[m][j] <= l:
                    sim[i][j] = sim[j][i] = min(sim[i][j], sim[i][m] + sim[m][j] - 1)

graph = defaultdict(list)
for i in range(len(words)):
    for j in range(len(words)):
        if sim[i][j] <= k:
            graph[i].append(j)

visited = [False] * len(words)
chains = [0] * len(words)

def dfs(node):
    if visited[node]:
        return

    visited[node] = True

    for neighbor in graph[node]:
        dfs(neighbor)

    chains[node] = 1 + max(chains[neighbor] for neighbor in graph[node])

for i in range(len(words)):
    dfs(i)

with open("lant.out", "w") as f:
    f.write(str(sum(chains)))
```

