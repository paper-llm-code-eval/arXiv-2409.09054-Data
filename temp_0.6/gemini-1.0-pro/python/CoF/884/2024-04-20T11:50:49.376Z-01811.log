        str: The player who won the round, or 'draw' if the game ended in a draw.
    """
    player = "X" if round_data[0] % 2 == 1 else "0"

    for move in round_data:
        row = (move - 1) // n
        col = (move - 1) % n
        board[row][col] = player
        winner = check_win(board, player)
        if winner != False:
            return winner

    return None


if __name__ == "__main__":
    with open("joc.in", "r") as f:
        c = int(f.readline())
        n, k = map(int, f.readline().split())
        board = [['.'] * n for _ in range(n)]
        winners = [0, 0]

        for round_data in [list(map(int, line.split())) for line in f]:
            winner = play_round(board, round_data)
            if winner == 'draw':
                break
            winners[0 if winner == "X" else 1] += 1

    with open("joc.out", "w") as f:
        if c == 1:
            f.write(" ".join(map(str, winners)))
        else:
            f.write(str(max([len(round_data) for round_data in [list(map(int, line.split())) for line in open("joc.in", "r").readlines()[2:]]])))
