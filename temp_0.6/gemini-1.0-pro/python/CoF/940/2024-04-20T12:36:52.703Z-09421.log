    with open('tort.in', 'r') as f_in, open('tort.out', 'w') as f_out:
        n = int(f_in.readline())
        a = list(map(int, f_in.readline().split()))
        
        # Prefix sum
        s = [0] * n
        for i in range(n):
            s[i] = a[i]
            if i > 0:
                s[i] += s[i - 1]
        
        # Suffix sum
        t = [0] * n
        for i in range(n - 1, -1, -1):
            t[i] = a[i]
            if i < n - 1:
                t[i] += t[i + 1]
        
        # Count the number of ways to split the cake
        ans = 0
        for i in range(n):
            for j in range(i + 1, n):
                if s[j - 1] == t[j]:
                    ans += 1
        
        f_out.write(str(ans))

if __name__ == '__main__':
    main()
