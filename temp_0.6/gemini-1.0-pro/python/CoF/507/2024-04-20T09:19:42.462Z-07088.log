import sys

MAX_TIME = 18
MAX_NOTE = 300000
MAX_HEIGHT = 10000

# Read input
n = int(sys.stdin.readline())
notes = []
for _ in range(n):
    x, y = map(int, sys.stdin.readline().split())
    notes.append((x, y))

# Group notes by duration
groups = [[] for _ in range(MAX_TIME + 1)]
for x, y in notes:
    groups[x].append(y)

# Calculate the maximum score
max_score = 0
for i in range(1, MAX_TIME + 1):
    group = groups[i]
    group.sort()
    while len(group) >= 2:
        a = group.pop()
        b = group.pop()
        max_score += (a + b) ** 2

# Write output
sys.stdout.write(str(max_score))
```
