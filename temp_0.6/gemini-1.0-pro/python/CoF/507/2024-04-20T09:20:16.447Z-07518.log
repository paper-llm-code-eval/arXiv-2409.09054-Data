import sys

sys.stdin = open("partitura.in", "r")
sys.stdout = open("partitura.out", "w")

n = int(input())
notes = []
for i in range(n):
    a, b = map(int, input().split())
    notes.append((a, b))

notes.sort(key=lambda x: x[0])

i = 0
j = 0
score = 0
while i < n:
    while j < n and notes[i][0] == notes[j][0]:
        j += 1
    if j - i > 0:
        sum = 0
        for k in range(i, j):
            sum += notes[k][1]
        score += sum * sum
    i = j

print(score)
```
