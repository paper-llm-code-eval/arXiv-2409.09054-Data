    if x1 == xpr and y1 == ypr:
        if cost < min_cost:
            min_cost = cost
            nrsol = 1
        elif cost == min_cost:
            nrsol += 1
        return

    # intoarcere daca s-a depasit costul minim
    if cost > min_cost:
        return

    # verificari daca se poate merge in continuare
