            if y1 > ypr:
                jos = False
            break

    # deplasare in sus
    if sus:
        back(x1, y1 + 1, cost + dist(x1, y1, x1, y1 + 1))

    # deplasare in jos
    if jos:
        back(x1, y1 - 1, cost + dist(x1, y1, x1, y1 - 1))

    # deplasare la dreapta
    if dreapta:
        back(x1 + 1, y1, cost + dist(x1, y1, x1 + 1, y1))

    # deplasare la stanga
    if stanga:
        back(x1 - 1, y1, cost + dist(x1, y1, x1 - 1, y1))

min_cost = float('inf')
nrsol = 0
back(xG, yG, 0)
