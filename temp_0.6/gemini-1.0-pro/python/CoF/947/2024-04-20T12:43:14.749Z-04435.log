with open('strips.in', 'r') as fin, open('strips.out', 'w') as fout:
    c, n, nr, l = map(int, fin.readline().split())
    if c == 1:
        penalties = [0, 0]
        board = [0] * n
        for pos in map(int, fin):
            if pos + l <= n and not board[pos:pos + l]:
                board[pos:pos + l] = [1] * l
            else:
                penalties[pos % 2] += 1
        fout.write(' '.join(map(str, penalties)))
    else:
        max_lengths = [0, 0]
        board = [0] * n
        for pos in map(int, fin):
            if pos + l <= n and not board[pos:pos + l]:
                board[pos:pos + l] = [1] * l
                max_lengths[pos % 2] = max(max_lengths[pos % 2], l)
            else:
                max_lengths[pos % 2] = max(max_lengths[pos % 2], pos)
        fout.write(' '.join(map(str, max_lengths)))
