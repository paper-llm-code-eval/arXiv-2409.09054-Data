        ans = solve_regio(N, C, X, D, dp)
    else:
        dp = [[[-1] * 3 for _ in range(C + 1)] for _ in range(N + 1)]
        ans = solve_expres(N, C, X, D, dp)

    with open('transport.out', 'w') as f:
        f.write(str(ans))

def solve_regio(N, C, X, D, dp):
    if dp[N][C][0] != -1:
        return dp[N][C][0]

    if N == 1:
        dp[N][C][0] = 1
    else:
        dp[N][C][0] = 0
        for i in range(1, C + 1):
            if N - 1 >= 0 and C - i >= D[N - 1]:
                dp[N][C][0] = (dp[N][C][0] + solve_regio(N - 1, C - i, X, D, dp)) % mod
    return dp[N][C][0]

def solve_expres(N, C, X, D, dp):
    if dp[N][C][1] != -1:
        return dp[N][C][1]

    if N == 1:
        dp[N][C][1] = 1
    else:
        dp[N][C][1] = 0
        for i in range(1, C + 1):
            if N < len(D) and C - i >= D[N]:
                dp[N][C][1] = (dp[N][C][1] + solve_expres(N - 1, C - i, X, D, dp)) % mod
            if N - 1 >= 0 and C - i >= D[N - 1]:
                dp[N][C][1] = (dp[N][C][1] + solve_regio(N - 1, C - i, X, D, dp)) % mod
    return dp[N][C][1]

if __name__ == '__main__':
    main()
