        dp[n][c][0][1] = (solve_expres(dp, x, d, n - 1, c - x[n] + x[n - 1]) + solve_expres(dp, x, d, n - 1, c - d[n])) % mod
        for i in range(2, n + 1):
            if x[i] - x[n - 1] <= c:
                dp[n][c][0][1] = (dp[n][c][0][1] + solve_expres(dp, x, d, i - 1, c - x[i] + x[n - 1])) % mod
            if x[i] - x[n] <= c:
                dp[n][c][0][1] = (dp[n][c][0][1] + solve_expres(dp, x, d, i - 1, c - x[i] + x[n])) % mod
    dp[n][c][1][1] = (dp[n][c][0][1] + solve_expres(dp, x, d, n - 1, c)) % mod
    return dp[n][c][1][1]

if __name__ == "__main__":
    main()
