fin = open('triunghiuri.in', 'r')
fout = open('triunghiuri.out', 'w')

p = int(fin.readline())
n = int(fin.readline())
x = [0] * n
y = [0] * n
for i in range(n):
    a, b = map(int, fin.readline().split())
    x[i] = a
    y[i] = b

if p == 1:
    mx = 1
    for i in range(n):
        cnt = 0
        for j in range(n):
            if x[i] == x[j]:
                cnt += 1
        mx = max(mx, cnt)
    fout.write(str(mx) + '\n')

elif p == 2:
