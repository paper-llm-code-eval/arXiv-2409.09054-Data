import sys

def defrag(pistes, secteurs, clusters):
    """
    Fonction qui défragmente un disque dur.

    Args:
        pistes (int): Le nombre de pistes.
        secteurs (int): Le nombre de secteurs.
        clusters (list): La liste des clusters occupés.

    Returns:
        list: La liste des nombres minimum de mouvements pour chaque piste.
    """

    # Création d'une matrice pour stocker les clusters occupés.
    matrice = [[0 for _ in range(secteurs)] for _ in range(pistes)]

    # Remplissage de la matrice avec les clusters occupés.
    for cluster in clusters:
        matrice[cluster[0] - 1][cluster[1] - 1] = 1

    # Création d'une liste pour stocker les nombres minimum de mouvements pour chaque piste.
    mouvements = [0 for _ in range(pistes)]

    # Parcours des pistes.
    for piste in range(pistes):
        # Comptage des clusters occupés sur la piste.
        clusters_occupes = sum(matrice[piste])

        # Si la piste est vide, on passe à la piste suivante.
        if clusters_occupes == 0:
            continue

        # Création d'une liste pour stocker les positions des clusters occupés.
        positions = []

        # Remplissage de la liste avec les positions des clusters occupés.
        for secteur in range(secteurs):
            if matrice[piste][secteur] == 1:
                positions.append(secteur)

        # Calcul du nombre minimum de mouvements.
        mouvements[piste] = len(positions) - 1

    # Retour de la liste des nombres minimum de mouvements.
    return mouvements

def main():
    """
    Fonction principale.
    """

    # Lecture du fichier d'entrée.
    with open("defrag.in", "r") as f:
        v = int(f.readline())
        p, s = map(int, f.readline().split())
        c = int(f.readline())
        clusters = [list(map(int, f.readline().split())) for _ in range(c)]

    # Défragmentation du disque dur.
    mouvements = defrag(p, s, clusters)

    # Écriture du fichier de sortie.
    with open("defrag.out", "w") as f:
        if v == 1:
            f.write(str(mouvements.count(0)) + "\n")
        else:
            f.write(" ".join(map(str, mouvements)) + "\n")

if __name__ == "__main__":
    main()
