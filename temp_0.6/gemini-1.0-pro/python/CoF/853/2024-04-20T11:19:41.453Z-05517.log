import sys

def defrag(pistes, secteurs, clusters):
    pistes_libres = 0
    min_muts = []
    for piste in pistes:
        if not piste:
            pistes_libres += 1
            min_muts.append(0)
            continue
        
        min_mut = len(piste) - 1
        for i in range(len(piste) - 1):
