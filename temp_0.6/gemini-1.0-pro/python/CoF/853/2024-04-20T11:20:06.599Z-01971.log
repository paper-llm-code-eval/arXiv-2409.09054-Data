import sys

with open('defrag.in') as f:
    v = int(f.readline().strip())
    p, s = map(int, f.readline().split())
    c = int(f.readline().strip())
    tracks = [[] for _ in range(p)]
    for _ in range(c):
        x, y = map(int, f.readline().split())
        tracks[x - 1].append(y - 1)

if v == 1:
    ans = 0
    for track in tracks:
        if len(track) == 0:
            ans += 1
    with open('defrag.out', 'w') as f:
        f.write(str(ans))
else:
    ans = []
    for track in tracks:
        track.sort()
        moves = 0
        last = -1
        for sector in track:
            if last != -1 and sector != last + 1:
                moves += sector - last - 1
            last = sector
        ans.append(moves)
    with open('defrag.out', 'w') as f:
        f.write(' '.join(map(str, ans)))
```
