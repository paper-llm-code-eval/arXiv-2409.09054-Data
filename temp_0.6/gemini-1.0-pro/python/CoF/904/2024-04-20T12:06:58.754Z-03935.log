for _ in range(M):
    x, y = map(int, input().split())
    grid[x][y] = 1

# Find the number of mines on each row and column
row_mines = [0] * (L+2)
col_mines = [0] * (C+2)
for i in range(1, L+1):
    for j in range(1, C+1):
        row_mines[i] += grid[i][j]
        col_mines[j] += grid[i][j]

# Find the row with the most mines
max_row_mines = max(row_mines)
max_row_indices = [i for i in range(1, L+1) if row_mines[i] == max_row_mines]

if V == 1:
    print(' '.join(map(str, max_row_indices)))
elif V == 2:
    # Check if the mines can be arranged in a compact rectangle
    can_arrange = False
    for i in range(1, L+1):
        for j in range(1, C+1):
            if grid[i][j] == 1:
                # Check if there is a compact rectangle containing this mine
                top = i
                bottom = i
                left = j
                right = j
                while top > 0 and grid[top-1][j] == 1:
                    top -= 1
                while bottom < L+1 and grid[bottom+1][j] == 1:
                    bottom += 1
                while left > 0 and grid[i][left-1] == 1:
                    left -= 1
                while right < C+1 and grid[i][right+1] == 1:
                    right += 1
                if (bottom - top + 1) * (right - left + 1) == row_mines[i] + col_mines[j] - grid[i][j]:
                    can_arrange = True
                    break
    if can_arrange:
        print(M - max(row_mines) - max(col_mines))
    else:
        print(-1)
