with open('deminare.in', 'r') as fin, open('deminare.out', 'w') as fout:
    v = int(fin.readline())
    l, c = map(int, fin.readline().split())
    m = int(fin.readline())
    mine = [list(map(int, fin.readline().split())) for _ in range(m)]

    if v == 1:
        max_mines = 0
        for i in range(l):
            mines_on_line = sum(1 for x, y in mine if x == i + 1)
            if mines_on_line > max_mines:
                max_mines = mines_on_line
                max_lines = [i + 1]
            elif mines_on_line == max_mines:
                max_lines.append(i + 1)
        fout.write(' '.join(map(str, max_lines)))
        fout.write('\n')
    elif v == 2:
        mine_map = [[False] * (c + 1) for _ in range(l + 1)]
        for x, y in mine:
            mine_map[x][y] = True

        min_moves = l * c + 1
        for x1 in range(1, l + 1):
            for y1 in range(1, c + 1):
                for x2 in range(x1, l + 1):
                    for y2 in range(y1, c + 1):
                        if all(mine_map[i][j] for i in range(x1, x2 + 1) for j in range(y1, y2 + 1)):
                            min_moves = min(min_moves, (x2 - x1 + 1) * (y2 - y1 + 1) - m)

        fout.write(str(min_moves if min_moves <= l * c else -1))
        fout.write('\n')
