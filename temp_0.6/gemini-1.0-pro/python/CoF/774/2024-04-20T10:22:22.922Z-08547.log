with open("turnuri.in") as f:
    m, n = map(int, f.readline().split())
    matrix = [[int(x) for x in f.readline().split()] for _ in range(m)]

towers = [0] * n
for i in range(m - 1, -1, -1):
    for j in range(n):
        if matrix[i][j] == 1:
            towers[j] += 1

max_height = 0
max_towers = 0
for i in range(n - 1):
    if towers[i] > 0 and towers[i + 1] > 0:
        new_height = towers[i] + towers[i + 1]
        if new_height > max_height:
            max_height = new_height
            max_towers = 1
        elif new_height == max_height:
            max_towers += 1
        towers[i] = 0
        towers[i + 1] = 0

with open("turnuri.out", "w") as f:
    f.write(" ".join(map(str, towers)) + "\n")
    f.write(str(max_height) + "\n")
    f.write(str(max_towers) + "\n")
```
