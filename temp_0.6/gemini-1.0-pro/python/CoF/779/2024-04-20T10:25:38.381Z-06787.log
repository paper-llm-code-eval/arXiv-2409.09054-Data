with open('turist.in') as f:
    M, N = map(int, f.readline().split())
    l, c = map(int, f.readline().split())
    k = int(f.readline())
    cities = set([(int(x), int(y)) for x, y in [f.readline().split() for _ in range(k)]])

max_cities = 0
for dx, dy in [(1, 0), (-1, 0), (0, 1), (0, -1)]:
    x, y = l, c
    num_cities = 0
    while 0 <= x < M and 0 <= y < N:
        if (x, y) in cities:
            num_cities += 1
        x += dx
        y += dy
    max_cities = max(max_cities, num_cities)

with open('turist.out', 'w') as f:
    f.write(str(max_cities))
```
