with open('valori-panta.in', 'r') as f:
    n = int(f.readline())
    v = [int(x) for x in f.readline().split()]

def este_panta(x):
    s = str(x)
    return all(s[i] <= s[i+1] for i in range(len(s)-1))

nr_pante = 0
max_panta = -1
poz_max = []
min_panta = 10**9
poz_min = []

for i in range(n):
    if este_panta(v[i]):
        nr_pante += 1
        if v[i] > max_panta:
            max_panta = v[i]
            poz_max = [i+1]
        elif v[i] == max_panta:
            poz_max.append(i+1)
        if v[i] < min_panta:
            min_panta = v[i]
            poz_min = [i+1]
        elif v[i] == min_panta:
            poz_min.append(i+1)

with open('valori-panta.out', 'w') as f:
    f.write(str(nr_pante) + '\n')
    if nr_pante > 0:
        f.write(str(max_panta) + ' ' + ' '.join(map(str, poz_max)) + '\n')
        f.write(str(min_panta) + ' ' + ' '.join(map(str, poz_min)) + '\n')
    else:
        f.write('NU EXISTA')
```
