with open("valori-panta.in") as f:
    n = int(f.readline())
    v = list(map(int, f.readline().split()))

cresc = [True for _ in range(n)]
decr = [True for _ in range(n)]

for i in range(1, n):
    if v[i] < v[i - 1]:
        cresc[i] = False
    if v[i] > v[i - 1]:
        decr[i] = False

nr_valori_panta = 0
max_valoare_panta = -1
poz_max_valoare_panta = []
min_valoare_panta = 10 ** 9
poz_min_valoare_panta = []

for i in range(n):
    if cresc[i] or decr[i]:
        nr_valori_panta += 1
        if v[i] > max_valoare_panta:
            max_valoare_panta = v[i]
            poz_max_valoare_panta = [i + 1]
        elif v[i] == max_valoare_panta:
            poz_max_valoare_panta.append(i + 1)
        if v[i] < min_valoare_panta:
            min_valoare_panta = v[i]
            poz_min_valoare_panta = [i + 1]
        elif v[i] == min_valoare_panta:
            poz_min_valoare_panta.append(i + 1)

with open("valori-panta.out", "w") as f:
    f.write(str(nr_valori_panta) + "\n")
    if nr_valori_panta > 0:
        f.write(str(max_valoare_panta) + " ")
        for poz in poz_max_valoare_panta:
            f.write(str(poz) + " ")
        f.write("\n")
        f.write(str(min_valoare_panta) + " ")
        for poz in poz_min_valoare_panta:
            f.write(str(poz) + " ")
    else:
        f.write("NU EXISTA")
```
