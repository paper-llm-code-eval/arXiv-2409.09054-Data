import sys

N, M, K = map(int, sys.stdin.readline().split())
edges = []
for i in range(M):
    a, b, w = map(int, sys.stdin.readline().split())
    edges.append((w, a, b))

edges.sort(reverse=True)

par = [i for i in range(N+1)]
rank = [0 for i in range(N+1)]

def findpar(x):
    if par[x] == x:
        return x
    return findpar(par[x])

def unite(a, b):
    a = findpar(a)
    b = findpar(b)
    if a != b:
        if rank[a] < rank[b]:
            par[a] = b
        else:
            par[b] = a
            if rank[a] == rank[b]:
                rank[a] += 1

ans = 0
for w, a, b in edges:
    if findpar(a) != findpar(b):
        ans += w
        unite(a, b)
        K -= 1
    if K == 0:
        break

print(ans)
