import sys

def read_input():
    n, m, k = map(int, sys.stdin.readline().split())
    edges = []
    for _ in range(m):
        a, b, w = map(int, sys.stdin.readline().split())
        edges.append((a, b, w))
    return n, m, k, edges

def write_output(gravmax, c, edges):
    sys.stdout.write(f'{gravmax}\n')
    sys.stdout.write(f'{c}\n')
    for edge in edges:
        sys.stdout.write(f'{edge[0]} {edge[1]}\n')

def find_max_weight_cut(n, m, k, edges):
    edges.sort(key=lambda x: x[2], reverse=True)
    par = [i for i in range(n + 1)]
    rnk = [0] * (n + 1)

    def find(x):
        if par[x] == x:
            return x
        return find(par[x])

    def union(a, b):
        a = find(a)
        b = find(b)
        if a != b:
            if rnk[a] < rnk[b]:
                par[a] = b
            else:
                par[b] = a
                if rnk[a] == rnk[b]:
                    rnk[a] += 1

    components = n
    max_weight = 0
    cut_edges = []
    for a, b, w in edges:
        if components == k:
            break
        if find(a) != find(b):
            union(a, b)
            max_weight += w
            cut_edges.append((a, b))
            components -= 1

    return max_weight, cut_edges

def main():
    n, m, k, edges = read_input()
    gravmax, cut_edges = find_max_weight_cut(n, m, k, edges)
    write_output(gravmax, len(cut_edges), cut_edges)

if __name__ == "__main__":
    main()
