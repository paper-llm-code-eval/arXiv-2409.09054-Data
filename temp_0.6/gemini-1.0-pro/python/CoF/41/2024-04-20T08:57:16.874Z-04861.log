with open("immortal.in", "r") as f:
    n, m, I = map(int, f.readline().split())
    grid = [[0] * m for _ in range(n)]
    for _ in range(I):
        x, y = map(lambda x: int(x) - 1, f.readline().split())
        grid[x][y] = 1

def valid(x, y):
    return 0 <= x < n and 0 <= y < m

def fight(x1, y1, x2, y2):
    grid[x1][y1] = 0
    grid[x2][y2] = 1

def solve():
    while True:
        found = False
        for i in range(n):
            for j in range(m):
                if grid[i][j]:
                    # check if there is a valid move
                    for dx, dy in [(1, 0), (-1, 0), (0, 1), (0, -1)]:
                        x, y = i + dx * 2, j + dy * 2
                        if valid(x, y) and grid[x][y] == 0:
                            fight(i, j, x, y)
                            with open("immortal.out", "a") as f:
                                f.write(f"{i+1} {j+1} {x+1} {y+1}\n")
                            found = True
                            break
            if found:
                break
        if not found:
            break

solve()
```
