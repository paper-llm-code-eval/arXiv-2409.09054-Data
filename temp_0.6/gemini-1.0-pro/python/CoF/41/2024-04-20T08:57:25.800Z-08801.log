with open('immortal.in') as f:
    n, m, i = map(int, f.readline().split())
    grid = [[0] * m for _ in range(n)]
    pos = [list(map(int, f.readline().split())) for _ in range(i)]
    for x, y in pos:
        grid[x - 1][y - 1] = 1

def valid(x, y):
    return 0 <= x < n and 0 <= y < m and grid[x][y]

def fight(x1, y1, x2, y2):
    grid[x1][y1], grid[x2][y2] = grid[x2][y2], grid[x1][y1]
    return x2, y2

with open('immortal.out', 'w') as f:
    while i > 1:
        for x1, y1 in pos:
            for dx, dy in [(0, 2), (0, -2), (2, 0), (-2, 0)]:
                x2, y2 = x1 + dx, y1 + dy
                if valid(x2, y2):
                    x1, y1 = fight(x1, y1, x2, y2)
                    f.write(f'{x1 + 1} {y1 + 1} {x2 + 1} {y2 + 1}\n')
                    i -= 1
                    pos.remove([x1 + 1, y1 + 1])
                    break
```
