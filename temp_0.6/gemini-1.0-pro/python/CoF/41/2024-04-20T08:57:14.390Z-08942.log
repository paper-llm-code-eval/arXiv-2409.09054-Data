with open("immortal.in", "r") as f:
    n, m, I = map(int, f.readline().split())
    pos = [list(map(int, f.readline().split())) for _ in range(I)]

def valid(x, y):
    return 1 <= x <= n and 1 <= y <= m

def fight(x1, y1, x2, y2):
    global pos
    if (x2, y2) in pos:
        pos.remove((x2, y2))
        pos.append((x1, y1))

while len(pos) > 1:
    for i in range(len(pos)):
        x1, y1 = pos[i]
        for dx, dy in [(2, 0), (-2, 0), (0, 2), (0, -2)]:
            x2, y2 = x1 + dx, y1 + dy
            if valid(x2, y2):
                fight(x1, y1, x2, y2)
                with open("immortal.out", "a") as f:
                    f.write(f"{x1} {y1} {x2} {y2}\n")
                break
```
