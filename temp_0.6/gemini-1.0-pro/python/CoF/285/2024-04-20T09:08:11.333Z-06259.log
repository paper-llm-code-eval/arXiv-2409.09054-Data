import math
from collections import deque

def read_input():
    with open('pulsar.in', 'r') as f:
        C, N, P = map(int, f.readline().split())
        pulsars = []
        for _ in range(P):
            x, y, r, t = map(int, f.readline().split())
            pulsars.append((x, y, r, t))
        x_s, y_s = map(int, f.readline().split())
        x_f, y_f = map(int, f.readline().split())
    return C, N, P, pulsars, x_s, y_s, x_f, y_f

def get_max_affected_sectors(N, P, pulsars):
    max_affected_sectors = 0
    for i in range(1, N + 1):
        for j in range(1, N + 1):
            affected_sectors = 0
            for pulsar in pulsars:
                x, y, r, t = pulsar
                distance = abs(x - i) + abs(y - j)
                if distance <= t:
                    affected_sectors += 1
            max_affected_sectors = max(max_affected_sectors, affected_sectors)
    return max_affected_sectors

def get_min_time_to_destination(N, P, pulsars, x_s, y_s, x_f, y_f):
    def is_safe(x, y):
        for pulsar in pulsars:
            x1, y1, r, t = pulsar
            distance = abs(x1 - x) + abs(y1 - y)
            if distance <= t:
                return False
        return True

    def bfs():
        queue = deque()
        queue.append((x_s, y_s))
        visited = set()
        visited.add((x_s, y_s))
        time = 0
        while queue:
            for _ in range(len(queue)):
                x, y = queue.popleft()
                if x == x_f and y == y_f:
                    return time
                for dx, dy in [(1, 0), (-1, 0), (0, 1), (0, -1)]:
                    nx, ny = x + dx, y + dy
                    if 1 <= nx <= N and 1 <= ny <= N and is_safe(nx, ny) and (nx, ny) not in visited:
                        queue.append((nx, ny))
                        visited.add((nx, ny))
            time += 1
        return -1

    return bfs()

def main():
    C, N, P, pulsars, x_s, y_s, x_f, y_f = read_input()
    if C == 1:
        max_affected_sectors = get_max_affected_sectors(N, P, pulsars)
        with open('pulsar.out', 'w') as f:
            f.write(str(max_affected_sectors))
    elif C == 2:
        min_time_to_destination = get_min_time_to_destination(N, P, pulsars, x_s, y_s, x_f, y_f)
        with open('pulsar.out', 'w') as f:
            f.write(str(min_time_to_destination))

if __name__ == '__main__':
    main()
