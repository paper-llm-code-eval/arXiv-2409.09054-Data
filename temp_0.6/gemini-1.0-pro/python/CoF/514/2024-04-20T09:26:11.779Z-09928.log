import sys

def is_prime(n):
    if n < 2:
        return False
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            return False
    return True

def closest_prime(n):
    if is_prime(n):
        return n
    i = n + 1
    while not is_prime(i):
        i += 1
    j = n - 1
    while not is_prime(j):
        j -= 1
    if abs(i - n) < abs(j - n):
        return i
    else:
        return j

def main():
    with open('primprim.in', 'r') as fin, open('primprim.out', 'w') as fout:
        C = int(fin.readline())
        n = int(fin.readline())
        arr = list(map(int, fin.readline().split()))
        if C == 1:
            fout.write(str(sum(abs(i - closest_prime(i)) for i in arr)))
        else:
            q = int(fin.readline())
            for line in fin:
                i, x, p = map(int, line.split())
                arr[i - 1] = x
                fout.write(str(sum(abs(arr[j - 1] - closest_prime(arr[j - 1])) for j in sorted(list(set(map(int, fin.readline().split())))))))

if __name__ == '__main__':
    main()
