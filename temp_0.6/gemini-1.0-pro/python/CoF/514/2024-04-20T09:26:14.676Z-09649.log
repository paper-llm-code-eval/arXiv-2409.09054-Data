import array

def is_prime(n: int) -> bool:
    if n < 2:
        return False
    for i in range(2, int(n ** 0.5) + 1):
        if n % i == 0:
            return False
    return True

with open('primprim.in', 'r') as fin, open('primprim.out', 'w') as fout:
    C = int(fin.readline())
    n = int(fin.readline())
    arr = array.array('i', map(int, fin.readline().split()))
    if C == 1:
        fout.write(str(sum(abs(x - ((x - 1) // 2 + 1)) for x in arr)))
    else:
        q = int(fin.readline())
        primes = [0] * n
        for i in range(n):
            if is_prime(arr[i]):
                primes[i] = arr[i]
            elif i > 0:
                primes[i] = primes[i - 1]
            else:
                primes[i] = -1
        for i in range(n - 1, -1, -1):
            if is_prime(arr[i]):
                primes[i] = arr[i]
            elif i < n - 1:
                primes[i] = primes[i + 1]
            else:
                primes[i] = -1
        for _ in range(q):
            i, x, p = map(int, fin.readline().split())
            i -= 1
            arr[i] = x
            if is_prime(x):
                primes[i] = x
            else:
                primes[i] = -1
                if i > 0:
                    primes[i] = max(primes[i - 1], primes[i])
                if i < n - 1:
                    primes[i] = max(primes[i], primes[i + 1])
            fout.write(str(sum(abs(arr[j] - primes[j]) for j in range(i, i + p))) + '\n')
