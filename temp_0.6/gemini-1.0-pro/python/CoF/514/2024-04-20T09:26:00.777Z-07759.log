                for i, x, p in (map(int, line.split()) for line in f.readlines()):
                    a[i - 1] = x
                    g.write(str(sum(min(abs(x - min(p, x - p)) for p in primes[max(0, x - 1000000):x + 1000001]))))

primes = sieve(1000000)
