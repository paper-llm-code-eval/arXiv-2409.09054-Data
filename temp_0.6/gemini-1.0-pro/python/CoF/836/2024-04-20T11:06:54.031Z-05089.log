with open("maxp.in", "r") as fin, open("maxp.out", "w") as fout:
    n = int(fin.readline())
    a = list(map(int, fin.readline().split()))

    max_power = 0
    max_power_count = 0
    power = [0] * n

    for i in range(n):
        for j in range(i):
            if a[i] > a[j]:
                power[i] += power[j] + 1
        if power[i] > max_power:
