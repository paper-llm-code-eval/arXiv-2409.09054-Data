def is_palindrome(n):
    # reverse the number
    rev = 0
    i = n
    while i > 0:
        rev = rev * 10 + i % 10
        i //= 10

    # if the reversed number is the same as the original number
    return n == rev

def max_palindrome(arr, n):
    # dp[i][j] stores the maximum palindrome number formed from arr[i] to arr[j]
    dp = [[0 for _ in range(n)] for _ in range(n)]

    # initialize the dp table for base cases
    for i in range(n):
        dp[i][i] = arr[i]

    # fill the dp table in bottom up manner
    for l in range(2, n + 1):
        for i in range(n - l + 1):
            j = i + l - 1
            if l == 2 and arr[i] == arr[j]:
                dp[i][j] = int(''.join(map(str, arr[i:j + 1])))
            else:
                if arr[i] == arr[j]:
                    dp[i][j] = int(''.join(map(str, arr[i:j + 1])))
                    dp[i][j] = max(dp[i][j], dp[i + 1][j - 1])
                else:
                    dp[i][j] = max(dp[i + 1][j], dp[i][j - 1])

    # find the maximum palindrome number
    max_palindrome = 0
    for i in range(n):
        for j in range(n):
            if is_palindrome(dp[i][j]) and dp[i][j] > max_palindrome:
                max_palindrome = dp[i][j]

    return max_palindrome

with open('mare.in', 'r') as f:
    n = int(f.readline())
    arr = list(map(int, f.readline().split()))

with open('mare.out', 'w') as f:
    f.write(str(max_palindrome(arr, n)))
```
