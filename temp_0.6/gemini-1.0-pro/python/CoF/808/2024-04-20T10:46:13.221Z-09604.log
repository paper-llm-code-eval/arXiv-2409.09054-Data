def solve(k, n):
    if k % 2 == 0:
        start = k * (k - 1) // 2 + 1
        end = k * (k + 1) // 2
    else:
        start = k * (k + 1) // 2
        end = k * (k - 1) // 2 + 1
    line = [end - i for i in range(k)]

    line_n = (n - 1) // k + 1
    col_n = (n - 1) % k + 1
    if line_n % 2 == 0:
        col_n = k - col_n + 1

    return line, line_n, chr(ord('A') + col_n - 1)

with open('numerus.in', 'r') as f:
    k, n = map(int, f.readline().split())

line, line_n, col_n = solve(k, n)
with open('numerus.out', 'w') as f:
    f.write(' '.join(map(str, line)) + '\n')
    f.write(str(line_n) + '\n')
    f.write(col_n)
```
