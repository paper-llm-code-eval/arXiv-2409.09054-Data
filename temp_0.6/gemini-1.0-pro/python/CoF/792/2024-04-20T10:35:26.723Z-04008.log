with open('reteta.in', 'r') as f_in, open('reteta.out', 'w') as f_out:
    line = f_in.readline().strip()
    stack = []
    ingredients = {}
    time = 0

    def process(string):
        nonlocal time
        if string[0] == '(':
            stack.append(string)
        else:
            ingredient, quantity = string.split()
            quantity = int(quantity)
            if ingredient not in ingredients:
                ingredients[ingredient] = quantity
            else:
                ingredients[ingredient] += quantity
            if stack:
                if stack[-1][-1] != ')':
                    stack[-1] += ' ' + string
            else:
                time += int(stack.pop()[-1])

    while line:
        process(line)
        line = f_in.readline().strip()

    f_out.write(str(time) + '\n')
    for ingredient in sorted(ingredients.keys()):
        f_out.write(ingredient + ' ' + str(ingredients[ingredient]) + '\n')
```
