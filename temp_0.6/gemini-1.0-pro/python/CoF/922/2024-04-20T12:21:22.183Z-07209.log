import sys

with open('furnica.in') as fin:
    cerinta = int(fin.readline())
    if cerinta == 1 or cerinta == 2:
        n = int(fin.readline())
        fo = [tuple(map(int, fin.readline().split())) for _ in range(n)]
    else:
        n, t = map(int, fin.readline().split())
        fo = [tuple(map(int, fin.readline().split())) for _ in range(n)]

    t_sus = 5
    t_jos = 2
    t_oriz = 3

    if cerinta == 1:
        timp = 0
        for i in range(n - 1):
            timp += fo[i][1] * t_sus
            timp += fo[i + 1][0] * t_oriz
        timp += fo[n - 1][1] * t_sus
        timp += fo[n - 1][0] * t_oriz
        with open('furnica.out', 'w') as fout:
            fout.write(str(timp))
    elif cerinta == 2:
        max_oriz = 0
        for i in range(n):
            max_oriz = max(max_oriz, fo[i][0])
        with open('furnica.out', 'w') as fout:
            fout.write(str(max_oriz))
    else:
        poz = 0
        timp = 0
        while timp < t:
            timp += fo[poz][1] * t_sus
            poz += 1
            if poz == n:
                poz = n - 1
                timp += fo[poz][0] * t_oriz
                while timp >= t:
                    timp -= fo[poz][0] * t_oriz
                    poz -= 1
            timp += fo[poz][0] * t_oriz
        timp -= fo[poz][0] * t_oriz
        timp += fo[poz][1] * t_sus
        while timp >= t:
            timp -= fo[poz][1] * t_sus
            poz -= 1
        with open('furnica.out', 'w') as fout:
            fout.write(str(poz + 1))
