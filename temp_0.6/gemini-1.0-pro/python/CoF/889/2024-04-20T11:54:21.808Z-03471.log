mod = 20173333
def fact(n):
    ans = 1
    for i in range(1, n + 1):
        ans = (ans * i) % mod
    return ans

def C(n, k):
    return (fact(n) * pow(fact(k) * fact(n - k), mod - 2)) % mod

def solve1(n, u):
    return C(n - 1, u - 1)

def solve2(n, r):
    return C(n + r - 1, r - 1)

with open('sir.in') as fin, open('sir.out', 'w') as fout:
    p, n, x = map(int, fin.readline().split())
    if p == 1:
        fout.write(str(solve1(n, x)))
    else:
        fout.write(str(solve2(n, x)))
