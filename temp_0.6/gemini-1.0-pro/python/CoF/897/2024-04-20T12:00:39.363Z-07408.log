import sys

with open('tbile.in', 'r') as fin, open('tbile.out', 'w') as fout:
    c = int(fin.readline())
    n, m = map(int, fin.readline().split())
    if c == 2:
        v = int(fin.readline())
    
    x = 1
    y = 2
    tube_red = 0
    tube_red_count = 0
    level_sum = 0
    
    if c == 1:
        while x <= n:
            if tube_red + y <= m:
                tube_red += y
                tube_red_count += 1
            else:
                tube_red = y
                tube_red_count = 1
            x = x + y
            y = x - y
            
        fout.write(str(tube_red_count) + ' ' + str(tube_red) + '\n')
    
    else:
        while x <= n:
            if tube_red + y <= m:
                tube_red += y
            else:
                tube_red = y
            if tube_red >= v:
                for i in range(v - 1, tube_red):
                    level_sum += x + i
                break
            x = x + y
            y = x - y
            
        fout.write(str(level_sum) + '\n')
