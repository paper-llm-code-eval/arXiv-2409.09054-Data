import sys

def solve1(n, m):
    # Calculate the number of red tubes and the total number of balls in them
    num_red_tubes = (n - 1) // (m - 1)
    num_balls_in_red_tubes = (num_red_tubes - 1) * (m - 1) + n - (num_red_tubes - 1) * m
    return num_red_tubes, num_balls_in_red_tubes

def solve2(n, m, v):
    # Calculate the sum of numbers on level v
    start = (v - 1) * (m - 1) + 1
    end = min(start + m - 1, n)
    return sum(range(start, end + 1))

def main():
    # Read input
    with open('tbile.in', 'r') as f:
        c = int(f.readline())
        n = int(f.readline())
        m = int(f.readline())
        if c == 2:
            v = int(f.readline())

    # Solve the problem
    if c == 1:
        num_red_tubes, num_balls_in_red_tubes = solve1(n, m)
    else:
        sum_on_level_v = solve2(n, m, v)

    # Write output
    with open('tbile.out', 'w') as f:
        if c == 1:
            f.write('{} {}\n'.format(num_red_tubes, num_balls_in_red_tubes))
        else:
            f.write('{}\n'.format(sum_on_level_v))

if __name__ == "__main__":
    main()
