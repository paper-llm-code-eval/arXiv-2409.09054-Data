with open('bete.in', 'r') as fin, open('bete.out', 'w') as fout:
    n = int(fin.readline())
    a = list(map(int, fin.readline().split()))
    b = list(map(int, fin.readline().split()))
    a.sort()
    b.sort(reverse=True)
    L = 0
    for i in range(n):
        L += max(a[i], b[i])
    fout.write(str(L) + '\n')

    K = a[n - 1] + b[0]
    fout.write(str(K) + '\n')

    P = 0
    for i in range(n):
        if a[i] + b[i] == K:
            P += 1
    fout.write(str(P) + '\n')
