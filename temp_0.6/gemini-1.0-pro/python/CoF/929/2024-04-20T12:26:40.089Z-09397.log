def leftmax():
    with open('leftmax.in') as fin, open('leftmax.out', 'w') as fout:
        n = int(fin.readline())
        heights = list(map(int, fin.readline().split()))
        
        # dp[i][j] = numarul de moduri de a selecta elevi de la i la j astfel incat cel mai inalt elev sa fie in prima jumatate
        dp = [[0 for _ in range(n+1)] for _ in range(n+1)]
        dp[n][n] = 1  # cazul de baza: nu selectam niciun elev
        
        for i in range(n-1,-1,-1):
            for j in range(i,n):
                if heights[i] > heights[j]:
                    dp[i][j] = (dp[i+1][j] + dp[i][j-1]) % 1000000007
                else:
                    dp[i][j] = dp[i+1][j]
        
        fout.write(str(dp[0][n-1]))

leftmax()
