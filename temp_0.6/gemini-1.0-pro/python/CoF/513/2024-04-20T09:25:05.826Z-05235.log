import sys

def solve1(n, arr):
    ans = 0
    for num in arr:
        m = len(num) // 2
        i = 0
        while i < m and num[i] == num[len(num) - i - 1]:
            i += 1
        ans += m - i
    return ans

def solve2(n, arr, S):
    ans = 0
    for num in arr:
        m = len(num) // 2
        i = 0
        while i < m and num[i] == num[len(num) - i - 1]:
            i += 1
        if m - i <= S:
            ans += 1
            S -= m - i
        else:
            break
    return ans

def main():
    with open("palindrom.in", "r") as f:
        C = int(f.readline())
        n = int(f.readline())
        arr = [line.strip() for line in f]
        if C == 2:
            S = int(f.readline())

    if C == 1:
        ans = solve1(n, arr)
    else:
        ans = solve2(n, arr, S)

    with open("palindrom.out", "w") as f:
        f.write(str(ans))

if __name__ == "__main__":
    main()
