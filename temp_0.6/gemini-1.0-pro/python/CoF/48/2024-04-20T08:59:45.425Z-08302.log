import sys

def main():
    n, k = map(int, sys.stdin.readline().split())
    edges = [[] for _ in range(n+1)]
    for _ in range(n-1):
        a, b = map(int, sys.stdin.readline().split())
        edges[a].append(b)
        edges[b].append(a)

    visited = [False] * (n+1)
    min_cost = float('inf')
    for i in range(1, n+1):
        if not visited[i]:
            cost, _ = dfs(i, -1, edges, visited, k, 0)
            min_cost = min(min_cost, cost)

    sys.stdout.write(str(min_cost) + '\n')

def dfs(node, parent, edges, visited, k, cost):
    visited[node] = True
    min_cost = float('inf')
    cnt = 0
    for adjacent in edges[node]:
        if adjacent == parent:
            continue
        if k > 0:
            child_cost, child_cnt = dfs(adjacent, node, edges, visited, k-1, cost+1)
            if child_cnt > 0:
                cnt += child_cnt
                min_cost = min(min_cost, child_cost)
        else:
            child_cost, child_cnt = dfs(adjacent, node, edges, visited, k, cost+1)
            cnt += child_cnt
            min_cost = min(min_cost, child_cost)

    return cost*cnt, cnt

if __name__ == '__main__':
    main()
