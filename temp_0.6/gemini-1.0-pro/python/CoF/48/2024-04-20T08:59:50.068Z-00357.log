import sys
import heapq

def read_input():
    n, k = map(int, sys.stdin.readline().split())
    graph = [[] for _ in range(n + 1)]
    for _ in range(n - 1):
        a, b = map(int, sys.stdin.readline().split())
        graph[a].append(b)
        graph[b].append(a)
    return n, k, graph

def dijkstra(graph, source):
    dist = [float('inf')] * (len(graph) + 1)
    dist[source] = 0
    pq = [(0, source)]
    while pq:
        d, u = heapq.heappop(pq)
        if d > dist[u]:
            continue
        for v in graph[u]:
            if dist[v] > d + 1:
                dist[v] = d + 1
                heapq.heappush(pq, (d + 1, v))
    return dist

def solve(n, k, graph):
    # Find the center of the graph
    dist = dijkstra(graph, 1)
    max_dist = max(dist)
    center = 1
    for i in range(2, n + 1):
        if dist[i] == max_dist:
            center = i
    # Find the k shortest paths from the center
    pq = [(0, center)]
    k_paths = []
    while pq and len(k_paths) < k:
        d, u = heapq.heappop(pq)
        if d > dist[u]:
            continue
        k_paths.append((d, u))
        for v in graph[u]:
            if dist[v] > d + 1:
                dist[v] = d + 1
                heapq.heappush(pq, (d + 1, v))
    # Calculate the cost of transporting all senators
    cost = 0
    for i in range(1, n + 1):
        if i == center:
            continue
        for d, u in k_paths:
            if dist[i] == d + dist[u]:
                cost += d
                break
    return cost

if __name__ == "__main__":
    n, k, graph = read_input()
    cost = solve(n, k, graph)
    sys.stdout.write(str(cost) + "\n")
