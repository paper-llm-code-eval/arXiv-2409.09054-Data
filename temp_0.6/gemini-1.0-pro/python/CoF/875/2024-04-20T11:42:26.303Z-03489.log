import sys

with open('miting.in') as f, open('miting.out', 'w') as out:
    p = int(f.readline().strip())
    n, m = map(int, f.readline().strip().split())
    word = f.readline().strip()
    grid = [f.readline().strip() for _ in range(n)]

def find_min_area():
    min_area = n * m
    for i in range(n):
        for j in range(m):
            if grid[i][j] in word:
                visited = [[False] * m for _ in range(n)]
                area = dfs(i, j, visited)
                min_area = min(min_area, area)
    return min_area

def dfs(x, y, visited):
    if x < 0 or x >= n or y < 0 or y >= m or visited[x][y] or grid[x][y] == '#':
        return 0
    visited[x][y] = True
    area = 1
    if grid[x][y] in word:
        area += dfs(x - 1, y, visited) + dfs(x + 1, y, visited) + dfs(x, y - 1, visited) + dfs(x, y + 1, visited)
    return area

def find_min_fuel():
    cars = []
    for i in range(n):
        for j in range(m):
            if grid[i][j] in word:
                cars.append((i, j))

    # Build the graph
    graph = {}
    for car in cars:
        graph[car] = []
        for other_car in cars:
            if car != other_car and can_merge(car, other_car):
                graph[car].append(other_car)

    # Find the shortest path
    visited = [False] * len(cars)
    distance = [sys.maxsize] * len(cars)
    distance[0] = 0
    queue = [0]
    while queue:
        current = queue.pop(0)
        visited[current] = True
        for neighbor in graph[cars[current]]:
            index = cars.index(neighbor)
            if distance[current] + 1 < distance[index]:
                distance[index] = distance[current] + 1
                queue.append(index)

    # Check if all cars can merge
    if max(distance) == sys.maxsize:
        return -1

    # Calculate the total fuel consumption
    total_fuel = 0
    for i in range(1, len(cars)):
        car1, car2 = cars[i], cars[distance[i]]
        total_fuel += abs(car1[0] - car2[0]) + abs(car1[1] - car2[1])

    return total_fuel

def can_merge(car1, car2):
    i1, j1 = car1
    i2, j2 = car2
    if abs(i1 - i2) + abs(j1 - j2) > 1:
        return False
    if grid[i1][j1] == grid[i2][j2]:
        return False
    return True

if p == 1:
    out.write(str(find_min_area()))
else:
    result = find_min_fuel()
    if result == -1:
        out.write('-1')
    else:
        out.write(str(result))
