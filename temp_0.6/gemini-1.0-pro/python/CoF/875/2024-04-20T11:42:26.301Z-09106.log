# Function to check if two cars can merge
def can_merge(car1, car2):
    # Check if the cars have adjacent positions
    if abs(car1[0] - car2[0]) + abs(car1[1] - car2[1]) == 1:
        # Check if the letters on the cars form a subsequence of the word
        if word.find(car1[0] + car2[0]) != -1:
            return True
    return False

# Function to find the minimum area and fuel consumption
def solve(p):
    # Initialize the minimum area and fuel consumption
    min_area = n * m
    min_fuel = -1

    # Iterate over all possible submatrices
    for i in range(n):
        for j in range(m):
            for k in range(i, n):
                for l in range(j, m):
                    # Check if the submatrix contains all the cars
                    all_cars = True
                    for car in cars.values():
                        if not (i <= car[0] <= k and j <= car[1] <= l):
                            all_cars = False
                            break

                    # If all cars are in the submatrix, calculate the area and fuel consumption
                    if all_cars:
                        area = (k - i + 1) * (l - j + 1)
                        fuel = 0
                        visited = set()
                        queue = [(i, j)]
                        while queue:
                            x, y = queue.pop(0)
                            if (x, y) not in visited:
                                visited.add((x, y))
                                for dx, dy in directions:
                                    nx, ny = x + dx, y + dy
                                    if 0 <= nx < n and 0 <= ny < m and grid[nx][ny] != "#" and (nx, ny) not in visited:
                                        if grid[nx][ny] in word:
                                            if can_merge((x, y), (nx, ny)):
                                                queue.append((nx, ny))
                                            else:
                                                fuel += 1
                                        else:
                                            queue.append((nx, ny))

                        # Update the minimum area and fuel consumption
                        if area < min_area:
                            min_area = area
                        if p == 2 and (min_fuel == -1 or fuel < min_fuel):
                            min_fuel = fuel

    # Return the minimum area or fuel consumption
    return min_area if p == 1 else min_fuel

# Solve the problem
result = solve(p)

# Write the result to the output file
with open("miting.out", "w") as f:
    f.write(str(result))
