import sys

with open("puncte.in") as f:
    n = int(f.readline())
    puncte = []
    for _ in range(n):
        x, y = map(int, f.readline().split())
        puncte.append((x, y))

# a)
nrp = max(
    max(abs(x) for x, y in puncte),
    max(abs(y) for x, y in puncte),
    max(x + y for x, y in puncte),
    max(-x + y for x, y in puncte),
)

# b)
nrd = 0
diag1 = set()
diag2 = set()
for x, y in puncte:
    if x == y:
        nrd += len(diag1)
        diag1.add(x)
    if x + y == 0:
        nrd += len(diag2)
        diag2.add(x)

# c)
xp = -sys.maxsize
puncte.sort()
for i in range(0, len(puncte), 3):
    xp = max(xp, puncte[i][0])

with open("puncte.out", "w") as f:
    f.write(f"{nrp}\n")
    f.write(f"{nrd}\n")
    f.write(f"{xp}\n")
```
