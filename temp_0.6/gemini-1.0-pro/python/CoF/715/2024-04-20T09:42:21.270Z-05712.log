def prime(n: int) -> bool:
    if n < 2:
        return False
    for i in range(2, int(n ** 0.5) + 1):
        if n % i == 0:
            return False
    return True

def is_valid(n: int) -> bool:
    n = str(n)
    if int(n[0]) != len(n):
        return False
    for i in range(len(n) - 1):
        if not prime(int(n[i:i + 2])):
            return False
    return True

n = int(open('visul.in').read())
for i in range(10 ** (n - 1), 10 ** n):
    if is_valid(i):
        open('visul.out', 'w').write(str(i))
        exit()
