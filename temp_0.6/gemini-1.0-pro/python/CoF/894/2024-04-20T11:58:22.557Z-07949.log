with open("numere.in", "r") as f:
    c = int(f.readline())
    if c == 1:
        n = int(f.readline())
        x = 1
        while x < n:
            x *= 10
        x = x // 10
        numere = list(range(1, x))
        numere.extend(range(x + 1, x * 10))
        while len(numere) != n:
            if numere[-1] % 10 == numere[-1] // 1000 % 10:
                numere.pop()
            else:
                numere.extend([numere[-1] % 1000, numere[-1] // 1000 % 10])
        with open("numere.out", "w") as g:
            g.write(str(numere[-1]))
    elif c == 2:
        x = int(f.readline())
        y = x
        n1 = 0
        n2 = 0
        n3 = 0
        while y > 0:
            if y % 10 == 0:
                y //= 10
                continue
            z = y
            if z % 10 == z // 1000 % 10:
                n1 += 1
            else:
                n1 += 2
            z //= 10
            if z % 10 == z // 100 % 10:
                n2 += 1
            else:
                n2 += 2
            z //= 10
            if z % 10 == z // 10 % 10:
                n3 += 1
            else:
                n3 += 2
            y //= 10
        with open("numere.out", "w") as g:
            g.write(str(n1) + " " + str(n2) + " " + str(n3))
    elif c == 3:
        k = int(f.readline())
        numere = list(range(1, 10 ** k))
        numere.extend(range(10 ** k + 1, 10 ** (k + 1)))
        while len(numere) != len(range(1, 10 ** k)) + len(range(10 ** k + 1, 10 ** (k + 1))):
            if numere[-1] % 10 == numere[-1] // 1000 % 10:
                numere.pop()
            else:
                numere.extend([numere[-1] % 1000, numere[-1] // 1000 % 10])
        with open("numere.out", "w") as g:
            g.write(str(len(numere)))
