import sys

H, N, M, p = map(int, sys.stdin.readline().split())

dp = [[[1e9 for k in range(M + 1)] for j in range(N + 1)] for i in range(H + 1)]
dp[0][0][0] = 0

for i in range(H + 1):
    for j in range(N + 1):
        for k in range(M + 1):
            if j < N:
                dp[i][j + 1][k] = min(dp[i][j + 1][k], dp[i][j][k])
            if i + k <= H:
                dp[i + k][j][k] = min(dp[i + k][j][k], dp[i][j][k] + (i + k) / j + p)

stairs = []
i = H
j = N
k = M
while i > 0 or j > 0 or k > 0:
    if i > 0 and dp[i - 1][j][k] == dp[i][j][k] - (i - 1) / j - p:
        i -= 1
    elif j > 0 and dp[i][j - 1][k] == dp[i][j][k]:
        j -= 1
    elif k > 0 and dp[i][j][k - 1] == dp[i][j][k]:
        k -= 1
    stairs.append(i)

sys.stdout.write(f'{dp[H][N][M]:.2f}\n')
sys.stdout.write(' '.join(map(str, stairs[::-1])))
```
