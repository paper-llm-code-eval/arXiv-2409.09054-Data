import sys

mod = int(1e9 + 9)

n, k = map(int, sys.stdin.readline().split())
intervals = []
for _ in range(n):
    intervals.append(list(map(int, sys.stdin.readline().split())))

intervals.sort(key=lambda x: x[1])

dp = [[0] * (k + 1) for _ in range(n + 1)]
dp[0][0] = 1

for i in range(n):
    start, end, foot = intervals[i]
    for j in range(k, 0, -1):
        if j == 1:
            if not dp[i][j - 1]:
                continue
            if foot:
                dp[i + 1][j] += dp[i][j - 1]
            else:
                for l in range(i + 1, n):
                    if intervals[l][0] < end:
                        dp[i + 1][j] += dp[i][j - 1] * dp[l][j]
                    else:
                        break
        else:
            if not dp[i][j - 1]:
                continue
            for l in range(i + 1, n):
                if intervals[l][0] < end:
                    dp[i + 1][j] += dp[i][j - 1] * dp[l][j]
                else:
                    break

sys.stdout.write(str(dp[n][k] % mod))
