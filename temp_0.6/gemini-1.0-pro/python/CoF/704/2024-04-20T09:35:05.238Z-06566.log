import sys

with open('cuburi.in', 'r') as f:
    n = int(f.readline())
    culori = list(map(int, f.readline().split()))

frecventa = {}
for culoare in culori:
    if culoare not in frecventa:
        frecventa[culoare] = 0
    frecventa[culoare] += 1

numar_culori = len(frecventa)
culoare_maxima = max(frecventa, key=lambda culoare: frecventa[culoare])

pozitii_de_scos = []
culoare_curenta = -1
lungime_maxima = 0
pozitie_inceput = 0
for i in range(n):
    if culori[i] != culoare_curenta:
        if lungime_maxima < i - pozitie_inceput:
            lungime_maxima = i - pozitie_inceput
            pozitii_de_scos = [pozitie_inceput]
        culoare_curenta = culori[i]
        pozitie_inceput = i
if lungime_maxima < n - pozitie_inceput:
    pozitii_de_scos = [pozitie_inceput]

with open('cuburi.out', 'w') as f:
    f.write(str(numar_culori) + '\n')
    f.write(str(culoare_maxima) + '\n')
    f.write(str(len(pozitii_de_scos)) + '\n')
    for pozitie in pozitii_de_scos:
        f.write(str(pozitie + 1) + ' ')
```
