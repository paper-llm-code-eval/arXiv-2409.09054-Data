import sys

with open('cuburi.in', 'r') as fin, open('cuburi.out', 'w') as fout:
    n = int(fin.readline())
    culori = list(map(int, fin.readline().split()))
    culori_distincte = set(culori)
    culori_frecventa = [0] * len(culori_distincte)
    for culoare in culori:
        culori_frecventa[culoare - 1] += 1
    fout.write(str(len(culori_distincte)) + '\n')
    max_frecventa = max(culori_frecventa)
    culori_max_frecventa = [i + 1 for i, frecventa in enumerate(culori_frecventa) if frecventa == max_frecventa]
    fout.write(' '.join(map(str, culori_max_frecventa)) + '\n')
    pozitii_cuburi_scoase = []
    culoare_anterioara = culori[0]
    lungime_maxima = 0
    for i in range(n):
        if culori[i] == culoare_anterioara:
            lungime_maxima += 1
        else:
            pozitii_cuburi_scoase.append(i)
            lungime_maxima = 1
            culoare_anterioara = culori[i]
    fout.write(str(lungime_maxima) + '\n')
    fout.write(' '.join(map(str, pozitii_cuburi_scoase)) + '\n')
```
