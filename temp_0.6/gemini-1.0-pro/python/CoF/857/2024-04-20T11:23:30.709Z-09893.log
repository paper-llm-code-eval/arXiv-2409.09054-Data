import sys

def get_last_ball(balls):
    n = len(balls)
    last_ball = balls[n // 2] if n % 2 == 0 else balls[n - 1]
    return last_ball

def get_order(balls):
    n = len(balls)
    order = []
    balls = list(balls)
    while balls:
        for i in range(n // 2 + 1 if n % 2 == 0 else n):
            if balls[i] == n:
                order.append(n)
                balls.pop(i)
                n -= 1
                break
        else:
            order.append(balls[-1])
            balls.pop()
            n -= 1
    return order

with open('ordine.in', 'r') as f:
    n = int(f.readline())
    balls = list(map(int, f.readline().split()))
    cerinta = int(f.readline())

if cerinta == 1:
    last_ball = get_last_ball(balls)
    with open('ordine.out', 'w') as f:
        f.write(str(last_ball))
else:
    order = get_order(balls)
    with open('ordine.out', 'w') as f:
        f.write(' '.join(map(str, order)))
```
