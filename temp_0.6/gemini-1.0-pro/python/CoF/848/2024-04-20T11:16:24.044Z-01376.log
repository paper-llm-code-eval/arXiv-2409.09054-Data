import sys

with open('arrows.in', 'r') as f:
    task = int(f.readline())
    n, m = map(int, f.readline().split())
    grid = [list(map(int, f.readline().split())) for _ in range(n)]
    start_row, start_col = map(int, f.readline().split())

directions = [(0, 1), (1, 0), (0, -1), (-1, 0)]

def dfs(row, col, visited):
    if row < 0 or row >= n or col < 0 or col >= m or visited[row][col]:
        return 0
    visited[row][col] = True
    return 1 + dfs(row + directions[grid[row][col] - 1][0], col + directions[grid[row][col] - 1][1], visited)

def find_score(row, col):
    visited = [[False] * m for _ in range(n)]
    return dfs(row - 1, col - 1, visited) * (1000 if dfs(row - 1, col - 1, visited) != 0 else 1)

def find_num_favorable():
    cnt = 0
    for i in range(n):
        for j in range(m):
            visited = [[False] * m for _ in range(n)]
            if dfs(i, j, visited) != 0:
                cnt += 1
    return cnt

def find_max_score():
    max_score = 0
    for i in range(n):
        for j in range(m):
            max_score = max(max_score, find_score(i + 1, j + 1))
    return max_score

with open('arrows.out', 'w') as f:
    if task == 1:
        f.write(str(find_score(start_row, start_col)))
    elif task == 2:
        f.write(str(find_num_favorable()))
    elif task == 3:
        f.write(str(find_max_score()))
```
