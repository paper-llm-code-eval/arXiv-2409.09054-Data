with open('arrows.in', 'r') as fin, open('arrows.out', 'w') as fout:
    cerinta = int(fin.readline())
    n, m = map(int, fin.readline().split())
    tabla = [list(map(int, fin.readline().split())) for _ in range(n)]
    lin, col = map(int, fin.readline().split())

    def get_score(lin, col):
        vizitate = {(lin, col)}
        scor = 0
        while True:
            if tabla[lin - 1][col - 1] == 2:
                lin -= 1
            elif tabla[lin - 1][col - 1] == 1:
                col += 1
            elif tabla[lin - 1][col - 1] == 3:
                col -= 1
            elif tabla[lin - 1][col - 1] == 4:
                lin += 1
            scor += 1
            if (lin, col) in vizitate:
                return scor * 1000
            vizitate.add((lin, col))
            if lin < 1 or lin > n or col < 1 or col > m:
                return scor

    if cerinta == 1:
        fout.write(str(get_score(lin, col)))
    elif cerinta == 2:
        fout.write(str(sum(get_score(i, j) > 0 for i in range(1, n + 1) for j in range(1, m + 1))))
    else:
        fout.write(str(max(get_score(i, j) for i in range(1, n + 1) for j in range(1, m + 1))))
```
