                nr_celule_max = max(nr_celule_max, nr_celule)
                nr_celule_galerie_max = max(nr_celule_galerie_max, nr_celule_galerie)
                break
            if not vizitat[i+1][j]:
                celule.append((i+1, j))
                vizitat[i+1][j] = True
                nr_celule += 1
            if not vizitat[i][j+1]:
                celule.append((i, j+1))
                vizitat[i][j+1] = True
                nr_celule += 1
            if not vizitat[i-1][j]:
                celule.append((i-1, j))
                vizitat[i-1][j] = True
                nr_celule += 1
            if not vizitat[i][j-1]:
                celule.append((i, j-1))
                vizitat[i][j-1] = True
                nr_celule += 1
            nr_celule_galerie += 1

with open("galerie.out", "w") as f:
    if cerinta == 1:
        f.write(str(nr_celule_max))
    else:
        f.write(str(nr_celule_galerie_max))
