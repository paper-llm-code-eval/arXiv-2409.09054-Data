with open('cruce.in', 'r') as f:
    n, k = map(int, f.readline().split())
    mat = [list(map(int, l.split())) for l in f.readlines()]

sm = [[0]*n for _ in range(n)]
for i in range(n):
    for j in range(n):
        sm[i][j] = mat[i][j] + (sm[i-1][j] if i > 0 else 0) + (sm[i][j-1] if j > 0 else 0) - (sm[i-1][j-1] if i > 0 and j > 0 else 0)

max_sum = -1e9
res_i = 0
res_j = 0
for i in range(n-k+1):
    for j in range(n-k+1):
        cur_sum = sm[i+k-1][j+k-1] - (sm[i+k-1][j-1] if j > 0 else 0) - (sm[i-1][j+k-1] if i > 0 else 0) + (sm[i-1][j-1] if i > 0 and j > 0 else 0)
        if cur_sum > max_sum:
            max_sum = cur_sum
            res_i = i+1
            res_j = j+1

with open('cruce.out', 'w') as f:
    f.write(f'{max_sum} {res_i} {res_j}')
```
