with open('poarta.in') as f:
    p = int(f.readline())
    pos = []
    for _ in range(p):
        pos.append([int(x) for x in f.readline().split()])

pos_to_crew = {}
for i in range(p):
    pos_to_crew[(pos[i][0], pos[i][1])] = i

graph = [[0 for _ in range(p)] for _ in range(p)]
for i in range(p):
    for j in range(i+1, p):
        dist = abs(pos[i][0] - pos[j][0]) + abs(pos[i][1] - pos[j][1])
        graph[i][j] = dist
        graph[j][i] = dist

visited = [False for _ in range(p)]
dist = [float('inf') for _ in range(p)]
dist[0] = 0

for _ in range(p):
    u = -1
    for i in range(p):
        if not visited[i] and (u == -1 or dist[i] < dist[u]):
            u = i

    visited[u] = True

    for v in range(p):
        if graph[u][v] > 0 and dist[u] + graph[u][v] < dist[v]:
            dist[v] = dist[u] + graph[u][v]

ans = max(dist)
with open('poarta.out', 'w') as f:
    f.write(str(ans))
```
