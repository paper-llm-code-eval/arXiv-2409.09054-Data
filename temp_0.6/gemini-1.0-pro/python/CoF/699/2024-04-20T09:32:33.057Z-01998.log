import sys

with open("poarta.in", "r") as f:
    p = int(f.readline())
    echipaje = [list(map(int, f.readline().split())) for _ in range(p)]

pozitii_initiale = [echipaj[0:2] for echipaj in echipaje]
pozitii_finale = [echipaj[2:4] for echipaj in echipaje]

matrice = [[0 for _ in range(251)] for _ in range(251)]

for i in range(1, 251):
    for j in range(1, 251):
        matrice[i][j] = min(abs(i - x) + abs(j - y) for x, y in pozitii_initiale)

vizitat = [False for _ in range(p)]
nr_mutari = 0

while not all(vizitat):
    nr_mutari += 1
    min_dist = sys.maxsize
    echipaj_curent = -1

    # cautam echipajul cu cea mai mica distanta pana la o pozitie finala
    for i in range(p):
        if not vizitat[i]:
            distanta = min(abs(pozitii_initiale[i][0] - x) + abs(pozitii_initiale[i][1] - y) for x, y in pozitii_finale)
            if distanta < min_dist:
                min_dist = distanta
                echipaj_curent = i

    # mutam echipajul la pozitia finala
    pozitii_initiale[echipaj_curent] = pozitii_finale[echipaj_curent]
    vizitat[echipaj_curent] = True

# scriem numarul de mutari in fisierul de iesire
with open("poarta.out", "w") as f:
    f.write(str(nr_mutari))
