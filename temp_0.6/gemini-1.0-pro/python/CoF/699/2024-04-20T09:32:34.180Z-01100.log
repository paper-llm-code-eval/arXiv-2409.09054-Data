def read_input():
    with open('poarta.in', 'r') as f:
        p = int(f.readline())
        teams = []
        for _ in range(p):
            x1, y1, x2, y2 = map(int, f.readline().split())
            teams.append(((x1, y1), (x2, y2)))
    return p, teams


def write_output(result):
    with open('poarta.out', 'w') as f:
        f.write(str(result))


def manhattan_distance(p1, p2):
    x1, y1 = p1
    x2, y2 = p2
    return abs(x1 - x2) + abs(y1 - y2)


def solve(p, teams):
    # Create a graph where each node is a team's position and the edges are the distances between them
    graph = {}
    for team in teams:
        graph[team[0]] = {}
        for other_team in teams:
            if team[0] != other_team[0]:
                graph[team[0]][other_team[0]] = manhattan_distance(team[0], other_team[0])

    # Find the minimum number of moves using a breadth-first search
    visited = set()
    queue = [(0, teams[0][0])]
    while queue:
        moves, position = queue.pop(0)
        if position not in visited:
            visited.add(position)
            if position == teams[0][1]:
                return moves
            for next_position in graph[position]:
                queue.append((moves + graph[position][next_position], next_position))


if __name__ == '__main__':
    p, teams = read_input()
    result = solve(p, teams)
    write_output(result)
