import sys
from collections import Counter

def max_num(counts, num1, num2):
    counts[0] -= 1
    counts[num1 % 10] -= 1
    counts[num2 % 10] -= 1
    num = 0
    for i in range(9, 0, -1):
        while counts[i] > 0:
            num = num * 10 + i
            counts[i] -= 1
    return num

with open('cifre.in', 'r') as f_in, open('cifre.out', 'w') as f_out:
    p = int(f_in.readline())
    n = int(f_in.readline())

    counts = Counter()
