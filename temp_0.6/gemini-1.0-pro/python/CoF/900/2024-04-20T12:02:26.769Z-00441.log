        N = int(f.readline())
        H = [[int(x) for x in f.readline().split()] for _ in range(N)]
        return C, N, H

def numara_camere(H, N):
    viz = [[False] * N for _ in range(N)]
    nr_camere = 0

    def dfs(i, j):
        if i < 0 or i >= N or j < 0 or j >= N or viz[i][j] or (H[i][j] & 1 and (j == 0 or H[i][j - 1] & 1)) or (H[i][j] & 2 and (i == 0 or H[i - 1][j] & 2)) or (H[i][j] & 4 and (j == N - 1 or H[i][j + 1] & 4)) or (H[i][j] & 8 and (i == N - 1 or H[i + 1][j] & 8)):
            return
        viz[i][j] = True
        dfs(i, j - 1)
        dfs(i - 1, j)
        dfs(i, j + 1)
        dfs(i + 1, j)

    for i in range(N):
        for j in range(N):
            if not viz[i][j]:
                dfs(i, j)
                nr_camere += 1
    return nr_camere

def arie_maxima(H, N):
    viz = [[False] * N for _ in range(N)]
    max_arie = 0
    max_i = max_j = min_i = min_j = -1

    def dfs(i, j, arie, st, dr):
        nonlocal max_arie, max_i, max_j, min_i, min_j
        if i < 0 or i >= N or j < 0 or j >= N or viz[i][j] or (H[i][j] & 1 and (j == 0 or H[i][j - 1] & 1)) or (H[i][j] & 2 and (i == 0 or H[i - 1][j] & 2)) or (H[i][j] & 4 and (j == N - 1 or H[i][j + 1] & 4)) or (H[i][j] & 8 and (i == N - 1 or H[i + 1][j] & 8)):
            return
        viz[i][j] = True
        arie += 1
        if arie > max_arie:
            max_arie = arie
            max_i = i
            max_j = j
            min_i = st
            min_j = dr
        elif arie == max_arie:
            if st < min_i or (st == min_i and dr < min_j):
                min_i = st
                min_j = dr
        dfs(i, j - 1, arie, st, dr - 1)
        dfs(i - 1, j, arie, st - 1, dr)
        dfs(i, j + 1, arie, st, dr + 1)
        dfs(i + 1, j, arie, st + 1, dr)

    for i in range(N):
        for j in range(N):
            if not viz[i][j]:
                dfs(i, j, 0, i, j)
    return max_arie, min_i, min_j, max_i, max_j

def scriere(nume_fisier, C, rezultat):
    with open(nume_fisier, 'w') as f:
