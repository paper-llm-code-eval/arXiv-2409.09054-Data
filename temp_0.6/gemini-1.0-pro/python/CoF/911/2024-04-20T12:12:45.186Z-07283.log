    with open('valutar.out', 'w') as f:
        f.write(str(count))
else:
    max_euro = -1
    winner = -1
    for i, player in enumerate(players):
        if player and player["euro"] > max_euro:
            max_euro = player["euro"]
            winner = i + 1

    with open('valutar.out', 'w') as f:
