with open('munte.in') as f:
    n = int(f.readline())
    x = list(map(int, f.readline().split()))

def munte(x):
    m = []
    for i in range(1, n - 1):
        if x[i - 1] < x[i] > x[i + 1]:
            m.append(i)
    return m

def elimina(x, m):
    for i in m[::-1]:
        del x[i]
    return x

nr_munte = 0
while True:
    m = munte(x)
    if not m:
        break
    nr_munte += len(m)
    x = elimina(x, m)

with open('munte.out', 'w') as f:
    f.write(f'{len(m)}\n')
    f.write(f'{nr_munte}\n')
    f.write(f'{len(x)}\n')
```
