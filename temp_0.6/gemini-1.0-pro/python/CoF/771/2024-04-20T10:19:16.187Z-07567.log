import sys
f = open("pluricex.in")
n, k, D = map(int, f.readline().split())
a = [[] for i in range(n)]
for i in range(n):
    x = list(map(int, f.readline().split()))
    a[i] = x[1:]
f.close()
def ok(x):
    for i in range(1, D + 1):
        if not any(i in a[j] for j in x):
            return False
    return True
def back(x, p):
    if len(x) == k:
        if ok(x):
            f.write(' '.join(map(str, x)) + '\n')
        return
    for i in range(p, n):
        x.append(i + 1)
        back(x, i + 1)
        x.pop()
f = open("pluricex.out", "w")
back([], 0)
f.close()
```
