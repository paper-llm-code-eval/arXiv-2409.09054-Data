    n = len(s)
    cnt = [0] * 26
    for c in s:
        cnt[ord(c) - ord('a')] += 1

    ans = 0
    for i in range(26):
        for j in range(26):
            if i != j:
                ans = add(ans, sub(cnt[i] * cnt[j], cnt[i] * (cnt[i] - 1) // 2) * cnt[j] * (cnt[j] - 1) // 2)
