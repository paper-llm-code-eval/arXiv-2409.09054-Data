MOD = int(1e9) + 7

def add(x, y):
    return (x + y) % MOD

def sub(x, y):
    return (x - y + MOD) % MOD

def sdist(a, b):
    return sum(a[i] != b[i] for i in range(len(a)))

with open('sdistante.in') as f_in, open('sdistante.out', 'w') as f_out:
