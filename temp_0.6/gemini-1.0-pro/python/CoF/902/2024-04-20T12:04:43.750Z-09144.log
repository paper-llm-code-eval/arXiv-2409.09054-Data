def solve(n, towers):
    # find left and right greater towers
    left_greater = [0] * n
    right_greater = [0] * n
    stack = []
    for i in range(n):
        while stack and towers[stack[-1]] <= towers[i]:
            stack.pop()
        if stack:
            left_greater[i] = stack[-1] + 1
        stack.append(i)
    stack = []
    for i in range(n - 1, -1, -1):
