            string cell = string() + col + to_string(num);
            if (sheet[num - 1][col - 'A'] != 0 && col > 'A') {
              sum += sheet[num - 1][col - 'A'];
            }
            num--;
          }
        }
        sheet[i][j] = sum;
      }
    }
  }

  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      out << sheet[i][j] << " ";
    }
    out << endl;
  }

  return 0;
}
