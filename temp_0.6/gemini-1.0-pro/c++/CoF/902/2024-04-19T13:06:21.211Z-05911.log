#include <fstream>
#include <vector>

using namespace std;

const int MAXN = 100000;
const int MAX_VAL = 1000000000;

int n, h[MAXN + 5], l[MAXN + 5], r[MAXN + 5];
vector<int> adj[MAXN + 5];
int dp[MAXN + 5];

void read_input() {
  ifstream fin("turnuri.in");
  fin >> n;
  for (int i = 1; i <= n; i++) {
    fin >> h[i];
  }
  fin.close();
}

void compute_adj() {
  for (int i = 1; i <= n; i++) {
    for (int j = i + 1; j <= n; j++) {
      if (h[i] < h[j]) {
        adj[i].push_back(j);
      }
      if (h[j] < h[i]) {
        adj[j].push_back(i);
      }
    }
  }
}

void dfs(int node, int parent) {
  dp[node] = 1;
  for (int next : adj[node]) {
    if (next == parent) {
      continue;
    }
    dfs(next, node);
    dp[node] += dp[next];
  }
}

void solve() {
  compute_adj();
  for (int i = 1; i <= n; i++) {
    dfs(i, 0);
    int ans = 0;
    for (int next : adj[i]) {
      ans += dp[next];
    }
    l[i] = ans;
  }
  for (int i = n; i >= 1; i--) {
    dfs(i, 0);
    int ans = 0;
    for (int next : adj[i]) {
      ans += dp[next];
    }
    r[i] = ans;
  }
  ofstream fout("turnuri.out");
  for (int i = 1; i <= n; i++) {
    fout << l[i - 1] + r[i + 1] << "\n";
  }
  fout.close();
}

int main() {
  read_input();
  solve();
  return 0;
}
