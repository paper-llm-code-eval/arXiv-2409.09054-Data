#include <fstream>
#include <vector>

using namespace std;

ifstream in("submit.in");
ofstream out("submit.out");

int main() {
  int n, b, m, penalty = 0, score = 0;
  vector<int> p, sol;
  in >> n >> p >> b >> m >> sol;
  for (int i = 0; i < n; i++) {
    score += p[i];
  }
  for (int i = 0; i < m; i++) {
    int tmp = 0;
    for (int j = 0; j < n; j++) {
      tmp += p[j] * sol[i * n + j];
    }
    score = max(score, tmp - penalty);
    penalty += 2;
  }
  score += b;
  out << score << '\n';
  return 0;
}
