            cur.clear();
        } else {
            cur += line[i];
        }
    }
    if (!cur.empty()) {
        if (cur[0] >= '0' && cur[0] <= '9') {
            constant_terms.push_back(sign * stoi(cur));
        } else {
            vars[cur] += sign;
        }
    }
}

int main() {
