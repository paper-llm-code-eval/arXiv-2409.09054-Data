    in >> a >> b >> e;

    if (c == 1) {
        ll ans = 0;
        for (auto& [var, val] : vars) {
            ans += val;
        }
        for (ll term : constant_terms) {
            ans += term;
        }
        out << ans << endl;
    } else {
        set<vector<ll>> quartets;
        for (int x = a; x <= b; x++) {
            for (int y = a; y <= b; y++) {
                for (int z = a; z <= b; z++) {
                    for (int t = a; t <= b; t++) {
                        vector<ll> quartet = {x, y, z, t};
                        ll ans = 0;
                        for (auto& [var, val] : vars) {
                            ans += val * quartet[var[0] - 'x'];
                        }
                        for (ll term : constant_terms) {
                            ans += abs(term);
                        }
                        if (ans == e) {
                            quartets.insert(quartet);
                        } else if (ans < e) {
                            for (ll term : constant_terms) {
                                if (term < 0) {
