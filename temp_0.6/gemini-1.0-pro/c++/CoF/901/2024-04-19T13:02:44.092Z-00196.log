#include <fstream>
#include <sstream>
#include <vector>
#include <algorithm>

using namespace std;

struct Monom {
    int coef;
    char var;
    Monom(int coef, char var) : coef(coef), var(var) {}
};

struct Node {
    vector<Monom> monoms;
    int value;
    Node *left, *right;
    Node(vector<Monom> monoms, int value) : monoms(monoms), value(value), left(nullptr), right(nullptr) {}
};

int evaluate(const vector<Monom> &monoms, int x, int y, int z, int t) {
    int value = 0;
    for (const auto &monom : monoms) {
        switch (monom.var) {
            case 'x':
                value += monom.coef * x;
                break;
            case 'y':
                value += monom.coef * y;
                break;
            case 'z':
                value += monom.coef * z;
                break;
            case 't':
                value += monom.coef * t;
                break;
        }
    }
    return value;
}

Node *build_tree(const vector<Monom> &monoms, int a, int b, int E) {
    if (a > b) {
        return nullptr;
    }
    int mid = (a + b) / 2;
    vector<Monom> left_monoms, right_monoms;
    for (const auto &monom : monoms) {
        if (monom.var == 'x') {
            left_monoms.push_back(Monom(monom.coef, monom.var));
            right_monoms.push_back(Monom(monom.coef, monom.var));
        } else if (monom.var == 'y') {
            left_monoms.push_back(Monom(monom.coef * mid, monom.var));
            right_monoms.push_back(Monom(monom.coef * (mid + 1), monom.var));
        } else if (monom.var == 'z') {
            left_monoms.push_back(Monom(monom.coef * mid * (mid + 1) / 2, monom.var));
            right_monoms.push_back(Monom(monom.coef * (mid + 1) * (mid + 2) / 2, monom.var));
        } else if (monom.var == 't') {
            left_monoms.push_back(Monom(monom.coef * mid * (mid + 1) / 2 * (mid + 2) / 3, monom.var));
            right_monoms.push_back(Monom(monom.coef * (mid + 1) * (mid + 2) / 2 * (mid + 3) / 3, monom.var));
        }
    }
    Node *left = build_tree(left_monoms, a, mid - 1, E);
    Node *right = build_tree(right_monoms, mid + 1, b, E);
    int value = evaluate(monoms, mid, mid, mid, mid);
    return new Node(monoms, value + (left ? left->value : 0) + (right ? right->value : 0));
}

int count_solutions(Node *root, int a, int b, int E) {
    if (!root) {
        return 0;
    }
    if (a <= root->value && root->value <= b) {
        int left_count = count_solutions(root->left, a, b, E);
        int right_count = count_solutions(root->right, a, b, E);
        return left_count + right_count + (root->value == E);
    }
    if (root->value < a) {
        return count_solutions(root->right, a, b, E);
    }
    return count_solutions(root->left, a, b, E);
}

int main() {
    ifstream in("eq4.in");
    ofstream out("eq4.out");
    int C;
    in >> C;
    string expression;
    getline(in, expression);
    getline(in, expression);
    int a, b, E;
    in >> a >> b >> E;
    vector<Monom> monoms;
    stringstream ss(expression);
    char sign;
    int coef;
    char var;
    while (ss >> sign >> coef) {
        if (ss.peek() != EOF) {
            ss >> var;
        }
        monoms.push_back(Monom(coef, var));
    }
    if (C == 1) {
        out << evaluate(monoms, 1, 1, 1, 1) << endl;
    } else {
        Node *root = build_tree(monoms, a, b, E);
        out << count_solutions(root, a, b, E) << endl;
    }
    return 0;
}
