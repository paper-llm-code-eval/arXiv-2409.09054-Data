#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("livada.in");
ofstream fout("livada.out");

const int MAXN = 7e5 + 5;
const int MAXP = 1e9 + 5;

int n, m, p;
vector<int> v[MAXN];
int fr[MAXP];
int max_len[MAXN];
int nr_max_len;
int max_nr_rand;

int main() {
    fin >> n >> m >> p;
    for (int i = 1; i <= m; ++i) {
        for (int j = 1; j <= n; ++j) {
            int x;
            fin >> x;
            v[i].push_back(x);
        }
    }

    for (int i = 1; i <= m; ++i) {
        for (int j = 0; j < n; ++j) {
            ++fr[v[i][j]];
            if (fr[v[i][j]] > n / 2) {
                max_nr_rand++;
                break;
            }
        }
        for (int j = 0; j < n; ++j) {
            fr[v[i][j]] = 0;
        }
    }

    for (int i = 1; i <= m; ++i) {
        int len = 1;
        for (int j = 1; j < n; ++j) {
            if (v[i][j] == v[i][j - 1]) {
                ++len;
            } else {
                len = 1;
            }
            max_len[i] = max(max_len[i], len);
        }
        nr_max_len = max(nr_max_len, max_len[i]);
    }

    fout << max_nr_rand << '\n' << nr_max_len << '\n';
    return 0;
}
