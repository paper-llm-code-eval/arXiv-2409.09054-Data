#include <fstream>
#include <vector>

using namespace std;

const int dx[] = {-1, 0, 1, 0};
const int dy[] = {0, -1, 0, 1};
int n, m;
vector<vector<int>> v;
vector<vector<int>> viz;

int dfs(int x, int y, int nr) {
  if (x < 0 || x >= n || y < 0 || y >= m || viz[x][y] || v[x][y] != nr) { return 0; }
  viz[x][y] = 1;
  int ans = 1;
  for (int i = 0; i < 4; ++i) { ans += dfs(x + dx[i], y + dy[i], nr); }
  return ans;
}

int main() {
  ifstream fin("insule.in");
  ofstream fout("insule.out");
  fin >> n >> m;
  v.resize(n, vector<int>(m));
  viz.resize(n, vector<int>(m));
  for (int i = 0; i < n; ++i) {
    for (int j = 0; j < m; ++j) { fin >> v[i][j]; }
  }
  int nr = 0, ng = 0, nb = 0, lg = 2e9;
  for (int i = 0; i < n; ++i) {
    for (int j = 0; j < m; ++j) {
      if (!viz[i][j]) {
        int cnt = dfs(i, j, v[i][j]);
        if (v[i][j] == 1) { nr += 1; }
        if (v[i][j] == 2) { ng += 1; }
        if (v[i][j] == 3) { nb += 1; }
        if (v[i][j] == 0) { lg = min(lg, cnt - 1); }
      }
    }
  }
  fout << nr << " " << ng << " " << nb << " " << lg << "\n";
  return 0;
}
