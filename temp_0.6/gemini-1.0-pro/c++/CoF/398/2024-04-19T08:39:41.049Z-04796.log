#include <fstream>

using namespace std;

ifstream fin("insule.in");
ofstream fout("insule.out");

int n, m, a[101][101], viz[101][101], nr[4], nr_ins[4], length = 1000000000;
int dx[] = {0, 1, 0, -1}, dy[] = {1, 0, -1, 0};

void dfs(int x, int y, int nr_ins) {
    if (viz[x][y] == 1) return;
    viz[x][y] = 1;
    nr[a[x][y]]++;
    for (int i = 0; i < 4; i++) {
        int nx = x + dx[i], ny = y + dy[i];
        if (nx >= 1 && nx <= n && ny >= 1 && ny <= m && a[x][y] == a[nx][ny] && viz[nx][ny] == 0) {
            dfs(nx, ny, nr_ins);
        }
    }
}

void bfs(int x, int y, int nr_ins) {
    int d[101][101];
    for (int i = 1; i <= n; i++)
        for (int j = 1; j <= m; j++) d[i][j] = 1000000000;
    d[x][y] = 0;
    int q[10001], qb = 0, qe = 0;
    q[qe++] = x * 1000 + y;
    while (qb < qe) {
        int cx = q[qb] / 1000, cy = q[qb++] % 1000;
        for (int i = 0; i < 4; i++) {
            int nx = cx + dx[i], ny = cy + dy[i];
            if (nx >= 1 && nx <= n && ny >= 1 && ny <= m && a[nx][ny] == 0 && d[nx][ny] > d[cx][cy] + 1) {
                d[nx][ny] = d[cx][cy] + 1;
                q[qe++] = nx * 1000 + ny;
                if (nr_ins == 1 && a[nx][ny] == 1 || nr_ins == 2 && a[nx][ny] == 2) {
                    length = min(length, d[nx][ny]);
                }
            }
        }
    }
}

int main() {
    fin >> n >> m;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            fin >> a[i][j];
        }
    }
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            if (viz[i][j] == 0) {
                nr[a[i][j]] = 0;
                dfs(i, j, nr[a[i][j]]);
                nr_ins[nr[a[i][j]]]++;
            }
        }
    }
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            if (a[i][j] == 1 || a[i][j] == 2) {
                bfs(i, j, a[i][j]);
            }
        }
    }
    fout << nr_ins[1] << " " << nr_ins[2] << " " << nr_ins[3] << " " << length << "\n";
    return 0;
}
