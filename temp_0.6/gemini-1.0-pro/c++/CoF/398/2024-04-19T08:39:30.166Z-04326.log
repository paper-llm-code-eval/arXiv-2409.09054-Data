#include <fstream>
#include <vector>

using namespace std;

ifstream in("insule.in");
ofstream out("insule.out");

int n, m, countR, countG, countB, minLen;
vector<vector<int>> a;
vector<vector<bool>> viz;

void dfs(int x, int y, int type) {
  if (x < 0 || x >= n || y < 0 || y >= m || viz[x][y] || a[x][y] != type) {
    return;
  }
  viz[x][y] = true;
  if (type == 1) {
    countR++;
  } else if (type == 2) {
    countG++;
  } else if (type == 3) {
    countB++;
  }
  dfs(x + 1, y, type);
  dfs(x - 1, y, type);
  dfs(x, y + 1, type);
  dfs(x, y - 1, type);
}

void findMinLen(int x, int y) {
  if (x < 0 || x >= n || y < 0 || y >= m || viz[x][y] || a[x][y] != 0) {
    return;
  }
  viz[x][y] = true;
  if (x == 0 || x == n - 1 || y == 0 || y == m - 1) {
    minLen = min(minLen, x + y);
  }
  findMinLen(x + 1, y);
  findMinLen(x - 1, y);
  findMinLen(x, y + 1);
  findMinLen(x, y - 1);
}

int main() {
  in >> n >> m;
  a.resize(n, vector<int>(m));
  viz.resize(n, vector<bool>(m, false));
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      in >> a[i][j];
    }
  }
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      if (!viz[i][j]) {
        dfs(i, j, a[i][j]);
      }
    }
  }
  viz.assign(n, vector<bool>(m, false));
  minLen = n + m;
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      if (a[i][j] == 0) {
        findMinLen(i, j);
      }
    }
  }
  out << countR << ' ' << countG << ' ' << countB << ' ' << minLen << '\n';
  return 0;
}
