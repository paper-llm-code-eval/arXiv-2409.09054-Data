#include <fstream>

using namespace std;

ifstream fin("insule.in");
ofstream fout("insule.out");

const int MAXN = 105;
const int dx[] = {-1, 0, 1, 0};
const int dy[] = {0, 1, 0, -1};

int n, m, a[MAXN][MAXN], viz[MAXN][MAXN], nr, ng, nb, lg = 1e9;
int vecin[MAXN][MAXN][4];

void dfs(int x, int y, int t) {
  if (viz[x][y])
    return;
  viz[x][y] = 1;
  if (t) {
    if (t == 1)
      nr++;
    else if (t == 2)
      ng++;
    else
      nb++;
  }
  for (int d = 0; d < 4; ++d) {
    int nx = x + dx[d];
    int ny = y + dy[d];
    if (nx < 1 || nx > n || ny < 1 || ny > m)
      continue;
    if (a[nx][ny] == a[x][y]) {
      dfs(nx, ny, t);
      vecin[x][y][d] = 1;
      vecin[nx][ny][(d + 2) % 4] = 1;
    }
  }
}

int main() {
  fin >> n >> m;
  for (int i = 1; i <= n; ++i)
    for (int j = 1; j <= m; ++j)
      fin >> a[i][j];
  for (int i = 1; i <= n; ++i)
    for (int j = 1; j <= m; ++j)
      if (!viz[i][j])
        dfs(i, j, a[i][j]);
  for (int i = 1; i <= n; ++i)
    for (int j = 1; j <= m; ++j)
      for (int d = 0; d < 4; ++d)
        if (vecin[i][j][d] && a[i][j] == 0) {
          int nx = i + dx[d];
          int ny = j + dy[d];
          while (a[nx][ny] == 0) {
            nx += dx[d];
            ny += dy[d];
            lg = min(lg, nx - i + ny - j);
          }
        }
  fout << nr << ' ' << ng << ' ' << nb << ' ' << lg << '\n';
  return 0;
}
