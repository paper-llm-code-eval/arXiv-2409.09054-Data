#include <iostream>
#include <vector>
#include <queue>
#include <cstring>
using namespace std;
ifstream fin("insule.in");
ofstream fout("insule.out");
int n, m, a[101][101], nr[4], viz[101][101], lmin = 1e9;
vector<pair<int, int>> poz[4];
