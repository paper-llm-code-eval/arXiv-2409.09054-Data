      queue<point> q;
      q.push(point(i, j, 0));
      while (!q.empty()) {
        point nod = q.front();
        q.pop();
        if (viz[nod.x][nod.y]) continue;
        viz[nod.x][nod.y] = 1;
        cnt++;
        for (int k = 0; k < 4; ++k) {
          int x = nod.x + dx[k], y = nod.y + dy[k];
          if (x < 1 || x > n || y < 1 || y > m) continue;
          if (a[nod.x][nod.y] != a[x][y]) continue;
          q.push(point(x, y, nod.l + 1));
        }
      }
      nr[a[i][j]]++;
    }
  for (int i = 1; i <= 3; ++i) {
    for (auto p : poz[i]) {
      for (int k = 0; k < 4; ++k) {
        int x = p.first + dx[k], y = p.second + dy[k];
        if (x < 1 || x > n || y < 1 || y > m) continue;
        if (a[x][y] == 0) {
          memset(viz, 0, sizeof(viz));
          viz[p.first][p.second] = 1;
          queue<point> q;
          q.push(point(x, y, 0));
          while (!q.empty()) {
            point nod = q.front();
            q.pop();
            if (viz[nod.x][nod.y]) continue;
            viz[nod.x][nod.y] = 1;
            if (a[nod.x][nod.y] == 3 - i) {
              lmin = min(lmin, nod.l);
              break;
            }
            for (int k = 0; k < 4; ++k) {
              int x = nod.x + dx[k], y = nod.y + dy[k];
              if (x < 1 || x > n || y < 1 || y > m) continue;
              if (viz[x][y] || a[x][y] != 0) continue;
              q.push(point(x, y, nod.l + 1));
            }
          }
        }
      }
    }
  }
  fout << nr[1] << ' ' << nr[2] << ' ' << nr[3] << ' ' << lmin << '\n';
  return 0;
}
