#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

struct traseu {
    int dist;
    vector<int> izvoare;
    traseu() : dist(0) {}
    traseu(int d, const vector<int> &izv) : dist(d), izvoare(izv) {}
};

bool cmp(const traseu &a, const traseu &b) {
    return a.dist > b.dist;
}

int main() {
    ifstream fin("popas.in");
    ofstream fout("popas.out");
    int k, t, u;
    fin >> k;
    vector<traseu> trasee(k);
    for (int i = 0; i < k; i++) {
        int r;
        fin >> r;
        vector<int> izv(r);
        for (int j = 0; j < r; j++) {
            fin >> izv[j];
        }
        sort(izv.begin(), izv.end());
        izv.push_back(1);
        trasee[i] = traseu(izv[0], izv);
    }
    fin >> t >> u;
    fin.close();

    int min_popas = INT_MAX;
    int traseu_ales = -1;
    for (int i = 0; i < k; i++) {
        int popas = 0;
        int dist_curenta = 0;
        for (int j = 0; j < trasee[i].izvoare.size(); j++) {
            if (dist_curenta + trasee[i].izvoare[j] > t) {
                popas++;
                dist_curenta = 0;
            }
            dist_curenta += trasee[i].izvoare[j];
        }
        if (dist_curenta + u < trasee[i].dist) {
            popas++;
        }
        if (popas < min_popas) {
            min_popas = popas;
            traseu_ales = i + 1;
        }
    }

    if (traseu_ales == -1) {
        fout << 0;
    } else {
        fout << min_popas << " " << traseu_ales;
    }
    fout.close();
    return 0;
}
