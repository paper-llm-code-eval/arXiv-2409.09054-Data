#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream in("scara.in");
  ofstream out("scara.out");
  int H, N, M, p;
  in >> H >> N >> M >> p;
  vector<int> h(N), dp(H + 1, 1e9);
  dp[0] = 0;
  for (int i = 0; i < N; i++) {
    in >> h[i];
  }
  sort(h.begin(), h.end());
  for (int i = 0; i < H; i++) {
    for (int j = 0; j < N; j++) {
      if (i + h[j] <= H) {
        dp[i + h[j]] = min(dp[i + h[j]], dp[i] + h[j] / (1.0 * (j + 1)) + p);
      }
      if (i + M >= h[j]) {
        dp[i + M] = min(dp[i + M], dp[i] + M / (1.0 * (j + 1)) + p);
      }
    }
  }
  out << fixed << setprecision(2) << dp[H] << endl;
  int i = H;
  vector<int> ans;
  while (i > 0) {
    for (int j = 0; j < N; j++) {
      if (i - h[j] >= 0 && dp[i] == dp[i - h[j]] + h[j] / (1.0 * (j + 1)) + p) {
        ans.push_back(h[j]);
        i -= h[j];
        break;
      } else if (i - M >= h[j] && dp[i] == dp[i - M] + M / (1.0 * (j + 1)) + p) {
        ans.push_back(M);
        i -= M;
        break;
      }
    }
  }
  for (int j = ans.size() - 1; j >= 0; j--) {
    out << ans[j] << " ";
  }
  out << endl;
  return 0;
}
