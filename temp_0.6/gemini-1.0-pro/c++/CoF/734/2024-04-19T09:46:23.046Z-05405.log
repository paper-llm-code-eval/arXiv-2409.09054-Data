#include <fstream>
#include <cstring>
#include <queue>
#include <limits>

using namespace std;

const int dx[] = {-1, -1, -1, 0, 0, 1, 1, 1};
const int dy[] = {-1, 0, 1, -1, 1, -1, 0, 1};

int main() {
  ifstream in("rj.in");
  ofstream out("rj.out");

  int n, m;
  in >> n >> m;

  char grid[n + 2][m + 2];
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      in >> grid[i][j];
    }
  }

  int dist[n + 2][m + 2];
  memset(dist, -1, sizeof(dist));

  queue<pair<int, int>> q;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      if (grid[i][j] == 'R') {
        q.push(make_pair(i, j));
        dist[i][j] = 0;
      }
      if (grid[i][j] == 'J') {
        q.push(make_pair(i, j));
        dist[i][j] = 0;
      }
    }
  }

  while (!q.empty()) {
    int x = q.front().first;
    int y = q.front().second;
    q.pop();

    for (int k = 0; k < 8; k++) {
      int nx = x + dx[k];
      int ny = y + dy[k];
      if (nx < 1 || nx > n || ny < 1 || ny > m) continue;
      if (grid[nx][ny] == 'X') continue;
      if (dist[nx][ny] != -1) continue;
      dist[nx][ny] = dist[x][y] + 1;
      q.push(make_pair(nx, ny));
    }
  }

  int tmin = numeric_limits<int>::max();
  int x, y;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      if (dist[i][j] != -1 && dist[i][j] < tmin) {
        tmin = dist[i][j];
        x = i;
        y = j;
      }
    }
  }

  out << tmin << " " << x << " " << y << "\n";

  return 0;
}
