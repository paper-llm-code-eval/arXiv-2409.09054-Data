#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAX_N = 1000;
const int MAX_M = 1000;

int main() {
  ifstream fin("arme.in");
  ofstream fout("arme.out");

  int n, m;
  fin >> n >> m;

  vector<int> pb(n + 1);
  for (int i = 1; i <= n; i++) {
    fin >> pb[i];
  }

  vector<int> pc(m + 1);
  for (int i = 1; i <= m; i++) {
    fin >> pc[i];
  }

  vector<vector<int>> dp(n + 1, vector<int>(m + 1, 0));
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      dp[i][j] = max(dp[i - 1][j], dp[i][j - 1]);
      if (pb[i] < pc[j]) {
        dp[i][j] = max(dp[i][j], dp[i - 1][j - 1] + pc[j] - pb[i]);
      }
    }
  }

  fout << dp[n][m] << '\n';

  return 0;
}
```
