            else dp[x][y][bit] = a | b;
        }
    } else {
        int a = solve(x + 1, y * 2, bit);
        int b = solve(x + 1, y * 2 + 1, bit);
        if (circuit[x][y] == '&') {
            if (bit == 1) dp[x][y][bit] = a & b;
            else dp[x][y][bit] = a | b;
        } else {
            if (bit == 0) dp[x][y][bit] = a & b;
            else dp[x][y][bit] = a | b;
        }
    }
    return dp[x][y][bit];
}

int main() {
    freopen("logic.in", "r", stdin);
    freopen("logic.out", "w", stdout);
    scanf("%d", &C);
    scanf("%d", &N);
    for (int i = 0; i < N; i++) {
        char c;
        scanf(" %c", &c);
        circuit[0][i] = c;
    }
    for (int i = 1; i < N; i++) {
        for (int j = 0; j < (1 << i); j++) {
            char c;
            scanf(" %c", &c);
            circuit[i][j] = c;
        }
    }
