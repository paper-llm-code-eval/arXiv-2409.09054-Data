```c++
#include <iostream>
#include <fstream>
#include <cstdio>
#include <algorithm>
#include <stdint.h>

typedef unsigned long long ull;
typedef unsigned int uint;
typedef long long ll;
typedef int int32;
typedef short int16;
typedef char int8;

const int NMAX = 10000000;
const int KMAX = 50;
const int QMAX = 100000;

int64_t A, B, C, D;
int64_t N, K, Q;
int64_t X[NMAX + 1], Y[NMAX + 1];
int64_t J[QMAX + 1], R[QMAX + 1], L[QMAX + 1];
int64_t dp[NMAX + 1][KMAX + 1][2];

int main() {
    std::ifstream in("pergament.in");
    std::ofstream out("pergament.out");

    in >> N >> K >> Q;
    in >> A >> B >> C >> D;
    in >> X[1] >> Y[1];

    for (int64_t i = 2; i <= N; ++i) {
        X[i] = (1 + ((ull)X[i - 1] * A + B) % K);
        Y[i] = (1 + ((ull)Y[i - 1] * C + D) % (K - X[i] + 1));
    }

    for (int64_t i = 1; i <= Q; ++i) {
        in >> J[i] >> R[i] >> L[i];
    }

    for (int64_t i = 0; i <= N; ++i) {
        for (int64_t j = 0; j <= K; ++j) {
            for (int64_t k = 0; k < 2; ++k) {
                dp[i][j][k] = 0;
            }
        }
    }

    dp[0][0][0] = 1;

    for (int64_t i = 0; i < N; ++i) {
        for (int64_t j = 0; j <= K; ++j) {
            for (int64_t k = 0; k < 2; ++k) {
                if (dp[i][j][k]) {
                    if (k) {
                        dp[i + 1][j][k] += dp[i][j][k];
                    } else {
                        dp[i + 1][j][k] += dp[i][j][k];
                        for (int64_t l = X[i + 1]; l < X[i + 1] + Y[i + 1]; ++l) {
                            dp[i + 1][l][1] += dp[i][j][k];
                        }
                    }

                    if (j < K) {
                        dp[i][j + 1][k] += dp[i][j][k];
                    }
                }
            }
        }
    }

    for (int64_t i = 1; i <= Q; ++i) {
        for (int64_t j = R[i]; j < R[i] + L[i]; ++j) {
            dp[j][J[i]][1] = 0;
        }
    }

    int64_t ans = 0;

    for (int64_t i = 0; i <= N; ++i) {
        for (int64_t j = 0; j <= K; ++j) {
            ans += dp[i][j][1];
        }
    }

    out << ans << "\n";

    return 0;
