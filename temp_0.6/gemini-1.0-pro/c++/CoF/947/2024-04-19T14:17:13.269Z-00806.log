#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAXN = 1e9 + 10;

ifstream fin("strips.in");
ofstream fout("strips.out");

long long n, nr, l, x;
vector<pair<long long, int>> v;
vector<long long> a, b;
long long dp[MAXN], sol[2];

int main() {
    fin >> x;
    fin >> n >> nr >> l;
    while (fin >> x) {
        v.push_back(make_pair(x, 0));
        v.push_back(make_pair(x + l, 1));
    }
    sort(v.begin(), v.end());
    n += l;
    long long poz = 0;
    for (auto &it : v) {
        long long d = it.first - poz;
        if (it.second) {
            b.push_back(d);
        } else {
            a.push_back(d);
        }
        poz = it.first;
    }
    long long ans = 0;
    for (auto &it : a) {
        long long st = 0, dr = nr, mid;
        while (st <= dr) {
            mid = (st + dr) / 2;
            if (dp[mid] + it <= n) {
                ans = max(ans, mid);
                st = mid + 1;
            } else {
                dr = mid - 1;
            }
        }
        dp[ans] += it;
    }
    sol[0] = ans;
    ans = 0;
    for (auto &it : b) {
        long long st = 0, dr = nr, mid;
        while (st <= dr) {
            mid = (st + dr) / 2;
            if (dp[mid] + it <= n) {
                ans = max(ans, mid);
                st = mid + 1;
            } else {
                dr = mid - 1;
            }
        }
        dp[ans] += it;
    }
    sol[1] = ans;
    if (x == 1) {
        fout << nr - sol[0] << " " << nr - sol[1] << "\n";
    } else {
        fout << sol[0] << " " << sol[1] << "\n";
    }
    return 0;
}
