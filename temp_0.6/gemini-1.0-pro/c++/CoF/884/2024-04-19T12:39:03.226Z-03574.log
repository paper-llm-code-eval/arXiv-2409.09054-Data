#include <fstream>
#include <vector>

using namespace std;

ifstream f("joc.in");
ofstream g("joc.out");

int n, k, c, castig[2];
vector<int> v[26];

void check_win(int p) {
    for (int i = 1; i <= n; i++) {
        if (v[i][0] == p && v[i][1] == p && v[i][2] == p) {
            castig[p - 1]++;
            return;
        }
    }

    for (int i = 1; i <= n; i++) {
        if (v[0][i] == p && v[1][i] == p && v[2][i] == p) {
            castig[p - 1]++;
            return;
        }
    }

    if (v[0][0] == p && v[1][1] == p && v[2][2] == p) {
        castig[p - 1]++;
        return;
    }

    if (v[0][2] == p && v[1][1] == p && v[2][0] == p) {
        castig[p - 1]++;
        return;
    }
}

int main() {
    f >> c >> n >> k;

    for (int i = 1; i <= k; i++) {
        int p;
        f >> p;

        v[p].push_back(i % 3);
        v[p].push_back((i / 3) % 3);
        v[p].push_back(i / 9);

        if (v[p].size() >= 3) {
            check_win(p);
            v[p].clear();
        }
    }

    if (c == 1)
        g << castig[0] << " " << castig[1] << '\n';
    else {
        int mx = 0;
        for (int i = 1; i <= k; i++) {
            mx = max(mx, (int)v[i].size());
        }
        g << mx << '\n';
    }

    return 0;
}
