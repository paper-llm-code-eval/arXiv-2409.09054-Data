#include <fstream>

using namespace std;

int main() {
    ifstream fin("carte.in");
    ofstream fout("carte.out");

    int n;
    fin >> n;
    int v[n + 1];
    for (int i = 1; i <= n; i++) {
        fin >> v[i];
    }
    fin.close();

    int zile = 0, zi_max = 0, pagini_max = 0, zi_curenta = 1, pagini_curente = 0;
    for (int i = 1; i <= n; i++) {
        if (v[i] == zi_curenta) {
            pagini_curente++;
            if (pagini_curente > pagini_max) {
                pagini_max = pagini_curente;
                zi_max = zi_curenta;
            }
        } else {
            zile++;
            zi_curenta++;
            pagini_curente = 1;
        }
    }

    fout << zile << " " << zi_max << " " << pagini_max << endl;
    fout.close();
    return 0;
}
```
