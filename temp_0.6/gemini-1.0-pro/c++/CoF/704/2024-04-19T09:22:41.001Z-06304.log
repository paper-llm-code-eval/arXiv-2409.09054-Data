#include <fstream>
#include <map>

using namespace std;

ifstream cin("cuburi.in");
ofstream cout("cuburi.out");

int main() {
  int n;
  cin >> n;
  map<int, int> freq;
  int mx = 0;
  int mx_color = 0;
  int last_color = 0;
  int cnt = 0;
  int ans = 0;
  int ans_start = 0;
  int ans_end = 0;
  for (int i = 1; i <= n; ++i) {
    int x;
    cin >> x;
    ++freq[x];
    if (freq[x] > mx) {
      mx = freq[x];
      mx_color = x;
    }
    if (x == last_color) {
      ++cnt;
    } else {
      if (cnt > ans) {
        ans = cnt;
        ans_start = i - cnt - 1;
        ans_end = i - 1;
      }
      cnt = 1;
    }
    last_color = x;
  }
  if (cnt > ans) {
    ans = cnt;
    ans_start = n - cnt;
    ans_end = n - 1;
  }
  cout << freq.size() << '\n';
  cout << mx_color << '\n';
  cout << ans_start + 1 << ' ' << ans_end + 1 << '\n';
  return 0;
}
```
