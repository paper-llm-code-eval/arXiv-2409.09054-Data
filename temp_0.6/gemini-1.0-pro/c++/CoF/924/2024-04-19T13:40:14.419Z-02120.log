                for (int j = i - 1; j >= k - 1; j--) {
                    if (sum[i] - sum[j - 1] >= 0) {
                        dp[i][k] += dp[j - 1][k - 1];
                    }
                }
            }
        }

        fout << dp[n][n] << "\n";
    } else {
        ll sol = 0, sum_sol = 0;
        int max_k = 0;
        for (int k = 2; k <= n; k++) {
            for (int i = k; i <= n; i++) {
                for (int j = i - 1; j >= k - 1; j--) {
