#include <fstream>
#include <vector>
#include <algorithm>
using namespace std;

int main()
{
    ifstream in("ks.in");
    ofstream out("ks.out");
    int n, k;
    in >> n >> k;
    vector<int64_t> v(n);
    for (int64_t &x : v)
        in >> x;

    vector<int64_t> sum(n + 1);
    for (int i = 0; i < n; i++)
        sum[i + 1] = sum[i] + v[i];

    vector<int64_t> dp(n + 1);
    for (int i = 1; i <= n; i++)
    {
        dp[i] = sum[i];
        for (int j = 1; j < k && i - j >= 0; j++)
            dp[i] = max(dp[i], dp[i - j] + sum[i] - sum[i - j]);
    }

    int64_t ans = 0;
    for (int i = k; i <= n; i++)
        ans = max(ans, dp[i]);

    out << ans << '\n';
    return 0;
}
```
