#include <fstream>
#include <queue>
#include <vector>

using namespace std;

int main() {
  ifstream fin("pm.in");
  ofstream fout("pm.out");
  int n;
  fin >> n;
  vector<int> time(n);
  for (int i = 0; i < n; i++) { fin >> time[i]; }
  vector<vector<int>> dep(n);
  vector<int> deg_in(n);
  for (int i = 1; i < n; i++) {
    int m;
    fin >> m;
    dep[i].resize(m);
    for (int j = 0; j < m; j++) {
      int x;
      fin >> x;
      dep[i][j] = x - 1;
      deg_in[x - 1]++;
    }
  }
  queue<int> q;
  vector<int> dist(n);
  for (int i = 0; i < n; i++) {
    if (deg_in[i] == 0) {
      q.push(i);
      dist[i] = time[i];
    }
  }
  int max_dist = 0;
  while (!q.empty()) {
    int u = q.front();
    q.pop();
    max_dist = max(max_dist, dist[u]);
    for (int v : dep[u]) {
      deg_in[v]--;
      dist[v] = max(dist[v], dist[u] + time[v]);
      if (deg_in[v] == 0) { q.push(v); }
    }
  }
  fout << max_dist << "\n";
  vector<int> earliest(n), latest(n);
  for (int i = n - 1; i >= 0; i--) {
    int min_earliest = 1e9;
    for (int v : dep[i]) { min_earliest = min(min_earliest, earliest[v]); }
    earliest[i] = max(0, min_earliest - time[i]);
    latest[i] = max(earliest[i], max_dist - dist[i]);
  }
  for (int i = 0; i < n; i++) { fout << earliest[i] << " " << latest[i] << "\n"; }
  return 0;
}
