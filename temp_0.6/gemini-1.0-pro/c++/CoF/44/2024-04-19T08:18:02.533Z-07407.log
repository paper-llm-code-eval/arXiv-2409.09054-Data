#include <fstream>
#include <vector>
#include <queue>
#include <utility>

using namespace std;

ifstream fin("pm.in");
ofstream fout("pm.out");

const int MAXN = 100;
int n, t[MAXN + 1], in[MAXN + 1], c[MAXN + 1], d[MAXN + 1];
vector<int> a[MAXN + 1];

int main() {
    fin >> n;
    for (int i = 1; i <= n; i++)
        fin >> t[i];
    for (int i = 2; i <= n; i++) {
        int m;
        fin >> m;
        for (int j = 0; j < m; j++) {
            int x;
            fin >> x;
            a[i].push_back(x);
            in[i]++;
        }
    }

    queue<int> q;
    for (int i = 1; i <= n; i++)
        if (in[i] == 0) {
            q.push(i);
            c[i] = 0;
            d[i] = t[i];
        }

    int T = 0;
    while (!q.empty()) {
        int u = q.front();
        q.pop();
        T = max(T, c[u]);
        for (auto v : a[u]) {
            c[v] = max(c[v], c[u] + t[v]);
            d[v] = min(d[v], c[u] + t[v]);
            in[v]--;
            if (in[v] == 0)
                q.push(v);
        }
    }

    fout << T << '\n';
    for (int i = 1; i <= n; i++)
        fout << c[i] << ' ' << d[i] << '\n';
    return 0;
}
```
