#include <fstream>
#include <vector>
#include <algorithm>
#include <queue>
#include <unordered_map>

using namespace std;

struct Edge {
    int to, w;
};

struct Node {
    vector<Edge> edges;
    bool visited = false;
    int group = -1;
};

int n, m, k;
vector<Node> nodes;
vector<pair<int, int>> edges;
vector<int> priorities;

int find_max_gravity(int k) {
    // Find the minimum spanning tree of the graph
    priority_queue<pair<int, int>, vector<pair<int, int>>, greater<pair<int, int>>> pq;
    pq.push({0, 1});
    int total_weight = 0;
    while (!pq.empty()) {
        int weight, node;
        tie(weight, node) = pq.top();
        pq.pop();
        if (nodes[node].visited) {
            continue;
        }
        nodes[node].visited = true;
        total_weight += weight;
        for (Edge edge : nodes[node].edges) {
            if (!nodes[edge.to].visited) {
                pq.push({edge.w, edge.to});
            }
        }
    }

    // Find the maximum number of edges to remove to get k groups
    int max_edges_to_remove = total_weight / k;

    // Find the edges to remove
    vector<pair<int, int>> edges_to_remove;
    for (int i = 0; i < m; i++) {
        if (priorities[i] <= max_edges_to_remove) {
            edges_to_remove.push_back(edges[i]);
        }
    }

    // Remove the edges and assign nodes to groups
    int group_id = 0;
    for (pair<int, int> edge : edges_to_remove) {
        int from = edge.first, to = edge.second;
        nodes[from].group = group_id;
        nodes[to].group = group_id;
        group_id++;
    }

    // Calculate the gravity of the scenario
    int gravity = 0;
    for (pair<int, int> edge : edges_to_remove) {
        gravity += priorities[edge.second - 1];
    }

    return gravity;
}

int main() {
    ifstream in("urgenta.in");
    ofstream out("urgenta.out");
    in >> n >> m >> k;
    nodes.resize(n + 1);
    edges.resize(m);
    priorities.resize(m);
    for (int i = 0; i < m; i++) {
        int from, to, priority;
        in >> from >> to >> priority;
        nodes[from].edges.push_back({to, priority});
        nodes[to].edges.push_back({from, priority});
        edges[i] = {from, to};
        priorities[i] = priority;
    }
    in.close();

    int max_gravity = 0;
    for (int i = 1; i <= k; i++) {
        max_gravity = max(max_gravity, find_max_gravity(i));
    }

    out << max_gravity << endl;
    for (pair<int, int> edge : edges) {
        if (nodes[edge.first].group != nodes[edge.second].group) {
            out << edge.first << " " << edge.second << endl;
        }
    }
    out.close();
    return 0;
}
