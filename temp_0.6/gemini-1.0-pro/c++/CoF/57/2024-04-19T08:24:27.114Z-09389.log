#include <cstdio>
#include <vector>
#include <queue>
#include <algorithm>
#include <cstring>

using namespace std;

const int NMAX = 255;

struct Edge {
    int node, cap, flow, cost;
    Edge(int _node, int _cap, int _flow, int _cost) :
        node(_node), cap(_cap), flow(_flow), cost(_cost) {}
};

struct MinCostMaxFlow {
    int N;
    vector<Edge> E;
    vector<vector<int>> G;
    vector<int> dist, pi, prv;
    vector<bool> inQueue;

    MinCostMaxFlow(int _N) : N(_N) {
        G.resize(N + 1);
    }

    void addEdge(int u, int v, int cap, int cost) {
        E.push_back(Edge(v, cap, 0, cost));
        E.push_back(Edge(u, 0, 0, -cost));
        G[u].push_back(E.size() - 2);
        G[v].push_back(E.size() - 1);
    }

    bool spfa(int S, int T) {
        dist.assign(N + 1, 1e9);
        pi.assign(N + 1, 0);
        prv.assign(N + 1, -1);
        inQueue.assign(N + 1, false);
        queue<int> Q;
        Q.push(S);
        dist[S] = 0;
        pi[S] = 0;
        inQueue[S] = true;
        while (!Q.empty()) {
            int u = Q.front();
            Q.pop();
            inQueue[u] = false;
            for (int i : G[u]) {
                int v = E[i].node;
                if (E[i].flow < E[i].cap && dist[v] > dist[u] + E[i].cost) {
                    dist[v] = dist[u] + E[i].cost;
                    pi[v] = pi[u] + E[i].cap - E[i].flow;
                    prv[v] = i;
                    if (!inQueue[v]) {
                        Q.push(v);
                        inQueue[v] = true;
                    }
                }
            }
        }
        return dist[T] != 1e9;
    }

    pair<int, int> maxFlow(int S, int T) {
        int maxFlow = 0;
        int minCost = 0;
        while (spfa(S, T)) {
            int flow = pi[T];
            int cost = 0;
            for (int i = prv[T]; i != -1; i = prv[E[i ^ 1].node]) {
                cost += E[i].cost;
            }
            for (int i = prv[T]; i != -1; i = prv[E[i ^ 1].node]) {
                E[i].flow += flow;
                E[i ^ 1].flow -= flow;
            }
            maxFlow += flow;
            minCost += flow * cost;
        }
        return make_pair(maxFlow, minCost);
    }
};

int N, M, K;
int P[NMAX][NMAX];
int G[NMAX][NMAX];
int sol[NMAX][NMAX];

int main() {
    freopen("urgenta.in", "r", stdin);
    freopen("urgenta.out", "w", stdout);
    scanf("%d%d%d", &N, &M, &K);
    for (int i = 1; i <= M; i++) {
        int x, y, z;
        scanf("%d%d%d", &x, &y, &z);
        P[x][y] = z;
        P[y][x] = z;
        G[x][y] = 1;
        G[y][x] = 1;
    }
    MinCostMaxFlow mcmf(N + 2);
    int S = N + 1;
    int T = N + 2;
    for (int i = 1; i <= N; i++) {
        mc mf(i, S, 1, 0);
        mc mf(i, T, K - 1, 0);
    }
    for (int i = 1; i <= N; i++) {
        for (int j = i + 1; j <= N; j++) {
            if (G[i][j]) {
                mc mf(S, j, 1, P[i][j]);
            }
        }
    }
    pair<int, int> ans = mcmf.maxFlow(S, T);
    printf("%d\n", ans.second);
    int cnt = 0;
    for (Edge e : mcmf.E) {
        if (e.flow > 0 && e.node <= N && e.node != S && e.node != T) {
            sol[cnt][0] = e.node;
            sol[cnt][1] = e.cost;
            cnt++;
        }
    }
    printf("%d\n", cnt);
    for (int i = 0; i < cnt; i++) {
        printf("%d %d\n", sol[i][0], sol[i][1]);
    }
    return 0;
}
