#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int n, m, k, gravmax, c, k1, h1, k2, h2;
vector<vector<pair<int, int>>> graf;
vector<bool> viz;
vector<int> sol;
vector<pair<int, pair<int, int>>> sol2;

void dfs(int nod) {
    viz[nod] = true;
    sol.push_back(nod);
    for (auto& vecin : graf[nod]) {
        if (!viz[vecin.first]) {
            dfs(vecin.first);
        }
    }
}

void afisare(int grav, int cnt) {
    if (grav > gravmax) {
        gravmax = grav;
        c = cnt;
        sol2 = sol;
    }
}

void back(int nod, int cnt, int grav) {
    if (cnt == k) {
        afisare(grav, cnt);
        return;
    }
    if (nod > n) {
        return;
    }
    for (auto& vecin : graf[nod]) {
        if (viz[vecin.first]) {
            continue;
        }
        viz[vecin.first] = true;
        back(nod + 1, cnt + 1, grav + vecin.second);
        viz[vecin.first] = false;
        back(nod + 1, cnt, grav);
    }
}

int main() {
    ifstream in("urgenta.in");
    ofstream out("urgenta.out");
    in >> n >> m >> k;
    graf.resize(n + 1);
    viz.resize(n + 1);
    for (int i = 0; i < m; i++) {
        in >> k1 >> h1 >> k2;
        graf[k1].push_back({h1, k2});
        graf[h1].push_back({k1, k2});
    }
    back(1, 1, 0);
    out << gravmax << "\n" << c << "\n";
    for (auto& p : sol2) {
        out << p.first << " " << p.second.first << "\n";
    }
    in.close();
    out.close();
    return 0;
}
