#include <cstdio>
#include <vector>
#include <queue>
#include <algorithm>
#include <cstring>

using namespace std;

const int MAXN = 255;
const int MAXM = 32384;
const int MAXK = 255;
const int INF = 1<<30;

int N, M, K;

int prior[MAXM];
int a[MAXN], b[MAXN];
int deg[MAXN];
int vis[MAXN];

vector<int> G[MAXN];

void read_input() {
  FILE *fin = fopen("urgenta.in", "r");
  fscanf(fin, "%d%d%d", &N, &M, &K);
  for (int i = 0; i < M; ++i) {
    int x, y, z;
    fscanf(fin, "%d%d%d", &x, &y, &z);
    prior[i] = z;
    G[x].push_back(i);
    G[y].push_back(i);
    ++deg[x];
    ++deg[y];
  }
  fclose(fin);
}

int find_max_cut(int k) {
  memset(vis, 0, sizeof vis);
  int ans = 0;
  queue<int> Q;
  for (int i = 1; i <= N; ++i) {
    if (deg[i] == 1) {
      Q.push(i);
      vis[i] = 1;
    }
  }
  while (!Q.empty()) {
    int nod = Q.front();
    Q.pop();
    --k;
    for (int i = 0; i < G[nod].size(); ++i) {
      int id = G[nod][i];
      int other_node;
      if (a[id] == nod) other_node = b[id]; else other_node = a[id];
      if (vis[other_node]) continue;
      --deg[other_node];
      if (deg[other_node] == 1) {
        Q.push(other_node);
        vis[other_node] = 1;
      }
      ans += prior[id];
    }
    if (k == 0) break;
  }
  return ans;
}

int main() {
  read_input();
  int ans = -INF;
  vector<pair<int, int>> sol;
  for (int i = 0; i < M; ++i) {
    a[i] = G[G[i][0]][0];
    b[i] = G[G[i][0]][1];
  }
  for (int i = 0; i < M; ++i) {
    swap(a[i], b[i]);
    int current_ans = find_max_cut(K);
    if (current_ans > ans) {
      ans = current_ans;
      sol.clear();
      for (int i = 0; i < M; ++i) {
        if (a[i] != G[G[i][0]][0]) sol.push_back(make_pair(G[G[i][0]][0], G[G[i][0]][1]));
      }
    }
    swap(a[i], b[i]);
  }
  FILE *fout = fopen("urgenta.out", "w");
  fprintf(fout, "%d\n", ans);
  fprintf(fout, "%d\n", (int)sol.size());
  for (int i = 0; i < sol.size(); ++i) {
    fprintf(fout, "%d %d\n", sol[i].first, sol[i].second);
  }
  fclose(fout);
  return 0;
}
