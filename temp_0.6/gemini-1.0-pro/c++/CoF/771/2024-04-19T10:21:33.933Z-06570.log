#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

bool ok(const vector<vector<int>>& a, const vector<int>& b, int k) {
    for (int i = 0; i < a[0].size(); ++i) {
        bool ok = false;
        for (int j = 0; j < k; ++j) {
            ok |= find(a[b[j]].begin(), a[b[j]].end(), i) != a[b[j]].end();
        }
        if (!ok) {
            return false;
        }
    }
    return true;
}

int main() {
    ifstream in("pluricex.in");
    ofstream out("pluricex.out");

    int n, k, d;
    in >> n >> k >> d;
    vector<vector<int>> a(n + 1);
    for (int i = 1; i <= n; ++i) {
        int c;
        in >> c;
        for (int j = 0; j < c; ++j) {
            int x;
            in >> x;
            a[i].push_back(x - 1);
        }
    }

    vector<int> b(k);
    for (int i = 0; i < k; ++i) {
        b[i] = i + 1;
    }

    while (true) {
        if (ok(a, b, k)) {
            for (int i = 0; i < k; ++i) {
                out << b[i] << ' ';
            }
            out << '\n';
        }

        int i = k - 1;
        while (i >= 0 && b[i] == n - k + i + 1) {
            --i;
        }
        if (i < 0) {
            break;
        }
        ++b[i];
        for (int j = i + 1; j < k; ++j) {
            b[j] = b[j - 1] + 1;
        }
    }

    return 0;
}
