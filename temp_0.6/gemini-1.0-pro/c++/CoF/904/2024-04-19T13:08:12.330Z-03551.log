        count += grid[i][j];
      }
      if (count > max_mines) {
        max_mines = count;
        max_lines.clear();
        max_lines.push_back(i);
      } else if (count == max_mines) {
        max_lines.push_back(i);
      }
    }

    for (int i = 0; i < max_lines.size(); i++) {
      out << max_lines[i] << " ";
    }
    out << "\n";
  } else if (v == 2) {
    int min_moves = -1;

    for (int i = 1; i <= l; i++) {
      for (int j = 1; j <= c; j++) {
        if (grid[i][j] == 0) {
          int moves = 0;
          for (auto &mine : mines) {
            int x = mine.first;
            int y = mine.second;
            moves += abs(i - x) + abs(j - y);
          }
          if (min_moves == -1 || moves < min_moves) {
            min_moves = moves;
          }
        }
      }
    }

    out << min_moves << "\n";
  }

  return 0;
}
