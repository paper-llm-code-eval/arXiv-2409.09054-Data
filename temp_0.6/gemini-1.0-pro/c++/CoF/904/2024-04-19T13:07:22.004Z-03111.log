                    {
                        if (l == i && r == j)
                        {
                            dp[i][j][l][r] = 0;
                            continue;
                        }
                        if (l > i)
                            dp[i][j][l][r] = min(dp[i][j][l][r], dp[i][j][l - 1][r] + 1);
                        if (r > j)
                            dp[i][j][l][r] = min(dp[i][j][l][r], dp[i][j][l][r - 1] + 1);
                        f[i][j][l][r] = min(f[i][j][l][r], dp[i][j][l][r]);
                    }
            }
        for (int i = 1; i <= n; i++)
            for (int j = 1; j <= m; j++)
                for (int l = i; l <= n; l++)
                    for (int r = j; r <= m; r++)
                    {
                        if (l == i && r == j)
                            continue;
                        if (l > i)
                            f[i][j][l][r] = min(f[i][j][l][r], f[i][j][l - 1][r]);
                        if (r > j)
                            f[i][j][l][r] = min(f[i][j][l][r], f[i][j][l][r - 1]);
                    }
        int ans = 1e9;
        for (auto& x : poz)
        {
            int i = x.first, j = x.second;
            for (int l = i; l <= n; l++)
                for (int r = j; r <= m; r++)
                    ans = min(ans, f[i][j][l][r]);
        }
        if (ans == 1e9)
            fout << -1;
        else
            fout << ans;
    }
    return 0;
}
