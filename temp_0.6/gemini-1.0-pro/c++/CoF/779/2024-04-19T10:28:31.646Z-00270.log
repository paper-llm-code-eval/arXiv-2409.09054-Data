#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

struct Point {
    int x, y;
};

int main() {
    ifstream in("turist.in");
    ofstream out("turist.out");

    int M, N, x, y, k;
    in >> M >> N >> x >> y >> k;

    vector<Point> points;
    for (int i = 0; i < k; i++) {
        int a, b;
        in >> a >> b;
        points.push_back({a, b});
    }

    int ans = 0;
    for (int i = 0; i < 4; i++) {
        int cnt = 0;
        for (auto p : points) {
            if (i == 0 && p.x >= x) cnt++;
            if (i == 1 && p.y <= y) cnt++;
            if (i == 2 && p.x <= x) cnt++;
            if (i == 3 && p.y >= y) cnt++;
        }
        ans = max(ans, cnt);
    }

    out << ans << endl;

    return 0;
}
```
