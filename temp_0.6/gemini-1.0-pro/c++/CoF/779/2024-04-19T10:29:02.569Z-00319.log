            ((dx == 1 && l == 0 && y > c) || (dx == -1 && l == m && y > c) ||
             (dy == 1 && c == 0 && x > l) || (dy == -1 && c == n && x > l))) {
          ++cnt;
        }
      }
      ans = max(ans, cnt);
    }
  }
