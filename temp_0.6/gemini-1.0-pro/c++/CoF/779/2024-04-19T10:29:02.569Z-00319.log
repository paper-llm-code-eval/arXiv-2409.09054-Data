            (dy == 0 && c == n && x == l) ||
            (dx == 0 && l == 0 && c == 0) ||
            (dx == 0 && l == 0 && c == n) ||
            (dx == 0 && l == m && c == 0) ||
            (dx == 0 && l == m && c == n) ||
            (dy == 0 && c == 0 && l == 0) ||
            (dy == 0 && c == 0 && l == m) ||
