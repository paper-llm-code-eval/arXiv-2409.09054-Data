  vector<int> lines(M + 1), columns(N + 1);
  lines[l]++;
  columns[c]++;

  for (int i = 0; i < k; i++) {
    int x, y;
    in >> x >> y;
    lines[x]++;
    columns[y]++;
  }

  int max_lines = 0, max_columns = 0;
