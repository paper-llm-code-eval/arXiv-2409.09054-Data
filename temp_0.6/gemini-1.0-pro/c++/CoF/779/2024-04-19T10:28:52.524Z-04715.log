    max_lines = max(max_lines, lines[i]);
  }
  for (int i = 0; i <= N; i++) {
    max_columns = max(max_columns, columns[i]);
  }

  out << max(max_lines, max_columns) << '\n';

  in.close();
  out.close();

  return 0;
}
```
