#include <fstream>
#include <vector>

const int MAX_N = 1500;
const int MAX_M = 1500;
const int MAX_FIB = 46;

std::vector<int> fib;
std::vector<int> frecv;
std::vector<int> dp;

int n, m, c;
int a[MAX_N][MAX_M];
int f[MAX_N][MAX_M];
int b[MAX_N * MAX_M];
int nr_elem;

inline int get_fib(int x) {
    if (x < 0) x = -x;
    if (x >= MAX_FIB) return fib[MAX_FIB - 1];
    return fib[x];
}

int main() {
    std::ifstream fin("fibosnek.in");
    std::ofstream fout("fibosnek.out");
    fin >> c >> n >> m;
    fib.push_back(0);
    fib.push_back(1);
    for (int i = 2; i <= 45; i++) fib.push_back(fib[i - 1] + fib[i - 2]);
    for (int i = 1; i <= n * m; i++) {
        fin >> a[i / m + 1][i % m + 1];
        if (a[i / m + 1][i % m + 1] <= MAX_FIB) frecv[a[i / m + 1][i % m + 1]]++;
    }
    if (c == 1) {
        for (int i = 1; i <= MAX_FIB; i++) fout << frecv[i] << ' ';
        return 0;
    }
    for (int i = 1; i <= n * m; i++) {
        if (a[i / m + 1][i % m + 1] <= MAX_FIB) {
            dp[i] = dp[i - 1] + 1;
            f[i / m + 1][i % m + 1] = 1;
        } else {
            dp[i] = dp[i - 1];
            int diff = 2e9;
            int numar = 0;
            for (int j = 1; j <= MAX_FIB; j++) {
                if (diff > abs(a[i / m + 1][i % m + 1] - fib[j])) {
                    diff = abs(a[i / m + 1][i % m + 1] - fib[j]);
                    numar = fib[j];
                }
            }
            if (diff == 2e9) numar = 0;
            dp[i] = std::max(dp[i], dp[i - 1] + 1);
            if (dp[i] > dp[i - 1]) {
                b[++nr_elem] = numar;
                f[i / m + 1][i % m + 1] = 1;
            }
        }
    }
    fout << dp[n * m] << '\n';
    for (int i = n * m; i >= 1; i--) {
        if (f[i / m + 1][i % m + 1]) {
            fout << b[nr_elem--] << ' ';
        } else {
            fout << a[i / m + 1][i % m + 1] << ' ';
        }
    }
    return 0;
}
