#include <fstream>
#include <vector>
#include <algorithm>
#include <cmath>

using namespace std;

ifstream fin("fibosnek.in");
ofstream fout("fibosnek.out");

const int MAXN = 1500;

int n, m, c;
vector<vector<int>> a(MAXN, vector<int>(MAXN));

int fib[MAXN * MAXN * 4];

int main() {
    fin >> c >> n >> m;
    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
            fin >> a[i][j];

    int len = 0;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            bool ok = false;
            int nr = a[i][j];
            for (int k = 1; k <= len && !ok; k++)
                if (fib[k] == nr)
                    ok = true;
            if (!ok)
                len++;
            fib[len] = nr;
        }
    }

    if (c == 1) {
        fout << len << '\n';
        return 0;
    }

    for (int i = 1; i <= len; i++)
        for (int j = i + 1; j <= len; j++)
            if (fib[i] > fib[j])
                swap(fib[i], fib[j]);

    int ans = 0;
    int nr_fib = 0;
    int l = 1, r = 1;
    while (r <= len) {
        while (r <= len && fib[r] - fib[l] <= n + m - 1)
            r++;
        if (r - l > nr_fib) {
            nr_fib = r - l;
            ans = fib[l] + fib[r - 1];
        }
        l++;
    }

    fout << ans << '\n';

    return 0;
}
