#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("control.in");
    ofstream out("control.out");

    int n;
    in >> n;

    vector<int> weights(n);
    for (int& weight : weights) in >> weight;

    sort(weights.begin(), weights.end());

    int cnt1 = 0, cnt2 = 0;
    for (int i = 0; i < n; i += 3) {
        int a = 0, b = 0, c = 0;
        if (i + 0 < n) a = weights[i + 0];
        if (i + 1 < n) b = weights[i + 1];
        if (i + 2 < n) c = weights[i + 2];

        if (a % 2 == b % 2 && b % 2 == c % 2) cnt1++;
        else cnt2++;
    }

    int control = cnt1 * 10 + cnt2;
    out << control << endl;

    bool is_prime = true;
    if (control <= 1) is_prime = false;
    for (int i = 2; i * i <= control; i++) {
        if (control % i == 0) {
            is_prime = false;
            break;
        }
    }

    out << (is_prime ? 1 : 0) << endl;

    return 0;
}
