      if (cnt % 3 == 0) sum++;
      cnt = 1;
    } else
      cnt++;
  }
  out << sum * 10 + nr << endl;
  for (int i = 2; i <= sum * 10 + nr; i++) {
    if ((sum * 10 + nr) % i == 0) {
      prim = 0;
      break;
    }
  }
  out << prim << endl;
  return 0;
}
