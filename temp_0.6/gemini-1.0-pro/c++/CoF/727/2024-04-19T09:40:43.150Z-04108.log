#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream in("control.in");
  ofstream out("control.out");

  int n;
  in >> n;

  vector<int> weights(n);
  for (int i = 0; i < n; i++) {
    in >> weights[i];
  }

  sort(weights.begin(), weights.end());

  int numGroups = (n + 2) / 3;
  int numOnes = 0;
  int numZeros = 0;
  for (int i = 0; i < numGroups; i++) {
    int numEven = 0;
