  out << controlNumber << endl;

  bool isPrime = true;
  for (int i = 2; i * i <= controlNumber; i++) {
    if (controlNumber % i == 0) {
      isPrime = false;
      break;
    }
  }

  out << (isPrime ? 1 : 0) << endl;

