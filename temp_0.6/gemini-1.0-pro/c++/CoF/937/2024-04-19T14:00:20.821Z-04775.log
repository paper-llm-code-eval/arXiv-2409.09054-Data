#include <iostream>
#include <vector>
#include <queue>
#include <cstring>

using namespace std;

const int NMAX = 1000;
const int MMAX = 20000;
const int MAX_NODE = 2 * NMAX * MMAX;
const int INF = 1e9;

class Graph {
 public:
  struct Edge {
    int to, cost;
  };

  vector<Edge> adj[MAX_NODE];

  void addEdge(int from, int to, int cost) {
    adj[from].push_back({to, cost});
    adj[to].push_back({from, cost});
  }

  int dijkstra(int start, int end) {
    priority_queue<pair<int, int>> pq;
    vector<int> dist(MAX_NODE, INF);

    dist[start] = 0;
    pq.push({0, start});

    while (!pq.empty()) {
      int curr = pq.top().second;
      int currDist = -pq.top().first;
      pq.pop();

      if (curr == end) {
        return currDist;
      }

      for (Edge e : adj[curr]) {
        int next = e.to;
        int nextDist = currDist + e.cost;

        if (nextDist < dist[next]) {
          dist[next] = nextDist;
          pq.push({-nextDist, next});
        }
      }
    }

    return INF;
  }
};

int main() {
  ifstream in("tunel.in");
  ofstream out("tunel.out");

  int C, N, M, X;
  in >> C >> N >> M >> X;

  Graph g;

  int nodeCounter = 1;
  vector<int> tunnelStartNodes(N + 1, -1);
  vector<int> tunnelEndNodes(N + 1, -1);

  for (int i = 1; i <= N; i++) {
    tunnelStartNodes[i] = nodeCounter++;
    for (int j = 1; j < M; j++) {
      g.addEdge(nodeCounter - 1, nodeCounter, 1);
      nodeCounter++;
    }
    tunnelEndNodes[i] = nodeCounter - 1;
  }

  int P;
  for (int i = 1; i < N; i++) {
    in >> P;
    for (int j = 0; j < P; j++) {
      int pos;
      in >> pos;
      g.addEdge(tunnelEndNodes[i] + pos, tunnelStartNodes[i + 1] + pos, 0);
    }
  }

  int startNode = tunnelStartNodes[X];
  int endNode = tunnelEndNodes[N];

  int answer;
  if (C == 1) {
    answer = -1;
    for (int i = 1; i <= N; i++) {
      if (g.dijkstra(startNode, tunnelEndNodes[i]) < INF) {
        answer = i;
        break;
      }
    }

    out << answer << endl;
  } else {
    answer = g.dijkstra(startNode, endNode);

    if (answer == INF) {
      out << -1 << endl;
    } else {
      out << answer + 1 << endl;
    }
  }

  return 0;
}
