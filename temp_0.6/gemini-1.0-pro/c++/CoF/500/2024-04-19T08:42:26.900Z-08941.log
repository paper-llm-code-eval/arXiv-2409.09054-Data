            pq.push({v[idx].second, v[idx].first});
            idx++;
        }
        while (!pq.empty() && pq.top().first < i) {
            sol[pq.top().second] = -1;
            pq.pop();
        }
        if (!pq.empty()) {
            ans[pq.top().second - 1] = pq.top().second;
            sol[pq.top().second] = i;
            pq.pop();
