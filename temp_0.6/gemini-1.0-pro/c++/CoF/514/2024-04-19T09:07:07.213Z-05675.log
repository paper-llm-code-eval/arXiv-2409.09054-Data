#include <fstream>
#include <vector>

using namespace std;

const int MAX_N = 1000000;
const int MAX_Q = 200000;

int main() {
  ifstream fin("primprim.in");
  ofstream fout("primprim.out");

  int c, n, q;
  fin >> c >> n;
  vector<int> a(n + 1), p(MAX_N + 1, -1), cost(MAX_N + 1);

  for (int i = 1; i <= n; i++) {
    fin >> a[i];
    if (p[a[i]] == -1) { p[a[i]] = i; }
    cost[a[i]] += abs(a[i] - p[a[i]]);
  }

  if (c == 1) {
    fout << cost[a[1]] << '\n';
    return 0;
  }

  fin >> q;
  for (int i = 1; i <= q; i++) {
    int x, y, z;
    fin >> x >> y >> z;
    cost[a[x]] -= abs(a[x] - p[a[x]]);
    a[x] = y;
    if (p[y] == -1) { p[y] = x; }
    cost[a[x]] += abs(a[x] - p[a[x]]);
    int sum = cost[a[x]];
    for (int j = 1; j < z; j++) {
      int pos = p[a[x]];
      while (pos != -1 && j--) { pos = p[a[pos]]; }
      if (pos == -1) { break; }
      sum += cost[a[pos]];
    }
    fout << sum << '\n';
  }

  return 0;
}
```
