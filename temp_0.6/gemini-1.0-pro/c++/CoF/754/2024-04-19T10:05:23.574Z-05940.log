#include <fstream>
#include <vector>

using namespace std;

int n, k;
vector<char> sex;
vector<vector<int>> grupe;
vector<bool> vizitat;

bool valid() {
  for (int i = 0; i < n; i++) {
    if (!vizitat[i]) {
      return false;
    }
  }
  return true;
}

bool corect() {
  for (auto &g : grupe) {
    int fb = 0;
    for (auto &el : g) {
      fb += (sex[el - 1] == 'f');
    }
    if (abs(fb - (g.size() - fb)) > 1) {
      return false;
    }
  }
  return true;
}

int main() {
  ifstream in("grupe.in");
  ofstream out("grupe.out");

  in >> n >> k;
  sex.resize(n);
  vizitat.resize(n, false);
  in >> sex;

  int m;
  for (int i = 0; i < k; i++) {
    in >> m;
    grupe.push_back(vector<int>(m));
    for (int j = 0; j < m; j++) {
      in >> grupe[i][j];
      vizitat[grupe[i][j] - 1] = true;
    }
  }

  if (!valid()) {
    out << "NU";
    return 0;
  }

  if (!corect()) {
    out << "NU";
    return 0;
  }

  for (auto &g : grupe) {
    int fb = 0;
    for (auto &el : g) {
      fb += (sex[el - 1] == 'f');
    }
    out << g.size() - fb << ' ' << fb << '\n';
  }

  out << "DA";

  return 0;
}
```
