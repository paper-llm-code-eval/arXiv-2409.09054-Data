#include <fstream>

using namespace std;

ifstream fin("cartele.in");
ofstream fout("cartele.out");

struct Data {
  int ora;
  int minute;
  int secunde;
};

bool operator<(const Data &a, const Data &b) {
  if (a.ora < b.ora) return true;
  if (a.ora == b.ora && a.minute < b.minute) return true;
  return a.ora == b.ora && a.minute == b.minute && a.secunde < b.secunde;
}

struct Carte {
  char sex;
  char actiune;
  Data moment;
};

int main() {
  int cerinta;
  fin >> cerinta;
  int n;
  fin >> n;
  Carte carte[n];
  for (int i = 0; i < n; ++i) {
    fin >> carte[i].sex >> carte[i].actiune >> carte[i].moment.ora >>
        carte[i].moment.minute >> carte[i].moment.secunde;
  }

  if (cerinta == 1) {
    int baieti = 0, fete = 0;
    for (int i = 0; i < n; ++i) {
      if (carte[i].sex == 'b')
        baieti += carte[i].actiune == 'i' ? 1 : -1;
      else
        fete += carte[i].actiune == 'i' ? 1 : -1;
    }
    fout << baieti << " " << fete << "\n";
  } else if (cerinta == 2) {
    int egal = 0;
    int secunde = 0;
    for (int i = 0; i < n; ++i) {
      if (carte[i].actiune == 'i') {
        if (carte[i].sex == 'b')
          ++egal;
        else
          --egal;
      } else {
        if (carte[i].sex == 'b')
          --egal;
        else
          ++egal;
      }
      if (egal == 0)
        secunde += carte[i + 1].moment.secunde - carte[i].moment.secunde;
    }
    fout << secunde << "\n";
  } else {
    int impar = 0;
    int secunde = 0;
    for (int i = 0; i < n; ++i) {
      if (carte[i].actiune == 'i') {
        if (carte[i].sex == 'b')
          ++impar;
        else
          --impar;
        if (impar % 2 == 1)
          secunde = carte[i + 1].moment.secunde - carte[i].moment.secunde;
      } else {
        if (carte[i].sex == 'b')
          --impar;
        else
          ++impar;
        if (impar % 2 == 0)
          secunde = max(secunde, carte[i + 1].moment.secunde - carte[i].moment.secunde);
      }
    }
    fout << secunde << "\n";
  }

  return 0;
}
