      cnt++;
    }
  }
  return cnt == 1;
}

int dfs(int x, int y) {
  if (x < 1 || x > n || y < 1 || y > m || visited[x][y]) {
    return 0;
  }
  visited[x][y] = true;
  cells.push_back(make_pair(x, y));
  return 1 + dfs(x + dx[grid[x][y]], y + dy[grid[x][y]]);
}

int main() {
  ifstream in("arrows.in");
  ofstream out("arrows.out");

  int req;
  in >> req;
  in >> n >> m;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      in >> grid[i][j];
    }
  }
  int x, y;
  in >> x >> y;

  switch (req) {
    case 1:
      if (is_favorable(x, y)) {
        dfs(x, y);
        out << (cells.size() == 1 ? 0 : cells.size() * 1000) << '\n';
      } else {
        out << 0 << '\n';
      }
      break;
    case 2:
      for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
          if (!visited[i][j]) {
            cells.clear();
            dfs(i, j);
            if (cells.size() == 1) {
              out << cells.size() << '\n';
            }
          }
        }
      }
      break;
    case 3:
      int max_score = 0;
      for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
          if (!visited[i][j]) {
            cells.clear();
            int score = dfs(i, j);
            if (cells.size() == 1) {
              max_score = max(max_score, score);
            }
          }
        }
      }
      out << max_score * 1000 << '\n';
      break;
  }

  return 0;
}
