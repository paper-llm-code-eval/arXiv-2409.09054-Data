#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

struct Time {
    int h, m, s;
};

Time add(const Time& a, const Time& b) {
    Time r;
    r.s = a.s + b.s;
    r.m = a.m + b.m + r.s / 60;
    r.h = a.h + b.h + r.m / 60;
    r.s %= 60;
    r.m %= 60;
    return r;
}

int main() {
    ifstream in("schi.in");
    ofstream out("schi.out");

    Time start, first, last;
    in >> start.h >> start.m >> start.s;
    int n;
    in >> n;
    vector<Time> times(n);
    for (int i = 0; i < n; i++) {
        in >> times[i].m >> times[i].s;
    }
    sort(times.begin(), times.end());
    first = add(start, times[0]);
    last = add(start, times[n - 1]);
    out << first.h << ' ' << first.m << ' ' << first.s << endl;
    out << last.h << ' ' << last.m << ' ' << last.s << endl;
    return 0;
}
