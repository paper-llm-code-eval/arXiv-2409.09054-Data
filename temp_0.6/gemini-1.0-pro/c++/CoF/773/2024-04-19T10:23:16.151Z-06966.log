#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

struct Time {
    int hour, minute, second;

    Time(int _hour, int _minute, int _second) : hour(_hour), minute(_minute), second(_second) {}

    bool operator<(const Time& other) const {
        if (hour != other.hour) {
            return hour < other.hour;
        }
        if (minute != other.minute) {
            return minute < other.minute;
        }
        return second < other.second;
    }

    void add(int minutes, int seconds) {
        second += seconds;
        if (second >= 60) {
            second -= 60;
            minute++;
        }
        minute += minutes;
        if (minute >= 60) {
            minute -= 60;
            hour++;
        }
    }
};

int main() {
    ifstream in("schi.in");
    ofstream out("schi.out");

    int startHour, startMinute, startSecond, numCompetitors;
    in >> startHour >> startMinute >> startSecond >> numCompetitors;

    vector<Time> times;
    for (int i = 0; i < numCompetitors; i++) {
        int minutes, seconds;
        in >> minutes >> seconds;
        times.push_back(Time(0, minutes, seconds));
    }

    sort(times.begin(), times.end());

    Time firstTime(startHour, startMinute, startSecond);
    Time lastTime(startHour, startMinute, startSecond);
