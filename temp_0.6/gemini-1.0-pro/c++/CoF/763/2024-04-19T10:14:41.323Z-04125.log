        nx = x - 1;
        ny = y + 1;
        break;
      case 8:
        nx = x;
        ny = y + 1;
        break;
      case 5:
        nx = x + 1;
        ny = y + 1;
        break;
    }
    if (nx >= 0 && nx < n && ny >= 0 && ny < n) {
      total += table[nx][ny];
      count[nx * n + ny]++;
      if (count[nx * n + ny] > max_count) {
        max_count = count[nx * n + ny];
        max_count_square = nx * n + ny;
      }
      x = nx;
      y = ny;
    }
  }
  out << total << " " << max_count << endl;
  return 0;
}
