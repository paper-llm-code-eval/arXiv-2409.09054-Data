        ny = y - 1;
        break;
      case 2:
        nx = x;
        ny = y - 1;
        break;
      case 3:
        nx = x + 1;
        ny = y - 1;
        break;
      case 4:
        nx = x - 1;
        ny = y;
        break;
      case 6:
        nx = x + 1;
        ny = y;
        break;
