#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream in("ocr.in");
  ofstream out("ocr.out");

  int n, m;
  in >> n >> m;

  vector<vector<double>> img(n, vector<double>(m));
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      in >> img[i][j];
    }
  }

  double sum = 0;
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      sum += img[i][j];
    }
  }

  double min_diff = 1e9;
  int best_i = -1, best_j = -1;

  for (int i = 0; i < n; i++) {
    double sum_above = 0, sum_below = 0;
    for (int j = 0; j < m; j++) {
      sum_above += img[i][j];
      sum_below += img[n - 1 - i][j];
    }
    double diff = abs(sum_above - sum_below);
    if (diff < min_diff) {
      min_diff = diff;
      best_i = i;
    }
  }

  min_diff = 1e9;

  for (int j = 0; j < m; j++) {
    double sum_left = 0, sum_right = 0;
    for (int i = 0; i < n; i++) {
      sum_left += img[i][j];
      sum_right += img[i][m - 1 - j];
    }
    double diff = abs(sum_left - sum_right);
    if (diff < min_diff) {
      min_diff = diff;
      best_j = j;
    }
  }

  out << best_i + 1 << " " << best_j + 1 << endl;

  return 0;
}
