#include <fstream>
#include <vector>
#include <algorithm>
using namespace std;
const int NMAX = 102;
int p, s, c, v;
int cluster[NMAX][NMAX];
int main() {
    ifstream in("defrag.in");
    ofstream out("defrag.out");
    in >> v;
    in >> p >> s;
    in >> c;
    while (c--) {
        int i, j;
        in >> i >> j;
        cluster[i][j] = 1;
    }
    if (v == 1) {
        int nr = 0;
        for (int i = 1; i <= p; i++) {
            bool ok = true;
            for (int j = 1; j <= s; j++)
                if (cluster[i][j] == 1) ok = false;
            if (ok) nr++;
        }
        out << nr << "\n";
    } else {
        for (int i = 1; i <= p; i++) {
            vector<int> poz;
            for (int j = 1; j <= s; j++)
                if (cluster[i][j] == 1) poz.push_back(j);
            int nr = 0;
            if (poz.size() > 0) {
                int last = poz[0] - 1;
                for (int j = 1; j < poz.size(); j++) {
                    if (poz[j] - last > 1) nr += poz[j] - last - 1;
                    last = poz[j];
                }
                int last_sector = (s + poz[poz.size() - 1] - 1) % s + 1;
                if (last_sector - last > 1) nr += last_sector - last - 1;
            }
            out << nr << " ";
        }
        out << "\n";
    }
    return 0;
}
