#include <fstream>
#include <vector>

using namespace std;

const int maxn = 100;
const int maxe = 40;

int m, n, k;
int a[maxn][maxn];
int e[maxe][5];
int cnt1, cnt2;

void ReadInput() {
  ifstream fin("elicop.in");
  fin >> m >> n;
  for (int i = 1; i <= m; i++) {
    for (int j = 1; j <= n; j++) {
      fin >> a[i][j];
    }
  }
  fin >> k;
  for (int i = 1; i <= k; i++) {
    fin >> e[i][1] >> e[i][2] >> e[i][3] >> e[i][4] >> e[i][0];
  }
  fin.close();
}

void CheckElicopter(int nr) {
  int x1 = e[nr][1], y1 = e[nr][2], x2 = e[nr][3], y2 = e[nr][4], p = e[nr][0];
  int sum = 0;
  if (p == 1) {
    for (int i = x1; i <= x2; i++) {
      int j = y1 + i - x1;
      sum += a[i][j];
    }
  } else {
    for (int i = x1; i >= x2; i--) {
      int j = y1 - (x1 - i);
      sum += a[i][j];
    }
  }
  if (sum > (x2 - x1 + 1) / 2) {
    cnt2++;
  }
}

void Solve() {
  for (int i = 1; i <= m; i++) {
    for (int j = 1; j <= n; j++) {
      if (a[i][j] == 1) {
        cnt1++;
      }
    }
  }
  for (int i = 1; i <= k; i++) {
    CheckElicopter(i);
  }
}

void WriteOutput() {
  ofstream fout("elicop.out");
  fout << cnt1 << '\n';
  fout << cnt2;
  for (int i = 1; i <= k; i++) {
    if (a[e[i][1]][e[i][2]] == 1 && a[e[i][3]][e[i][4]] == 1) {
      fout << ' ' << i;
    }
  }
  fout << '\n';
  fout.close();
}

int main() {
  ReadInput();
  Solve();
  WriteOutput();
  return 0;
}
