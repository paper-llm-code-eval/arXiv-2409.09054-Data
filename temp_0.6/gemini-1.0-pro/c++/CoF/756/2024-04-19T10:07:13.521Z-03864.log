  for (int i = 0; i < s.size(); i++) {
    // Check for palindromes of odd length
    for (int j = i - 1, k = i + 1; j >= 0 && k < s.size(); j--, k++) {
      if (s[j] != s[k]) {
        break;
      }
      int len = k - j + 1;
      if (len > max_len) {
        max_len = len;
        start = j;
      }
    }

    // Check for palindromes of even length
    for (int j = i, k = i + 1; j >= 0 && k < s.size(); j--, k++) {
      if (s[j] != s[k]) {
        break;
      }
      int len = k - j + 1;
      if (len > max_len) {
        max_len = len;
        start = j;
      }
    }
  }

  fout << s.substr(start, max_len) << "\n";

  return 0;
}
