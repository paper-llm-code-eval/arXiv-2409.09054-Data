#include <fstream>
#include <vector>
#include <algorithm>
using namespace std;

int n;
vector<int> v;

int main() {
    ifstream in("mare.in");
    ofstream out("mare.out");

    in >> n;
    for (int i = 0; i < n; i++) {
        int x;
        in >> x;
        v.push_back(x);
    }
    in.close();

    vector<vector<int>> dp(n + 1, vector<int>(n + 1, -1));
    for (int i = 0; i < n; i++) {
        dp[i][i] = v[i];
    }
    for (int len = 2; len <= n; len++) {
        for (int i = 0; i <= n - len; i++) {
            int j = i + len - 1;
            for (int k = i; k < j; k++) {
                if (v[k] == v[j]) {
                    dp[i][j] = max(dp[i][j], dp[i][k] * 10 + v[j]);
                }
            }
        }
    }

    int ans = 0;
    for (int i = 0; i < n; i++) {
        for (int j = i; j < n; j++) {
            ans = max(ans, dp[i][j]);
        }
    }

    out << ans << endl;
    out.close();
    return 0;
}
```
