      if (x[i] == a % 10) {
        if (last == -1) { last = i; }
        if (x[i] < a % 10) { break; }
        if (x[i] == a % 10) { cnt++; }
      }
      a /= 10;
    }
    if (last != -1) {
      sol = n - 1 - last;
      for (int i = 0; i < last; i++) {
        if (x[i] < a % 10) {
          sol++;
          break;
        }
      }
    }
    fout << sol;
  }
  return 0;
}
```
