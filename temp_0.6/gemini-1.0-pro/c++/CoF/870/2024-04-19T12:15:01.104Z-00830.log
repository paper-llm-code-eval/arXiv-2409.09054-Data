  fin >> p;
  fin >> a;
  fin >> n;
  for (int i = 0; i < n; i++) { fin >> x[i]; }
  if (p == 1) {
    sort(x, x + n, greater<int>());
    for (int i = 0; i < n; i++) { fout << x[i]; }
  } else {
    int last = -1;
