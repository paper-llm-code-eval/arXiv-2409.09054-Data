            platoonSizes.push_back(1);
        }
    }

    fout << platoons.size() << endl;
    int maxPlatoonSize = 0;
    for (int i = 0; i < platoonSizes.size(); i++) {
        maxPlatoonSize = max(maxPlatoonSize, platoonSizes[i]);
    }
    fout << maxPlatoonSize << endl;
    int numMaxPlatoons = 0;
    for (int i = 0; i < platoonSizes.size(); i++) {
        if (platoonSizes[i] == maxPlatoonSize) {
            numMaxPlatoons++;
        }
