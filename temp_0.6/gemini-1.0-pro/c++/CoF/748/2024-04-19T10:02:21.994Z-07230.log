#include <fstream>
#include <vector>
#include <algorithm>
#include <unordered_map>

using namespace std;

int main() {
  ifstream in("pluton.in");
  ofstream out("pluton.out");
  int n;
  in >> n;
  unordered_map<int, vector<int>> m;
  for (int i = 0; i < n; i++) {
    int x;
    in >> x;
    vector<int> digits;
    while (x > 0) {
      digits.push_back(x % 10);
      x /= 10;
    }
    sort(digits.begin(), digits.end());
    int key = 0;
    for (int d : digits) {
      key = key * 10 + d;
    }
    m[key].push_back(i + 1);
  }
  int max_size = 0;
  vector<int> max_pluton;
  for (auto &p : m) {
    if (p.second.size() > max_size) {
      max_size = p.second.size();
      max_pluton = p.second;
    }
  }
  out << m.size() << '\n';
  out << max_size << '\n';
  out << max_pluton.size() << '\n';
  for (int i : max_pluton) {
    out << i << ' ';
  }
  out << '\n';
  return 0;
}
```
