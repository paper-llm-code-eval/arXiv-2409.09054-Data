#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAXN = 4000, MAXLEN = 9;
int n, maxSoldiers, maxPlatoon, cntPlatoon;
vector<int> A[MAXN + 1];

int main() {
    ifstream fin("pluton.in");
    ofstream fout("pluton.out");

    fin >> n;
    for (int i = 1; i <= n; i++) {
        int x;
        fin >> x;
        string s = to_string(x);
        sort(s.begin(), s.end());
        int ind = stoi(s);
        A[ind].push_back(x);
    }

    for (int i = 0; i <= 2000000000; i++) {
        if (!A[i].empty()) {
            int cnt = A[i].size();
            cntPlatoon++;
            if (cnt > maxSoldiers) {
                maxSoldiers = cnt;
                maxPlatoon = i;
            }
        }
    }

    fout << cntPlatoon << "\n";
    fout << maxSoldiers << "\n";
    fout << cntPlatoon << "\n";
    for (auto x : A[maxPlatoon]) {
        fout << x << " ";
    }
    fout << "\n";

    fin.close();
    fout.close();
    return 0;
}
