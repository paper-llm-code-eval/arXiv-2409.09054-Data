#include <fstream>
#include <vector>
#include <algorithm>
using namespace std;

ifstream in("campionat.in");
ofstream out("campionat.out");

const int NMAX = 1000;
int n, d, t;
vector<int> puncte(NMAX + 1, 0);
vector<int> meciuri[NMAX + 1];
vector<int> grad(NMAX + 1, 0);

void citire() {
    in >> t >> n >> d;
    for (int i = 1; i <= n; i++) {
        in >> puncte[i];
    }
    for (int i = 1; i <= d; i++) {
        int x, y;
        in >> x >> y;
        if (x > y) {
            swap(x, y);
        }
        grad[x]++;
        grad[y]++;
        meciuri[x].push_back(y);
        meciuri[y].push_back(x);
    }
}

void egalitate() {
    vector<int> ord;
    for (int i = 1; i <= n; i++) {
        ord.push_back(i);
    }
    sort(ord.begin(), ord.end(), [](int a, int b) { return puncte[a] > puncte[b]; });
    int mx = puncte[ord[0]];
    for (int i = 0; i < ord.size(); i++) {
        if (puncte[ord[i]] == mx) {
            out << ord[i] << " ";
        } else {
            break;
        }
    }
    out << "\n";
}

bool depind() {
    vector<int> ord;
    for (int i = 1; i <= n; i++) {
        ord.push_back(i);
    }
    sort(ord.begin(), ord.end(), [](int a, int b) { return puncte[a] < puncte[b]; });
    int mx = puncte[ord[n - 1]];
    for (int i = n - 1; i >= 0; i--) {
        if (puncte[ord[i]] == mx) {
            int castigate = puncte[ord[i]] + grad[ord[i]] * 3;
            if (castigate > mx) {
                out << ord[i] << " ";
            }
        } else {
            break;
        }
    }
    if (out.tellp() == 0) {
        out << 0;
    }
    out << "\n";
    return true;
}

int main() {
    citire();
    if (t == 1) {
        egalitate();
    } else {
        depind();
    }
    return 0;
}
