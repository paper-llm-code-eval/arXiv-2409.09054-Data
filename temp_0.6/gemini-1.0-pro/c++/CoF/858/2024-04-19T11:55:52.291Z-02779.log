  while (n > 0) {
    v.push_back(n % 10);
    n /= 10;
  }
  int sum_even = 0, sum_odd = 0;
  for (int i = 0; i < v.size(); i++) {
    if (i % 2 == 0) {
      sum_even += v[i];
    } else {
      sum_odd += v[i];
    }
  }
  int carry = 0;
  for (int i = v.size() - 1; i >= 0; i--) {
    if (i % 2 == 0) {
      int sum = v[i] + carry;
      if (sum >= sum_odd) {
        carry = 0;
      } else {
        carry = (sum_odd - sum) / 10;
        v[i] = (sum_odd - sum) % 10;
      }
    } else {
      int sum = v[i] + carry;
      if (sum >= sum_even) {
        carry = 0;
      } else {
        carry = (sum_even - sum) / 10;
        v[i] = (sum_even - sum) % 10;
      }
    }
  }
  if (carry > 0) {
    v.insert(v.begin(), carry);
  }
  for (int i = v.size() - 1; i >= 0; i--) {
    fout << v[i];
  }
  fout << "\n";
  return 0;
}
