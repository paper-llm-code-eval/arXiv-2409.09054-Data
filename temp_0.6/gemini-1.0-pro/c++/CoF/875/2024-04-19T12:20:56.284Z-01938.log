            return 0;
        }
        for (int dir = 0; dir < 4; dir++) {
            int nx = x + dx[dir];
            int ny = y + dy[dir];
            if (nx < 1 || nx > n || ny < 1 || ny > m || g[nx][ny] == '#')
                continue;
            char c = g[nx][ny];
            int nmask;
            if ('A' <= c && c <= 'Z') {
                nmask = mask | (1 << (c - 'A'));
                if (dist[nx][ny][nmask] == -1 || dist[nx][ny][nmask] > dist[x][y][mask] + 1) {
                    dist[nx][ny][nmask] = dist[x][y][mask] + 1;
                    q.push({nmask, {nx, ny}});
