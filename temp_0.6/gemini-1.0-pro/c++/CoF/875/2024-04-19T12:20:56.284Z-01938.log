                path += c;
                if (check(path)) {
                    nmask = mask;
                    if (dist[nx][ny][nmask] == -1 || dist[nx][ny][nmask] > dist[x][y][mask]) {
                        dist[nx][ny][nmask] = dist[x][y][mask];
                        q.push({nmask, {nx, ny}});
                    }
                }
            }
        }
    }
    fout << "-1\n";
    return 0;
}
