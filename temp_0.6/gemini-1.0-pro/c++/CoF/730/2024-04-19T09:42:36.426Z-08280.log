#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("joc.in");
ofstream fout("joc.out");

const int MAXN = 100;

int n, aruncare_zar[MAXN], poz[MAXN], puncte[MAXN], castigator;
vector<int> valori;

int main() {
    fin >> n;
    valori.resize(n + 1);
    for (int i = 1; i <= n; i++) {
        fin >> valori[i];
    }
    int m;
    fin >> m;
    for (int i = 1; i <= m; i++) {
        fin >> aruncare_zar[i];
    }

    // Simularea jocului
    for (int i = 1; i <= 2; i++) {
        poz[i] = 1;
        puncte[i] = 0;
    }
    int jucator = 1;
    for (int i = 1; i <= m; i++) {
        poz[jucator] = (poz[jucator] + aruncare_zar[i]) % n;
        if (poz[jucator] == 0) {
            poz[jucator] = 1;
            puncte[jucator] = 0;
        } else if (valori[poz[jucator]] == 10) {
            puncte[jucator] += 10;
        } else if (valori[poz[jucator]] == 1) {
            puncte[jucator] += 1;
        } else if (poz[jucator] == poz[3 - jucator]) {
            puncte[3 - jucator] = 0;
            poz[3 - jucator] = 1;
        }
        if (poz[jucator] == 1 && puncte[jucator] != 0) {
            castigator = jucator;
            break;
        }
        jucator = 3 - jucator;
    }

    // Scrierea rezultatelor
    if (castigator == 0) {
        castigator = puncte[1] > puncte[2] ? 1 : 2;
    }
    fout << castigator << '\n';
    for (int i = 1; i <= 2; i++) {
        fout << poz[i] << ' ' << puncte[i] << '\n';
    }

    return 0;
}
