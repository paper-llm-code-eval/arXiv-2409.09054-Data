#include <fstream>
#include <vector>
#include <cmath>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("arma.in");
    ofstream out("arma.out");

    int c, n;
    in >> c >> n;

    vector<long long> dist(n);
    for (long long &d : dist) in >> d;

    sort(dist.begin(), dist.end());

    if (c == 1) {
        long long ans = 0;
        for (const long long &d : dist) {
            long long x = sqrt(d);
            if (x * x == d) ans += x;
            else ans += x + 1;
        }
        out << ans << endl;
    } else {
        for (const long long &d : dist) {
            long long p = 2;
            while (pow(p, p) <= d) p++;
            out << p - 1 << endl;
        }
    }

    in.close();
    out.close();

    return 0;
}
```
