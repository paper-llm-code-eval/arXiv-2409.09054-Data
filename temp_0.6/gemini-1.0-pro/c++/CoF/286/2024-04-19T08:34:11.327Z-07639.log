#include <bits/stdc++.h>
#define ll long long
using namespace std;
const int MOD = 1e9 + 7;

int main() {
    ifstream fin("transport.in");
    ofstream fout("transport.out");
    int T, N, C;
    fin >> T >> N >> C;
    vector<ll> X(N + 1), D(N + 1);
    for (int i = 1; i <= N; i++) {
        fin >> X[i] >> D[i];
    }
    if (T == 1) {
        ll ans = 0;
        for (int i = 1; i <= N; i++) {
            for (int j = i + 1; j <= N; j++) {
                if (C * (X[j] - X[i]) >= D[i] + D[j]) {
                    ans++;
                }
            }
        }
        fout << ans % MOD << '\n';
    } else {
        vector<vector<ll>> dp(N + 1, vector<ll>(N + 1, -1));
        function<ll(int, int)> solve = [&](int l, int r) {
            if (l > r) {
                return 0;
            }
            if (dp[l][r] != -1) {
                return dp[l][r];
            }
            ll ans = 0;
            for (int i = l; i <= r; i++) {
                if (l == 1 || C * (X[i] - X[l - 1]) >= D[l - 1] + D[i]) {
                    ans = (ans + solve(l, i - 1) * solve(i + 1, r)) % MOD;
                }
                if (r == N || C * (X[r] - X[i]) >= D[i] + D[r + 1]) {
                    ans = (ans + solve(l, i - 1) * solve(i + 1, r)) % MOD;
                }
            }
            return dp[l][r] = ans;
        };
        fout << solve(1, N) << '\n';
    }
    return 0;
}
