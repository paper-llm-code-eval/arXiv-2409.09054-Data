    ull sum = 0;
    for (int i = 1; i < N; i++) {
      sum += (ull)(stations[i].first - stations[i - 1]) * (ull)stations[i].second;
    }
    out << (sum * C) % MOD << endl;
  } else {
    // Expres
    vector<ull> dp(N, 0);
    for (int i = 1; i < N; i++) {
      dp[i] = (ull)(stations[i].first - stations[i - 1]) * (ull)stations[i].second;
      for (int j = i - 1; j >= 0; j--) {
        dp[i] = max(dp[i], dp[j] + (ull)(stations[i].first - stations[j]) * (ull)stations[i].second);
      }
    }
    out << (dp[N - 1] * C) % MOD << endl;
  }

  return 0;
}
