#include <fstream>
#include <iostream>
#include <vector>

using namespace std;

typedef unsigned long long ull;
const int MOD = 1e9 + 7;

int main() {
  ifstream in("transport.in");
  ofstream out("transport.out");

  int T;
  in >> T;
  int N, C;
