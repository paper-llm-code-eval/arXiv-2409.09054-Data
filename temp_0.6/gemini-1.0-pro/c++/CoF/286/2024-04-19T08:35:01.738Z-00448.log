#include <fstream>
#include <iostream>
#include <vector>

using namespace std;

typedef unsigned long long ull;
const int MOD = 1e9 + 7;

int main() {
  ifstream in("transport.in");
  ofstream out("transport.out");

  int T;
  in >> T;
  int N, C;
  in >> N >> C;

  vector<pair<int, int>> stations(N);
  for (int i = 0; i < N; i++) {
    in >> stations[i].first >> stations[i].second;
  }

  if (T == 1) {
    // Regio
    ull sum = 0;
    for (int i = 1; i < N; i++) {
      sum += (ull)(stations[i].first - stations[i - 1]) * (ull)stations[i].second;
    }
    out << (sum * C) % MOD << endl;
  } else {
    // Expres
    vector<ull> dp(N, 0);
    for (int i = 1; i < N; i++) {
      dp[i] = (ull)(stations[i].first - stations[i - 1]) * (ull)stations[i].second;
      for (int j = i - 1; j >= 0; j--) {
        dp[i] = max(dp[i], dp[j] + (ull)(stations[i].first - stations[j]) * (ull)stations[i].second);
      }
    }
    out << (dp[N - 1] * C) % MOD << endl;
  }

  return 0;
}
```
