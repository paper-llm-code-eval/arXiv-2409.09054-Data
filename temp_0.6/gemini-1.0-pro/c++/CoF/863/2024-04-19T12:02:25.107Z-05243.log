#include <bits/stdc++.h>
using namespace std;

const int MAXN = 505;
const int MAXS = 8;

int n, m, T, L, C, k, S, p;
int a[MAXN][MAXN], b[MAXN][MAXN];
int dx[] = {0, 1, 0, -1}, dy[] = {1, 0, -1, 0};
vector<pair<int, int>> food;

int get_val(int x, int y) {
  if (1 <= x && x <= n && 1 <= y && y <= m) return a[x][y];
  return -1;
}

int get_bit(int x, int pos) { return (x >> pos) & 1; }

bool accesibil(int x) {
  int cnt = 0;
  for (int i = 0; i < S; i++)
    if (get_bit(x, i) != get_bit(k, i)) cnt++;
  return cnt <= 1;
}

void pre() {
  for (int i = 0; i < n; i++)
    for (int j = 0; j < m; j++) {
      int x = a[i][j];
      for (int l = 0; l < S; l++) b[i][j] = b[i][j] * 2 + get_bit(x, l);
    }
}

int main() {
  ifstream fin("panda.in");
  ofstream fout("panda.out");

  fin >> p;
  fin >> n >> m >> T;
  fin >> L >> C >> k >> S;

  for (int i = 0; i < T; i++) {
    int x, y;
    fin >> x >> y;
    food.push_back({x, y});
  }
  for (int i = 0; i < n; i++)
    for (int j = 0; j < m; j++) fin >> a[i][j];

  pre();

  if (p == 1) {
    int ans = 0;
    for (int i = 0; i < n; i++)
      for (int j = 0; j < m; j++)
        if (accesibil(b[i][j]) && !(i == L - 1 && j == C - 1)) ans++;
    fout << ans << '\n';
  } else {
    int dist[MAXN][MAXN];
    memset(dist, -1, sizeof(dist));
    dist[L - 1][C - 1] = 0;
    queue<pair<int, int>> q;
    q.push({L - 1, C - 1});
    int ans1 = 1e9, ans2 = 0;
    while (!q.empty()) {
      int x = q.front().first, y = q.front().second;
      q.pop();
      for (int i = 0; i < 4; i++) {
        int nx = x + dx[i], ny = y + dy[i];
        if (1 <= nx && nx <= n && 1 <= ny && ny <= m) {
          if (accesibil(b[nx][ny]) && dist[nx][ny] == -1) {
            dist[nx][ny] = dist[x][y] + 1;
            q.push({nx, ny});
            for (auto& food : food) {
              if (nx == food.first - 1 && ny == food.second - 1) {
                if (dist[nx][ny] < ans1) {
                  ans1 = dist[nx][ny];
                  ans2 = 1;
                } else if (dist[nx][ny] == ans1)
                  ans2++;
              }
            }
          }
        }
      }
    }
    fout << ans1 << ' ' << ans2 << '\n';
  }

  return 0;
}
