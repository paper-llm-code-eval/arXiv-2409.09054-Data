#include <fstream>
#include <vector>
#include <queue>
#include <bitset>
using namespace std;

const int MAXN = 505;
const int MAXM = 505;

int n, m, p, t, l, c, k, s;
int dist[MAXN][MAXM];
int coduri[MAXN][MAXM];
int dx[] = {0, 0, 1, -1};
int dy[] = {1, -1, 0, 0};
bool viz[MAXN][MAXM];
vector<pair<int, int>> mancare;
queue<pair<int, int>> q;

bitset<64> toBin(int x, int l) {
    bitset<64> b;
    for (int i = 0; i < l; i++) {
        b[i] = x % 2;
        x /= 2;
    }
    return b;
}

int main() {
    ifstream fin("panda.in");
    ofstream fout("panda.out");

    fin >> p;
    fin >> n >> m >> t;
    fin >> l >> c >> k >> s;
    for (int i = 0; i < t; i++) {
        int x, y;
        fin >> x >> y;
        mancare.push_back({x, y});
    }
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            fin >> coduri[i][j];
        }
    }

    if (p == 1) {
        bitset<64> cod_k = toBin(k, s);
        int cnt = 0;
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < m; j++) {
                if (i != l - 1 || j != c - 1) {
                    bitset<64> cod = toBin(coduri[i][j], s);
                    if (cod.to_ullong() == cod_k.to_ullong()) {
                        cnt++;
                    }
                }
            }
        }
        fout << cnt << "\n";
    } else {
        bitset<64> cod_k = toBin(k, s);
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < m; j++) {
                dist[i][j] = -1;
            }
        }
        dist[l - 1][c - 1] = 0;
        q.push({l - 1, c - 1});
        while (!q.empty()) {
            int x = q.front().first;
            int y = q.front().second;
            q.pop();
            if (viz[x][y]) {
                continue;
            }
            viz[x][y] = true;
            for (int dir = 0; dir < 4; dir++) {
                int nx = x + dx[dir];
                int ny = y + dy[dir];
                if (nx >= 0 && nx < n && ny >= 0 && ny < m && !viz[nx][ny]) {
                    bitset<64> cod = toBin(coduri[nx][ny], s);
                    if (cod.to_ullong() == cod_k.to_ullong()) {
                        dist[nx][ny] = dist[x][y] + 1;
                        q.push({nx, ny});
                    }
                }
            }
        }
        int min_dist = INT_MAX;
        int nr_mancare = 0;
        for (auto& poz : mancare) {
            int x = poz.first - 1;
            int y = poz.second - 1;
            if (dist[x][y] != -1) {
                min_dist = min(min_dist, dist[x][y]);
                nr_mancare++;
            }
        }
        fout << min_dist << " " << nr_mancare << "\n";
    }

    return 0;
}
