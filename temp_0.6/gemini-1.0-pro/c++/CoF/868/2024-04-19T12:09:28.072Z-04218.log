    }
  }
  if (P == 1) {
    out << ans1 << '\n';
    return 0;
  }
  int ans2 = 0;
  for (int i = 0; i < N; i++) {
    vector<int> c = pairs[i].first, d = pairs[i].second;
    for (int j = 0; j < 10; j++) {
      if (c[j] > 0 && d[j] > 0) {
        if (c[j] == 1 || d[j] == 1) {
          c[j]--;
          break;
        }
        c[j]--;
        d[j]--;
        break;
      }
    }
    int number = 0;
    for (int j = 9; j >= 0; j--) {
      while (c[j] > 0 || d[j] > 0) {
        number = number * 10 + j;
        if (c[j] > 0) c[j]--;
        else d[j]--;
      }
    }
    ans2 = max(ans2, number);
  }
  out << ans2 << '\n';
  return 0;
}
```

This code correctly handles the case where both students in a pair have the same digit with a count of 1. It decrements the count of this digit for one of the students, and then proceeds to build the number as before. This ensures that both students can still form their original numbers, and that Andrei can form a non-zero number from the digits he takes.
