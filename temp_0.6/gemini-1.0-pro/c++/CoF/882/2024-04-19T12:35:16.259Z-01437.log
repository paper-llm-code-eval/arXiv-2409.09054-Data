#include <fstream>
#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAX_N = 100;

int n, c;
int d[MAX_N + 1];
int q[MAX_N + 1];
int dist[MAX_N + 1];
int prev[MAX_N + 1];

int main() {
  ifstream in("fermier.in");
  ofstream out("fermier.out");

  in >> n >> c;
  for (int i = 0; i <= n; i++) {
    in >> d[i];
  }
  for (int i = 1; i <= n; i++) {
    in >> q[i];
  }

  for (int i = 1; i <= n; i++) {
    dist[i] = d[i] + d[i - 1];
  }

  for (int i = 2; i <= n; i++) {
    dist[i] = min(dist[i], d[i] + d[n] + d[0]);
  }

  int curr = 0;
  int ans = 0;
  while (curr < n) {
    int next = -1;
    int min_dist = 1e9;
    for (int i = curr + 1; i <= n; i++) {
      if (q[i] <= c) {
        if (dist[i] < min_dist) {
          min_dist = dist[i];
          next = i;
        }
      }
    }
    if (next == -1) {
      break;
    }
    ans += min_dist;
    curr = next;
    c -= q[curr];
  }

  out << ans << endl;

  return 0;
}
