#include <fstream>
#include <vector>
#include <algorithm>

int main() {
    std::ifstream in("fermier.in");
    std::ofstream out("fermier.out");
    int n, c;
    in >> n >> c;
    std::vector<int> d(n + 1), q(n + 1);
    for (int i = 0; i <= n; i++) in >> d[i];
    for (int i = 1; i <= n; i++) in >> q[i];
    int dist = 0, last = 0, curr = 0;
    for (int i = 1; i <= n; i++) {
        int dist1 = d[last] + d[curr];
        int dist2 = d[last] + d[n] + d[curr];
        if (dist1 < dist2) {
            dist += dist1;
            last = curr;
        } else {
            dist += dist2;
            last = n;
        }
        curr = (curr + 1) % (n + 1);
        q[curr] -= std::min(q[curr], c);
        while (!q[curr]) {
            curr = (curr + 1) % (n + 1);
            q[curr] -= std::min(q[curr], c);
        }
    }
    dist += d[last];
    out << dist << '\n';
    return 0;
}
