int dist[MAXN], par[MAXN], viz[MAXN];
vector<pair<int, int>> G[MAXN];

int dijkstra(int st, int dr) {
  for (int i = 1; i <= N; ++i) dist[i] = INF, viz[i] = 0, par[i] = 0;
  dist[st] = 0;
  while (1) {
