#include <fstream>
#include <vector>
using namespace std;

const int MAXN = 10005;
const int MAXM = 20005;
const int INF = 1 << 30;

int N, T, C;
int dist[MAXN], par[MAXN], viz[MAXN];
vector<pair<int, int>> G[MAXN];

int dijkstra(int st, int dr) {
  for (int i = 1; i <= N; ++i) dist[i] = INF, viz[i] = 0, par[i] = 0;
  dist[st] = 0;
  while (1) {
    int nod = -1, minim = INF;
    for (int i = 1; i <= N; ++i)
      if (!viz[i] && dist[i] < minim) nod = i, minim = dist[i];
    if (nod == -1 || nod == dr) return dist[dr];
    viz[nod] = 1;
    for (auto& edge : G[nod]) {
      int vecin = edge.first, cost = edge.second;
      if (dist[nod] + cost < dist[vecin]) {
        dist[vecin] = dist[nod] + cost;
        par[vecin] = nod;
      }
    }
  }
}

int main() {
  ifstream fin("furnica.in");
  ofstream fout("furnica.out");

  fin >> C;
  if (C == 1 || C == 2) {
    fin >> N;
    for (int i = 1; i <= N; ++i) {
      int a, b;
      fin >> a >> b;
      G[i].push_back({i + 1, 3 * a});
      G[i + 1].push_back({i, 3 * a});
      G[i].push_back({i + b, 5 * b});
      G[i + b].push_back({i, 2 * b});
    }
    if (C == 1) fout << dijkstra(1, N + 1);
    else {
      int ans = 0;
      for (int i = 1; i <= N; ++i) {
        int j = i;
        while (j <= N && par[j] != i + 1) j = par[j];
        ans = max(ans, j - i);
        i = j;
      }
      fout << ans;
    }
  } else {
    fin >> N >> T;
    for (int i = 1; i <= N; ++i) {
      int a, b;
      fin >> a >> b;
      G[i].push_back({i + 1, 3 * a});
      G[i + 1].push_back({i, 3 * a});
      G[i].push_back({i + b, 5 * b});
      G[i + b].push_back({i, 2 * b});
    }
    int st = 1, dr = N + 1, timp = 0;
    while (st != dr) {
      int d = dijkstra(st, dr);
      if (timp + d > T) {
        int dif = T - timp;
        int nod = st;
        while (dif > 0) {
          int vecin = par[nod];
          if (G[nod][0].first == vecin) dif -= G[nod][0].second;
          else dif -= G[nod][1].second;
          nod = vecin;
        }
        fout << nod;
        return 0;
      }
      timp += d;
      st = par[dr];
    }
  }
  return 0;
}
