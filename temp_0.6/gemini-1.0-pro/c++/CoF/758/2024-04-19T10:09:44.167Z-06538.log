#include <fstream>

using namespace std;

#define MAX_LENGTH 60000
#define MAX_LINE_LENGTH 255

int main() {
    ifstream in("paritate.in");
    ofstream out("paritate.out");

    char text[MAX_LENGTH + 1];
    in.getline(text, MAX_LENGTH + 1);

    int length = in.gcount() - 1;
    if (length % 8 != 0) {
        out << "NU";
        return 0;
    }

    int errors = 0;
    int error_positions[MAX_LENGTH];

    for (int i = 0; i < length; i += 8) {
        int parity = 0;
        for (int j = 0; j < 7; j++) {
            parity += text[i + j] - '0';
        }
        parity %= 2;
        if (parity != text[i + 7] - '0') {
            error_positions[errors++] = i / 8;
        }
    }

    if (errors == 0) {
        out << "DA\n";
        for (int i = 0; i < length; i += 8) {
            for (int j = 0; j < 7; j++) {
                out << (char)(text[i + j] - '0' + 32);
            }
            if (text[i + 7] == '0') {
                out << '\n';
            }
        }
    } else {
        out << "NU\n";
        for (int i = 0; i < errors; i++) {
            out << error_positions[i] << ' ';
        }
    }

    return 0;
}
```
