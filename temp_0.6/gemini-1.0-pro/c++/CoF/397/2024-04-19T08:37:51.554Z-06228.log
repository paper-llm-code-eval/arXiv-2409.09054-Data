#include <fstream>
#include <iostream>
#include <sstream>
#include <vector>

using namespace std;

int main() {
  ifstream fin("ecuatii.in");
  ofstream fout("ecuatii.out");

  int n;
  fin >> n;

  for (int i = 0; i < n; i++) {
    string ecuatie;
    fin >> ecuatie;

    vector<int> coefx;
    vector<int> coefc;
    int c = 0;
    for (char ch : ecuatie) {
      if (ch == 'x') {
        if (c != 0) coefx.push_back(c);
        c = 1;
      } else if (ch == '+' || ch == '-') {
        if (c != 0) coefc.push_back(c);
        c = (ch == '+' ? 1 : -1);
      } else {
        c = c * 10 + ch - '0';
      }
    }
    coefc.push_back(c);

    int sx = 0, sc = 0;
    for (int i = 0; i < coefx.size(); i++) {
      sx += coefx[i];
    }
    for (int i = 0; i < coefc.size(); i++) {
      sc += coefc[i];
    }

    if (sx == 0) {
      if (sc == 0) {
        fout << "infinit" << endl;
      } else {
        fout << "imposibil" << endl;
      }
    } else {
      double x = double(sc) / double(sx);
      fout << fixed << setprecision(4) << x << endl;
    }
  }

  fin.close();
  fout.close();

  return 0;
}
