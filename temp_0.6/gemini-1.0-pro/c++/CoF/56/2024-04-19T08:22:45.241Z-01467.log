#include <bits/stdc++.h>
using namespace std;
const int NMAX = 205;
const int LGMAX = 10;
const int INF = 1e9;

int n, p, k;
int dp[NMAX][NMAX][LGMAX];
bool viz[NMAX][NMAX][LGMAX];
int d[NMAX], c[NMAX];
bool adj[NMAX][NMAX];
int scc[NMAX];
vector<int> G[NMAX];

int main() {
    ifstream fin("zmeu.in");
    ofstream fout("zmeu.out");

    fin >> n >> p >> k;
    for (int i = 1; i <= p; ++i) {
        fin >> d[i] >> c[i];
    }
    for (int i = 1; i <= k; ++i) {
        int u, v;
        fin >> u >> v;
        adj[u][v] = true;
    }
    fin.close();

    int nr_scc = 0;
    for (int i = 1; i <= p; ++i) {
        if (scc[i] == 0) {
            ++nr_scc;
            stack<int> stk;
            stk.push(i);
            while (!stk.empty()) {
                int node = stk.top();
                stk.pop();
                if (scc[node] != 0) continue;
                scc[node] = nr_scc;
                for (int j = 1; j <= p; ++j) {
                    if (adj[node][j] == false) continue;
                    stk.push(j);
                }
            }
        }
    }

    for (int i = 1; i <= p; ++i) {
        for (int j = 1; j <= p; ++j) {
            if (scc[i] == scc[j]) {
                G[i].push_back(j);
            }
        }
    }

    for (int i = 0; i <= p; ++i) {
        for (int j = 0; j <= p; ++j) {
            for (int k = 0; k <= LGMAX; ++k) {
                dp[i][j][k] = INF;
            }
        }
    }

    dp[1][1][0] = d[1];
    for (int i = 1; i <= p; ++i) {
        for (int j = 1; j <= p; ++j) {
            for (int k = 0; k <= LGMAX; ++k) {
                if (dp[i][j][k] == INF) continue;
                for (int u : G[j]) {
                    if (k + 1 <= LGMAX && n - c[u] >= 0) {
                        dp[i][u][k + 1] = min(dp[i][u][k + 1], dp[i][j][k] + d[u]);
                    }
                }
            }
        }
    }

    int ans = INF;
    for (int i = 0; i <= LGMAX; ++i) {
        ans = min(ans, dp[1][p][i]);
    }
    if (ans == INF) ans = -1;
    fout << ans << '\n';
    fout.close();
    return 0;
}
