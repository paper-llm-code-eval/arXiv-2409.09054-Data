vector<int> v, sol;

int main() {
    ifstream in("blis.in");
    ofstream out("blis.out");
    in >> k >> v;
    n = v.size();
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < k; j++) {
            dp[i][j] = -1;
            if (i == 0)
                pref[i][j] = (1 << v[i]);
            else
                pref[i][j] = pref[i - 1][j] + (1 << v[i]);
        }
    }
    for (int i = n - 1; i >= 0; i--) {
        for (int j = 0; j < k; j++) {
            if (j == 0)
                dp[i][j] = (1 << v[i]);
            else
                dp[i][j] = max(dp[i][j], dp[i + 1][j - 1] + (1 << v[i]));
            ans = max(ans, dp[i][j]);
        }
    }
    out << ans << '\n';
    int cnt = 0, last = -1;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < k; j++) {
