#include <algorithm>
#include <cmath>
#include <fstream>
#include <vector>

using namespace std;

typedef long long ll;

const ll MAXN = 100005;

struct Point {
    ll x, y;
};

struct Segment {
    Point a, b;
};

int n;
Segment segments[MAXN];
vector<double> angles;

bool cmp(double a, double b) {
    return a < b;
}

int main() {
    ifstream fin("arhitect.in");
    ofstream fout("arhitect.out");

    fin >> n;
    for (int i = 0; i < n; i++) {
        fin >> segments[i].a.x >> segments[i].a.y >> segments[i].b.x >> segments[i].b.y;
    }
    fin.close();

    for (int i = 0; i < n; i++) {
        if (segments[i].a.x != segments[i].b.x) {
            double angle = atan2(segments[i].b.y - segments[i].a.y, segments[i].b.x - segments[i].a.x);
            angles.push_back(angle);
        }
        if (segments[i].a.y != segments[i].b.y) {
            double angle = atan2(segments[i].b.x - segments[i].a.x, segments[i].b.y - segments[i].a.y);
            angles.push_back(angle);
        }
    }

    sort(angles.begin(), angles.end(), cmp);

    int maxSegments = 0;
    int currentSegments = 0;
    for (int i = 0; i < angles.size(); i++) {
        if (i == 0 || angles[i] != angles[i - 1]) {
            maxSegments = max(maxSegments, currentSegments);
            currentSegments = 0;
        }
        currentSegments++;
    }
    maxSegments = max(maxSegments, currentSegments);

    fout << maxSegments << '\n';
    fout.close();

    return 0;
}
