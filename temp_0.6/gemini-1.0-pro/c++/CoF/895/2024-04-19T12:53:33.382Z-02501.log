using namespace std;

int main() {
    ifstream in("turnuri.in");
    ofstream out("turnuri.out");
    int c, n, t = 0, h = 0, nMax = 0;
    vector<pair<int, char>> v;
    in >> c >> n;
    for (int i = 0; i < n; i++) {
        int x;
        char y;
        in >> x >> y;
        v.push_back({x, y});
    }
    if (c == 1) {
        for (int i = 1; i < n; i++) {
