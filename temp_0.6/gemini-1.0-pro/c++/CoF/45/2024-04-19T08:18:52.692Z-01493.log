#include <fstream>
#include <vector>

using namespace std;
const int MOD = 30011;

int n, k, dp[101][101][2];
vector<int> adj[101];

void dfs(int u, int p) {
  dp[u][0][1] = 1;
  for (auto v : adj[u]) {
    if (v == p) continue;
    dfs(v, u);
    for (int i = 0; i <= k; i++) {
      for (int j = 0; j < 2; j++) {
        for (int t = i + 1; t <= k; t++) {
          dp[u][i][j] = (dp[u][i][j] + dp[v][t][j]) % MOD;
        }
        if (j == 1) {
          for (int t = i + 1; t <= k; t++) {
            dp[u][i][j] = (dp[u][i][j] + dp[v][t][0]) % MOD;
          }
        }
      }
    }
  }
}

int main() {
  ifstream fin("iepuri.in");
  ofstream fout("iepuri.out");

  fin >> n >> k;
  for (int i = 1; i < n; i++) {
    int a, b;
    fin >> a >> b;
    adj[a].push_back(b);
  }

  dfs(1, 0);
  fout << dp[1][0][1] << "\n";
  return 0;
}
```
