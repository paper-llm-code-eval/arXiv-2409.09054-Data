#include <fstream>
#include <algorithm>

using namespace std;

ifstream fin("inimioare.in");
ofstream fout("inimioare.out");

int main() {
    int n;
    fin >> n;

    int a[10000][4], b[10000][4];
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < 4; j++) {
            fin >> a[i][j];
        }
    }
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < 4; j++) {
            fin >> b[i][j];
        }
    }

    int m = 0;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < 4; j++) {
            for (int k = 0; k < n; k++) {
                for (int l = 0; l < 4; l++) {
                    int x[4], y[4];
                    if (j == l) {
                        x[0] = a[i][j];
                        x[1] = a[i][3 - j];
                        x[2] = b[k][l];
                        x[3] = b[k][3 - l];
                    } else {
                        x[0] = a[i][j];
                        x[1] = a[i][3 - j];
                        y[2] = b[k][l];
                        y[3] = b[k][3 - l];
                        sort(y, y + 4);
                        x[2] = y[2];
                        x[3] = y[3];
                    }
                    sort(x, x + 4);
                    int aux = x[3] * 1000 + x[2] * 100 + x[1] * 10 + x[0];
                    m = max(m, aux);
                }
            }
        }
    }

    fout << m << '\n';
    return 0;
}
