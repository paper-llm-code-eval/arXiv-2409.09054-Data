  for (int i = 1; i <= n; i++) { in >> a[i]; }
  do {
    ok = 0;
    r++;
    for (int i = 1; i <= n; i++) {
      x = a[i];
      y = a[i + 1];
      if (x <= y and y != 0) {
        a[i + 1] = y - x;
        t++;
        ok = 1;
      }
    }
  } while (ok);
  out << r << " " << t << endl;
  return 0;
}
