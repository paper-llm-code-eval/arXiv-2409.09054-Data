#include <fstream>
#include <vector>
using namespace std;

int main() {
  ifstream fin("cochilie.in");
  ofstream fout("cochilie.out");

  int c;
  fin >> c;

  if (c == 1) {
    int n;
    fin >> n;
    int nrlin = 2 * n - 1;
    int nrcol = 4 * n - 3;
    fout << nrlin << ' ' << nrcol << '\n';
  } else {
    int n, p;
    fin >> n >> p;
    vector<vector<int>> v(2 * n - 1, vector<int>(4 * n - 3, 0));
    int x = 0, y = 0, val = 1, dir = 0, st = 1, dr = 1, sus = 1, jos = 1;
    while (val <= n) {
      v[x][y] = val;
      if (dir == 0 && y < dr)
        y++;
      else if (dir == 1 && x < jos)
        x++;
      else if (dir == 2 && y > st)
        y--;
      else if (dir == 3 && x > sus)
        x--;
      if (dir == 0 && y == dr) {
        dir = 1;
        st++;
        sus++;
      }
      if (dir == 1 && x == jos) {
        dir = 2;
        dr--;
        jos++;
      }
      if (dir == 2 && y == st) {
        dir = 3;
        sus++;
        dr--;
      }
      if (dir == 3 && x == sus) {
        dir = 0;
        st++;
        jos--;
      }
      val++;
    }
    for (int i = 0; i < 4 * n - 3; i++)
      fout << v[p - 1][i] << ' ';
    fout << '\n';
  }
  return 0;
}
