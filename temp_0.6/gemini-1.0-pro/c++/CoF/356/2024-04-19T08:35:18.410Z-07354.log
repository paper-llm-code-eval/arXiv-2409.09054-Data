#include <fstream>

using namespace std;

const int MAX = 30;
int n, p;
int nrlin, nrcol;
int a[MAX][MAX];

int main() {
    ifstream in("cochilie.in");
    ofstream out("cochilie.out");
    in >> n;
    nrlin = nrcol = 2 * n - 1;
    int x = n, y = n, val = 1;
    while (val <= n) {
        for (int i = 1; i <= x; i++) { a[y][x + i] = val; }  // dreapta
        for (int i = 1; i <= x; i++) { a[y + i][x] = val; }  // jos
        for (int i = 1; i <= x; i++) { a[y][x - i] = val; }  // stanga
        for (int i = 1; i <= x; i++) { a[y - i][x] = val; }  // sus
        x++;
        y--;
        val++;
    }
    if (p) {
        for (int i = 1; i <= nrcol; i++) { out << a[p][i] << " "; }
    } else {
        out << nrlin << " " << nrcol << "\n";
    }
    return 0;
}
```
