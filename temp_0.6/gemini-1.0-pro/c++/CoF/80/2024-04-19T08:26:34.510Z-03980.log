#include <bits/stdc++.h>

using namespace std;

#define ll long long
#define pb push_back
#define mp make_pair
#define ins insert

int N, T, P;
int A[1001][1001], B[1001][1001], C[1001][1001];
vector<pair<int, int>> ans;

bool solve() {
    for (int i = 1; i <= N; i++) {
        for (int j = 1; j <= N; j++) {
            if (A[i][j] != B[i][j]) {
                if (C[i][j] != C[i - 1][j]) {
                    ans.pb({1, i, i - 1});
                    swap(C[i][j], C[i - 1][j]);
                    swap(A[i][j], B[i][j]);
                } else if (C[i][j] != C[i][j - 1]) {
                    ans.pb({2, i, j - 1});
                    swap(C[i][j], C[i][j - 1]);
                    swap(A[i][j], B[i][j]);
                } else
                    return false;
            }
        }
    }
    return true;
}

int main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    cin >> P >> T;
    while (T--) {
        cin >> N;
        string s;
        for (int i = 1; i <= N; i++) {
            cin >> s;
            for (int j = 0; j < N; j++) {
                A[i][j + 1] = s[j] - '0';
                B[i][j + 1] = i % 2 == j % 2;
                C[i][j + 1] = (i + j) % 2;
            }
        }
        if (P == 1) {
            cout << (solve() ? 1 : 0);
        } else if (P == 2) {
            solve();
            cout << ans.size();
        } else if (P == 3) {
            solve();
            cout << ans.size() << '\n';
            for (auto it : ans) {
                if (it.first == 1)
                    cout << "L " << it.second << ' ' << it.third << '\n';
                else
                    cout << "C " << it.second << ' ' << it.third << '\n';
            }
        }
        ans.clear();
        cout << '\n';
    }
    return 0;
}
