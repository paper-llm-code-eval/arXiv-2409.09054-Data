#include <fstream>
#include <iostream>
#include <string>
#include <vector>

using namespace std;

int main() {
  ifstream in("text.in");
  ofstream out("text.out");
  int max_line_length;
  in >> max_line_length;
  string line;
  getline(in, line);  // skip the first line
  while (getline(in, line)) {
    if (line.empty()) {
      out << line << endl;
      continue;
    }
    vector<string> words;
    int start = 0, end = 0;
    while (end < line.length()) {
      while (end < line.length() && line[end] != ' ') end++;
      words.push_back(line.substr(start, end - start));
      start = end + 1;
      end++;
    }
    int num_spaces = max_line_length - words[words.size() - 1].length();
    int num_spaces_per_word = num_spaces / (words.size() - 1);
    int extra_spaces = num_spaces % (words.size() - 1);
    for (int i = 0; i < words.size() - 1; i++) {
      out << words[i];
      for (int j = 0; j < num_spaces_per_word; j++) out << ' ';
      if (extra_spaces > 0) {
        out << ' ';
        extra_spaces--;
      }
    }
    out << words[words.size() - 1] << endl;
  }
  return 0;
}
