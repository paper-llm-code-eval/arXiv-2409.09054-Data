#include <fstream>
#include <iostream>
#include <string>
#include <vector>

using namespace std;

int main() {
  ifstream fin("text.in");
  ofstream fout("text.out");
  int max_chars;
  fin >> max_chars;
  string line;
  vector<string> words;
  while (getline(fin, line)) {
    words.clear();
    istringstream iss(line);
    string word;
    while (iss >> word) words.push_back(word);
    int spaces = max_chars - words.size();
    if (words.size() > 1) {
      int extra = spaces % (words.size() - 1);
      spaces -= extra;
      int spaces_between = spaces / (words.size() - 1);
      for (int i = 0; i < words.size(); i++) {
        fout << words[i];
        if (i < words.size() - 1) {
          for (int j = 0; j < spaces_between; j++) fout << ' ';
          if (i < words.size() - 2 || extra > 0) fout << ' ';
          extra--;
        }
      }
    } else
      fout << words[0];
    fout << endl;
  }
  fin.close();
  fout.close();
  return 0;
}
