u64 reverse_bits(u64 x) {
  u64 r = 0;
  for (int i = 0; i < 64; i++) {
    r <<= 1;
    r |= (x >> i) & 1;
  }
  return r;
}

u64 mirror(u64 x) {
  u64 h = x >> 32;
  u64 l = x & 0xFFFFFFFF;
  return (l << 32) | h;
}

u64 next_palindrome(u64 x) {
  u64 rev = reverse_bits(x);
  if (x == rev) {
    return x;
  }
  u64 inc = 1;
  while (!((x + inc) >> 32)) {
    inc <<= 1;
  }
  u64 new_x = x + inc;
  while (new_x < rev) {
    new_x += inc;
  }
  return new_x;
}

u64 prev_palindrome(u64 x) {
  u64 rev = reverse_bits(x);
  if (x == rev) {
    return x;
  }
  u64 dec = 1;
  while (!((x - dec) >> 32)) {
    dec <<= 1;
  }
  u64 new_x = x - dec;
  while (new_x > rev) {
    new_x -= dec;
  }
  return new_x;
}

bool is_palindrome(u64 x) {
  return x == reverse_bits(x);
}

u64 get_size(u64 x1, u64 x2) {
  u64 size = 0;
  while (x1 <= x2) {
    size++;
    x1 = next_palindrome(x1);
  }
  return size;
}

std::vector<u64> get_sequence(u64 x1, u64 x2) {
  std::vector<u64> seq;
  while (x1 <= x2) {
    seq.push_back(x1);
    x1 = next_palindrome(x1);
  }
  return seq;
}

