    if (heights[height] == 2) {
      identical_heights[height]++;
    }
  }

  int total_identical_heights = 0;
  for (auto it = identical_heights.begin(); it != identical_heights.end(); ++it) {
    total_identical_heights += it->second;
  }

  int min_cubes = 0;
  for (auto it = heights.begin(); it != heights.end(); ++it) {
    int current_height = it->first;
    int current_height_count = it->second;
    if (current_height_count % 2 == 1) {
      auto next_even_height = heights.upper_bound(current_height);
      if (next_even_height != heights.end()) {
        min_cubes += next_even_height->first - current_height;
      }
    }
  }

  out << max_height << ' ' << max_height_count << endl;
  out << total_identical_heights << endl;
  out << min_cubes << endl;

  return 0;
}
