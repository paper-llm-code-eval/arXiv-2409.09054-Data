#include <fstream>
#include <vector>
#include <algorithm>
#include <limits>

int main() {
    std::ifstream in("summax.in");
    std::ofstream out("summax.out");
    int v;
    in >> v;
    int n, st, dr;
    in >> n >> st >> dr;
    int max_sum = 0;
    std::vector<std::vector<int>> mat(n, std::vector<int>(n, 0));
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j <= i; ++j) {
            in >> mat[i][j];
            max_sum += mat[i][j];
        }
    }
    std::vector<std::vector<int>> dp(n, std::vector<int>(n, 0));
    dp[0][0] = mat[0][0];
    for (int i = 1; i < n; ++i) {
        for (int j = 0; j <= i; ++j) {
            if (j == 0) {
                dp[i][j] = dp[i - 1][j] + mat[i][j];
            } else {
                dp[i][j] = std::max(dp[i - 1][j], dp[i - 1][j - 1]) + mat[i][j];
            }
        }
    }
    std::vector<int> path(n, 0);
    int j = 0;
    for (int i = n - 1; i >= 0; --i) {
        if (dp[i][j] == dp[i - 1][j]) {
            path[i] = 0;
        } else {
            path[i] = 1;
            ++j;
        }
    }
    if (v == 1) {
        out << dp[n - 1][0] << "\n";
    } else {
        for (int i = st - 1; i < dr; ++i) {
            for (int j = 0; j < n; ++j) {
                out << path[j] + 1 << " ";
            }
            out << "\n";
        }
    }
    return 0;
}
