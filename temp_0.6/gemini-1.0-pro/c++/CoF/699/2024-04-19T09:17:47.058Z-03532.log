#include <fstream>
#include <vector>
#include <queue>

using namespace std;

const int MAX_N = 250;
const int INF = 1e9;

int main() {
    ifstream in("poarta.in");
    ofstream out("poarta.out");

    int n;
    in >> n;

    vector<pair<int, int>> start(n), end(n);
    for (int i = 0; i < n; i++) {
        in >> start[i].first >> start[i].second >> end[i].first >> end[i].second;
    }

    vector<vector<int>> dist(MAX_N + 1, vector<int>(MAX_N + 1, INF));
    for (int i = 1; i <= MAX_N; i++) {
        for (int j = 1; j <= MAX_N; j++) {
            queue<pair<int, int>> q;
            q.push({i, j});
            dist[i][j] = 0;

            while (!q.empty()) {
                int x = q.front().first, y = q.front().second;
                q.pop();

                if (x + 1 <= MAX_N && dist[x + 1][y] > dist[x][y] + 1) {
                    dist[x + 1][y] = dist[x][y] + 1;
                    q.push({x + 1, y});
                }
                if (x - 1 >= 1 && dist[x - 1][y] > dist[x][y] + 1) {
                    dist[x - 1][y] = dist[x][y] + 1;
                    q.push({x - 1, y});
                }
                if (y + 1 <= MAX_N && dist[x][y + 1] > dist[x][y] + 1) {
                    dist[x][y + 1] = dist[x][y] + 1;
                    q.push({x, y + 1});
                }
                if (y - 1 >= 1 && dist[x][y - 1] > dist[x][y] + 1) {
                    dist[x][y - 1] = dist[x][y] + 1;
                    q.push({x, y - 1});
                }
            }
        }
    }

    int ans = 0;
    for (int i = 0; i < n; i++) {
        ans += dist[end[i].first][end[i].second] - dist[start[i].first][start[i].second];
    }

    out << ans << endl;

    return 0;
}
