using namespace std;

int main() {
  ifstream in("poarta.in");
  ofstream out("poarta.out");

  int n;
  in >> n;
  vector<tuple<int, int, int, int>> p(n);
  for (auto &x : p) in >> get<0>(x) >> get<1>(x) >> get<2>(x) >> get<3>(x);
  vector<vector<int>> d(251, vector<int>(251, -1));
  queue<tuple<int, int, int>> q;
  for (const auto &x : p) {
    if (d[get<0>(x)][get<1>(x)] == -1) {
      q.push({get<0>(x), get<1>(x), 0});
      d[get<0>(x)][get<1>(x)] = 0;
    }
  }
  while (!q.empty()) {
    auto [x, y, dist] = q.front();
    q.pop();
    if (x == get<2>(p[0]) && y == get<3>(p[0])) {
      out << dist << endl;
      return 0;
    }
    for (const auto &dir :
         vector<pair<int, int>>{{1, 0}, {-1, 0}, {0, 1}, {0, -1}}) {
      int xx = x + dir.first, yy = y + dir.second;
      if (xx < 1 || xx > 250 || yy < 1 || yy > 250 ||
          d[xx][yy] != -1)
        continue;
      q.push({xx, yy, dist + 1});
      d[xx][yy] = dist + 1;
    }
  }
  return 0;
}
