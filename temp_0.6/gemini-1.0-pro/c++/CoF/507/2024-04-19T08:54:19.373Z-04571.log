#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

struct Note {
    int duration, height;
    bool operator<(const Note& other) const {
        return duration < other.duration;
    }
};

int main() {
    ifstream in("partitura.in");
    ofstream out("partitura.out");

    int n;
    in >> n;
    vector<Note> notes(n);
    for (int i = 0; i < n; i++) {
        in >> notes[i].duration >> notes[i].height;
    }

    sort(notes.begin(), notes.end());

    int score = 0;
    vector<int> dp(n);
    for (int i = 0; i < n; i++) {
        dp[i] = notes[i].height * notes[i].height;
        for (int j = 0; j < i; j++) {
            if (notes[i].duration + notes[j].duration <= 1) {
                dp[i] = max(dp[i], dp[j] + notes[i].height * notes[i].height);
            }
        }
        score = max(score, dp[i]);
    }

    out << score << endl;

    return 0;
}
```
