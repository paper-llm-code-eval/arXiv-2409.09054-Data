using namespace std;

struct coord {
    int x, y;
};

const int dx[] = {-1, 0, 1, 0};
const int dy[] = {0, 1, 0, -1};
const int max_n = 205;
int n, m, t;
bool used[max_n][max_n];
int dist[max_n][max_n];

int main() {
    ifstream fin("galerie.in");
