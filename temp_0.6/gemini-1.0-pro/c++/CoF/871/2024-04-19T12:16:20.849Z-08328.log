    vector<coord> cartite(t);
    for (int i = 0; i < t; i++) {
        fin >> cartite[i].x;
    }
    queue<coord> q;
    int max_dist = 0;
    for (int i = 0; i < t; i++) {
        memset(used, 0, sizeof(used));
        memset(dist, 0, sizeof(dist));
        q.push(cartite[i]);
        used[cartite[i].x][cartite[i].y] = true;
        while (!q.empty()) {
            coord crt = q.front();
            q.pop();
            max_dist = max(max_dist, dist[crt.x][crt.y]);
            for (int dir = 0; dir < 4; dir++) {
                int nx = crt.x + dx[dir];
                int ny = crt.y + dy[dir];
                if (nx >= 1 && nx <= n + 1 && ny >= 1 && ny <= m + 1 && !used[nx][ny]) {
                    used[nx][ny] = true;
                    dist[nx][ny] = dist[crt.x][crt.y] + 1;
                    if (nx == n + 1 || nx == 1 || ny == m + 1 || ny == 1) {
                        max_dist = max(max_dist, dist[nx][ny]);
                    } else if (used[nx][ny]) {
                        max_dist = max(max_dist, dist[nx][ny]);
                    } else {
                        q.push({nx, ny});
                    }
                }
            }
        }
    }
    if (cerinta == 1) {
        fout << max_dist << '\n';
    } else {
        int max_lungime = 0;
        for (int i = 1; i <= n + 1; i++) {
            for (int j = 1; j <= m + 1; j++) {
                if (used[i][j]) {
                    max_lungime = max(max_lungime, dist[i][j]);
                }
            }
        }
        fout << max_lungime << '\n';
    }
    return 0;
}
