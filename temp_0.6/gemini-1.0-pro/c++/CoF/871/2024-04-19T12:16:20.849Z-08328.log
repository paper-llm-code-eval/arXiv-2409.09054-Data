                int nx = crt.x + dx[dir];
                int ny = crt.y + dy[dir];
                if (nx >= 1 && nx <= n + 1 && ny >= 1 && ny <= m + 1 && !used[nx][ny]) {
                    used[nx][ny] = true;
                    dist[nx][ny] = dist[crt.x][crt.y] + 1;
                    q.push({nx, ny});
                }
            }
        }
