#include <fstream>
#include <vector>

using namespace std;

ifstream fin("joc.in");
ofstream fout("joc.out");

vector<int> v;
int n, s[100001];

int main() {
  fin >> n;
  v.resize(n);
  for (int i = 0; i < n; ++i) fin >> v[i];

  int p = 0, sum = 0;
  for (int i = 0; i < n; ++i) {
    int st = p, dr = n - 1, poz = -1;
    while (st <= dr) {
      int mid = (st + dr) / 2;
      if (s[mid] + v[i] <= sum)
        st = mid + 1;
      else {
        poz = mid;
        dr = mid - 1;
      }
    }
    if (poz != -1) {
      p = poz;
      sum = s[poz] + v[i];
    }
  }

  fout << sum << "\n";
  return 0;
}
```
