#include <fstream>
#include <vector>

using namespace std;

int main() {
    ifstream fin("mosia.in");
    ofstream fout("mosia.out");

    int n;
    fin >> n;

    vector<pair<int, int>> p(n);
    vector<int> d(n);
    for (int i = 0; i < n; i++) {
        fin >> p[i].first >> p[i].second >> d[i];
    }

    double ans = 0;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            if (i == j) continue;

            int dx = p[j].first - p[i].first;
            int dy = p[j].second - p[i].second;
            double dist = sqrt(dx * dx + dy * dy);
            if (dist <= d[i] + d[j]) {
                ans = max(ans, 0.5 * abs(dx * p[i].second - dy * p[i].first + dx * p[j].second - dy * p[j].first));
            }
        }
    }

    fout.precision(4);
    fout << fixed << ans << endl;

    return 0;
}
```
