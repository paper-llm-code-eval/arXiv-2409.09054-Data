using namespace std;

const double EPS = 1e-9;

struct Point {
    double x, y;

    Point() : x(0), y(0) {}
    Point(double x, double y) : x(x), y(y) {}

    Point operator+(const Point& other) const {
        return Point(x + other.x, y + other.y);
    }

    Point operator-(const Point& other) const {
        return Point(x - other.x, y - other.y);
    }

    double operator*(const Point& other) const {
        return x * other.x + y * other.y;
    }

    double operator^(const Point& other) const {
        return x * other.y - y * other.x;
    }

    double norm() const {
        return sqrt(x * x + y * y);
    }

    Point normalize() const {
        return Point(x / norm(), y / norm());
    }

    bool operator<(const Point& other) const {
        return x < other.x || (x == other.x && y < other.y);
    }
};

struct Segment {
    Point p1, p2;

    Segment() : p1(Point()), p2(Point()) {}
    Segment(const Point& p1, const Point& p2) : p1(p1), p2(p2) {}

    double length() const {
        return (p2 - p1).norm();
    }

    Point middle() const {
        return (p1 + p2) / 2;
    }

    bool operator<(const Segment& other) const {
        return p1 < other.p1 || (p1 == other.p1 && p2 < other.p2);
    }
};

struct Pole {
    Point p;
    double d;

    Pole() : p(Point()), d(0) {}
    Pole(const Point& p, double d) : p(p), d(d) {}
};

double area(const vector<Point>& points) {
    double sum = 0;
    for (int i = 0; i < points.size(); i++) {
        sum += points[i] ^ points[(i + 1) % points.size()];
    }
    return abs(sum) / 2;
}

bool cmp(const Pole& p1, const Pole& p2) {
    return p1.d > p2.d;
}

int main() {
    ifstream in("mosia.in");
    ofstream out("mosia.out");

    int n;
    in >> n;

    vector<Pole> poles(n);
    for (int i = 0; i < n; i++) {
        in >> poles[i].p.x >> poles[i].p.y >> poles[i].d;
    }

    sort(poles.begin(), poles.end(), cmp);

    vector<Point> points;
    for (int i = 0; i < n; i++) {
        points.push_back(poles[i].p);
    }

    double maxArea = area(points);

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            if (i == j) {
                continue;
            }

            Segment segment(poles[i].p, poles[j].p);
            double segmentLength = segment.length();

            if (segmentLength > poles[i].d + poles[j].d) {
                continue;
            }

            double d = (poles[i].d - segmentLength) / 2;
            Point p1 = segment.p1 + segment.normalize() * d;

            d = (poles[j].d - segmentLength) / 2;
            Point p2 = segment.p2 + segment.normalize() * d;

            points.clear();
            points.push_back(p1);
            points.push_back(p2);

            for (int k = 0; k < n; k++) {
                if (k == i || k == j) {
                    continue;
                }

                points.push_back(poles[k].p);
            }

            maxArea = max(maxArea, area(points));
        }
    }

    out.precision(4);
    out << fixed << maxArea << endl;

