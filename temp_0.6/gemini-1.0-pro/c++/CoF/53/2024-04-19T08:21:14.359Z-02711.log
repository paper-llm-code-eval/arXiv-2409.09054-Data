    Point(double x = 0, double y = 0) : x(x), y(y) {}
    Point operator+(const Point& p) const { return Point(x + p.x, y + p.y); }
    Point operator-(const Point& p) const { return Point(x - p.x, y - p.y); }
    double operator*(const Point& p) const { return x * p.x + y * p.y; }
    double operator^(const Point& p) const { return x * p.y - y * p.x; }
    double dist(const Point& p) const { return sqrt((x - p.x) * (x - p.x) + (y - p.y) * (y - p.y)); }
};

struct Segment {
    Point p1, p2;
    Segment(Point p1 = Point(), Point p2 = Point()) : p1(p1), p2(p2) {}
    double dist(const Point& p) const {
        double d = (p2 - p1) ^ (p - p1);
        return abs(d) / (p2 - p1).dist(Point());
    }
    bool contains(const Point& p) const {
        return ((p1 - p) ^ (p2 - p)) == 0 && (p1 - p) * (p2 - p) <= 0;
    }
};

struct Polygon {
    vector<Point> points;
    Polygon(const vector<Point>& points) : points(points) {}
    double area() const {
        double area = 0;
        for (int i = 0; i < points.size(); i++) {
            area += points[i] ^ points[(i + 1) % points.size()];
        }
        return abs(area) / 2;
    }
    Point centroid() const {
        Point centroid(0, 0);
        double area = 0;
        for (int i = 0; i < points.size(); i++) {
            double d = points[i] ^ points[(i + 1) % points.size()];
            area += d;
            centroid = centroid + (points[i] + points[(i + 1) % points.size()]) * d;
        }
        return centroid / (3 * area);
    }
    bool contains(const Point& p) const {
        for (int i = 0; i < points.size(); i++) {
            Segment s(points[i], points[(i + 1) % points.size()]);
            if (s.contains(p)) return true;
