        fin >> privileg[i].first >> privileg[i].second;
    }

    fill(sol, sol + n, 'X');

    if (cerinta == 1) {
        sort(privileg, privileg + n, compare_real);
        for (i = 0; i < n && i < m; i++) {
            if (privileg[i].first >= real[i]) {
                best_sol[i] = 'R';
                best_sol_nr++;
            }
        }
    } else {
        sort(privileg, privileg + n, compare_real);
        for (i = 0; i < n && i < m; i++) {
            if (privileg[i].first >= real[i]) {
                sol[i] = 'R';
            }
        }
        sort(privileg, privileg + n, compare_uman);
        for (i = 0; i < n && i < m; i++) {
            if (privileg[i].second > uman[i]) {
                if (sol[i] == 'R') {
                    continue;
                }
                sol[i] = 'U';
                best_sol_nr++;
            }
        }
        for (i = 0; i < n; i++) {
            best_sol[i] = sol[i];
        }
    }

    fout << best_sol_nr << '\n';
    for (i = 0; i < n; i++) {
        fout << best_sol[i];
    }

    return 0;
