#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAX_N = 2000;
const int MAX_M = 2000;

int n, m;
bool cerinta;
vector<int> real, uman;
pair<int, int> privileg[MAX_N];
int sol[MAX_N];
int best_sol[MAX_N];
int best_sol_nr;

bool compare_real(const pair<int, int>& a, const pair<int, int>& b) {
    return a.first > b.first;
}

bool compare_uman(const pair<int, int>& a, const pair<int, int>& b) {
    return a.second > b.second;
}

int main() {
    ifstream fin("admitere.in");
    ofstream fout("admitere.out");

    int i;

    fin >> cerinta;
    fin >> n >> m;
    real.resize(m);
    uman.resize(m);
    for (i = 0; i < m; i++) {
        fin >> real[i];
    }
    for (i = 0; i < m; i++) {
        fin >> uman[i];
    }
    for (i = 0; i < n; i++) {
        fin >> privileg[i].first >> privileg[i].second;
    }

    if (cerinta == 1) {
        sort(privileg, privileg + n, compare_real);
        for (i = 0; i < n && i < m; i++) {
            if (privileg[i].first > real[i]) {
                best_sol[i] = 'R';
                best_sol_nr++;
            }
        }
    } else {
        sort(privileg, privileg + n, compare_real);
        for (i = 0; i < n && i < m; i++) {
            if (privileg[i].first > real[i]) {
                sol[i] = 'R';
            }
        }
        sort(privileg, privileg + n, compare_uman);
        for (i = 0; i < n && i < m; i++) {
            if (privileg[i].second > uman[i]) {
                if (sol[i] == 'R') {
                    continue;
                }
                sol[i] = 'U';
                best_sol_nr++;
            }
        }
        for (i = 0; i < n; i++) {
            best_sol[i] = sol[i];
        }
    }

    fout << best_sol_nr << '\n';
    for (i = 0; i < n; i++) {
        fout << best_sol[i];
    }

    return 0;
}
