#include <fstream>
#include <vector>
#include <algorithm>
#include <climits>
using namespace std;

typedef long long ll;

const int NMAX = 63365;
const int MAX_LEVEL = 25;
const int dx[] = {0, 1, 1, 0};
const int dy[] = {0, 0, 1, 1};

int n, m, c[NMAX];
bool viz[NMAX];
ll dp[MAX_LEVEL][NMAX];

void dfs(int level, int node, ll sum) {
    if (level == m) {
        dp[level][node] = sum + c[node];
        return;
    }
    if (dp[level][node] != -1) {
        return;
    }
    dp[level][node] = LLONG_MAX;
    for (int i = 0; i < 4; i++) {
        int x = node + dx[i] * (1 << level) + dy[i];
        if (!viz[x]) {
            viz[x] = true;
            dfs(level + 1, x, sum + c[node]);
            viz[x] = false;
        }
    }
    dp[level][node] = min(dp[level][node], sum + c[node]);
}

int main() {
    ifstream in("suma.in");
    ofstream out("suma.out");
    in >> n;
    for (int i = 1; i <= n; i++) {
        in >> c[i];
    }
    m = 1;
    while ((1 << m) <= n) {
        m++;
    }
    m--;
    fill_n(&dp[0][0], MAX_LEVEL * NMAX, -1);
    dfs(0, 1, 0);
    ll ans = LLONG_MAX;
    int node = 1;
    for (int i = 1; i <= n; i++) {
        if (dp[m][i] < ans) {
            ans = dp[m][i];
            node = i;
        }
    }
    out << m << ' ' << ans << endl;
    vector<int> path;
    path.push_back(node);
    for (int level = m - 1; level >= 0; level--) {
        for (int i = 0; i < 4; i++) {
            int x = node - dx[i] * (1 << level) - dy[i];
            if (dp[level][x] + c[node] == dp[level + 1][node]) {
                path.push_back(x);
                node = x;
                break;
            }
        }
    }
    reverse(path.begin(), path.end());
    for (int i = 0; i < path.size(); i++) {
        out << path[i] << ' ';
    }
    out << endl;
    return 0;
}
