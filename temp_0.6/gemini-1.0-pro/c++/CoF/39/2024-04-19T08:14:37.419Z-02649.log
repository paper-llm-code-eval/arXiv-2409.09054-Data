#include <fstream>
#include <vector>

using namespace std;

const int MAX_N = 63365;

int n, m, cost[MAX_N], dp[MAX_N][4];
vector<int> path[MAX_N], ans;

void read() {
    ifstream fin("suma.in");
    fin >> n;
    for (int i = 1; i <= n; i++)
        fin >> cost[i];
    fin.close();
}

void solve() {
    dp[1][1] = cost[1];
    for (int i = 2; i <= n; i++) {
        dp[i][1] = min(dp[i - 1][1], dp[i - 1][2]) + cost[i];
        dp[i][2] = min({dp[i - 1][1], dp[i - 1][2], dp[i - 1][3]}) + cost[i];
        dp[i][3] = min({dp[i - 1][2], dp[i - 1][3], dp[i - 1][4]}) + cost[i];
        dp[i][4] = min({dp[i - 1][3], dp[i - 1][4]}) + cost[i];
    }

    m = dp[n][1] < dp[n][2] ? 1 : 2;
    for (int i = n; i >= 1; i--) {
        if (m == 1 && dp[i][1] < dp[i][2]) {
            path[i].push_back(i);
            m = 1;
        } else if (m == 1 && dp[i][2] < dp[i][1]) {
            path[i].push_back(i);
            m = 2;
        } else if (m == 2 && dp[i][2] < dp[i][3]) {
            path[i].push_back(i);
            m = 2;
        } else if (m == 2 && dp[i][3] < dp[i][2]) {
            path[i].push_back(i);
            m = 3;
        } else if (m == 3 && dp[i][3] < dp[i][4]) {
            path[i].push_back(i);
            m = 3;
        } else if (m == 3 && dp[i][4] < dp[i][3]) {
            path[i].push_back(i);
            m = 4;
        } else if (m == 4 && dp[i][4] < dp[i][3]) {
            path[i].push_back(i);
            m = 4;
        }
    }

    for (int i = 1; i <= n; i++) {
        if (path[i].size() > 0) {
            ans.push_back(path[i][0]);
            break;
        }
    }

    for (int i = ans[0]; i <= n; i++) {
        if (path[i].size() > 0) {
            ans.push_back(path[i][0]);
        }
    }
}

void write() {
    ofstream fout("suma.out");
    fout << m << " " << dp[n][1] << "\n";
    for (int i = 0; i < ans.size(); i++) {
        fout << ans[i] << " ";
    }
    fout.close();
}

int main() {
    read();
    solve();
    write();
    return 0;
}
