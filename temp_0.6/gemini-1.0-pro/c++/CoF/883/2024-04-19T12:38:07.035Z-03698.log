#include <fstream>
#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream fin("cursuri.in");
  ofstream fout("cursuri.out");

  int c, n, k;
  fin >> c >> n >> k;

  vector<pair<int, int>> intervals(n);
  for (int i = 0; i < n; i++) {
    fin >> intervals[i].first >> intervals[i].second;
  }

  sort(intervals.begin(), intervals.end());

  int max_courses = 0;
  int max_duration = intervals[n - 1].second - intervals[n - 1].first;

  if (c == 1) {
    vector<int> dp(k, 0);
    for (int i = 0; i < n; i++) {
      int j = 0;
      while (j < k && dp[j] >= intervals[i].first) j++;
      if (j < k) {
        dp[j] = intervals[i].second;
        max_courses++;
      }
    }
    fout << max_courses << endl;
  } else if (c == 2) {
    int lo = 0, hi = max_duration;
    while (lo <= hi) {
      int mid = (lo + hi) / 2;
      int rooms_needed = 0;
      for (int i = 0; i < n; i++) {
        rooms_needed += (intervals[i].second - intervals[i].first + mid - 1) / mid;
      }
      if (rooms_needed <= k) {
        max_duration = mid;
        hi = mid - 1;
      } else {
        lo = mid + 1;
      }
    }
    fout << max_duration << endl;
  }

  return 0;
}
