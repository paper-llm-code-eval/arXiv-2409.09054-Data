            affectedOnLevel.push_back(level);
        } else if (numAffectedOnLevel == maxAffected) {
            affectedOnLevel.push_back(level);
        }
        numAffected += numAffectedOnLevel;
    }

    fout << numAffected << endl;
    fout << maxAffected << endl;
    for (int level : affectedOnLevel) {
        fout << level << ' ';
    }
