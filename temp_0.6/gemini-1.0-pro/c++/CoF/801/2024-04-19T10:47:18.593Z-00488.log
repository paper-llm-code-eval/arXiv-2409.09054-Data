#include <fstream>
#include <vector>
#include <cmath>
#include <algorithm>

using namespace std;

struct Point {
    int x, y;
};

int main() {
    ifstream fin("cladiri.in");
    ofstream fout("cladiri.out");

    int centerX, centerY, intensity;
    fin >> centerX >> centerY >> intensity;

    vector<Point> buildings;
    int numBuildings;
    fin >> numBuildings;
    for (int i = 0; i < numBuildings; i++) {
        int x, y, resistance;
        fin >> x >> y >> resistance;
        buildings.push_back({x, y});
