#include <bits/stdc++.h>
using namespace std;

ifstream fin("cladiri.in");
ofstream fout("cladiri.out");

const int mxN = 1e5;
int n, cX, cY, cI;
pair<int, int> coord[mxN];
int rez[mxN];

vector<int> levels[10001];

int main() {
  fin >> cX >> cY >> cI;
  fin >> n;
  for (int i = 0; i < n; i++) {
    int x, y, r;
    fin >> x >> y >> r;
    coord[i] = {x, y};
    rez[i] = r;
  }

  for (int i = 0; i < n; i++) {
    int distX = abs(coord[i].first - cX);
    int distY = abs(coord[i].second - cY);
    int dist = max(distX, distY);
    if (dist <= cI && rez[i] <= cI) {
      levels[dist].push_back(i);
    }
  }

  int maxLevel = 0, maxCount = 0;
  for (int i = 0; i <= cI; i++) {
    if (levels[i].size() > maxCount) {
      maxCount = levels[i].size();
      maxLevel = i;
    }
  }

  int count = 0;
  for (int i = 0; i <= cI; i++) {
    count += levels[i].size();
  }

  fout << count << '\n';
  fout << maxCount << '\n';
  for (int i = 0; i <= cI; i++) {
    if (levels[i].size() == maxCount) {
      fout << i << ' ';
    }
  }
  fout << '\n';

  return 0;
}
```
