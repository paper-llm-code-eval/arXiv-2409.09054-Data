#include <fstream>
#include <iostream>
#include <vector>
using namespace std;

ifstream fin("telecabina.in");
ofstream fout("telecabina.out");

const int MAXN = 40;
int n, a[MAXN], b[MAXN];
vector<int> stații_speciale;

int main() {
  fin >> n;
  for (int i = 0; i < n; i++) {
    fin >> a[i] >> b[i];
  }

  int nr_oameni = 0, consum = 0, nr_stații_speciale = 0;
  bool urcare = true;
  for (int i = 0; i < n - 1; i++) {
    nr_oameni += b[i];
    if (a[i] > a[i + 1]) {
      if (urcare) {
        consum += 3 * (a[i] - a[i + 1]);
        stații_speciale.push_back(i + 1);
        urcare = false;
      }
    } else if (a[i] < a[i + 1]) {
      if (!urcare) {
        consum += (a[i + 1] - a[i]);
        stații_speciale.push_back(i + 1);
        urcare = true;
      }
    }
  }

  fout << nr_oameni << endl;
  fout << consum << endl;
  fout << stații_speciale.size() << endl;
  for (int i = 0; i < stații_speciale.size(); i++) {
    fout << stații_speciale[i] << ' ';
  }
  fout << endl;

  return 0;
}
```
