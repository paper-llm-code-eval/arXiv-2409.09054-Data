#include <fstream>
#include <vector>

using namespace std;

const int MAX_N = 40;

int main() {
  ifstream fin("telecabina.in");
  ofstream fout("telecabina.out");

  int n;
  fin >> n;

  vector<pair<int, int>> stations(n);
  for (int i = 0; i < n; i++) {
    fin >> stations[i].first >> stations[i].second;
  }

  int people = 0, fuel = 0, special = 0;
  for (int i = 0; i < n - 1; i++) {
    if (stations[i].first < stations[i + 1].first) {
      fuel += 3 * (stations[i + 1].first - stations[i].first);
      people += stations[i].second;
    } else {
      fuel += (stations[i + 1].first - stations[i].first);
      special++;
    }
  }

  fout << people << endl;
  fout << fuel << endl;
  fout << special << endl;

  return 0;
}
```
