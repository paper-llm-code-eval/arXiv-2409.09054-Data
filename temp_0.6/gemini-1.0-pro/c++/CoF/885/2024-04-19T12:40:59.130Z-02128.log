              if (a[i][j] == 1) {
                ans -= 2;
                c[j] = 0;
                nr_op++;
                ok = 1;
                break;
              }
          if (ok) break;
        }
      if (!ok) {
        for (int j = 1; j <= N; j++)
          if (c[j] == 0) {
            for (int i = 1; i <= N; i++)
              if (l[i] == 1)
                if (a[i][j] == 1) {
                  ans -= 2;
                  l[i] = 0;
                  nr_op++;
                  break;
                }
          }
      }
    }
    out << nr_op << "\n";
  }
  return 0;
}
