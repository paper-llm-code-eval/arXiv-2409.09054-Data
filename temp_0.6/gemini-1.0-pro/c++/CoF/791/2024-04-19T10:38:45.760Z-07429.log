#include <fstream>
#include <vector>

using namespace std;

int main() {
    ifstream in("vecini.in");
    ofstream out("vecini.out");
    int m, n, k;
    in >> m >> n >> k;
    vector<vector<int>> mat(m + 2, vector<int>(n + 2));
    for (int i = 0; i < k; i++) {
        int x, y;
        in >> x >> y;
        mat[x][y] = 1;
    }
    int mx = 0, cnt = 0;
    for (int i = 1; i <= m; i++) {
        for (int j = 1; j <= n; j++) {
            int sum = 0;
            sum += mat[i - 1][j - 1] == mat[i][j];
            sum += mat[i - 1][j] == mat[i][j];
            sum += mat[i - 1][j + 1] == mat[i][j];
            sum += mat[i][j - 1] == mat[i][j];
            sum += mat[i][j + 1] == mat[i][j];
            sum += mat[i + 1][j - 1] == mat[i][j];
            sum += mat[i + 1][j] == mat[i][j];
            sum += mat[i + 1][j + 1] == mat[i][j];
            if (sum > mx) {
                mx = sum;
                cnt = 1;
            } else if (sum == mx) {
                cnt++;
            }
        }
    }
    out << mx << " " << cnt << "\n";
    return 0;
}
