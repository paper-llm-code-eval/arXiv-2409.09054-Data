#include <fstream>
#include <vector>

int main() {
  std::ifstream in("vecini.in");
  std::ofstream out("vecini.out");

  int m, n, k;
  in >> m >> n >> k;

  std::vector<std::vector<int>> mat(m + 2, std::vector<int>(n + 2));
  for (int i = 0; i < k; ++i) {
    int x, y;
    in >> x >> y;
    mat[x][y] = 1;
  }

  int max_neigh = 0, cnt = 0;
  for (int i = 1; i <= m; ++i) {
    for (int j = 1; j <= n; ++j) {
      int neigh = 0;
      for (int di = -1; di <= 1; ++di) {
        for (int dj = -1; dj <= 1; ++dj) {
          if (di == 0 && dj == 0) {
            continue;
          }
          neigh += mat[i + di][j + dj] == mat[i][j];
        }
      }
      if (neigh > max_neigh) {
        max_neigh = neigh;
        cnt = 1;
      } else if (neigh == max_neigh) {
        ++cnt;
      }
    }
  }

  out << max_neigh << ' ' << cnt << '\n';
  return 0;
}
```
