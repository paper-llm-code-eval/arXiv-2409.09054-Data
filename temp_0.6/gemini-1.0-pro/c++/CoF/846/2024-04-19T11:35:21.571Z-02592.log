    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            bool shining = true;
            for (int x = i - 1; x <= i + 1; x++) {
                for (int y = j - 1; y <= j + 1; y++) {
                    if (x >= 1 && x <= n && y >= 1 && y <= m && !(x == i && y == j) && a[x][y] >= a[i][j]) {
