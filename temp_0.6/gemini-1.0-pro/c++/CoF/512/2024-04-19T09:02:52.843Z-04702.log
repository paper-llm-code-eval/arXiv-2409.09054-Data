    vector<int> a(n);
    for (int i = 0; i < n; i++) {
        fin >> a[i];
    }

    if (C == 1) {
        nth_element(a.begin(), a.begin() + k - 1, a.end(), greater<int>());
        fout << a[k - 1] << '\n';
    } else if (C == 2) {
        int max_unificare = 0;
        for (int i = 0; i < n - 1; i++) {
            vector<bool> digits(10, false);
            int x = a[i], y = a[i + 1];
            while (x > 0) {
                digits[x % 10] = true;
                x /= 10;
            }
            while (y > 0) {
                digits[y % 10] = true;
                y /= 10;
            }
            int unificare = 0;
            for (int j = 9; j >= 0; j--) {
                if (digits[j]) {
                    unificare *= 10;
                    unificare += j;
                }
            }
            max_unificare = max(max_unificare, unificare);
        }
        fout << max_unificare << '\n';
    } else if (C == 3) {
        vector<int> dp(n, 0);
        for (int i = 0; i < n; i++) {
            vector<bool> digits(10, false);
            int x = a[i];
            while (x > 0) {
                digits[x % 10] = true;
                x /= 10;
            }
            int unificare = 0;
            for (int j = 9; j >= 0; j--) {
                if (digits[j]) {
                    unificare *= 10;
                    unificare += j;
                }
            }
            dp[i] = unificare;
            if (i >= k) {
                dp[i] = max(dp[i], dp[i - k]);
            }
            if (i >= 1) {
                dp[i] = max(dp[i], dp[i - 1]);
            }
        }
        fout << dp[n - 1] << '\n';
    }

    return 0;
}
