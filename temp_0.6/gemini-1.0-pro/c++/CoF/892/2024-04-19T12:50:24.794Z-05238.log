int main() {
  ifstream in("tnia.in");
  ofstream out("tnia.out");

  int n, m, q;
  in >> n >> m;
  vector<int> h(n);
  int sum = 0;
  for (int i = 0; i < n; i++) {
    in >> h[i];
    sum += h[i];
  }
  in >> q;

  vector<int> dp(sum + 1);
  int last = 0;
  for (int i = 1; i <= sum; i++) {
    dp[i] = dp[i - 1];
    if (last + h[i % n] <= i) {
      dp[i]++;
      last += h[i % n];
    }
  }

  while (q--) {
    int a, b, c, d;
    in >> a >> b >> c >> d;
    out << dp[d * n + c] - dp[(b - 1) * n + c] - dp[d * n + a - 1] + dp[(b - 1) * n + a - 1] << "\n";
  }

  return 0;
}
```
