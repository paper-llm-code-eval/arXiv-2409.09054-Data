#include <iostream>
#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream fin("tnia.in");
    ofstream fout("tnia.out");

    int n, m, q;
    fin >> n >> m;
    vector<int> h(n);
    for (int& i : h) fin >> i;
    fin >> q;

    vector<int> pref(m + 1);
    for (int i = 0; i < n; ++i) {
        for (int j = h[i]; j <= m; ++j) ++pref[j];
    }

    for (int i = 1; i <= m; ++i) pref[i] += pref[i - 1];

    while (q--) {
        int l1, c1, l2, c2;
        fin >> l1 >> c1 >> l2 >> c2;
        fout << pref[l2] - pref[l1 - 1] << '\n';
    }

    return 0;
}
```
