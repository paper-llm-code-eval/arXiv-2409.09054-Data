#include <fstream>
#include <vector>

std::vector<int> sir;

int main() {
  std::ifstream in("sir.in");
  std::ofstream out("sir.out");
  int n;
  in >> n;
  sir.push_back(1);
  while (sir.size() < n) {
    int last = sir.back();
    std::vector<int> aux;
    while (last) {
      int cnt = 0;
      int last_digit = last % 10;
      while (last_digit == last % 10) {
        ++cnt;
        last /= 10;
      }
      aux.push_back(cnt);
      aux.push_back(last_digit);
    }
    sir.push_back(0);
    for (int i = aux.size() - 1; i >= 0; --i) {
      sir.back() *= 10;
      sir.back() += aux[i];
    }
  }
  out << sir[n - 1];
  return 0;
}
```
