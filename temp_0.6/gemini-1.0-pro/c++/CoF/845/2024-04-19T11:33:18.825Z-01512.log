#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream fin("munte.in");
  ofstream fout("munte.out");
  int n;
  fin >> n;
  vector<int> v(n);
  for (int i = 0; i < n; i++) {
    fin >> v[i];
  }
  int num_munti = 0;
  int num_total_munti = 0;
  int num_elemente_finale = n;
  while (true) {
    int i = 0;
    int j = 0;
    while (i < n - 1) {
      int varf = v[i];
      int max_stanga = v[i];
      int max_dreapta = v[i];
      while (i < n - 1 && v[i] < v[i + 1]) {
        max_stanga = v[i];
        i++;
      }
      while (i < n - 1 && v[i] > v[i + 1]) {
        max_dreapta = v[i];
        i++;
      }
      if (max_stanga != max_dreapta) {
        num_munti++;
        num_total_munti++;
        v.erase(v.begin() + i);
        num_elemente_finale--;
        i--;
      } else {
        i++;
      }
    }
    if (num_munti == 0) {
      break;
    }
    num_munti = 0;
  }
  fout << num_total_munti << "\n" << num_total_munti << "\n" << num_elemente_finale << "\n";
  return 0;
}
