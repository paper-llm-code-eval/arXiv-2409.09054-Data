#include <fstream>

using namespace std;

const int MAXN = 500;

int n, d, k, fact[MAXN + 1], invFact[MAXN + 1], pw[MAXN + 1][MAXN + 1];
long long ans;

int powMod(int b, int e, int mod) {
    int ans = 1;
    while (e) {
        if (e & 1) ans = (1LL * ans * b) % mod;
        b = (1LL * b * b) % mod;
        e >>= 1;
    }
    return ans;
}

int main() {
    ifstream in("expozitie.in");
    ofstream out("expozitie.out");

    in >> n >> d >> k;

    fact[0] = 1;
    for (int i = 1; i <= MAXN; ++i) fact[i] = (1LL * fact[i - 1] * i) % d;

    invFact[0] = 1;
    for (int i = 1; i <= MAXN; ++i) invFact[i] = powMod(fact[i], d - 2, d);

    pw[0][0] = 1;
    for (int i = 0; i <= MAXN; ++i) {
        for (int j = 1; j <= MAXN; ++j) {
            pw[i][j] = (1LL * pw[i][j - 1] * i) % d;
        }
    }

    for (int i = 0; i <= n - k; ++i) {
        for (int j = 0; j <= d; ++j) {
            int tmp = (1LL * pw[j][k - i] * fact[i + j]) % d;
            tmp = (1LL * tmp * invFact[i]) % d;
            ans = (ans + tmp) % d;
        }
    }

    out << ans << '\n';
    return 0;
}
