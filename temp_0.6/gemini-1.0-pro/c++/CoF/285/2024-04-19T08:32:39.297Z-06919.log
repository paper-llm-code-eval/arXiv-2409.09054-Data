          for (int k = 0; k < pulsar->r; k++) {
            times.insert((pulsar->t + k) % pulsar->r);
          }
        }
        ans = max(ans, (int)times.size());
      }
    }
    out << ans << '\n';
  } else {
    queue<pair<int, int>> q;
    q.push({xs, ys});
    sectors[xs][ys].t = 0;
    int min_time = INT_MAX;
    while (!q.empty()) {
      auto [x, y] = q.front();
      q.pop();
      if (x == xf && y == yf) {
        min_time = min(min_time, sectors[x][y].t);
        continue;
      }
      for (auto pulsar : sectors[x][y].pulsars) {
        if ((sectors[x][y].t + 1) % pulsar->r == pulsar->t) {
          q.push({x, y});
          break;
        }
      }
      if (x + 1 <= n && sectors[x + 1][y].t > sectors[x][y].t + 1) {
        sectors[x + 1][y].t = sectors[x][y].t + 1;
        q.push({x + 1, y});
      }
      if (x - 1 >= 1 && sectors[x - 1][y].t > sectors[x][y].t + 1) {
        sectors[x - 1][y].t = sectors[x][y].t + 1;
        q.push({x - 1, y});
      }
      if (y + 1 <= n && sectors[x][y + 1].t > sectors[x][y].t + 1) {
