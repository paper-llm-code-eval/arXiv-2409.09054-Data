struct Pulsar {
  int x, y, r, t;
};

struct Sector {
  int x, y, t;
  vector<Pulsar*> pulsars;
};

int main() {
  ifstream in("pulsar.in");
  ofstream out("pulsar.out");

  int c, n, p;
  in >> c >> n >> p;
  vector<vector<Sector>> sectors(n + 1, vector<Sector>(n + 1));
  for (int i = 1; i <= p; i++) {
    int x, y, r, t;
    in >> x >> y >> r >> t;
    sectors[x][y].pulsars.push_back(new Pulsar{x, y, r, t});
  }
  int xs, ys, xf, yf;
  in >> xs >> ys >> xf >> yf;

  if (c == 1) {
    int ans = 0;
    for (int i = 1; i <= n; i++) {
      for (int j = 1; j <= n; j++) {
        set<int> times;
