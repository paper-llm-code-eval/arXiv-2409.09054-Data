    ifstream in("unific.in");
    ofstream out("unific.out");
    in >> n;
    for (int i = 1; i <= n; i++) {
        int x;
        in >> x;
        while (x) {
            fr[x % 10]++;
            fr_cif[i]++;
            x /= 10;
        }
        sol[i] = x;
