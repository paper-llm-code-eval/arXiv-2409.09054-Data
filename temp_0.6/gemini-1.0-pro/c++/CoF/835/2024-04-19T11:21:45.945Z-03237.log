#include <vector>

using namespace std;

#define MAXN 100001

int n, fr[10], fr_cif[MAXN], sol[MAXN];
vector<int> v;

bool unific(int &a, int &b) {
    int na = 0, nb = 0;
    while (a || b) {
        int ca = a % 10, cb = b % 10;
        if (ca == cb) {
            a /= 10;
            b /= 10;
        } else {
            na = na * 10 + ca;
            nb = nb * 10 + cb;
            a /= 10;
