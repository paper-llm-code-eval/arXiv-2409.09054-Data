#include <iostream>
#include <fstream>
#include <sstream>
#include <vector>

using namespace std;

int main() {
    ifstream in("numar.in");
    ofstream out("numar.out");

    int ni, nz;
    in >> ni >> nz;
    string int_part, dec_part;
    in >> int_part >> dec_part;

    int num = 0;
    for (int i = 0; i < ni; i++) {
        num = num * 10 + (int_part[i] - '0');
    }
    for (int i = 0; i < nz; i++) {
        num = num * 10 + (dec_part[i] - '0');
    }

    vector<int> divisors;
    for (int i = 1; i <= num; i++) {
        if (num % i == 0) {
            divisors.push_back(i);
        }
    }

    int min_diff = num;
    int a, b;
    for (int i = 0; i < divisors.size(); i++) {
        int diff = abs(divisors[i] - (num / divisors[i]));
        if (diff < min_diff) {
            min_diff = diff;
            a = divisors[i];
            b = num / divisors[i];
        }
    }

    stringstream ss;
    ss << a;
    string a_str = ss.str();
    ss.clear();
    ss << b;
    string b_str = ss.str();

    out << a_str.size() << endl;
    out << a_str << endl;
    out << b_str.size() << endl;
    out << b_str << endl;

    in.close();
    out.close();

    return 0;
}
```
