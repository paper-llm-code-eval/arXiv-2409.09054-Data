    for (int i = ans1.size() - 1; i >= max(0, (int)ans1.size() - 3); i--) {
        fout << ans1[i] << " ";
    }
    fout << "\n";
    fout << ans2 << "\n";
    int mn = 1000000000;
    int mx = 0;
    for (int i = 0; i < k; i++) {
        mn = min(mn, dp[i][0]);
        mx = max(mx, dp[i][0]);
    }
    if (mn == 1000000000) {
        mn = 0;
    }
    fout << mn << " " << mx << "\n";
    int ans6 = 0;
    int ans7 = 0;
    for (int i = 0; i < k; i++) {
        ans6 += acc[i][0];
        ans7 += acc[i][1];
    }
    fout << ans6 << " " << ans7 << "\n";
    return 0;
}
