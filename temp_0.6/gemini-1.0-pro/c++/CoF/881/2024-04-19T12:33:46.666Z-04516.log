            int d = x % 10;
            if (d < last) {
                rem = 1;
            }
            if (d == last + 1) {
                acc++;
            }
            last = d;
            x /= 10;
        }
        if (acc == 0) {
            continue;
        }
        if (rem == 0) {
            ans1.push_back(a[i]);
        } else {
            ans2++;
        }
    }
