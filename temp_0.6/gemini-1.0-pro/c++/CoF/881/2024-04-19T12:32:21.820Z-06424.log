    ofstream out("accesibil.out");
    int p;
    in >> p;
    int k, n;
    in >> k >> n;
    vector<int> a(n);
    for (int i = 0; i < n; i++) {
        in >> a[i];
    }
    if (p == 1) {
        vector<int> accessible;
        for (int i = 0; i < n; i++) {
            if (a[i] >= 10) {
                bool ok = true;
                int aux = a[i];
                int last = aux % 10;
                aux /= 10;
                while (aux > 0) {
                    int digit = aux % 10;
                    if (digit <= last) {
                        ok = false;
                    }
                    last = digit;
                    aux /= 10;
                }
                if (ok == true) {
                    accessible.push_back(a[i]);
                }
            }
        }
        sort(accessible.begin(), accessible.end(), greater<int>());
        if (accessible.size() < 3) {
            for (int i = 0; i < accessible.size(); i++) {
                out << accessible[i] << " ";
            }
        } else {
            for (int i = 0; i < 3; i++) {
                out << accessible[i] << " ";
            }
        }
        out << endl;
