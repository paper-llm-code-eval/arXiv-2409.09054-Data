#include <fstream>

using namespace std;

const int NMAX = 100;
const int MMAX = 15000;
const int PMAX = 100000000;

int n, m;
int p[NMAX];
int ciur[PMAX];
int v[MMAX];

int main() {
    ifstream fin("numar.in");
    ofstream fout("numar.out");

    fin >> n >> m;
    for (int i = 0; i < n; ++i) fin >> p[i];

    int cnt = 0;
    for (int i = 2; i < PMAX; ++i) {
        if (!ciur[i]) {
            v[++cnt] = i;
            for (int j = i + i; j < PMAX; j += i) ciur[j] = 1;
        }
    }

    int pos = 0;
    for (int i = 1; i < m; ++i) {
        while (v[pos + 1] * p[0] <= v[i]) ++pos;
        for (int j = 0; j < n; ++j) {
            while (v[pos + 1] * p[j] <= v[i]) ++pos;
        }
    }

    fout << v[pos + 1] << '\n';

    return 0;
}
