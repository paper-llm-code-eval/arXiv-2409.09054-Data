#include <fstream>
#include <iostream>
#include <vector>
#include <queue>

using namespace std;

struct Point {
  int x, y;
};

int n, m, I;
vector<vector<int>> grid;
vector<Point> points;

bool valid(int x, int y) {
  return x >= 1 && x <= n && y >= 1 && y <= m && grid[x][y] == 0;
}

bool fight(int x1, int y1, int x2, int y2) {
  if (!valid(x2, y2)) return false;
  if (grid[x2][y2] == 0) {
    grid[x2][y2] = grid[x1][y1];
    grid[x1][y1] = 0;
    points.push_back({x2, y2});
    return true;
  }
  return false;
}

int main() {
  ifstream fin("immortal.in");
  ofstream fout("immortal.out");
  fin >> n >> m >> I;
  grid.resize(n + 2, vector<int>(m + 2, 0));
  for (int i = 0; i < I; i++) {
    int x, y;
    fin >> x >> y;
    grid[x][y] = i + 1;
    points.push_back({x, y});
  }
  while (I > 1) {
    for (int i = 1; i <= n; i++) {
      for (int j = 1; j <= m; j++) {
        if (grid[i][j] != 0) {
          if (fight(i, j, i - 2, j)) I--;
          else if (fight(i, j, i + 2, j)) I--;
          else if (fight(i, j, i, j - 2)) I--;
          else if (fight(i, j, i, j + 2)) I--;
        }
      }
    }
  }
  for (int i = 0; i < I - 1; i++) {
    fout << points[i].x << " " << points[i].y << " " << points[i + 1].x << " " << points[i + 1].y << "\n";
  }
  return 0;
}
