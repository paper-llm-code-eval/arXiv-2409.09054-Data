      d[x][y] = 0;
      for (int l = 0; l < 4; l++) {
        int nx = x + dir[l][0], ny = y + dir[l][1];
        if (nx >= 1 && nx <= n && ny >= 1 && ny <= m && a[nx][ny]) {
          t[nx][ny] = k;
        }
      }
    }
    for (int k = 1; k <= n; k++)
      for (int l = 1; l <= m; l++)
        if (d[k][l] >= 0) {
          for (int o = 0; o < 4; o++) {
