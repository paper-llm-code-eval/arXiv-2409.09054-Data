            if (nx >= 1 && nx <= n && ny >= 1 && ny <= m && d[nx][ny] < 0 &&
                !a[nx + dir[o][0]][ny + dir[o][1]]) {
              d[nx][ny] = d[k][l] + 1;
            }
          }
        }
    int mx = -1, mx_x, mx_y;
    for (int k = 1; k <= n; k++)
      for (int l = 1; l <= m; l++)
        if (d[k][l] > mx) {
          mx = d[k][l];
          mx_x = k;
          mx_y = l;
        }
    for (int k = 0; k < v.size(); k++) {
      int x = v[k].first, y = v[k].second;
      if (t[x][y] == k) {
        fout << x << " " << y << " " << mx_x << " " << mx_y << "\n";
        a[x][y] = 0;
        a[mx_x][mx_y] = a[x][y];
        v[k] = {mx_x, mx_y};
        i--;
        break;
      }
    }
  }
  fout.close();
  return 0;
}
