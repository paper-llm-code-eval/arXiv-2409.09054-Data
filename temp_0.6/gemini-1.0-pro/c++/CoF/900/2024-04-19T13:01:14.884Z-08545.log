#include <vector>

using namespace std;

const int MAX_N = 100;

int n, m, c;
int h[MAX_N][MAX_N];
int viz[MAX_N][MAX_N];
int nr_camere, max_arie, xs, ys, xd, yd;

int dx[] = {0, 1, 0, -1};
int dy[] = {1, 0, -1, 0};

void dfs(int x, int y) {
  if (x < 0 || x >= n || y < 0 || y >= m || viz[x][y]) {
    return;
  }
  viz[x][y] = 1;
  if (h[x][y] == 15) {
    nr_camere++;
  }
  if (h[x][y] & 1) {
    dfs(x, y - 1);
  }
  if (h[x][y] & 2) {
    dfs(x + 1, y);
  }
  if (h[x][y] & 4) {
    dfs(x, y + 1);
  }
  if (h[x][y] & 8) {
    dfs(x - 1, y);
  }
}

int main() {
  ifstream fin("castel.in");
  ofstream fout("castel.out");
  fin >> c >> n;
  m = n;
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      fin >> h[i][j];
    }
  }
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      if (!viz[i][j]) {
        dfs(i, j);
      }
    }
  }
  fout << nr_camere << '\n';
  return 0;
}
