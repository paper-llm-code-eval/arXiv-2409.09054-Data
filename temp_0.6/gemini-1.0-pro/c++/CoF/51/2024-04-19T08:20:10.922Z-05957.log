            if (line[i] >= 'a' && line[i] <= 'z') {
                word[pos][leng[pos]++] = line[i];
            } else if (leng[pos] > 0) {
                std::string s(word[pos], word[pos] + leng[pos]);
                if (!words.count(s)) {
                    words[s] = pos;
                }
                pos = words[s];
                leng[pos] = 0;
            } else if (i == 0) {
                first_word_invalid = true;
            }
        }
    }
    n = pos;
    if (first_word_invalid) {
        n--;
    }
    if (words.count(word[0]) == 0) {
        fprintf(fout, "0\n");
        return 0;
    }
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < n; ++j) {
            if (i == j) {
                continue;
            }
            int diff = 0;
            int m = std::min(leng[i], leng[j]);
            for (int l = 0; l < m; ++l) {
                diff += word[i][l] != word[j][l];
            }
            diff += std::max(leng[i], leng[j]) - m;
            if (diff <= k) {
                graph[i].push_back(find(j));
            }
        }
    }
    for (int i = 0; i < n; ++i) {
        dp[i][0] = 1;
    }
    for (int j = 1; j <= k; ++j) {
        for (int i = 0; i < n; ++i) {
            for (auto it : graph[i]) {
                dp[i][j] = (dp[i][j] + dp[it][j - 1]) % MOD;
            }
        }
    }
    fprintf(fout, "%d\n", dp[find(words[word[0]])][k]);
    return 0;
}
