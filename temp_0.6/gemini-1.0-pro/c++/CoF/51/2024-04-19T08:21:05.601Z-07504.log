	for (int j = 0; j <= m; j++) {
		dp[0][j] = j;
	}
	for (int i = 1; i <= n; i++) {
		for (int j = 1; j <= m; j++) {
			dp[i][j] = min({dp[i - 1][j] + 1, dp[i][j - 1] + 1, dp[i - 1][j - 1] + (a[i - 1] != b[j - 1])});
		}
	}
	return dp[n][m];
}

void dfs(int node, int parent) {
	vis[node] = 1;
	low[node] = vis[node];
