  fin.close();
  sort(v.begin(), v.end());
  int sum = 0;
  for (int x : v) sum += x;
  int mx = 0, ans = 0;
  for (int mask = 0; mask < (1 << 10); mask++) {
    int sum1 = 0, sum2 = 0;
    vector<int> v1, v2;
    for (int i = 0; i < 10; i++) {
      if ((mask >> i) & 1) {
        sum1 += v[i];
        v1.push_back(v[i]);
      } else {
        sum2 += v[i];
        v2.push_back(v[i]);
      }
    }
    if (sum1 == sum2 && sum1 > mx) {
      mx = sum1;
      ans = mask;
    }
  }
  fout << __builtin_popcount(ans) << " " << mx << endl;
  for (int i = 0; i < 10; i++) {
    if ((ans >> i) & 1) fout << v[i] << " ";
  }
  fout << endl;
  for (int i = 0; i < 10; i++) {
    if (!((ans >> i) & 1)) fout << v[i] << " ";
  }
  fout << endl;
  fout.close();
  return 0;
}
