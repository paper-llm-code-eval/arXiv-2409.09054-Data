int main() {
  ifstream in("culori.in");
  ofstream out("culori.out");

  int n, m, t;
  in >> n >> m >> t;

  vector<vector<char>> grid(n, vector<char>(m));
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      in >> grid[i][j];
    }
  }

  while (t--) {
    vector<vector<char>> next(n, vector<char>(m));
    for (int i = 0; i < n; i++) {
      for (int j = 0; j < m; j++) {
        int count_r = 0;
        int count_a = 0;
        if (i > 0) {
          count_r += (grid[i - 1][j] == 'r');
          count_a += (grid[i - 1][j] == 'a');
        }
        if (i < n - 1) {
          count_r += (grid[i + 1][j] == 'r');
          count_a += (grid[i + 1][j] == 'a');
        }
        if (j > 0) {
          count_r += (grid[i][j - 1] == 'r');
