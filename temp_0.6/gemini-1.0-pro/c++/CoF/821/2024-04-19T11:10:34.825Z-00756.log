#include <fstream>
#include <vector>
#include <algorithm>
using namespace std;

ifstream in("cifru.in");
ofstream out("cifru.out");

int main() {
    int N;
    in >> N;
    vector<int> cifre(N);
    for (int i = 0; i < N; i++) {
        in >> cifre[i];
    }
    int max_cifra = *max_element(cifre.begin(), cifre.end());
    int min_cifra = *min_element(cifre.begin(), cifre.end());
    int min_mutari = N;
    for (int i = 0; i < N; i++) {
        min_mutari = min(min_mutari, min(cifre[i], N - 1 - cifre[i]));
    }
    int cifra_minima = min_cifra;
    for (int i = 0; i < N; i++) {
        if (cifre[i] == min_mutari) {
            cifra_minima = min(cifra_minima, cifre[i]);
        }
    }
    int nr_combinatii = 1;
    for (int i = 0; i < N; i++) {
        if (cifre[i] == min_mutari) {
            nr_combinatii *= 2;
        }
    }
    out << max_cifra << endl;
    out << min_mutari << endl;
    out << cifra_minima << endl;
    out << nr_combinatii << endl;
    return 0;
}
```
