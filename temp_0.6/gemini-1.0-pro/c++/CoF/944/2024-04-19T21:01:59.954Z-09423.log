#include <iostream>
#include <fstream>
#include <vector>
#include <algorithm>
#include <queue>
#include <utility>
#include <cstring>

using namespace std;

ifstream in("patratele.in");
ofstream out("patratele.out");

int n, m, t, nr_patrate;
int frecventa[61];
vector<vector<int>> v;
int dx[4] = {0, 1, 0, -1};
int dy[4] = {1, 0, -1, 0};
bool viz[61][61];

struct pozitie {
    int x, y, cost;
    pozitie(int _x, int _y, int _cost) : x(_x), y(_y), cost(_cost) {}
};

inline bool operator<(const pozitie &a, const pozitie &b) {
    return a.cost > b.cost;
}

void citire() {
    in >> n >> m >> t;
    v.resize(n + 1, vector<int>(m + 1));
    for (int i = 1; i <= n; i++)
        for (int j = 1; j <= m; j++) {
            in >> v[i][j];
            if (v[i][j] > 0)
                nr_patrate++;
        }
}

void dfs(int x, int y) {
    viz[x][y] = true;
    for (int d = 0; d < 4; d++) {
        int nx = x + dx[d];
        int ny = y + dy[d];
        if (nx >= 1 && nx <= n && ny >= 1 && ny <= m && !viz[nx][ny] &&
            (v[nx][ny] & (1 << d)) == 0) {
            frecventa[v[x][y] + v[nx][ny]]++;
            dfs(nx, ny);
        }
    }
}

void rezolva_cerinta_1() {
    out << nr_patrate << "\n";
}

void rezolva_cerinta_2() {
    for (int i = 1; i <= n; i++)
        for (int j = 1; j <= m; j++)
            if (!viz[i][j]) {
                dfs(i, j);
            }
    for (int i = 1; i <= 15; i++)
        if (frecventa[i] > 0)
            out << i << " " << frecventa[i] << "\n";
}

int maxim;
pozitie poz;

void bfs(int x, int y, int cost) {
    queue<pozitie> q;
    q.push(pozitie(x, y, cost));
    maxim = max(maxim, cost);
    viz[x][y] = true;
    while (!q.empty()) {
        pozitie p = q.front();
        q.pop();
        for (int d = 0; d < 4; d++) {
            int nx = p.x + dx[d];
            int ny = p.y + dy[d];
            if (nx >= 1 && nx <= n && ny >= 1 && ny <= m && !viz[nx][ny]) {
                if ((v[nx][ny] & (1 << d)) == 0) {
                    q.push(pozitie(nx, ny, p.cost + 1));
                    viz[nx][ny] = true;
                    if (p.cost + 1 > maxim) {
                        maxim = p.cost + 1;
                        poz.x = nx;
                        poz.y = ny;
                    }
                } else {
                    q.push(pozitie(nx, ny, p.cost));
                    viz[nx][ny] = true;
                }
            }
        }
    }
}

void rezolva_cerinta_3() {
    for (int i = 1; i <= n; i++)
        for (int j = 1; j <= m; j++)
            if (v[i][j] == 0) {
                memset(viz, false, sizeof(viz));
                bfs(i, j, 0);
            }
    out << maxim << "\n";
    if (maxim == nr_patrate)
        out << "0 0 NU\n";
    else {
        out << poz.x << " " << poz.y << " ";
        if (v[poz.x][poz.y] & 1)
            out << "SUS\n";
        else if (v[poz.x][poz.y] & 2)
            out << "DREAPTA\n";
        else if (v[poz.x][poz.y] & 4)
            out << "JOS\n";
        else
            out << "STANGA\n";
    }
}

int main() {
    citire();
    if (t == 1)
        rezolva_cerinta_1();
    else if (t == 2)
        rezolva_cerinta_2();
    else
        rezolva_cerinta_3();
    return 0;
}
