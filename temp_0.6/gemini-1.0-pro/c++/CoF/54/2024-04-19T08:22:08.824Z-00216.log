#include <fstream>
#include <queue>
#include <vector>
using namespace std;

struct Edge {
  int to, w, t;
};

ifstream in("lanterna.in");
ofstream out("lanterna.out");

const int NMAX = 51, MMAX = 2501, KMAX = 1001;
const int INF = 0x3f3f3f3f;
int n, k, dist[NMAX][KMAX];
bool viz[NMAX][KMAX], base[NMAX];
vector<Edge> graph[NMAX];

void dijkstra(int start) {
  priority_queue<pair<int, pair<int, int>>> pq;
  pq.push({0, {start, k}});
  while (!pq.empty()) {
    int cost = -pq.top().first, node = pq.top().second.first,
        lantern = pq.top().second.second;
    pq.pop();
    if (viz[node][lantern]) continue;
    viz[node][lantern] = true;
    dist[node][lantern] = cost;
    for (Edge e : graph[node]) {
      int newCost = cost + e.t;
      if (newCost <= e.w) pq.push({-newCost, {e.to, lantern}});
      if (base[e.to] && newCost + e.t <= k) pq.push({-newCost, {e.to, k}});
    }
  }
}

int main() {
  in >> n >> k;
  for (int i = 1; i <= n; i++) {
    int x;
    in >> x;
    base[i] = x;
  }
  int m;
  in >> m;
  while (m--) {
    int a, b, t, w;
    in >> a >> b >> t >> w;
    graph[a].push_back({b, w, t});
    graph[b].push_back({a, w, t});
  }
  for (int i = 1; i <= n; i++)
    for (int j = 0; j <= k; j++) dist[i][j] = INF;
  dijkstra(1);
  int ans = INF, best = -1;
  for (int i = 0; i <= k; i++) {
    if (dist[n][i] < ans) {
      ans = dist[n][i];
      best = i;
    }
  }
  out << ans << ' ' << best << '\n';
  return 0;
}
