            } else if (new_time + 2 * edge.weight < dist[edge.to][0][lantern]) {
                dist[edge.to][0][lantern] = new_time + 2 * edge.weight;
                pq.push({edge.to, 0, new_time + 2 * edge.weight});
            } else if (new_time + edge.weight < dist[node][k][lantern]) {
                dist[node][k][lantern] = new_time + edge.weight;
                pq.push({node, k, new_time + edge.weight});
            }
        }
    }

    out << -1 << endl;
    return 0;
}
