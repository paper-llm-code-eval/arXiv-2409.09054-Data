#include <fstream>
#include <vector>
#include <algorithm>
#include <array>
#include <utility>

using namespace std;

int main() {
    ifstream in("sudest.in");
    ofstream out("sudest.out");
    int n, k;
    in >> n >> k;
    vector<vector<int>> grid(n, vector<int>(n));
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            in >> grid[i][j];
        }
    }
    vector<int> steps(k);
    for (int i = 0; i < k; i++) {
        in >> steps[i];
    }

    vector<vector<array<int, 3>>> dp(n, vector<array<int, 3>>(n));
    dp[0][0][0] = grid[0][0];
    dp[0][0][1] = 0;
    dp[0][0][2] = 0;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            for (int s = 0; s < k; s++) {
                int ni, nj;
                if (s < k - 1) {
                    ni = i + steps[s];
                    nj = j + steps[s + 1];
                } else {
                    ni = n - 1;
                    nj = n - 1;
                }
                if (ni < n && nj < n) {
                    dp[ni][nj][0] = max(dp[ni][nj][0], dp[i][j][0] + grid[ni][nj]);
                    dp[ni][nj][1] = max(dp[ni][nj][1], dp[i][j][0] + grid[ni][nj]);
                    dp[ni][nj][2] = max(dp[ni][nj][2], dp[i][j][2] + grid[ni][nj]);
                }
                if (i + steps[s] < n) {
                    dp[i + steps[s]][j][0] = max(dp[i + steps[s]][j][0], dp[i][j][1] + grid[i + steps[s]][j]);
                    dp[i + steps[s]][j][2] = max(dp[i + steps[s]][j][2], dp[i][j][1] + grid[i + steps[s]][j]);
                }
                if (j + steps[s] < n) {
                    dp[i][j + steps[s]][0] = max(dp[i][j + steps[s]][0], dp[i][j][2] + grid[i][j + steps[s]]);
                    dp[i][j + steps[s]][1] = max(dp[i][j + steps[s]][1], dp[i][j][2] + grid[i][j + steps[s]]);
                }
            }
        }
    }

    int max_value = 0;
    int end_i = -1;
    int end_j = -1;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            if (dp[i][j][0] > max_value) {
                max_value = dp[i][j][0];
                end_i = i;
                end_j = j;
            }
        }
    }

    out << max_value << endl;
    vector<pair<int, int>> path;
    while (end_i != 0 || end_j != 0) {
        path.push_back(make_pair(end_i + 1, end_j + 1));
        if (dp[end_i][end_j][1] == dp[end_i][end_j][0]) {
            end_i -= steps[k - 1];
            end_j -= steps[k];
        } else if (dp[end_i][end_j][2] == dp[end_i][end_j][0]) {
            end_i -= steps[k];
            end_j -= steps[k - 1];
        } else {
            end_i -= steps[k - 1];
            end_j -= steps[k];
        }
    }
    path.push_back(make_pair(1, 1));
    reverse(path.begin(), path.end());
    for (const auto& p : path) {
        out << p.first << " " << p.second << endl;
    }

    return 0;
}
