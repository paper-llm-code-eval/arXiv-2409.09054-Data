#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAX_N = 100;
const int MAX_K = 2 * MAX_N - 2;
const int MAX_C = 10;

int n, k;
int grid[MAX_N][MAX_N];
int costs[MAX_K];
int dir[MAX_K];
int dp[MAX_K][2];
int path[MAX_K][2];

int main() {
    ifstream in("sudest.in");
    ofstream out("sudest.out");

    in >> n;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            in >> grid[i][j];
        }
    }

    in >> k;
    for (int i = 0; i < k; i++) {
        in >> costs[i];
    }

    for (int i = 0; i < k; i++) {
        dir[i] = (i % 2 == 0) ? 1 : 0;
    }

    dp[0][0] = grid[0][0];
    dp[0][1] = grid[0][0];
    for (int i = 1; i < k; i++) {
        if (dir[i] == 1) {
            dp[i][0] = max(dp[i - 1][0], dp[i - 1][1] + grid[i][i]);
            dp[i][1] = dp[i - 1][1] + grid[i][i];
        } else {
            dp[i][0] = dp[i - 1][0] + grid[i][i];
            dp[i][1] = max(dp[i - 1][1], dp[i - 1][0] + grid[i][i]);
        }
    }

    int max_cost = max(dp[k - 1][0], dp[k - 1][1]);
    int cur_dir = (dp[k - 1][0] > dp[k - 1][1]) ? 0 : 1;
    int cur_cost = max_cost;

    for (int i = k - 1; i >= 0; i--) {
        path[i][0] = i + 1;
        path[i][1] = (dir[i] == 1) ? i + 1 : i;
        if (dir[i] == cur_dir && costs[i] <= cur_cost) {
            cur_cost -= costs[i];
        } else {
            cur_dir = 1 - cur_dir;
            cur_cost -= costs[i];
        }
    }

    out << max_cost << endl;
    for (int i = 0; i < k + 1; i++) {
        out << path[i][0] << " " << path[i][1] << endl;
    }

    return 0;
}
