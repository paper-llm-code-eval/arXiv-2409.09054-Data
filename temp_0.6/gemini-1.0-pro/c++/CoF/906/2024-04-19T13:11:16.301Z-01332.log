#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream fin("aur.in");
    ofstream fout("aur.out");

    int c, n, k, p;
    fin >> c >> n;
    if (c == 1) {
        vector<int> v(n);
        for (int i = 0; i < n; i++) {
            fin >> v[i];
        }
        int ans = 0;
        for (int i = 0; i < n; i++) {
            int x = v[i];
            while (x > 0) {
                x /= 10;
                ans++;
            }
        }
        fout << ans << "\n";
    } else if (c == 2) {
        fin >> k;
        vector<int> v(n);
        for (int i = 0; i < n; i++) {
            fin >> v[i];
        }
        int ans = 0;
        for (int i = 0; i < n; i++) {
            int x = v[i];
            while (x > 0) {
                x /= 10;
                ans++;
            }
            if (ans >= k) {
                x = v[i];
                for (int j = 0; j < k - ans; j++) {
                    x /= 10;
                }
                fout << x % 10 << "\n";
                break;
            }
        }
    } else if (c == 3) {
        fin >> p;
        vector<int> v(n);
        for (int i = 0; i < n; i++) {
            fin >> v[i];
        }
        vector<int> digits;
        for (int i = 0; i < n; i++) {
            int x = v[i];
            while (x > 0) {
                digits.push_back(x % 10);
                x /= 10;
            }
        }
        sort(digits.rbegin(), digits.rend());
        int ans = 0;
        for (int i = 0; i < min(p, (int)digits.size()); i++) {
            ans = ans * 10 + digits[i];
        }
        fout << ans << "\n";
    }

    return 0;
}
