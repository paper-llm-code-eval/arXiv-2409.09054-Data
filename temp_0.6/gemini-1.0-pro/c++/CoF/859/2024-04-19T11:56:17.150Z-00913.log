                    queue<pair<int, int>> q;
                    q.push(make_pair(i, j));
                    viz[i][j] = 1;
                    comp[i][j] = nr_comp;
                    while (!q.empty()) {
                        int x = q.front().first;
                        int y = q.front().second;
                        q.pop();
                        for (int k = 0; k < 4; k++) {
