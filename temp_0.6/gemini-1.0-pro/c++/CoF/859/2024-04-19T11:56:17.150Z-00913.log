                            int ny = y + dy[k];
                            if (nx >= 1 && nx <= n && ny >= 1 && ny <= n && !viz[nx][ny] && mat[nx][ny] < mat[x][y]) {
                                q.push(make_pair(nx, ny));
                                viz[nx][ny] = 1;
                                comp[nx][ny] = nr_comp;
                            }
                        }
                    }
                }
            }
        }
        vector<int> sz(nr_comp + 1, 0);
        for (int i = 1; i <= n; i++) {
            for (int j = 1; j <= n; j++) {
                sz[comp[i][j]]++;
            }
        }
        ans = 0;
        for (int i = 1; i <= nr_comp; i++) {
            if (sz[i] > 1) {
                ans++;
            }
        }
        fout << ans << '\n';
    }
    return 0;
}
