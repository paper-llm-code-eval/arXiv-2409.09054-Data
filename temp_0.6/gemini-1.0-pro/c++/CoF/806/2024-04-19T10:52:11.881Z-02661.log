#include <fstream>
#include <vector>
#include <algorithm>
#include <numeric>

using namespace std;

struct Node {
    int x, y, sum, cnt;
    Node() : x(0), y(0), sum(0), cnt(0) {}
    Node(int x, int y, int sum, int cnt) : x(x), y(y), sum(sum), cnt(cnt) {}
};

vector<vector<int>> grid;
vector<vector<Node>> dp;
int n, m, x, y;

int main() {
    ifstream in("cri.in");
    in >> n >> m >> x >> y;
    grid.resize(n + 1, vector<int>(m + 1, 0));
    dp.resize(n + 1, vector<Node>(m + 1));
    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= m; ++j) {
            in >> grid[i][j];
        }
    }
    in.close();
    for (int i = n; i >= 1; --i) {
        for (int j = m; j >= 1; --j) {
            int sum1 = dp[i + 1][j].sum + grid[i][j];
            int cnt1 = dp[i + 1][j].cnt + 1;
            int sum2 = dp[i][j + 1].sum + grid[i][j];
            int cnt2 = dp[i][j + 1].cnt + 1;
            if (sum1 > sum2 || (sum1 == sum2 && cnt1 < cnt2)) {
                dp[i][j] = Node(i + 1, j, sum1, cnt1);
            } else {
                dp[i][j] = Node(i, j + 1, sum2, cnt2);
            }
        }
    }
    vector<Node> path;
    int z = 1, t = dp[1][1].sum, k = dp[1][1].cnt;
    for (int i = 1, j = 1; i <= n && j <= m;) {
        path.push_back(Node(i, j, grid[i][j], 0));
        if (i == x && j == y) {
            z = 2;
            t -= grid[i][j];
        }
        int i1 = dp[i + 1][j].x;
        int j1 = dp[i + 1][j].y;
        int i2 = dp[i][j + 1].x;
        int j2 = dp[i][j + 1].y;
        if (i1 == i && j1 == j) {
            i = i2;
            j = j2;
        } else {
            i = i1;
            j = j1;
        }
    }
    if (path[0].x == x && path[0].y == y) {
        z = 3;
        t -= grid[x][y];
    }
    if (path[path.size() - 1].x == x && path[path.size() - 1].y == y) {
        z = 4;
        t -= grid[x][y];
    }
    reverse(path.begin(), path.end());
    ofstream out("cri.out");
    out << z << ' ' << t << ' ' << k << '\n';
    for (auto& node : path) {
        out << node.x << ' ' << node.y << '\n';
    }
    out.close();
    return 0;
}
