    return a.start < b.start;
  });
  vector<int> dp(n + 1, 0);
  dp[0] = 1;
  for (int i = 0; i < n; i++) {
    if (players[i].stg) {
      dp[i + 1] = (dp[i + 1] + dp[i]) % MOD;
    }
    for (int j = i + 1; j < n; j++) {
      if (players[j].stg != players[i].stg && players[j].start <= players[i].end) {
        dp[j + 1] = (dp[j + 1] + dp[i]) % MOD;
      }
    }
  }
  fout << dp[n] << '\n';
  return 0;
}
