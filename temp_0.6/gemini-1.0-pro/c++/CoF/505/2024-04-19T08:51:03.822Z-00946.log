ifstream fin("fotbal.in");
ofstream fout("fotbal.out");

const int MOD = 1e9 + 9;

struct player {
  int start, end, stg;
};

int main() {
  int n, k;
  fin >> n >> k;
  vector<player> players(n);
  for (int i = 0; i < n; i++) {
    fin >> players[i].start >> players[i].end >> players[i].stg;
