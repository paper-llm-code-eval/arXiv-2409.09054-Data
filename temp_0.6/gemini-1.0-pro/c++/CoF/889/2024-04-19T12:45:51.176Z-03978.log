#include <fstream>
#include <vector>
#include <algorithm>
using namespace std;
ifstream fin("sir.in");
ofstream fout("sir.out");
const int mod = 20173333;
int n, u, x, p;
long long comb[100005], invcomb[100005];
vector<int> fact;
int powmod(int b, int e) {
  int ans = 1;
  while (e) {
    if (e % 2) ans = 1LL * ans * b % mod;
    b = 1LL * b * b % mod;
    e /= 2;
  }
  return ans;
}
int inv(int a) { return powmod(a, mod - 2); }
long long combmod(int n, int k) {
  if (k > n) return 0;
  return 1LL * comb[n] * invcomb[k] % mod * invcomb[n - k] % mod;
}
int main() {
  comb[0] = 1;
  for (int i = 1; i <= 100000; i++) comb[i] = 1LL * comb[i - 1] * i % mod;
  invcomb[100000] = inv(comb[100000]);
  for (int i = 99999; i >= 0; i--) invcomb[i] = 1LL * invcomb[i + 1] * (i + 1) % mod;
  fin >> p >> n >> x;
  if (p == 1) {
    fout << combmod(n - 1, x - 1) << "\n";
    return 0;
  }
  fout << combmod(n + x - 1, x - 1) << "\n";
  return 0;
}
