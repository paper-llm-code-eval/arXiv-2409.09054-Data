#include <fstream>
#include <vector>

using namespace std;

ifstream in("sir.in");
ofstream out("sir.out");

const int MOD = 20173333;

int n, x;
vector<int> dp[100001][101];

int main() {
    in >> n >> x;
    dp[0][0][0] = 1;
    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= x; ++j) {
            for (int k = 0; k <= n; ++k) {
                dp[i][j][k] = (dp[i - 1][j - 1][k] + dp[i - 1][j][k] + dp[i - 1][j][k - 1]) % MOD;
            }
        }
    }
    out << dp[n][x][n] << "\n";
    return 0;
}
