#include <iostream>
#include <vector>

using namespace std;

int main() {
    ifstream in("zona.in");
    ofstream out("zona.out");

    int x, y, n, l;
    in >> x >> y >> n >> l;

    vector<int> v(l);
    for (int i = 0; i < l; i++) {
        in >> v[i];
    }

    int cx = x, cy = y, cnt = 0;
    vector<vector<int>> vis(n, vector<int>(n));
    vis[x][y] = 1;

    while (true) {
        int nx = cx, ny = cy;
        switch (v[cnt % l]) {
            case 1:
                nx--;
                break;
            case 2:
                ny++;
                break;
            case 3:
                nx++;
                break;
            case 4:
                ny--;
                break;
        }

        if (vis[nx][ny] != 0) {
            out << cnt % l << endl;
            out << vis[nx][ny] - 1 << endl;
            return 0;
        }

        vis[nx][ny] = ++cnt;
        cx = nx;
        cy = ny;
    }

    return 0;
}
