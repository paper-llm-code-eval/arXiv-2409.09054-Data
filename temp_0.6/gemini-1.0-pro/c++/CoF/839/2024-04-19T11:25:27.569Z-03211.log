#include <fstream>
using namespace std;

ifstream in("zona.in");
ofstream out("zona.out");

int N, X, Y, L, a, b, x, y, dx[] = {0, 0, 1, 0, -1}, dy[] = {0, 1, 0, -1, 0}, viz[51][51];

void citire() {
    in >> X >> Y >> N >> L;
    for (int i = 1; i <= L; ++i) {
        in >> a;
        x += dx[a];
        y += dy[a];
        viz[x][y]++;
    }
}

int main() {
    citire();
    while (!viz[x][y]) {
        x += dx[a];
        y += dy[a];
        b++;
    }
    out << b << '\n' << (b - 1) * (b - 1);
    return 0;
}
