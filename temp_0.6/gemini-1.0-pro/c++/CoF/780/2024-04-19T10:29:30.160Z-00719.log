using namespace std;

const int NMAX = 100;

int n, m, p;
int a[NMAX + 5], b[NMAX + 5], c[NMAX + 5], d[NMAX + 5];
vector<int> G[8005];
int par[8005];

int find(int x) {
  if (par[x] == x) {
    return x;
  }
  return par[x] = find(par[x]);
}

