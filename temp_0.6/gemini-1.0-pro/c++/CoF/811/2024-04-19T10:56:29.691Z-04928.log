#include <fstream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    ifstream in("grupe.in");
    ofstream out("grupe.out");
    int m, n;
    in >> m >> n;
    vector<vector<int>> a(m, vector<int>(n));
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            in >> a[i][j];
        }
    }
    vector<pair<int, vector<int>>> groups;
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            int count = 0;
            for (int k = 1; k * k <= a[i][j]; k++) {
                if (a[i][j] % k == 0) {
                    count++;
                    if (a[i][j] / k != k) {
                        count++;
                    }
                }
            }
            groups.push_back({count, {a[i][j], i, j}});
        }
    }
    sort(groups.begin(), groups.end(), greater<pair<int, vector<int>>>());
    vector<vector<int>> groupA, groupB;
    for (int i = 0; i < groups.size(); i++) {
        if (groupA.empty() || groupA.back()[0] == groups[i].first) {
            groupA.push_back(groups[i].second);
        } else if (groupB.empty() || groupB.back()[0] == groups[i].first) {
            groupB.push_back(groups[i].second);
        } else {
            break;
        }
    }
    int divA = groupA.back()[0], cntA = groupA.size(), maxA = groupA[0][1];
    int divB = groupB.empty() ? 0 : groupB.back()[0], cntB = groupB.size(), maxB = groupB.empty() ? 0 : groupB[0][1];
    out << divA << ' ' << cntA << ' ' << maxA << '\n';
    out << divB << ' ' << cntB << ' ' << maxB << '\n';
    return 0;
}
