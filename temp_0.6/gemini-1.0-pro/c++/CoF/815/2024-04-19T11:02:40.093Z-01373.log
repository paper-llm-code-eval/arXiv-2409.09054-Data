#include <fstream>
#include <iostream>
#include <queue>

const int dx[] = {0, 0, 1, -1};
const int dy[] = {1, -1, 0, 0};

using namespace std;

struct node {
    int x, y, dist;
    node() {}
    node(int _x, int _y, int _dist) : x(_x), y(_y), dist(_dist) {}
    bool operator<(const node &other) const {
        return dist > other.dist;
    }
};

int n, k, t1, t2, s1, s2, s3, s4, r1, r2, r3, r4, ans1, ans2;
vector<vector<bool>> grid, visited;
vector<vector<int>> dist;
vector<pair<int, int>> obstacles;

void dijkstra(int x, int y) {
    priority_queue<node> q;
    q.push(node(x, y, 0));
    dist[x][y] = 0;
    while (!q.empty()) {
        node current = q.top();
        q.pop();
        if (current.dist > dist[current.x][current.y]) {
            continue;
        }
        for (int i = 0; i < 4; i++) {
            int nx = current.x + dx[i];
            int ny = current.y + dy[i];
            if (nx >= 1 && nx <= n && ny >= 1 && ny <= n && !grid[nx][ny] && dist[nx][ny] > current.dist + 1) {
                dist[nx][ny] = current.dist + 1;
                q.push(node(nx, ny, dist[nx][ny]));
            }
        }
    }
}

int main() {
    ifstream fin("ai.in");
    ofstream fout("ai.out");
    fin >> n;
    grid.resize(n + 1, vector<bool>(n + 1, false));
    visited.resize(n + 1, vector<bool>(n + 1, false));
    dist.resize(n + 1, vector<int>(n + 1, INT_MAX));
    fin >> t1 >> t2 >> s1 >> s2 >> s3 >> s4 >> r1 >> r2 >> r3 >> r4;
    fin >> k;
    for (int i = 0; i < k; i++) {
        int x, y;
        fin >> x >> y;
        grid[x][y] = true;
        obstacles.push_back({x, y});
    }
    dijkstra(t1, t2);
    for (auto &obstacle : obstacles) {
        if (dist[obstacle.first][obstacle.second] != INT_MAX) {
            ans1 = max(ans1, dist[obstacle.first][obstacle.second]);
        }
    }
    visited[r1][r2] = true;
    visited[r3][r4] = true;
    queue<pair<int, int>> q;
    q.push({r1, r2});
    q.push({r3, r4});
    while (!q.empty()) {
        int x = q.front().first;
        int y = q.front().second;
        q.pop();
        if (x == t1 && y == t2) {
            ans2 = dist[x][y];
            break;
        }
        for (int i = 0; i < 4; i++) {
            int nx = x + dx[i];
            int ny = y + dy[i];
            if (nx >= 1 && nx <= n && ny >= 1 && ny <= n && !grid[nx][ny] && !visited[nx][ny]) {
                visited[nx][ny] = true;
                dist[nx][ny] = dist[x][y] + 1;
                q.push({nx, ny});
            }
        }
    }
    fout << ans1 << '\n';
    if (ans2 != INT_MAX) {
        fout << ans2 << '\n';
    }
    return 0;
}
