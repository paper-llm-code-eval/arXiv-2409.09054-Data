#include <iostream>
#include <vector>
#include <queue>
#include <algorithm>

using namespace std;

const int INF = 1e9;

int n, k;
vector<vector<pair<int, int>>> graph;
vector<int> dist;

void dijkstra(int start) {
    dist.assign(n + 1, INF);
    priority_queue<pair<int, int>> pq;
    pq.push({0, start});
    dist[start] = 0;
    while (!pq.empty()) {
        int u = pq.top().second;
        pq.pop();
        for (auto& edge : graph[u]) {
            int v = edge.first;
            int w = edge.second;
            if (dist[u] + w < dist[v]) {
                dist[v] = dist[u] + w;
                pq.push({-dist[v], v});
            }
        }
    }
}

int main() {
    ifstream fin("cezar.in");
    ofstream fout("cezar.out");
    fin >> n >> k;
    graph.resize(n + 1);
    for (int i = 1; i < n; i++) {
        int u, v;
        fin >> u >> v;
        graph[u].push_back({v, 1});
        graph[v].push_back({u, 1});
    }
    int min_cost = INF;
    for (int i = 1; i <= n; i++) {
        dijkstra(i);
        vector<int> costs(k + 1, INF);
        costs[0] = 0;
        for (int j = 1; j <= k; j++) {
            for (int u = 1; u <= n; u++) {
                costs[j] = min(costs[j], costs[j - 1] + dist[u]);
            }
        }
        min_cost = min(min_cost, costs[k]);
    }
    fout << min_cost << "\n";
    return 0;
}
