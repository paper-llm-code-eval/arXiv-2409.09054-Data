            max_dist = dist[i];
            node = i;
        }
    }

    for (int i = 1; i <= n; i++) {
        dist[i] = INF;
    }

    q.push(node);
    dist[node] = 0;

    while (!q.empty()) {
        int u = q.front();
        q.pop();

        for (int v : g[u]) {
            if (dist[v] > dist[u] + 1) {
                dist[v] = dist[u] + 1;
                q.push(v);
            }
        }
    }

    max_dist = 0;

    for (int i = 1; i <= n; i++) {
        max_dist = max(max_dist, dist[i]);
    }

    int ans = n - 1 - max_dist + k - 3;

    cout << ans << endl;

    return 0;
}
