#include <fstream>
#include <vector>
#include <algorithm>
#include <queue>

using namespace std;

struct Edge {
    int to, cost;
};

struct Dijkstra {
    vector<vector<Edge>> graph;
    vector<int> dist;
    vector<bool> visited;

    Dijkstra(int n) {
        graph.resize(n);
        dist.resize(n, INT_MAX);
        visited.resize(n, false);
    }

    void addEdge(int from, int to, int cost) {
        graph[from].push_back({to, cost});
    }

    int solve(int start, int end) {
        priority_queue<pair<int, int>, vector<pair<int, int>>, greater<pair<int, int>>> pq;
        dist[start] = 0;
        pq.push({0, start});

        while (!pq.empty()) {
            int currDist = pq.top().first;
            int currNode = pq.top().second;
            pq.pop();

            if (currNode == end) {
                return dist[end];
            }

            if (visited[currNode]) {
                continue;
            }

            visited[currNode] = true;

            for (Edge edge : graph[currNode]) {
                int nextDist = currDist + edge.cost;
                if (nextDist < dist[edge.to]) {
                    dist[edge.to] = nextDist;
                    pq.push({nextDist, edge.to});
                }
            }
        }

        return -1;
    }
};

int main() {
    ifstream fin("cezar.in");
    ofstream fout("cezar.out");

    int n, k;
    fin >> n >> k;

    Dijkstra dijkstra(n);

    for (int i = 0; i < n - 1; i++) {
        int a, b;
        fin >> a >> b;
        dijkstra.addEdge(a - 1, b - 1, 1);
        dijkstra.addEdge(b - 1, a - 1, 1);
    }

    int minCost = INT_MAX;
    for (int i = 0; i < n; i++) {
        int totalCost = 0;
        for (int j = 0; j < n; j++) {
            totalCost += dijkstra.solve(i, j);
        }
        minCost = min(minCost, totalCost);
    }

    fout << minCost << endl;

    return 0;
}
