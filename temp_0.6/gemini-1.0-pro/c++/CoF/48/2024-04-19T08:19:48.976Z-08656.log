  dfs2(1, 1, 0);

  int diameter = 0;
  int u = -1;
  int v = -1;
  for (int i = 1; i <= n; i++) {
    if (dist[i] > diameter) {
      diameter = dist[i];
      u = i;
    }
  }

  dfs2(u, u, 0);
  for (int i = 1; i <= n; i++) {
    if (dist[i] > diameter) {
      diameter = dist[i];
      v = i;
    }
  }

  vector<int> path;
  while (u != v) {
    path.push_back(u);
    if (!adj[u].empty()) {
      u = adj[u][0];
    }
    if (u == v) break;
    if (!adj[u].empty()) {
      u = adj[u][0];
    }
  }
  path.push_back(v);

  int center = path[path.size() / 2];

  int ans = 0;
  for (int i = 1; i <= n; i++) {
    ans += dist[i] - dist[center];
  }

  fout << ans << endl;

  return 0;
}
