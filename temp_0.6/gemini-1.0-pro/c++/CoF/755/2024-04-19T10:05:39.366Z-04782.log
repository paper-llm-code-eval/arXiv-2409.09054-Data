#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>

using namespace std;

const int MAXN = 5005;
const int INF = 1e9;

int n, c[MAXN], p[MAXN], a[MAXN], b[MAXN], d[MAXN], dist[MAXN], prv[MAXN];
vector<int> sol[MAXN];

int main() {
    ifstream fin("elfi.in");
    ofstream fout("elfi.out");

    fin >> n;
    for (int i = 1; i <= n; i++) {
        fin >> c[i] >> p[i];
    }
    fin.close();

    for (int i = 1; i <= n; i++) {
        a[i] = c[i] + p[i] + 1;
        b[i] = a[i] + p[i];
    }

    priority_queue<pair<int, int>> pq;
    pq.push({0, 0});
    while (!pq.empty()) {
        int u = pq.top().second;
        int dist_u = -pq.top().first;
        pq.pop();
        if (dist_u > dist[u]) continue;
        for (int i = 1; i <= n; i++) {
            if (u == i) continue;
            int new_dist = max(dist_u + a[u], b[i]);
            if (new_dist < dist[i]) {
                dist[i] = new_dist;
                prv[i] = u;
                pq.push({-new_dist, i});
            }
        }
    }

    int last = 0;
    for (int i = 1; i <= n; i++) {
        if (dist[i] > dist[last]) {
            last = i;
        }
    }

    int cnt = 0;
    while (last != 0) {
        sol[++cnt].push_back(last);
        last = prv[last];
    }

    fout << cnt << '\n';
    for (int i = cnt; i >= 1; i--) {
        for (int j = 0; j < (int)sol[i].size(); j++) {
            fout << sol[i][j] << ' ';
        }
        fout << '\n';
    }
    fout.close();

    return 0;
}
