#include <cstdio>
#include <vector>
#include <queue>
#include <limits>
using namespace std;

int n, m, k, c[16];
vector<pair<int, int>> g[2001];
vector<int> d;

int main() {
    freopen("ubuntzei.in", "r", stdin);
    freopen("ubuntzei.out", "w", stdout);
    scanf("%d%d%d", &n, &m, &k);
    for (int i = 1; i <= k; i++) scanf("%d", c + i);
    for (int i = 0; i < m; i++) {
        int x, y, z;
        scanf("%d%d%d", &x, &y, &z);
        g[x].emplace_back(y, z);
        g[y].emplace_back(x, z);
    }
    d.assign(n + 1, numeric_limits<int>::max());
    d[1] = 0;
    priority_queue<pair<int, int>> q;
    q.emplace(0, 1);
    while (!q.empty()) {
        int u = q.top().second;
        int du = -q.top().first;
        q.pop();
        if (du > d[u]) continue;
        for (auto& [v, w]: g[u]) {
            if (du + w < d[v]) {
                d[v] = du + w;
                q.emplace(-d[v], v);
            }
        }
    }
    int ans = d[n];
    for (int i = 1; i <= k; i++) ans += d[c[i]] * 2;
    printf("%d", ans);
    return 0;
}
