#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream in("startrek.in");
  ofstream out("startrek.out");

  int n, t;
  in >> n >> t;

  vector<int> d(n), v(n);
  for (int i = 0; i < n; i++) { in >> d[i]; }
  for (int i = 0; i < n; i++) { in >> v[i]; }

  // sort distances and velocities in decreasing order
  sort(d.rbegin(), d.rend());
  sort(v.rbegin(), v.rend());

  int p = 0;
  for (int i = 0; i < n; i++) {
    // if the projectile reaches the Enterprise before the laser recharges
    if (d[i] <= v[i] * t) {
      p++;
    } else {
      break;
    }
  }

  out << p << endl;

  return 0;
}
```
