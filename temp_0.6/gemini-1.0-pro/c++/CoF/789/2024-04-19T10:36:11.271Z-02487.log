#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAXN = 4005;

struct Proiectil
{
    int distanta, viteza, moment_distrugere;
};

bool cmp(const Proiectil &a, const Proiectil &b)
{
    return a.moment_distrugere < b.moment_distrugere;
}

int main()
{
    ifstream fin("startrek.in");
    ofstream fout("startrek.out");

    int n, t;
    fin >> n >> t;

    vector<Proiectil> proiectile(n);
    for (int i = 0; i < n; i++)
    {
        fin >> proiectile[i].distanta;
    }

    for (int i = 0; i < n; i++)
    {
        fin >> proiectile[i].viteza;
        proiectile[i].moment_distrugere = (proiectile[i].distanta + t) / proiectile[i].viteza;
    }

    sort(proiectile.begin(), proiectile.end(), cmp);

    int numar_proiectile_distruse = 0;
    int timp_curent = 0;

    for (int i = 0; i < n; i++)
    {
        if (proiectile[i].moment_distrugere >= timp_curent)
        {
            numar_proiectile_distruse++;
            timp_curent = proiectile[i].moment_distrugere + t;
        }
    }

    fout << numar_proiectile_distruse << '\n';

    return 0;
}
