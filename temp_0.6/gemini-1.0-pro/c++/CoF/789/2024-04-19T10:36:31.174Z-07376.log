#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>

using namespace std;

int main() {
  ifstream fin("startrek.in");
  ofstream fout("startrek.out");

  int n, t;
  fin >> n >> t;

  vector<int> dist(n), viteza(n);
  for (int i = 0; i < n; i++) {
    fin >> dist[i];
  }
  for (int i = 0; i < n; i++) {
    fin >> viteza[i];
  }

  priority_queue<pair<double, int>> pq;  // {timp_atingere, nava}
  for (int i = 0; i < n; i++) {
    double timp_atingere = (double)dist[i] / viteza[i];
    pq.push({timp_atingere, i});
  }

  int numar_distruse = 0;
  double timp_curent = 0;
  while (!pq.empty()) {
    double timp_atingere = pq.top().first;
    int nava = pq.top().second;
    pq.pop();

    if (timp_atingere >= timp_curent) {
      numar_distruse++;
      timp_curent = timp_atingere + t;
    }
  }

  fout << numar_distruse << '\n';

  return 0;
}
