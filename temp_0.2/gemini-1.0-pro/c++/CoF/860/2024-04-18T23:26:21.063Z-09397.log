  ifstream in("dominant.in");
  ofstream out("dominant.out");
  int v;
  in >> v;
  string s;
  in >> s;
  int n = s.size();
  vector<int> pref(n);
  pref[0] = (s[0] == '1');
  for (int i = 1; i < n; i++) {
    pref[i] = pref[i - 1] + (s[i] == '1');
  }
  int ans = 0;
  int cnt = 0;
  for (int i = 0; i < n; i++) {
    int j = i;
    while (j < n && pref[j] > pref[i] - (j - i + 1) / 2) {
      j++;
    }
    if (j - i > ans) {
      ans = j - i;
      cnt = 1;
    } else if (j - i == ans) {
      cnt++;
    }
    i = j;
  }
  if (v == 1) {
    out << ans << endl;
  } else {
    out << cnt << endl;
  }
  return 0;
}
