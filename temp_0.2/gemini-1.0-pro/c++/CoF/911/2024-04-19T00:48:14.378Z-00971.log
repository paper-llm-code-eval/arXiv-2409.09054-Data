#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("valutar.in");
ofstream fout("valutar.out");

const int MAX_N = 10005;
const int MAX_M = 105;

int a, b, nr0, n, m, l, e, x;
int nr_mutari[MAX_M];
int nr_jucatori;
int poz[MAX_M];
int bani[MAX_M][2];
int culoare[MAX_N];
int cost_cumparare[MAX_N];
int cost_vanzare[MAX_N];

void citire() {
    int cerinta;
    fin >> cerinta;
    fin >> a >> b >> nr0;
    fin >> n >> m >> l >> e >> x;
    for (int i = 1; i <= n; ++i) {
        char c;
        fin >> c >> cost_cumparare[i] >> cost_vanzare[i];
        culoare[i] = c;
    }
}

void initializare() {
    nr_jucatori = m;
    for (int i = 1; i <= m; ++i) {
        poz[i] = 1;
        bani[i][0] = l;
        bani[i][1] = e;
    }
}

int genereaza_nr(int nr_anterior) {
    return (a * nr_anterior + b) % n + 1;
}

void simulare() {
    for (int mutare = 1; mutare <= x; ++mutare) {
        for (int jucator = 1; jucator <= m; ++jucator) {
            if (nr_mutari[jucator] == 0) {
                continue;
            }
            int nr = genereaza_nr(nr_mutari[jucator]);
            nr_mutari[jucator] = nr;
            poz[jucator] = (poz[jucator] + nr - 1) % n + 1;
            switch (culoare[poz[jucator]]) {
                case 'A':
                    break;
                case 'R':
                    bani[jucator][1]++;
                    break;
                case 'G':
                    if (bani[jucator][0] < cost_cumparare[poz[jucator]]) {
                        if (bani[jucator][1] > 0) {
                            bani[jucator][1]--;
                        } else {
                            nr_mutari[jucator] = 0;
                            nr_jucatori--;
                        }
                    } else {
                        bani[jucator][0] -= cost_cumparare[poz[jucator]];
                        bani[jucator][1]++;
                    }
                    break;
                case 'V':
                    if (bani[jucator][1] < cost_vanzare[poz[jucator]]) {
                        if (bani[jucator][0] > 0) {
                            bani[jucator][0] -= cost_vanzare[poz[jucator]];
                        } else {
                            nr_mutari[jucator] = 0;
                            nr_jucatori--;
                        }
                    } else {
                        bani[jucator][0] += cost_vanzare[poz[jucator]];
                        bani[jucator][1]--;
                    }
                    break;
            }
        }
    }
}

void afisare_cerinta1() {
    fout << nr_jucatori << '\n';
}

void afisare_cerinta2() {
    int castigator = 0;
    int max_euro = 0;
    for (int i = 1; i <= m; ++i) {
        if (nr_mutari[i] > 0 && bani[i][1] > max_euro) {
            castigator = i;
            max_euro = bani[i][1];
        }
    }
    fout << castigator << '\n';
}

int main() {
    citire();
    initializare();
    simulare();
    if (x == 1) {
        afisare_cerinta1();
    } else {
        afisare_cerinta2();
    }
    return 0;
}
