#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

struct Interval {
    int start, end;
};

bool compareIntervals(const Interval& a, const Interval& b) {
    return a.end < b.end;
}

int main() {
    ifstream fin("cursuri.in");
    ofstream fout("cursuri.out");

    int C;
    fin >> C;

    int N, K;
    fin >> N >> K;

    vector<Interval> intervals(N);
    for (int i = 0; i < N; i++) {
        fin >> intervals[i].start >> intervals[i].end;
    }

    if (C == 1) {
        sort(intervals.begin(), intervals.end(), compareIntervals);

        int count = 0;
        int lastEnd = -1;
        for (const Interval& interval : intervals) {
            if (interval.start >= lastEnd) {
                count++;
                lastEnd = interval.end;
            }
        }

        fout << count << endl;
    } else if (C == 2) {
        int maxDuration = 0;
        for (const Interval& interval : intervals) {
            maxDuration = max(maxDuration, interval.end - interval.start);
        }

        int left = 0;
        int right = maxDuration;
        while (left <= right) {
            int mid = (left + right) / 2;

            int count = 0;
            int lastEnd = -1;
            for (const Interval& interval : intervals) {
                if (interval.start + mid <= lastEnd) {
                    continue;
                }

                count++;
                lastEnd = interval.start + mid;
            }

            if (count <= K) {
                left = mid + 1;
            } else {
                right = mid - 1;
            }
        }

        fout << right << endl;
    }

    return 0;
}
