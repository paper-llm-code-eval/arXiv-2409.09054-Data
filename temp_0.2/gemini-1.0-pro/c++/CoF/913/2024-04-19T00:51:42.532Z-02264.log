#include <fstream>
#include <vector>

using namespace std;

ifstream fin("paralele.in");
ofstream fout("paralele.out");

const int maxn = 40005;

int n, m, t, ans;
vector<int> a[maxn];
int cnt[maxn];

int main() {
  fin >> t >> n >> m;
  for (int i = 1; i <= n; ++i) {
    for (int j = 1; j <= m; ++j) {
      int x;
      fin >> x;
      a[i].push_back(x);
    }
  }
  if (t == 1) {
    for (int i = 1; i <= n; ++i) {
      for (int j = 1; j <= m; ++j) {
        if (a[i][j] == 1) {
          ++cnt[i];
          if (j > 1 && a[i][j - 1] == 1) {
            ans -= cnt[i] - 1;
          }
        }
      }
      ans += cnt[i] * (cnt[i] - 1) / 2;
    }
  } else {
    for (int i = 1; i <= n; ++i) {
      for (int j = 1; j <= m; ++j) {
        if (a[i][j] == 1) {
          ++cnt[j];
          if (i > 1 && a[i - 1][j] == 1) {
            ans -= cnt[j] - 1;
          }
        }
      }
      ans += cnt[j] * (cnt[j] - 1) / 2;
    }
  }
  fout << ans << '\n';
  return 0;
}
