        for (int l = 0; l < n; l++) {
          cnt_diag += (grid[l][n - 1 - l - j * (n - 1)] == (i % 2 + 1));
        }
        if (cnt_diag == n) {
          found = true;
          break;
        }
      }
    }

    if (found) {
      if (c == 1) {
        if (i % 2 == 0) {
          out << 1 << " " << 0 << "\n";
        } else {
          out << 0 << " " << 1 << "\n";
        }
        return 0;
      } else {
        out << i * n * n << "\n";
        return 0;
      }
    }
  }

  return 0;
}
