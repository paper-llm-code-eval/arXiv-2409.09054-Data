#include <fstream>
#include <vector>

using namespace std;

const int MAX_N = 100;
const int MAX_K = 25;

int n, k, c;
int grid[MAX_N][MAX_N];
int dx[] = {0, 1, 1, 1, 0, -1, -1, -1};
int dy[] = {1, 1, 0, -1, -1, -1, 0, 1};

int main() {
  ifstream in("joc.in");
  ofstream out("joc.out");

  in >> c >> n >> k;

  for (int i = 0; i < k; i++) {
    int x;
    for (int j = 0; j < n * n; j++) {
      in >> x;
      grid[x / n][x % n] = (i % 2 == 0) ? 1 : 2;
    }

    bool found = false;
    for (int j = 0; j < n; j++) {
      int cnt_row = 0, cnt_col = 0;
      for (int l = 0; l < n; l++) {
        cnt_row += (grid[j][l] == (i % 2 + 1));
        cnt_col += (grid[l][j] == (i % 2 + 1));
      }
      if (cnt_row == n || cnt_col == n) {
        found = true;
        break;
      }
    }

    if (!found) {
      for (int j = 0; j < 2; j++) {
        int cnt_diag = 0;
        for (int l = 0; l < n; l++) {
          cnt_diag += (grid[l][l + j * (n - 1)] == (i % 2 + 1));
        }
        if (cnt_diag == n) {
          found = true;
          break;
        }
      }
    }

    if (!found) {
      for (int j = 0; j < 2; j++) {
        int cnt_diag = 0;
        for (int l = 0; l < n; l++) {
          cnt_diag += (grid[l][n - 1 - l - j * (n - 1)] == (i % 2 + 1));
        }
        if (cnt_diag == n) {
          found = true;
          break;
        }
      }
    }

    if (found) {
      if (c == 1) {
        if (i % 2 == 0) {
          out << 1 << " " << 0 << "\n";
        } else {
          out << 0 << " " << 1 << "\n";
        }
        return 0;
      } else {
        out << i * n * n << "\n";
        return 0;
      }
    }
  }

  return 0;
}
