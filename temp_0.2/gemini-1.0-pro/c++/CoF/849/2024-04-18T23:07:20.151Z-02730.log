#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream fin("tcif.in");
    ofstream fout("tcif.out");

    int A, B, C, c1, c2, c3;
    fin >> A >> B >> C >> c1 >> c2 >> c3;
    string N;
    fin >> N;

    vector<int> cnt(10);
    for (char c : N) {
        cnt[c - '0']++;
    }

    int ans = stoi(N) + 1;
    while (true) {
        string s = to_string(ans);
        fill(cnt.begin(), cnt.end(), 0);
        for (char c : s) {
            cnt[c - '0']++;
        }
        if (cnt[c1] == A && cnt[c2] == B && cnt[c3] == C) {
            break;
        }
        ans++;
    }

    fout << ans << '\n';
    return 0;
}
