#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>

using namespace std;

const int dx[] = {0, 1, 0, -1};
const int dy[] = {1, 0, -1, 0};

int n, m;
vector<vector<int>> grid;
vector<vector<int>> dist;
queue<pair<int, int>> q;

int main() {
    ifstream in("alee.in");
    ofstream out("alee.out");

    in >> n >> m;
    grid.resize(n + 2, vector<int>(n + 2, 1));
    dist.resize(n + 2, vector<int>(n + 2, -1));

    for (int i = 0; i < m; i++) {
        int x, y;
        in >> x >> y;
        grid[x][y] = 0;
    }

    int x1, y1, x2, y2;
    in >> x1 >> y1 >> x2 >> y2;

    q.push({x1, y1});
    dist[x1][y1] = 0;

    while (!q.empty()) {
        int x = q.front().first;
        int y = q.front().second;
        q.pop();

        for (int i = 0; i < 4; i++) {
            int nx = x + dx[i];
            int ny = y + dy[i];

            if (grid[nx][ny] && dist[nx][ny] == -1) {
                dist[nx][ny] = dist[x][y] + 1;
                q.push({nx, ny});
            }
        }
    }

    out << dist[x2][y2] << endl;

    return 0;
}
