#include <fstream>
#include <vector>
#include <queue>
#include <tuple>
#include <algorithm>

using namespace std;

const int MAXN = 175;
const int dx[] = {0, 1, 0, -1};
const int dy[] = {1, 0, -1, 0};

int n, m;
vector<vector<int>> grid(MAXN, vector<int>(MAXN));
vector<vector<int>> dist(MAXN, vector<int>(MAXN, -1));
int sx, sy, ex, ey;

int main() {
    ifstream fin("alee.in");
    ofstream fout("alee.out");

    fin >> n >> m;
    for (int i = 0; i < m; i++) {
        int x, y;
        fin >> x >> y;
        grid[x][y] = -1;
    }
    fin >> sx >> sy >> ex >> ey;

    queue<tuple<int, int, int>> q;
    q.push({sx, sy, 0});
    dist[sx][sy] = 0;

    while (!q.empty()) {
        int x, y, d;
        tie(x, y, d) = q.front();
        q.pop();

        if (x == ex && y == ey) {
            fout << d << endl;
            return 0;
        }

        for (int i = 0; i < 4; i++) {
            int nx = x + dx[i];
            int ny = y + dy[i];
            if (nx < 1 || nx > n || ny < 1 || ny > n || grid[nx][ny] == -1 || dist[nx][ny] != -1) {
                continue;
            }
            dist[nx][ny] = d + 1;
            q.push({nx, ny, d + 1});
        }
    }

    fout << -1 << endl;
    return 0;
}
