#include <algorithm>

using namespace std;

const int MAXN = 10;
const int MAX_DIGITS = 10;
const int MAX_PRIME = 100;

vector<int> primes;
bool is_prime[MAX_PRIME];

void sieve() {
  for (int i = 2; i < MAX_PRIME; i++) {
    if (!is_prime[i]) {
      primes.push_back(i);
      for (int j = i * i; j < MAX_PRIME; j += i) {
        is_prime[j] = true;
      }
    }
  }
}

bool is_valid(int n) {
  if (n < 10) {
    return true;
  }
  int first_digit = n % 10;
  int num_digits = 0;
  while (n > 0) {
    num_digits++;
    n /= 10;
  }
  if (first_digit != num_digits) {
    return false;
  }
  int prev_digit = first_digit;
  n = first_digit;
  while (n > 0) {
    int digit = n % 10;
    if (digit == prev_digit) {
      return false;
    }
    if (!is_prime[digit * 10 + prev_digit]) {
      return false;
    }
    prev_digit = digit;
    n /= 10;
  }
  return true;
}

