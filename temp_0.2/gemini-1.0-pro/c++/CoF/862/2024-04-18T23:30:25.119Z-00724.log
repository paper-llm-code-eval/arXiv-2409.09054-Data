#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("charlie.in");
ofstream fout("charlie.out");

int main() {
    int p;
    fin >> p;
    string s;
    fin >> s;
    int n = s.size();
    vector<int> f(26);
    for (int i = 0; i < 26; i++) {
        f[s[i] - 'a'] = i + 1;
    }
    if (p == 1) {
        int ans = 1;
        for (int i = 1; i < n; i++) {
            if (s[i] < s[i - 1]) {
                ans++;
            } else {
                ans = 1;
            }
        }
        fout << ans << '\n';
    } else {
        vector<int> dp(n, 0);
        dp[0] = f[s[0] - 'a'];
        for (int i = 1; i < n; i++) {
            if (s[i] > s[i - 1]) {
                dp[i] = max(dp[i - 1], f[s[i] - 'a']);
            } else {
                dp[i] = max(dp[i - 1], f[s[i] - 'a'] + dp[i - 2]);
            }
        }
        fout << s;
        for (int i = n - 2; i >= 0; i--) {
            if (s[i] > s[i + 1] && dp[i] == dp[i + 1]) {
                s.erase(s.begin() + i + 1);
            } else if (s[i] < s[i + 1] && dp[i] == dp[i + 2] + f[s[i] - 'a']) {
                s.erase(s.begin() + i + 1);
            }
        }
        fout << '\n' << dp[n - 1] << '\n';
    }
    return 0;
}
