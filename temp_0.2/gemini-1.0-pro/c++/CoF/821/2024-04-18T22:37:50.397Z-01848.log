    vector<int> v(n);
    for (int i = 0; i < n; i++) {
        in >> v[i];
    }
    int mx = *max_element(v.begin(), v.end());
    int mn = *min_element(v.begin(), v.end());
    int ans = 0;
    for (int i = 0; i < n; i++) {
        if (v[i] != mx) {
            ans++;
        }
    }
    out << mx << endl;
    out << ans << endl;
    out << mn << endl;
    out << (ans == 0 ? n : n - ans) << endl;
    return 0;
}
