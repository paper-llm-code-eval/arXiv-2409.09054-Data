#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("dir.in");
ofstream fout("dir.out");

const int MAXN = 1600;
const int MAXL = 255;
char s[MAXN];
vector<string> v;

int main() {
    fin.getline(s, MAXN);
    int n = 0;
    for (int i = 0; s[i]; i++) {
        if (s[i] == '(') {
            int j = i + 1;
            while (s[j] != ')') j++;
            v.push_back(string(s + i + 1, s + j));
            i = j;
        } else if (s[i] == ',') {
            n++;
        }
    }
    n++;
    fout << n << '\n';
    sort(v.begin(), v.end());
    for (int i = 0; i < n; i++) {
        string path;
        int j = 0;
        while (j < v[i].size() && v[i][j] != '(') {
            path += v[i][j];
            j++;
        }
        path += '\\';
        while (j < v[i].size()) {
            if (v[i][j] == '(') {
                int k = j + 1;
                while (v[i][k] != ')') k++;
                path += v[i].substr(j + 1, k - j - 1);
                path += '\\';
                j = k;
            } else {
                j++;
            }
        }
        path.pop_back();
        fout << path << '\n';
    }
    return 0;
}
