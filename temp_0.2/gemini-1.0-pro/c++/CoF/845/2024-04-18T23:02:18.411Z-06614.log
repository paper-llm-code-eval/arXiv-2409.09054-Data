#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("munte.in");
    ofstream out("munte.out");

    int n;
    in >> n;
    vector<int> v(n);
    for (int i = 0; i < n; i++) {
        in >> v[i];
    }

    int nr_munti = 0;
    int nr_total_munti = 0;
    int nr_elemente_finale = n;

    while (true) {
        int i = 0;
        int j = 0;
        int k = 0;
        bool gasit = false;

        while (i < n - 2) {
            if (v[i] < v[i + 1] && v[i + 1] > v[i + 2]) {
                j = i;
                k = i + 1;
                gasit = true;
                break;
            }
            i++;
        }

        if (!gasit) {
            break;
        }

        while (j >= 0 && v[j] < v[j + 1]) {
            j--;
        }

        while (k < n && v[k] > v[k - 1]) {
            k++;
        }

        nr_munti++;
        nr_total_munti++;
        nr_elemente_finale -= k - j - 1;

        for (int l = j + 1; l < k; l++) {
            v.erase(v.begin() + l);
            l--;
            k--;
            n--;
        }
    }

    out << nr_munti << endl;
    out << nr_total_munti << endl;
    out << nr_elemente_finale << endl;

    return 0;
}
