#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream in("ruleta.in");
  ofstream out("ruleta.out");

  int n;
  in >> n;
  vector<int> v(n);
  for (int i = 0; i < n; i++) { in >> v[i]; }

  int r = 0, t = 0;
  while (true) {
    bool changed = false;
    for (int i = 0; i < n; i++) {
      if (v[i] > 0 && v[i] <= v[(i + 1) % n]) {
        v[(i + 1) % n] -= v[i];
        v[i] = 0;
        changed = true;
      }
    }
    if (!changed) {
      for (int i = 0; i < n; i++) {
        if (v[i] > 0) { t++; }
      }
      out << r << " " << t << endl;
      return 0;
    }
    r++;
  }

  return 0;
}
```
