#include <fstream>
#include <vector>

using namespace std;

const int MAXN = 1000;
const int MAXP = 10000;
const int MAXK = 100000;

int N, P, K;
int ducks[MAXP][3];
char commands[MAXK][2];
int dx[] = {0, -1, 0, 1};
int dy[] = {1, 0, -1, 0};

int main() {
  ifstream fin("pelican.in");
  ofstream fout("pelican.out");

  fin >> N >> P >> K;
  for (int i = 0; i < P; i++) {
    fin >> ducks[i][0] >> ducks[i][1] >> ducks[i][2];
    ducks[i][0]--;
    ducks[i][1]--;
    ducks[i][2]--;
  }
  for (int i = 0; i < K; i++) {
    fin >> commands[i][0] >> commands[i][1];
  }

  for (int i = 0; i < K; i++) {
    if (commands[i][0] == 'A') {
      int nr = commands[i][1] - '0';
      for (int j = 0; j < P; j++) {
        ducks[j][0] = (ducks[j][0] + nr * dx[ducks[j][2]]) % N;
        ducks[j][1] = (ducks[j][1] + nr * dy[ducks[j][2]]) % N;
      }
    } else if (commands[i][0] == 'R') {
      int nr = commands[i][1] - '0';
      for (int j = 0; j < P; j++) {
        ducks[j][2] = (ducks[j][2] + nr) % 4;
      }
    } else if (commands[i][0] == 'Z') {
      int nr = commands[i][1] - '0';
      for (int j = 0; j < P; j++) {
        ducks[j][0] = nr / N;
        ducks[j][1] = nr % N;
      }
    }
  }

  for (int i = 0; i < P; i++) {
    fout << ducks[i][0] + 1 << ' ' << ducks[i][1] + 1 << '\n';
  }

  return 0;
}
