#include <fstream>
#include <string>
#include <vector>

using namespace std;

int main() {
  ifstream in("sir.in");
  ofstream out("sir.out");
  int n;
  in >> n;
  vector<string> sir = {"1", "11", "21", "1211"};
  while (sir.size() < n) {
    string last = sir.back();
    string next;
    int cnt = 1;
    for (int i = 0; i < last.size(); i++) {
      if (i + 1 < last.size() && last[i] == last[i + 1]) {
        cnt++;
      } else {
        next += to_string(cnt) + last[i];
        cnt = 1;
      }
    }
    sir.push_back(next);
  }
  out << sir[n - 1] << "\n";
  return 0;
}
