#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("bete.in");
    ofstream out("bete.out");

    int n;
    in >> n;

    vector<int> a(n), b(n);
    for (int i = 0; i < n; i++) {
        in >> a[i];
    }
    for (int i = 0; i < n; i++) {
        in >> b[i];
    }

    // Sort the fragments in descending order
    sort(a.begin(), a.end(), greater<int>());
    sort(b.begin(), b.end(), greater<int>());

    // Find the initial length of the sticks
    int l = 0;
    for (int i = 0; i < n; i++) {
        l += max(a[i], b[i]);
    }

    // Find the length of the longest stick that can be obtained by gluing a fragment from Ana with a fragment from Bogdan
    int k = 0;
    for (int i = 0; i < n; i++) {
        k = max(k, a[i] + b[i]);
    }

    // Find the number of sticks of maximum length that can be obtained by gluing a fragment from Ana with a fragment from Bogdan
    int p = 0;
    for (int i = 0; i < n; i++) {
        if (a[i] + b[i] == k) {
            p++;
        }
    }

    out << l << endl;
    out << k << endl;
    out << p << endl;

    return 0;
}
```
