#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAXN = 1000001;
const int INF = 1e9;

int n, q, c;
vector<int> v;
vector<int> p;
vector<int> dp;

int main() {
    ifstream in("primprim.in");
    ofstream out("primprim.out");

    in >> c >> n;
    v.resize(n + 1);
    p.resize(n + 1);
    dp.resize(MAXN);

    for (int i = 1; i <= n; i++) {
        in >> v[i];
    }

    for (int i = 1; i < MAXN; i++) {
        dp[i] = INF;
    }

    for (int i = 2; i < MAXN; i++) {
        if (dp[i] == INF) {
            for (int j = i + i; j < MAXN; j += i) {
                dp[j] = min(dp[j], i);
            }
        }
    }

    if (c == 1) {
        int sum = 0;
        for (int i = 1; i <= n; i++) {
            sum += dp[v[i]];
        }
        out << sum << endl;
    } else {
        in >> q;
        for (int i = 0; i < q; i++) {
            int x, y, z;
            in >> x >> y >> z;
            v[x] = y;
            int sum = 0;
            for (int j = 1; j <= z; j++) {
                in >> p[j];
                sum += dp[v[p[j]]];
            }
            out << sum << endl;
        }
    }

    return 0;
}
