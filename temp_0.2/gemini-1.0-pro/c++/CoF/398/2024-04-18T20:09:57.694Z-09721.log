#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>

using namespace std;

const int dx[] = {0, 1, 0, -1};
const int dy[] = {1, 0, -1, 0};

int n, m;
vector<vector<int>> a;
vector<vector<bool>> viz;
vector<vector<int>> dist;

int bfs(int x, int y) {
    queue<pair<int, int>> q;
    q.push({x, y});
    viz[x][y] = true;
    dist[x][y] = 0;
    int ans = 0;
    int country = a[x][y];
    while (!q.empty()) {
        auto [x, y] = q.front();
        q.pop();
        ans = max(ans, dist[x][y]);
        for (int i = 0; i < 4; ++i) {
            int nx = x + dx[i];
            int ny = y + dy[i];
            if (nx >= 0 && nx < n && ny >= 0 && ny < m && !viz[nx][ny] && a[nx][ny] == country) {
                q.push({nx, ny});
                viz[nx][ny] = true;
                dist[nx][ny] = dist[x][y] + 1;
            }
        }
    }
    return ans;
}

