      vector<int> current_sequence = {type};
      for (auto& transformation : transformations) {
        bool valid = true;
        for (int j = 0; j < transformation.size(); j++) {
          if (current_sequence.size() + j >= sequence.size() || current_sequence[current_sequence.size() - 1 - j] != transformation[transformation.size() - 1 - j]) {
            valid = false;
            break;
