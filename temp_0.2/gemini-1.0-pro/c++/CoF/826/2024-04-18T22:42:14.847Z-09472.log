#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

struct Segment {
    int x1, y1, x2, y2, nr;
};

struct Laser {
    int x;
};

int main() {
    ifstream in("ozn.in");
    ofstream out("ozn.out");

    int n, k;
    in >> n >> k;

    vector<Segment> segments(n);
    for (int i = 0; i < n; i++) {
        in >> segments[i].x1 >> segments[i].y1 >> segments[i].x2 >> segments[i].y2 >> segments[i].nr;
    }

    vector<Laser> lasers(k);
    for (int i = 0; i < k; i++) {
        in >> lasers[i].x;
    }

    vector<int> ans(k);
    for (int i = 0; i < k; i++) {
        for (int j = 0; j < n; j++) {
            if (lasers[i].x >= min(segments[j].x1, segments[j].x2) && lasers[i].x <= max(segments[j].x1, segments[j].x2)) {
                ans[i] += segments[j].nr;
            }
        }
    }

    for (int i = 0; i < k; i++) {
        out << ans[i] << endl;
    }

    return 0;
}
