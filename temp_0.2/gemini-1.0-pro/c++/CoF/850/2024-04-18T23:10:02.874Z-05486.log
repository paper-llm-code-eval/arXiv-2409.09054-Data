#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAX_N = 400;

int n, m;
char grid[MAX_N][MAX_N];
int visited[MAX_N][MAX_N];
int dx[] = {-1, 0, 1, 0};
int dy[] = {0, 1, 0, -1};

int dfs(int x, int y) {
  if (visited[x][y]) {
    return 0;
  }
  visited[x][y] = 1;
  int ans = 1;
  for (int i = 0; i < 4; i++) {
    int nx = x + dx[i];
    int ny = y + dy[i];
    if (nx >= 0 && nx < n && ny >= 0 && ny < m && grid[x][y] == grid[nx][ny]) {
      ans += dfs(nx, ny);
    }
  }
  return ans;
}

int main() {
  ifstream fin("ferma.in");
  ofstream fout("ferma.out");
  int v;
  fin >> v;
  fin >> n >> m;
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      fin >> grid[i][j];
    }
  }
  if (v == 1) {
    int ans = 0;
    for (int i = 0; i < n; i++) {
      for (int j = 0; j < m; j++) {
        ans = max(ans, dfs(i, j));
      }
    }
    fout << ans << endl;
  } else {
    int ans = 0;
    int x, y;
    char c;
    for (int i = 0; i < n; i++) {
      for (int j = 0; j < m; j++) {
        for (int k = 0; k < 26; k++) {
          if (grid[i][j] != 'a' + k) {
            grid[i][j] = 'a' + k;
            int area = dfs(i, j);
            if (area > ans) {
              ans = area;
              x = i;
              y = j;
              c = 'a' + k;
            }
            grid[i][j] = grid[i][j];
          }
        }
      }
    }
    fout << x + 1 << " " << y + 1 << endl;
    fout << c << endl;
  }
  return 0;
}
