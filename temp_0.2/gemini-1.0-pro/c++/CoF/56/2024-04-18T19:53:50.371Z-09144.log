#include <fstream>
#include <vector>
#include <queue>
#include <limits>

using namespace std;

struct Edge {
    int to;
    int weight;
};

struct Node {
    int id;
    int distance;
    int heads;
};

bool operator<(const Node& a, const Node& b) {
    return a.distance > b.distance;
}

int main() {
    ifstream in("zmeu.in");
    ofstream out("zmeu.out");

    int n, p, k;
    in >> n >> p >> k;

    vector<vector<Edge>> graph(p + 1);
    vector<pair<int, int>> durations(p + 1);
    vector<bool> visited(p + 1, false);

    for (int i = 1; i <= p; i++) {
        int d, c;
        in >> d >> c;
        durations[i] = {d, c};
    }

    for (int i = 0; i < k; i++) {
        int a, b;
        in >> a >> b;
        graph[a].push_back({b, 0});
    }

    priority_queue<Node> pq;
    pq.push({1, 0, n});

    int min_distance = numeric_limits<int>::max();

    while (!pq.empty()) {
        Node current = pq.top();
        pq.pop();

        if (current.id == p) {
            min_distance = min(min_distance, current.distance);
            continue;
        }

        if (visited[current.id]) {
            continue;
        }

        visited[current.id] = true;

        for (Edge edge : graph[current.id]) {
            if (current.heads - durations[edge.to].second >= 1) {
                pq.push({edge.to, current.distance + durations[edge.to].first, current.heads - durations[edge.to].second});
            }
        }
    }

    if (min_distance == numeric_limits<int>::max()) {
        out << -1 << endl;
    } else {
        out << min_distance << endl;
    }

    return 0;
}
