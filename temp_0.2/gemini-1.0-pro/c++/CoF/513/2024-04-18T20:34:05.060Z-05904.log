#include <algorithm>
#include <cstring>

using namespace std;

const int MAXN = 50005;
const int MAXL = 55;

int n, s, c;
int dp[MAXN][MAXL];
int l[MAXN], r[MAXN];
char a[MAXN][MAXL];

int main() {
    ifstream fin("palindrom.in");
    ofstream fout("palindrom.out");

    fin >> c >> n;
    for (int i = 1; i <= n; ++i) {
        fin >> a[i];
        l[i] = strlen(a[i]);
    }
    if (c == 2) {
        fin >> s;
    }
    fin.close();

    for (int i = 1; i <= n; ++i) {
        for (int j = 0; j <= l[i] / 2; ++j) {
            if (a[i][j] == a[i][l[i] - 1 - j]) {
                dp[i][j] = dp[i - 1][j];
            } else {
                dp[i][j] = dp[i - 1][j] + 1;
            }
        }
    }

    if (c == 1) {
        int ans = 0;
        for (int i = 1; i <= n; ++i) {
            ans += dp[i][0];
        }
        fout << ans << '\n';
    } else {
        for (int i = 1; i <= n; ++i) {
            for (int j = 0; j <= l[i] / 2; ++j) {
                if (dp[i][j] <= s) {
                    r[i] = max(r[i], l[i] - 1 - j);
                }
            }
        }
        int ans = 0;
        int st = 1;
        for (int i = 2; i <= n; ++i) {
            if (r[i] >= r[i - 1]) {
                continue;
            }
            ans = max(ans, r[i - 1] - st + 1);
            st = i;
        }
        ans = max(ans, r[n] - st + 1);
        fout << ans << '\n';
    }
    fout.close();
    return 0;
}
