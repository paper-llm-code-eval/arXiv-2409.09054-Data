#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("livada.in");
    ofstream out("livada.out");

    int m, n, p;
    in >> m >> n >> p;

    vector<vector<int>> livada(m, vector<int>(n));
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            in >> livada[i][j];
        }
    }

    int nr_randuri_majoritate = 0;
    int max_consecutive = 0;
    for (int i = 0; i < m; i++) {
        int frecventa_max = 0;
        int soi_majoritar = -1;
        int consecutive = 1;
        for (int j = 0; j < n; j++) {
            if (livada[i][j] == livada[i][j - 1]) {
                consecutive++;
            } else {
                consecutive = 1;
            }
            max_consecutive = max(max_consecutive, consecutive);

            int frecventa = count(livada[i].begin(), livada[i].end(), livada[i][j]);
            if (frecventa > frecventa_max) {
                frecventa_max = frecventa;
                soi_majoritar = livada[i][j];
            }
        }

        if (frecventa_max > n / 2) {
            nr_randuri_majoritate++;
        }
    }

    out << nr_randuri_majoritate << endl;
    out << max_consecutive << endl;

    return 0;
}
