                for (int k = 0; k < 8; ++k) {
                  int x = i + dx[k], y = j + dy[k];
                  if (x < 1 || x > n || y < 1 || y > n) continue;
                  viz[x][y] = true;
                }
              }
            }
          }
        }
      }
    }
  }
  fout << ans << '\n';
  return 0;
}
