#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream in("aur.in");
  ofstream out("aur.out");

  int c, n, k, p;
  in >> c >> n;
  if (c == 2) in >> k;
  if (c == 3) in >> p;

  vector<int> v(n);
  for (int i = 0; i < n; i++) in >> v[i];

  int ans;
  if (c == 1) {
    for (int i = 0; i < n; i++) {
      int x = v[i];
      while (x) {
        ans++;
        x /= 10;
      }
    }
  } else if (c == 2) {
    int cnt = 0;
    for (int i = 0; i < n; i++) {
      int x = v[i];
      while (x) {
        cnt++;
        if (cnt == k) {
          ans = x % 10;
          break;
        }
        x /= 10;
      }
      if (cnt == k) break;
    }
  } else if (c == 3) {
    vector<int> digits;
    for (int i = 0; i < n; i++) {
      int x = v[i];
      while (x) {
        digits.push_back(x % 10);
        x /= 10;
      }
    }
    sort(digits.rbegin(), digits.rend());
    ans = 0;
    for (int i = 0; i < p; i++) ans = ans * 10 + digits[i];
  }

  out << ans << '\n';
  return 0;
}
