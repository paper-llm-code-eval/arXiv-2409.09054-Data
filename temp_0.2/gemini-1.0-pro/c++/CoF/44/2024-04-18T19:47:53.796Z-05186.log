#include <fstream>
#include <vector>
#include <queue>

using namespace std;

ifstream fin("pm.in");
ofstream fout("pm.out");

const int NMAX = 100;
const int INF = 1e9;

int n, t[NMAX], in[NMAX], dp[NMAX], c[NMAX], d[NMAX];
vector<int> g[NMAX];

void dfs(int nod) {
    for (int vecin : g[nod]) {
        dfs(vecin);
        dp[nod] = max(dp[nod], dp[vecin] + t[vecin]);
    }
    c[nod] = dp[nod];
    for (int vecin : g[nod]) {
        d[nod] = max(d[nod], dp[nod] - (dp[vecin] + t[vecin]));
    }
}

int main() {
    fin >> n;
    for (int i = 0; i < n; i++) {
        fin >> t[i];
    }
    for (int i = 1; i < n; i++) {
        int m;
        fin >> m;
        for (int j = 0; j < m; j++) {
            int x;
            fin >> x;
            g[x - 1].push_back(i);
            in[i]++;
        }
    }
    queue<int> q;
    for (int i = 0; i < n; i++) {
        if (in[i] == 0) {
            q.push(i);
        }
    }
    while (!q.empty()) {
        int nod = q.front();
        q.pop();
        dfs(nod);
        for (int vecin : g[nod]) {
            in[vecin]--;
            if (in[vecin] == 0) {
                q.push(vecin);
            }
        }
    }
    fout << dp[0] << '\n';
    for (int i = 0; i < n; i++) {
        fout << c[i] << ' ' << d[i] << '\n';
    }
    return 0;
}
```
