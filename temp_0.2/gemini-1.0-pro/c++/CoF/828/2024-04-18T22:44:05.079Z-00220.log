#include <fstream>

using namespace std;

int main() {
  ifstream in("culori.in");
  ofstream out("culori.out");

  int n;
  in >> n;

  long long dp[n + 1][5];
  dp[1][0] = 1;
  dp[1][1] = 1;
  dp[1][2] = 1;
  dp[1][3] = 1;
  dp[1][4] = 1;

  for (int i = 2; i <= n; i++) {
    dp[i][0] = dp[i - 1][1];
    dp[i][1] = dp[i - 1][0] + dp[i - 1][2];
    dp[i][2] = dp[i - 1][1] + dp[i - 1][3];
    dp[i][3] = dp[i - 1][2] + dp[i - 1][4];
    dp[i][4] = dp[i - 1][3];
  }

  long long ans = 0;
  for (int i = 0; i < 5; i++) {
    ans += dp[n][i];
  }

  out << ans << endl;

  return 0;
}
