#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream fin("joc.in");
    ofstream fout("joc.out");

    int n;
    fin >> n;
    vector<int> v(n);
    for (int i = 0; i < n; i++) {
        fin >> v[i];
    }

    int sum = 0;
    int last = 0;
    int turn = 1;
    while (true) {
        int best = -1;
        for (int i = 0; i < n; i++) {
            if (v[i] > last && (v[i] + sum) % 2 == turn) {
                if (best == -1 || v[i] < v[best]) {
                    best = i;
                }
            }
        }
        if (best == -1) {
            break;
        }
        sum += v[best];
        last = v[best];
        turn = 1 - turn;
    }

    fout << sum << endl;

    return 0;
}
```
