#include <fstream>
#include <vector>

using namespace std;

ifstream fin("betasah.in");
ofstream fout("betasah.out");

const int MAXN = 1005;

int n, d, k;
int x[MAXN], y[MAXN], z[MAXN], t[MAXN];
int dx[] = {0, 1, 1, 1, 0, -1, -1, -1};
int dy[] = {1, 1, 0, -1, -1, -1, 0, 1};
vector<int> adj[MAXN * MAXN];
bool vis[MAXN * MAXN];
int dist[MAXN * MAXN];

int main() {
  fin >> n >> d >> k;
  for (int i = 1; i <= d; ++i) {
    fin >> x[i] >> y[i];
  }
  for (int i = 1; i <= k; ++i) {
    fin >> z[i] >> t[i];
  }
  for (int i = 1; i <= n; ++i) {
    for (int j = 1; j <= i; ++j) {
      int u = (i * (i + 1)) / 2 + j;
      if (i == 1 || j == 1 || i == n || j == i) {
        adj[u].push_back(0);
      }
      for (int dir = 1; dir <= 8; ++dir) {
        int nx = i + dx[dir];
        int ny = j + dy[dir];
        if (nx >= 1 && nx <= n && ny >= 1 && ny <= nx) {
          int v = (nx * (nx + 1)) / 2 + ny;
          adj[u].push_back(v);
        }
      }
    }
  }
  for (int i = 1; i <= d; ++i) {
    int u = (x[i] * (x[i] + 1)) / 2 + y[i];
    vis[u] = true;
  }
  for (int i = 1; i <= k; ++i) {
    int u = (z[i] * (z[i] + 1)) / 2 + t[i];
    adj[u].clear();
  }
  int max_row = 0;
  for (int i = 1; i <= n; ++i) {
    int cnt = 0;
    for (int j = 1; j <= i; ++j) {
      int u = (i * (i + 1)) / 2 + j;
      if (!vis[u]) {
        ++cnt;
      }
    }
    max_row = max(max_row, cnt);
  }
  int cnt = 0;
  for (int i = 1; i <= n; ++i) {
    for (int j = 1; j <= i; ++j) {
      int u = (i * (i + 1)) / 2 + j;
      if (!vis[u]) {
        if (!vis[u]) {
          ++cnt;
          vis[u] = true;
          vector<int> q;
          q.push_back(u);
          while (!q.empty()) {
            int v = q.back();
            q.pop_back();
            for (int w : adj[v]) {
              if (!vis[w]) {
                vis[w] = true;
                q.push_back(w);
              }
            }
          }
        }
      }
    }
  }
  fout << max_row << '\n' << cnt << '\n';
  return 0;
}
