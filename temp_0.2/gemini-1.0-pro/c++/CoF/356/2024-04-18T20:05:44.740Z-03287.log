#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream in("cochilie.in");
  ofstream out("cochilie.out");
  int c;
  in >> c;
  if (c == 1) {
    int n;
    in >> n;
    out << (n * n) << " " << (n * n) << "\n";
  } else {
    int n, p;
    in >> n >> p;
    vector<vector<int>> cochilie(n * n, vector<int>(n * n));
    int val = 1, l = 1, c = 1, d = 0;
    while (val <= n) {
      for (int i = 0; i < l; i++) {
        if (d == 0)
          cochilie[c - 1][c + i] = val;
        else if (d == 1)
          cochilie[c + i][c + l] = val;
        else if (d == 2)
          cochilie[c + l][c + l - i] = val;
        else
          cochilie[c + l - i][c - 1] = val;
      }
      if (d == 0)
        c += l;
      else if (d == 1)
        l++;
      else if (d == 2)
        c -= l;
      else
        l--;
      d = (d + 1) % 4;
      val++;
    }
    for (int i = 0; i < n * n; i++) {
      for (int j = 0; j < n * n; j++) {
        if (cochilie[i][j] == p) out << j + 1 << " ";
      }
    }
    out << "\n";
  }
  return 0;
}
