#include <fstream>
#include <vector>

using namespace std;

ifstream fin("cladiri.in");
ofstream fout("cladiri.out");

const int MAXN = 100000;
const int MAXL = 10000;

int main() {
    int long_c, lat_c, intensitate;
    fin >> long_c >> lat_c >> intensitate;

    vector<vector<int>> cladiri(MAXL + 1, vector<int>(MAXL + 1, 0));
    int n;
    fin >> n;
    for (int i = 0; i < n; i++) {
        int long_i, lat_i, grad;
        fin >> long_i >> lat_i >> grad;
        cladiri[long_i][lat_i] = grad;
    }

    int numar_afectate = 0;
    int numar_max_afectate = 0;
    vector<int> nivele_max_afectate;

    for (int nivel = 0; nivel <= intensitate; nivel++) {
        int numar_afectate_nivel = 0;
        for (int long_i = long_c - nivel; long_i <= long_c + nivel; long_i++) {
            for (int lat_i = lat_c - nivel; lat_i <= lat_c + nivel; lat_i++) {
                if (long_i >= 0 && long_i <= MAXL && lat_i >= 0 && lat_i <= MAXL && cladiri[long_i][lat_i] <= intensitate - nivel) {
                    numar_afectate++;
                    numar_afectate_nivel++;
                }
            }
        }
        if (numar_afectate_nivel > numar_max_afectate) {
            numar_max_afectate = numar_afectate_nivel;
            nivele_max_afectate.clear();
            nivele_max_afectate.push_back(nivel);
        } else if (numar_afectate_nivel == numar_max_afectate) {
            nivele_max_afectate.push_back(nivel);
        }
    }

    fout << numar_afectate << '\n';
    fout << numar_max_afectate << '\n';
    for (int nivel : nivele_max_afectate) {
        fout << nivel << ' ';
    }
    fout << '\n';

    return 0;
}
