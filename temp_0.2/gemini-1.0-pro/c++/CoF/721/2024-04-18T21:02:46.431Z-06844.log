#include <fstream>

using namespace std;

const int MAXN = 50;
int n, k, a[MAXN][MAXN], b[MAXN][MAXN], x[MAXN * MAXN], y[MAXN * MAXN];

int main() {
  ifstream fin("spirala.in");
  ofstream fout("spirala.out");
  fin >> n >> k;
  int nr = 0;
  for (int i = 1; i <= n; i++) {
    if (i % 2 == 1) {
      for (int j = 1; j <= n; j++) {
        a[i][j] = ++nr;
        x[nr] = i;
        y[nr] = j;
      }
    } else {
      for (int j = n; j >= 1; j--) {
        a[i][j] = ++nr;
        x[nr] = i;
        y[nr] = j;
      }
    }
  }
  int ans = 0;
  while (k > 0) {
    for (int i = 1; i <= n; i++) {
      for (int j = 1; j <= n; j++) {
        b[x[a[i][j]]][y[a[i][j]]] = a[i][j];
      }
    }
    for (int i = 1; i <= n; i++) {
      for (int j = 1; j <= n; j++) {
        a[i][j] = b[i][j];
      }
    }
    ans++;
    k = 0;
    for (int i = 1; i <= n; i++) {
      for (int j = 1; j <= n; j++) {
        if (a[i][j] == i * n + j) {
          k++;
        }
      }
    }
  }
  fout << ans << '\n';
  return 0;
}
