#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream in("spirala.in");
  ofstream out("spirala.out");
  int n, k;
  in >> n >> k;
  vector<int> v(n * n);
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < n; j++) {
      v[i * n + j] = i * n + j + 1;
    }
  }
  int cnt = 0;
  while (true) {
    vector<int> u(n * n);
    int idx = 0;
    for (int i = 0; i < n; i++) {
      if (i % 2 == 0) {
        for (int j = 0; j < n; j++) {
          u[idx++] = v[i * n + j];
        }
      } else {
        for (int j = n - 1; j >= 0; j--) {
          u[idx++] = v[i * n + j];
        }
      }
    }
    v = u;
    cnt++;
    int ok = 0;
    for (int i = 0; i < n * n; i++) {
      if (v[i] == i + 1) {
        ok++;
      }
    }
    if (ok == k) {
      break;
    }
  }
  out << cnt << endl;
  return 0;
}
