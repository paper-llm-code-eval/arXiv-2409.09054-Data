#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream in("spirala.in");
  ofstream out("spirala.out");
  int n, k;
  in >> n >> k;
  vector<vector<int>> a(n, vector<int>(n));
  int x = 1, y = 0, dx = 1, dy = 0;
  for (int i = 0; i < n * n; i++) {
    a[x][y] = i + 1;
    if (x + dx < 0 || x + dx >= n || y + dy < 0 || y + dy >= n || a[x + dx][y + dy] != 0) {
      int t = dx;
      dx = dy;
      dy = -t;
    }
    x += dx;
    y += dy;
  }
  int ans = 0;
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < n; j++) {
      if (a[i][j] == i * n + j + 1) {
        k--;
      }
    }
  }
  while (k > 0) {
    ans++;
    vector<vector<int>> b(n, vector<int>(n));
    for (int i = 0; i < n; i++) {
      for (int j = 0; j < n; j++) {
        b[i][j] = a[n - j - 1][i];
      }
    }
    a = b;
    for (int i = 0; i < n; i++) {
      for (int j = 0; j < n; j++) {
        if (a[i][j] == i * n + j + 1) {
          k--;
        }
      }
    }
  }
  out << ans << endl;
  return 0;
}
