#include <vector>
#include <queue>
#include <algorithm>
using namespace std;

const int NMAX = 5005;

int n, m, s, a, b, c, dist[NMAX], dist2[NMAX], dist3[NMAX], dist4[NMAX];
vector<int> g[NMAX], gt[NMAX];
vector<pair<int, int>> sol[3];

void bfs(int nod, int *dist, vector<int> *g) {
    queue<int> q;
    q.push(nod);
    dist[nod] = 0;
    while (!q.empty()) {
        int nodc = q.front();
        q.pop();
        for (int vecin : g[nodc]) {
            if (dist[vecin] == -1) {
                dist[vecin] = dist[nodc] + 1;
                q.push(vecin);
            }
        }
    }
}

int main() {
    ifstream fin("veri.in");
    ofstream fout("veri.out");
    fin >> c >> n >> m >> s >> a >> b;
    for (int i = 1; i <= m; i++) {
        int x, y;
        fin >> x >> y;
        g[x].push_back(y);
        gt[y].push_back(x);
    }
    fill(dist, dist + NMAX, -1);
    bfs(s, dist, g);
    fill(dist2, dist2 + NMAX, -1);
    bfs(a, dist2, gt);
