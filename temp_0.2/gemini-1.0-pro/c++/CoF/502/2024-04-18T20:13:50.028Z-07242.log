            ans = min(ans, max(dist[i] + dist2[i], dist[i] + dist3[i]));
        }
    }
    if (c == 1) {
        fout << ans << '\n';
    } else {
        fill(dist4, dist4 + NMAX, -1);
        bfs(s, dist4, g);
        int nod = -1;
        for (int i = 1; i <= n; i++) {
            if (dist[i] != -1 && dist2[i] != -1 && dist3[i] != -1 &&
                max(dist[i] + dist2[i], dist[i] + dist3[i]) == ans) {
                nod = i;
                break;
            }
        }
        int t = dist[nod];
        int t1 = dist2[nod];
        int t2 = dist3[nod];
        int t3 = dist4[nod];
        sol[0].push_back({t3, s});
        while (t3 != nod) {
            for (int vecin : g[t3]) {
                if (dist4[vecin] == t3 - 1) {
                    t3 = vecin;
                    sol[0].push_back({1, vecin});
                    break;
                }
            }
        }
        sol[1].push_back({t1, nod});
        while (t1 != a) {
            for (int vecin : gt[t1]) {
                if (dist2[vecin] == t1 - 1) {
                    t1 = vecin;
                    sol[1].push_back({1, vecin});
                    break;
                }
            }
        }
        sol[2].push_back({t2, nod});
        while (t2 != b) {
            for (int vecin : gt[t2]) {
                if (dist3[vecin] == t2 - 1) {
                    t2 = vecin;
                    sol[2].push_back({1, vecin});
                    break;
                }
            }
        }
        for (int i = 0; i < 3; i++) {
            fout << sol[i].size() - 1 << '\n';
            for (auto &p : sol[i]) {
                fout << p.second << ' ';
            }
            fout << '\n';
        }
    }
    return 0;
}
