#include <bits/stdc++.h>
using namespace std;

const int NMAX = 5005;
const int MMAX = 20005;

struct Edge {
    int to, cost;
};

vector<Edge> G[NMAX];
int n, m, S, A, B, c;
int dist[NMAX][2];
int ans;
vector<int> path[NMAX];

void dijkstra(int start, int type) {
    priority_queue<pair<int, int>> pq;
    pq.push({0, start});
    dist[start][type] = 0;

    while (!pq.empty()) {
        int node = pq.top().second;
        int cost = -pq.top().first;
        pq.pop();

        if (dist[node][type] < cost) continue;

        for (Edge e : G[node]) {
            int to = e.to;
            int new_cost = cost + e.cost;
            if (dist[to][type] > new_cost) {
                dist[to][type] = new_cost;
                pq.push({-new_cost, to});
                path[to][type] = path[node][type];
                path[to][type].push_back(to);
            }
        }
    }
}

int main() {
    ifstream fin("veri.in");
    ofstream fout("veri.out");

    fin >> c;
    fin >> n >> m;
    fin >> S >> A >> B;

    for (int i = 1; i <= n; i++) {
        dist[i][0] = dist[i][1] = INT_MAX;
        path[i][0].clear();
        path[i][1].clear();
    }

    for (int i = 1; i <= m; i++) {
        int x, y;
        fin >> x >> y;
        G[x].push_back({y, 1});
    }

    dijkstra(S, 0);
    dijkstra(S, 1);

    for (int i = 1; i <= n; i++) {
        if (i == A || i == B) continue;
        ans = max(ans, max(dist[i][0] + dist[A][1], dist[i][0] + dist[B][1]));
    }

    if (c == 1) {
        fout << ans << '\n';
    } else {
        int Z = -1;
        for (int i = 1; i <= n; i++) {
            if (i == A || i == B) continue;
            if (max(dist[i][0] + dist[A][1], dist[i][0] + dist[B][1]) == ans) {
                Z = i;
                break;
            }
        }

        fout << path[Z][0].size() << '\n';
        for (int node : path[Z][0]) {
            fout << node << ' ';
        }
        fout << '\n';

        fout << path[A][1].size() << '\n';
        for (int node : path[A][1]) {
            fout << node << ' ';
        }
        fout << '\n';

        fout << path[B][1].size() << '\n';
        for (int node : path[B][1]) {
            fout << node << ' ';
        }
        fout << '\n';
    }

    return 0;
}
```
