#include <fstream>
#include <iostream>
#include <vector>
using namespace std;

const int MAX_K = 100000;
const int MAX_Q = 50000;
const int MAX_N = 1e18;
const int MOD = 1e9 + 7;

int main() {
  ifstream fin("caps.in");
  ofstream fout("caps.out");

  int K, Q;
  fin >> K >> Q;
  string S;
  fin >> S;

  // Preprocess the string S to compute the number of occurrences of each letter in the final string.
  vector<int> count(26, 0);
  for (int i = 0; i < K; i++) {
    count[S[i] - 'a']++;
  }
  for (int i = 0; i < K; i++) {
    count[S[i] - 'a'] = (count[S[i] - 'a'] * 2) % MOD;
  }
  for (int i = 0; i < K; i++) {
    count[S[i] - 'a']++;
  }

  // Process the queries.
  for (int i = 0; i < Q; i++) {
    long long N;
    fin >> N;

    // Compute the position of the letter in the final string.
    long long pos = N % (2 * K);
    if (pos < K) {
      fout << S[pos] << ' ' << (N / (2 * K)) * count[S[pos] - 'a'] + 1 << '\n';
    } else {
      pos -= K;
      fout << S[(pos / 2) % K] << ' ' << (N / (2 * K)) * count[S[(pos / 2) % K] - 'a'] + (pos % 2) + 1 << '\n';
    }
  }

  return 0;
}
