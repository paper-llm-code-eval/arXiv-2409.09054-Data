#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("ciocolata.in");
    ofstream out("ciocolata.out");

    int c, n;
    in >> c >> n;

    vector<int> v(n);
    for (int i = 0; i < n; i++) {
        in >> v[i];
    }

    if (c == 1) {
        sort(v.begin(), v.end());
        int cnt = 1, max_cnt = 1;
        int prev = v[0], ans = v[0];
        for (int i = 1; i < n; i++) {
            if (v[i] == prev) {
                cnt++;
            } else {
                if (cnt > max_cnt) {
                    max_cnt = cnt;
                    ans = prev;
                } else if (cnt == max_cnt) {
                    ans = min(ans, prev);
                }
                cnt = 1;
            }
            prev = v[i];
        }
        if (cnt > max_cnt) {
            ans = prev;
        } else if (cnt == max_cnt) {
            ans = min(ans, prev);
        }
        out << ans << endl;
    } else {
        int sum_i = 0, sum_m = 0;
        int min_diff = 1e9;
        for (int i = 0, j = n - 1; i < n; i++, j--) {
            sum_i += v[i];
            sum_m += v[j];
            min_diff = min(min_diff, abs(sum_i - sum_m));
        }
        out << min_diff << endl;
    }

    return 0;
}
