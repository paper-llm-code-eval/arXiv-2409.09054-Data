            if (i > 0 && j < m - 1 && grid[i][j] <= grid[i - 1][j + 1]) is_star = false;
            if (i < n - 1 && j > 0 && grid[i][j] <= grid[i + 1][j - 1]) is_star = false;
            if (i < n - 1 && j < m - 1 && grid[i][j] <= grid[i + 1][j + 1]) is_star = false;
            if (is_star) stars++;
        }
    }

    int squares = 0;
    int max_side = 0;
    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < m - 1; j++) {
            bool is_square = true;
