            bool is_square = true;
            int side = 1;
            while (is_square && i + side < n && j + side < m) {
                for (int k = 0; k < side; k++) {
                    if (grid[i][j + k] < grid[i + side][j + k]) is_square = false;
                    if (grid[i + k][j] < grid[i + k][j + side]) is_square = false;
                }
                if (is_square) side++;
            }
            if (is_square) {
                squares++;
                max_side = max(max_side, side);
            }
        }
    }

    out << stars << endl;
    out << squares << endl;
    out << max_side << endl;

    return 0;
}
