using namespace std;

struct Point {
    int x, y;
};

struct Circle {
    Point center;
    int radius;
};

struct Line {
    Point p1, p2;
};

int main() {
    ifstream in("cerc.in");
    ofstream out("cerc.out");

    int n;
    in >> n;

    vector<Circle> circles(n);
    for (int i = 0; i < n; i++) {
        in >> circles[i].center.x >> circles[i].center.y >> circles[i].radius;
    }

