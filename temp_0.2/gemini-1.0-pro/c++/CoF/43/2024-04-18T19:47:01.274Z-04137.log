    vector<double> slopes;
    for (int i = 0; i < n; i++) {
        if (circles[i].center.x == 0) {
            slopes.push_back(1e9);
        } else {
            slopes.push_back((double)circles[i].center.y / circles[i].center.x);
        }
    }

    // Sort the slopes.
    sort(slopes.begin(), slopes.end());

    // Count the number of distinct slopes.
    int m = 0;
    for (int i = 0; i < slopes.size(); i++) {
        if (i == 0 || slopes[i] != slopes[i - 1]) {
            m++;
        }
    }

    // Find the maximum number of circles that are exterior to each other and lie on the same line.
    int q = 0;
    for (int i = 0; i < m; i++) {
        int count = 0;
        for (int j = 0; j < n; j++) {
            if (slopes[i] == 1e9) {
                if (circles[j].center.x == 0) {
                    count++;
                }
            } else {
                if (circles[j].center.y == slopes[i] * circles[j].center.x) {
                    count++;
                }
            }
        }
        q = max(q, count);
    }

    // Count the number of lines that have q circles on them.
    int p = 0;
    for (int i = 0; i < m; i++) {
        int count = 0;
        for (int j = 0; j < n; j++) {
            if (slopes[i] == 1e9) {
                if (circles[j].center.x == 0) {
                    count++;
                }
            } else {
                if (circles[j].center.y == slopes[i] * circles[j].center.x) {
                    count++;
                }
            }
        }
        if (count == q) {
            p++;
        }
    }

    out << m << " " << q << " " << p << endl;

    return 0;
}
