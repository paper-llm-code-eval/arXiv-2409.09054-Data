#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>

using namespace std;

ifstream fin("galerie.in");
ofstream fout("galerie.out");

const int dx[] = {0, 1, 0, -1};
const int dy[] = {1, 0, -1, 0};

int main() {
    int cerinta, n, m, t;
    fin >> cerinta >> n >> m >> t;
    vector<int> poz(t);
    for (int i = 0; i < t; i++) {
        fin >> poz[i];
    }
    vector<vector<int>> dist(n + 2, vector<int>(m + 2, -1));
    vector<vector<int>> viz(n + 2, vector<int>(m + 2, 0));
    queue<pair<int, int>> q;
    for (int i = 0; i < t; i++) {
        int x, y;
        if (poz[i] <= n) {
            x = 1;
            y = poz[i];
        } else if (poz[i] <= n + m) {
            x = poz[i] - n;
            y = m + 1;
        } else if (poz[i] <= n + m + n) {
            x = n + 1;
            y = n + m + n - poz[i];
        } else {
            x = n + m + n - poz[i] + 1;
            y = 1;
        }
        q.push({x, y});
        dist[x][y] = 0;
        viz[x][y] = 1;
    }
    int max_dist = 0;
    int max_galerie = 0;
    while (!q.empty()) {
        int x, y;
        tie(x, y) = q.front();
        q.pop();
        max_dist = max(max_dist, dist[x][y]);
        for (int i = 0; i < 4; i++) {
            int nx = x + dx[i];
            int ny = y + dy[i];
            if (nx >= 1 && nx <= n + 1 && ny >= 1 && ny <= m + 1 && !viz[nx][ny]) {
                q.push({nx, ny});
                dist[nx][ny] = dist[x][y] + 1;
                viz[nx][ny] = 1;
                if (nx == x && ny == y) {
                    max_galerie = max(max_galerie, dist[nx][ny]);
                }
            }
        }
    }
    if (cerinta == 1) {
        fout << max_dist << '\n';
    } else {
        fout << max_galerie << '\n';
    }
    return 0;
}
