#include <fstream>
#include <vector>
#include <queue>
#include <limits>

using namespace std;

const int MAX_N = 500;
const int MAX_G = 5000;
const int MAX_A = 10000;

int n, g;
int a[MAX_N][MAX_N];
int dist[MAX_N][MAX_N];
bool visited[MAX_N][MAX_N];

int dx[] = {-1, 0, 1, 0};
int dy[] = {0, 1, 0, -1};

struct State {
    int x, y, cost;
    State(int x, int y, int cost) : x(x), y(y), cost(cost) {}
};

bool operator<(const State& a, const State& b) {
    return a.cost > b.cost;
}

int dijkstra(int v) {
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            dist[i][j] = numeric_limits<int>::max();
            visited[i][j] = false;
        }
    }

    priority_queue<State> pq;
    pq.push(State(0, 0, 0));
    dist[0][0] = 0;

    while (!pq.empty()) {
        State s = pq.top();
        pq.pop();

        if (visited[s.x][s.y]) {
            continue;
        }
        visited[s.x][s.y] = true;

        if (s.x == n - 1 && s.y == n - 1) {
            return s.cost;
        }

        for (int i = 0; i < 4; i++) {
            int nx = s.x + dx[i];
            int ny = s.y + dy[i];

            if (nx >= 0 && nx < n && ny >= 0 && ny < n) {
                int cost = s.cost;
                if (v == 1) {
                    cost += (a[nx][ny] < g);
                } else {
                    cost += (a[nx][ny] >= g);
                }

                if (dist[nx][ny] > cost) {
                    dist[nx][ny] = cost;
                    pq.push(State(nx, ny, cost));
                }
            }
        }
    }

    return -1;
}

int main() {
    ifstream fin("rover.in");
    ofstream fout("rover.out");

    int v;
    fin >> v;

    if (v == 1) {
        fin >> n >> g;
    } else {
        fin >> n;
    }

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            fin >> a[i][j];
        }
    }

    int result = dijkstra(v);
    fout << result << endl;

    return 0;
}
