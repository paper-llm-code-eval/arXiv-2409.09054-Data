            int ny = s.y + dy[i];

            if (nx >= 0 && nx < n && ny >= 0 && ny < n) {
                int cost = s.cost;
                if (v == 1) {
                    cost += (a[nx][ny] < g);
                } else {
                    cost += (a[nx][ny] >= g);
                }

                if (dist[nx][ny] > cost) {
                    dist[nx][ny] = cost;
                    pq.push(State(nx, ny, cost));
                }
            }
        }
    }

    return -1;
}

int main() {
    ifstream fin("rover.in");
    ofstream fout("rover.out");

    int v;
    fin >> v;

    if (v == 1) {
        fin >> n >> g;
    } else {
        fin >> n;
    }

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            fin >> a[i][j];
        }
    }

    int result = dijkstra(v);
    fout << result << endl;

    return 0;
}
