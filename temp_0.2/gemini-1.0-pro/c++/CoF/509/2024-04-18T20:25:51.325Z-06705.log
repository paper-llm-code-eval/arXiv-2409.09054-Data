    int C, N;
    fin >> C >> N;

    vector<Flight> flights(N);
    for (int i = 0; i < N; i++) {
        fin >> flights[i].dep_airport >> flights[i].arr_airport >> flights[i].dep_time >> flights[i].arr_time;
        flights[i].is_special = false;
    }

    // Check for special flights
    for (int i = 0; i < N; i++) {
        int dep_airport = flights[i].dep_airport;
        int arr_airport = flights[i].arr_airport;

        bool is_prime = true;
        for (int j = 2; j * j <= dep_airport; j++) {
            if (dep_airport % j == 0) {
                is_prime = false;
                break;
            }
        }

        int sum_of_digits = 0;
        while (arr_airport > 0) {
            sum_of_digits += arr_airport % 10;
            arr_airport /= 10;
        }

        if (is_prime && arr_airport % sum_of_digits == 0) {
            flights[i].is_special = true;
        }
    }

    // Calculate the maximum duration of a flight
    int max_duration = 0;
    for (int i = 0; i < N; i++) {
        int duration = flights[i].arr_time - flights[i].dep_time;
        if (duration < 0) {
            duration += MAX_TIME;
        }
        max_duration = max(max_duration, duration);
    }

    // Calculate the maximum duration of a flight after the virus attack
    if (C == 2) {
        for (int i = 0; i < N; i++) {
            if (flights[i].is_special) {
                swap(flights[i].dep_time, flights[i].arr_time);
            }
        }

        max_duration = 0;
        for (int i = 0; i < N; i++) {
            int duration = flights[i].arr_time - flights[i].dep_time;
            if (duration < 0) {
                duration += MAX_TIME;
            }
            max_duration = max(max_duration, duration);
        }
    }

    // Output the result
    fout << max_duration / 60 << " " << max_duration % 60 << "\n";

    return 0;
}
