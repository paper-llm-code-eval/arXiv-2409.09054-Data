                if (i < n - 1 && grid[i + 1][j] != 'x') {
                    if (grid[i + 1][j] == 'r') r++;
                    else a++;
                }
                if (j > 0 && grid[i][j - 1] != 'x') {
                    if (grid[i][j - 1] == 'r') r++;
                    else a++;
                }
                if (j < m - 1 && grid[i][j + 1] != 'x') {
                    if (grid[i][j + 1] == 'r') r++;
                    else a++;
                }
                if (r > a) new_grid[i][j] = 'r';
                else if (a > r) new_grid[i][j] = 'a';
                else new_grid[i][j] = grid[i][j];
            }
        }
        grid = new_grid;
    }

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            out << grid[i][j];
        }
        out << endl;
    }

    return 0;
}
