#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream in("culori.in");
  ofstream out("culori.out");

  int n, m, t;
  in >> n >> m >> t;

  vector<vector<char>> grid(n, vector<char>(m));
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      in >> grid[i][j];
    }
  }

  while (t--) {
    vector<vector<char>> new_grid(n, vector<char>(m));
    for (int i = 0; i < n; i++) {
      for (int j = 0; j < m; j++) {
        int r = 0, a = 0;
        if (i > 0) {
          if (grid[i - 1][j] == 'r') r++;
          if (grid[i - 1][j] == 'a') a++;
        }
        if (i < n - 1) {
          if (grid[i + 1][j] == 'r') r++;
          if (grid[i + 1][j] == 'a') a++;
        }
        if (j > 0) {
          if (grid[i][j - 1] == 'r') r++;
          if (grid[i][j - 1] == 'a') a++;
        }
        if (j < m - 1) {
          if (grid[i][j + 1] == 'r') r++;
          if (grid[i][j + 1] == 'a') a++;
        }
        new_grid[i][j] = r > a ? 'r' : (r == a ? grid[i][j] : 'a');
      }
    }
    grid = new_grid;
  }

  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      out << grid[i][j];
    }
    out << '\n';
  }

  return 0;
}
```
