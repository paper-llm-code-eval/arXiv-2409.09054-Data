      if (!ok) {
        ans++;
      }
    }
    out << ans << "\n";
  } else if (p == 3) {
    int min_k, max_k;
    min_k = 1;
    for (int i = 1; i < k; i++) {
      min_k *= 10;
      min_k++;
    }
    max_k = 1;
    for (int i = 1; i <= k; i++) {
      max_k *= 10;
    }
    out << min_k << " " << max_k << "\n";
  } else if (p == 4) {
    int ans_even = 0, ans_odd = 0;
    int min_k, max_k;
    min_k = 1;
    for (int i = 1; i < k; i++) {
      min_k *= 10;
      min_k++;
    }
    max_k = 1;
    for (int i = 1; i <= k; i++) {
      max_k *= 10;
    }
    for (int i = min_k; i <= max_k; i++) {
      int x = i;
      bool ok = true;
      while (x > 0) {
        int d = x % 10;
        x /= 10;
        if (d >= 1 && (x == 0 || d != x % 10 + 1)) {
          ok = false;
          break;
        }
      }
      if (ok) {
        if (i % 2 == 0) {
          ans_even++;
        } else {
          ans_odd++;
        }
      }
    }
    out << ans_even << " " << ans_odd << "\n";
  }
  return 0;
}
