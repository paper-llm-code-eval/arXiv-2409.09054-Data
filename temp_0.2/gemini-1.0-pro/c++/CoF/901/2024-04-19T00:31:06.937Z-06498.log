#include <fstream>
#include <vector>
#include <string>
#include <iostream>
#include <sstream>
#include <algorithm>

using namespace std;

struct Monom {
    int coef;
    char var;
    Monom(int coef, char var) : coef(coef), var(var) {}
};

struct Term {
    int coef;
    Term(int coef) : coef(coef) {}
};

vector<Monom> monoame;
vector<Term> termeni;

int eval(int x, int y, int z, int t) {
    int sum = 0;
    for (auto& monom : monoame) {
        switch (monom.var) {
            case 'x': sum += monom.coef * x; break;
            case 'y': sum += monom.coef * y; break;
            case 'z': sum += monom.coef * z; break;
            case 't': sum += monom.coef * t; break;
        }
    }
    for (auto& term : termeni) {
        sum += term.coef;
    }
    return sum;
}

int main() {
    ifstream in("eq4.in");
    ofstream out("eq4.out");

    int C;
    in >> C;

    string expr;
    getline(in, expr);
    getline(in, expr);

    int a, b, E;
    in >> a >> b >> E;

    int start = 0;
    for (int i = 0; i < expr.size(); i++) {
        if (expr[i] == '+' || expr[i] == '-') {
            if (i > start) {
                string token = expr.substr(start, i - start);
                if (token[0] >= '0' && token[0] <= '9') {
                    termeni.push_back(Term(stoi(token)));
                } else {
                    int coef = 1;
                    if (token[0] == '+' || token[0] == '-') {
                        coef = (token[0] == '+' ? 1 : -1);
                        token = token.substr(1);
                    }
                    if (token.size() > 1) {
                        coef *= stoi(token.substr(1));
                    }
                    monoame.push_back(Monom(coef, token[0]));
                }
            }
            start = i + 1;
        }
    }
    if (start < expr.size()) {
        string token = expr.substr(start);
        if (token[0] >= '0' && token[0] <= '9') {
            termeni.push_back(Term(stoi(token)));
        } else {
            int coef = 1;
            if (token[0] == '+' || token[0] == '-') {
                coef = (token[0] == '+' ? 1 : -1);
                token = token.substr(1);
            }
            if (token.size() > 1) {
                coef *= stoi(token.substr(1));
            }
            monoame.push_back(Monom(coef, token[0]));
        }
    }

    if (C == 1) {
        out << eval(1, 1, 1, 1) << endl;
