#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("divizor.in");
ofstream fout("divizor.out");

const int MAXN = 1000000;
const int MAXM = 7;

int n, m, nrDiv[MAXN], nrDivMax, nrMaxDiv, divMax;
vector<int> numere[MAXM];

int main() {
    fin >> n;
    m = 0;
    do {
        numere[m].push_back(n);
        n = (n % 10) * 10 + n / 10;
        m++;
    } while (n != numere[m - 1][0]);

    for (int i = 0; i < m; i++) {
        for (int j = 1; j * j <= numere[i][0]; j++) {
            if (numere[i][0] % j == 0) {
                nrDiv[i]++;
                if (j * j != numere[i][0]) nrDiv[i]++;
            }
        }
        if (nrDiv[i] > nrDivMax) {
            nrDivMax = nrDiv[i];
            nrMaxDiv = 1;
        } else if (nrDiv[i] == nrDivMax) {
            nrMaxDiv++;
        }
    }

    for (int i = 0; i < m; i++) {
        if (nrDiv[i] == nrDivMax) {
            fout << numere[i][0] << ' ';
        }
    }
    fout << '\n';

    for (int i = MAXN - 1; i >= 1; i--) {
        int ok = 0;
        for (int j = 0; j < m; j++) {
            if (numere[j][0] % i == 0) {
                ok = 1;
                break;
            }
        }
        if (ok) {
            divMax = i;
            break;
        }
    }

    fout << divMax << '\n';

    return 0;
}
