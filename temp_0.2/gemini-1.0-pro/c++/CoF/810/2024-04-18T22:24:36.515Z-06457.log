#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("grad.in");
    ofstream out("grad.out");
    int n, k;
    in >> n >> k;
    vector<int> v(n);
    for (int i = 0; i < n; i++) {
        in >> v[i];
    }
    sort(v.begin(), v.end());
    int grad = 0;
    for (int i = 0; i < n; i++) {
        if (v[i] == v[i + 1]) {
            grad++;
        }
    }
    out << grad << endl;
    int max_grad = 0;
    int pos = 0;
    for (int i = 0; i <= n - k; i++) {
        int curr_grad = 0;
        for (int j = i; j < i + k; j++) {
            if (v[j] == v[j + 1]) {
                curr_grad++;
            }
        }
        if (curr_grad > max_grad) {
            max_grad = curr_grad;
            pos = i;
        }
    }
    out << pos + 1 << " " << max_grad << endl;
    return 0;
}
```
