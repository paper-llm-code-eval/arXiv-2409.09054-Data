#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream in("grad.in");
  ofstream out("grad.out");

  int n, k;
  in >> n >> k;

  vector<int> v(n);
  for (int i = 0; i < n; i++) {
    in >> v[i];
  }

  // Gradul intregului sir
  int grad = 0;
  for (int i = 0; i < n; i++) {
    if (v[i] == i + 1) {
      grad++;
    }
  }

  // Grad maxim si pozitia primului element din prima secventa cu grad maxim
  int grad_max = 0;
  int poz_max = -1;
  for (int i = 0; i <= n - k; i++) {
    int grad_secv = 0;
    for (int j = i; j < i + k; j++) {
      if (v[j] == j - i + 1) {
        grad_secv++;
      }
    }
    if (grad_secv > grad_max) {
      grad_max = grad_secv;
      poz_max = i + 1;
    }
  }

  out << grad << endl;
  out << poz_max << " " << grad_max << endl;

  return 0;
}
```
