#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("pluricex.in");
ofstream fout("pluricex.out");

int n, k, D;
vector<vector<int>> discipline;
vector<int> echipa;
vector<bool> vizitat;

void citire() {
    fin >> n >> k >> D;
    discipline.resize(n + 1);
    for (int i = 1; i <= n; i++) {
        int nr;
        fin >> nr;
        for (int j = 0; j < nr; j++) {
            int d;
            fin >> d;
            discipline[i].push_back(d);
        }
    }
}

bool valid() {
    vector<bool> exista(D + 1, false);
    for (int i = 0; i < k; i++) {
        for (int j = 0; j < discipline[echipa[i]].size(); j++) {
            exista[discipline[echipa[i]][j]] = true;
        }
    }
    for (int i = 1; i <= D; i++) {
        if (!exista[i]) {
            return false;
        }
    }
    return true;
}

void back(int poz) {
    if (poz == k) {
        if (valid()) {
            for (int i = 0; i < k; i++) {
                fout << echipa[i] << " ";
            }
            fout << "\n";
        }
        return;
    }
    for (int i = 1; i <= n; i++) {
        if (!vizitat[i]) {
            echipa[poz] = i;
            vizitat[i] = true;
            back(poz + 1);
            vizitat[i] = false;
        }
    }
}

int main() {
    citire();
    back(0);
    return 0;
}
