#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("panglica.in");
ofstream fout("panglica.out");

const int MAXN = 10005;
const int MAXC = 205;

int n, c;
int a[MAXN];
int f[MAXN][MAXC];
int g[MAXN][MAXC];

int main() {
    fin >> n >> c;
    for (int i = 1; i <= n; ++i) {
        fin >> a[i];
    }

    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= c; ++j) {
            f[i][j] = max(f[i - 1][j], g[i - 1][j]);
            if (a[i] == j) {
                g[i][j] = max(g[i - 1][j] + 1, f[i - 1][j] + 1);
            } else {
                g[i][j] = 0;
            }
        }
    }

    int ans = 0;
    int col = 0;
    for (int i = 1; i <= c; ++i) {
        if (f[n][i] > ans) {
            ans = f[n][i];
            col = i;
        }
    }

    int st = 0;
    int dr = 0;
    for (int i = 1; i <= n; ++i) {
        if (a[i] == col) {
            st = i;
            break;
        }
    }
    for (int i = n; i >= 1; --i) {
        if (a[i] == col) {
            dr = i;
            break;
        }
    }

    fout << ans << '\n';
    fout << col << '\n';
    fout << st - 1 << '\n';
    fout << n - dr << '\n';

    return 0;
}
