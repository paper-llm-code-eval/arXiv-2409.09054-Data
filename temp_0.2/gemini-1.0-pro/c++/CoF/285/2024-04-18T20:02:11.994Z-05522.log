#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>

using namespace std;

const int MAX_N = 500;
const int MAX_P = 15000;
const int MAX_R = 6;

int N, P, C;
int x_s, y_s, x_f, y_f;
int grid[MAX_N][MAX_N];
int dist[MAX_N][MAX_N];
int pulsar_grid[MAX_N][MAX_N][MAX_R];

struct Pulsar {
    int x, y, r, t;
};

vector<Pulsar> pulsars;

bool valid(int x, int y) {
    return x >= 1 && x <= N && y >= 1 && y <= N;
}

int manhattan(int x1, int y1, int x2, int y2) {
    return abs(x1 - x2) + abs(y1 - y2);
}

void update_grid() {
    for (int i = 0; i < P; i++) {
        int x = pulsars[i].x;
        int y = pulsars[i].y;
        int r = pulsars[i].r;
        int t = pulsars[i].t;

        for (int dx = -r; dx <= r; dx++) {
            for (int dy = -r; dy <= r; dy++) {
                int nx = x + dx;
                int ny = y + dy;

                if (!valid(nx, ny)) {
                    continue;
                }

                pulsar_grid[nx][ny][(t + manhattan(x, y, nx, ny)) % r] = 1;
            }
        }
    }
}

int bfs() {
    queue<pair<int, int>> q;
    q.push({x_s, y_s});
    dist[x_s][y_s] = 0;

    while (!q.empty()) {
        int x = q.front().first;
        int y = q.front().second;
        q.pop();

        if (x == x_f && y == y_f) {
            return dist[x][y];
        }

        for (int dx = -1; dx <= 1; dx++) {
            for (int dy = -1; dy <= 1; dy++) {
                int nx = x + dx;
                int ny = y + dy;

                if (!valid(nx, ny)) {
                    continue;
                }

                if (dist[nx][ny] != -1) {
                    continue;
                }

                bool safe = true;
                for (int i = 0; i < MAX_R; i++) {
                    if (pulsar_grid[nx][ny][i]) {
                        safe = false;
                        break;
                    }
                }

                if (!safe) {
                    continue;
                }

                dist[nx][ny] = dist[x][y] + 1;
                q.push({nx, ny});
            }
        }
    }

    return -1;
}

int main() {
    ifstream in("pulsar.in");
    ofstream out("pulsar.out");

    in >> C >> N >> P;

    for (int i = 0; i < P; i++) {
        int x, y, r, t;
        in >> x >> y >> r >> t;

        pulsars.push_back({x, y, r, t});
    }

    in >> x_s >> y_s >> x_f >> y_f;

    update_grid();

    if (C == 1) {
        int max_affected = 0;

        for (int i = 1; i <= N; i++) {
            for (int j = 1; j <= N; j++) {
                int affected = 0;

                for (int k = 0; k < MAX_R; k++) {
                    affected += pulsar_grid[i][j][k];
                }

                max_affected = max(max_affected, affected);
            }
        }

        out << max_affected << endl;
    } else {
        for (int i = 1; i <= N; i++) {
            for (int j = 1; j <= N; j++) {
                dist[i][j] = -1;
            }
        }

        int min_time = bfs();

        out << min_time << endl;
    }

    return 0;
}
