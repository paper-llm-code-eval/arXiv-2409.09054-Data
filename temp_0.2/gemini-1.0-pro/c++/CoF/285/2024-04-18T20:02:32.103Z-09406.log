    q.push({x_s, y_s});
    dist[x_s][y_s] = 0;

    while (!q.empty()) {
        int x = q.front().first;
        int y = q.front().second;
        q.pop();

        if (x == x_f && y == y_f) {
            return dist[x][y];
        }

        for (int dx = -1; dx <= 1; dx++) {
            for (int dy = -1; dy <= 1; dy++) {
                int nx = x + dx;
                int ny = y + dy;

                if (!in_bounds(nx, ny) || dist[nx][ny] != -1) {
                    continue;
