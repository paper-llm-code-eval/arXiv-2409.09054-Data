                    if (pulsar_grid[nx][ny][pt]) {
                        safe = false;
                        break;
                    }
                }

                if (safe) {
                    dist[nx][ny] = t;
                    q.push({nx, ny});
                }
            }
        }
    }

    return -1;
}

