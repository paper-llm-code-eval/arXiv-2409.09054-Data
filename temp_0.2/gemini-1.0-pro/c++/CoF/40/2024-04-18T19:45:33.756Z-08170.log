#include <fstream>
#include <vector>
#include <queue>

using namespace std;

const int INF = 1e9;

int main() {
    ifstream in("ubuntzei.in");
    ofstream out("ubuntzei.out");

    int n, m, k;
    in >> n >> m >> k;

    vector<vector<pair<int, int>>> adj(n + 1);
    for (int i = 0; i < m; i++) {
        int a, b, c;
        in >> a >> b >> c;
        adj[a].push_back({b, c});
        adj[b].push_back({a, c});
    }

    vector<int> friends(k);
    for (int i = 0; i < k; i++) {
        in >> friends[i];
    }

    vector<vector<int>> dist(n + 1, vector<int>(k + 1, INF));
    dist[1][0] = 0;

    priority_queue<pair<int, pair<int, int>>> pq;
    pq.push({0, {1, 0}});

    while (!pq.empty()) {
        int d = -pq.top().first;
        int u = pq.top().second.first;
        int mask = pq.top().second.second;
        pq.pop();

        if (u == n) {
            out << d << endl;
            return 0;
        }

        for (auto& edge : adj[u]) {
            int v = edge.first;
            int w = edge.second;

            if (dist[v][mask] > d + w) {
                dist[v][mask] = d + w;
                pq.push({-dist[v][mask], {v, mask}});
            }

            if (mask & (1 << (find(friends.begin(), friends.end(), v) - friends.begin()))) {
                continue;
            }

            if (dist[v][mask | (1 << (find(friends.begin(), friends.end(), v) - friends.begin()))] > d + w) {
                dist[v][mask | (1 << (find(friends.begin(), friends.end(), v) - friends.begin()))] = d + w;
                pq.push({-dist[v][mask | (1 << (find(friends.begin(), friends.end(), v) - friends.begin()))], {v, mask | (1 << (find(friends.begin(), friends.end(), v) - friends.begin()))}});
            }
        }
    }

    out << -1 << endl;
    return 0;
}
