#include <fstream>
#include <vector>
#include <queue>

using namespace std;

const int MAXN = 2000;
const int MAXM = 10000;
const int INF = 1e9;

int n, m, k;
vector<pair<int, int>> adj[MAXN + 1];
int dist[MAXN + 1];
bool visited[MAXN + 1];
vector<int> path;

void dijkstra(int start) {
    for (int i = 1; i <= n; i++) {
        dist[i] = INF;
        visited[i] = false;
    }
    dist[start] = 0;
    priority_queue<pair<int, int>> pq;
    pq.push({0, start});
    while (!pq.empty()) {
        int u = pq.top().second;
        pq.pop();
        if (visited[u]) continue;
        visited[u] = true;
        for (auto& edge : adj[u]) {
            int v = edge.first;
            int w = edge.second;
            if (dist[v] > dist[u] + w) {
                dist[v] = dist[u] + w;
                pq.push({-dist[v], v});
            }
        }
    }
}

int main() {
    ifstream fin("ubuntzei.in");
    ofstream fout("ubuntzei.out");
    fin >> n >> m >> k;
    vector<int> friends(k);
    for (int i = 0; i < k; i++) {
        fin >> friends[i];
    }
    for (int i = 0; i < m; i++) {
        int u, v, w;
        fin >> u >> v >> w;
        adj[u].push_back({v, w});
        adj[v].push_back({u, w});
    }
    dijkstra(1);
    int min_dist = INF;
    for (int friend : friends) {
        min_dist = min(min_dist, dist[friend]);
    }
    dijkstra(n);
    for (int friend : friends) {
        min_dist = min(min_dist, dist[friend]);
    }
    fout << min_dist << endl;
    return 0;
}
