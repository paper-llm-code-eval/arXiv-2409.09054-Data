int a[MAXN][MAXM];
int dp[MAXN][MAXM][4];
int dx[] = {0, 0, -1, 1};
int dy[] = {-1, 1, 0, 0};

struct Node {
    int x, y, zone;
    Node(int x, int y, int zone) : x(x), y(y), zone(zone) {}
};

int main() {
    fin >> n >> m >> x >> y;
    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= m; ++j) {
            fin >> a[i][j];
        }
    }

    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= m; ++j) {
            for (int k = 1; k <= 4; ++k) {
                dp[i][j][k] = -1;
            }
        }
    }

    queue<Node> q;
    q.push(Node(x, y, 0));
    dp[x][y][0] = 0;

    while (!q.empty()) {
        Node u = q.front();
        q.pop();

        for (int i = 0; i < 4; ++i) {
            int nx = u.x + dx[i];
            int ny = u.y + dy[i];
            int nz = u.zone;

            if (nx < 1 || nx > n || ny < 1 || ny > m) {
                continue;
            }

            if (u.zone == 0) {
                if (nx == 1 || ny == 1) {
                    nz = 1;
