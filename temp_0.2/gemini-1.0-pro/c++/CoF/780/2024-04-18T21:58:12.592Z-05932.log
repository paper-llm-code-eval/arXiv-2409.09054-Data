        set<pair<int, int>> intervals;
        for (int j = 0; j < v[i].size(); j++) {
            if (j == 0 || v[i][j] != v[i][j - 1]) {
                int l = v[i][j];
                int r = v[i][j];
                while (j + 1 < v[i].size() && v[i][j + 1] == r + 1) {
                    r++;
                    j++;
