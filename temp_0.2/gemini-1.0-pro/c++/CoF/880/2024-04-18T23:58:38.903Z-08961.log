#include <fstream>
#include <iostream>
#include <string>
#include <vector>

using namespace std;

int main() {
  ifstream in("robot.in");
  ofstream out("robot.out");
  int c;
  in >> c;
  string s;
  in >> s;
  int n = s.size();
  if (c == 1) {
    int ans = 0;
    for (int i = 0; i < n; i++) {
      if (s[i] == 'A') ans++;
    }
    out << ans << endl;
  } else if (c == 2) {
    vector<int> v;
    int pos = 0;
    for (int i = 0; i < n; i++) {
      if (s[i] == 'D') {
        pos += s[i + 1] - '0';
        pos %= 10;
      } else if (s[i] == 'S') {
        pos -= s[i + 1] - '0';
        pos %= 10;
        if (pos < 0) pos += 10;
      } else if (s[i] == 'A') {
        v.push_back(pos);
      }
    }
    for (int i = 0; i < v.size(); i++) {
      out << v[i];
    }
    out << endl;
  } else if (c == 3) {
    string ans = "";
    int pos = 0;
    int nr = 0;
    while (nr != n) {
      int d = n - nr;
      if (d > 9) d = 9;
      if (pos + d <= 9) {
        ans += 'D' + to_string(d);
        pos += d;
        nr += d;
      } else {
        int s = 10 - pos;
        ans += 'S' + to_string(s);
        pos -= s;
        nr += s;
      }
      ans += 'A';
      nr++;
    }
    ans += 'T';
    out << ans << endl;
  }
  return 0;
}
