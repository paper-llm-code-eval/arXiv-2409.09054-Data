#include <fstream>
#include <vector>

using namespace std;

ifstream in("tbile.in");
ofstream out("tbile.out");

int main() {
  int c, n, m, v;
  in >> c >> n >> m;
  if (c == 2) in >> v;
  vector<long long> x(n + 1), y(n + 1);
  x[1] = 1;
  y[1] = 2;
  y[2] = 4;
  int i = 3, j = 3;
  while (x[i - 1] + y[i - 1] <= n) {
    x[i] = x[i - 1] + y[i - 1];
    y[j++] = x[i - 1] + 1;
    y[j++] = x[i - 1] + 2;
    y[j++] = x[i - 1] + 3;
    i++;
  }
  while (i <= n) {
    y[j++] = i++;
  }
  if (c == 1) {
    int nr_tuburi = (j - 1) / m + 1;
    out << nr_tuburi << ' ' << (j - 1) << '\n';
  } else {
    long long sum = 0;
    for (int i = 1; i <= j; i++) {
      if (v <= (i - 1) / m + 1) {
        sum += y[i];
      }
    }
    out << sum << '\n';
  }
  return 0;
}
