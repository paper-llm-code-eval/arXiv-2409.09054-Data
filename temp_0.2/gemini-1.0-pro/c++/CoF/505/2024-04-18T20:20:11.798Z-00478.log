#include <vector>
#include <algorithm>

using namespace std;

const int MOD = 1e9 + 9;

struct interval {
    int start, end, type;
};

int main() {
    ifstream in("fotbal.in");
    ofstream out("fotbal.out");

    int n, k;
    in >> n >> k;

    vector<interval> intervals;
    for (int i = 0; i < n; i++) {
        int start, end, type;
        in >> start >> end >> type;
        intervals.push_back({start, end, type});
    }

    sort(intervals.begin(), intervals.end(), [](const interval& a, const interval& b) {
        return a.end < b.end;
    });

    int dp[n + 1][k + 1][2];
    for (int i = 0; i <= n; i++) {
        for (int j = 0; j <= k; j++) {
            for (int l = 0; l < 2; l++) {
                dp[i][j][l] = 0;
            }
        }
    }

    dp[0][0][0] = dp[0][0][1] = 1;

    for (int i = 0; i < n; i++) {
        for (int j = 0; j <= k; j++) {
            for (int l = 0; l < 2; l++) {
                dp[i + 1][j][l] = (dp[i + 1][j][l] + dp[i][j][l]) % MOD;
                if (j < k && (l == 0 || intervals[i].type == 1) && (i == 0 || intervals[i].start > intervals[i - 1].end || intervals[i].start == intervals[i - 1].start && intervals[i].type != intervals[i - 1].type || intervals[i].start == intervals[i - 1].start && intervals[i].type == intervals[i - 1].type && (j > 0 || i > 0)) || (i == n - 1 && j == k - 1)) {
                    dp[i + 1][j + 1][1] = (dp[i + 1][j + 1][1] + dp[i][j][l]) % MOD;
                }
                if (j < k && (l == 1 || intervals[i].type == 0) && (i == 0 || intervals[i].start > intervals[i - 1].end || intervals[i].start == intervals[i - 1].start && intervals[i].type != intervals[i - 1].type || intervals[i].start == intervals[i - 1].start && intervals[i].type == intervals[i - 1].type && (j > 0 || i > 0)) || (i == n - 1 && j == k - 1)) {
                    dp[i + 1][j + 1][0] = (dp[i + 1][j + 1][0] + dp[i][j][l]) % MOD;
                }
            }
        }
    }

    out << dp[n][k][0] << '\n';

    return 0;
}
