#include <numeric>

using namespace std;

const int MOD = 1e9 + 9;

int main() {
    ifstream fin("fotbal.in");
    ofstream fout("fotbal.out");

    int n, k;
    fin >> n >> k;

    vector<pair<int, int>> intervals(n);
    vector<bool> is_left(n);
    for (int i = 0; i < n; i++) {
        fin >> intervals[i].first >> intervals[i].second >> is_left[i];
    }

    sort(intervals.begin(), intervals.end());

    int num_left = 0;
    int num_right = 0;
    int ans = 0;
    for (int i = 0; i < n; i++) {
        if (is_left[i]) {
            num_left++;
        } else {
            num_right++;
        }

        if (num_left >= 1 && num_right >= 1) {
            int overlap = min(intervals[i].second, intervals[i + k - 1].second) - max(intervals[i].first, intervals[i + k - 1].first);
            if (overlap >= 0) {
                ans = (ans + overlap) % MOD;
            }
        }
    }

    fout << ans << '\n';

    return 0;
}
