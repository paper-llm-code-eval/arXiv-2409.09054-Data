using namespace std;

struct Tren {
    int linie, ora, minut, durata;
};

int main() {
    ifstream in("tren.in");
    ofstream out("tren.out");

    int T;
    in >> T;

    vector<Tren> trenuri(T);
    for (int i = 0; i < T; i++) {
        in >> trenuri[i].linie >> trenuri[i].ora >> trenuri[i].minut >> trenuri[i].durata;
    }

    // Sortam trenurile dupa momentul sosirii
    sort(trenuri.begin(), trenuri.end(), [](const Tren& a, const Tren& b) {
        return a.ora < b.ora || (a.ora == b.ora && a.minut < b.minut);
    });

    // Initializam variabilele de iesire
    int Z = 0, X = 0, Y = 0, max_trenuri = 0, timp_fara_trenuri = 0;
    int linii[2] = {0, 0};

    // Iteram prin trenuri
    for (const Tren& tren : trenuri) {
        // Actualizam numarul de trenuri pe fiecare linie
        linii[tren.linie - 1]++;

        // Actualizam numarul maxim de trenuri pe aceeasi linie
        max_trenuri = max(max_trenuri, linii[tren.linie - 1]);

        // Actualizam numarul de trenuri vizibile
        X += linii[0] + linii[1] - 1;

        // Actualizam timpul fara trenuri
        timp_fara_trenuri = max(timp_fara_trenuri, tren.ora * 60 + tren.minut - trenuri[0].ora * 60 - trenuri[0].minut);

        // Actualizam timpul fara trenuri dupa plecarea trenului
        timp_fara_trenuri = max(timp_fara_trenuri, tren.ora * 60 + tren.minut + tren.durata - trenuri[0].ora * 60 - trenuri[0].minut);

        // Actualizam momentul sosirii primului tren
        trenuri[0] = tren;
    }

    // Scriem rezultatele in fisierul de iesire
    out << max_trenuri << " " << X << " " << timp_fara_trenuri << endl;

    return 0;
}
