#include <algorithm>

using namespace std;

int main() {
    ifstream in("adunscad.in");
    ofstream out("adunscad.out");

    int n, m;
    in >> n >> m;
    vector<int> digits(m);
    for (int i = 0; i < m; i++) {
        in >> digits[i];
    }

    sort(digits.begin(), digits.end());

    int sum = 0;
    for (int i = 0; i < m; i++) {
        sum += digits[i];
    }

    if (sum < n || (sum - n) % 2 != 0) {
        out << 0 << endl;
        return 0;
    }

    int target = (sum - n) / 2;

    vector<int> subset;
