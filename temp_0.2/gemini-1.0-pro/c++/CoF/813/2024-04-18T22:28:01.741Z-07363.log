    for (int i = 0; i < subset.size(); i++) {
        subset_sum += subset[i];
    }

    string expression;
    for (int i = 0; i < subset.size(); i++) {
        if (i > 0) {
            expression += "+";
        }
        expression += to_string(subset[i]);
    }

    for (int i = subset.size(); i < m; i++) {
        expression += "-" + to_string(digits[i]);
    }

    out << expression << endl;

    return 0;
}
