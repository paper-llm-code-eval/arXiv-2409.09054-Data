#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("scara.in");
ofstream fout("scara.out");

int n, m, p, h;
vector<int> v;

int main() {
    fin >> h >> n >> m >> p;
    v.resize(n);
    for (int i = 0; i < n; i++) {
        v[i] = 1;
    }
    int sum = n;
    while (sum < h) {
        int mx = 0;
        for (int i = 0; i < n; i++) {
            if (v[i] > mx) {
                mx = v[i];
            }
