    double efort = 0;
    for (int i = 0; i < n; i++) {
        efort += (double)v[i] / 2;
    }
    efort += (double)p * (n - 1);
    fout << fixed << setprecision(2) << efort << '\n';
    for (int i = 0; i < n; i++) {
        fout << v[i] << ' ';
    }
    return 0;
}
