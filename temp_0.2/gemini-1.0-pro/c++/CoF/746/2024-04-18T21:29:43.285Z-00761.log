#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("scara.in");
ofstream fout("scara.out");

int H, N, M, p;
vector<int> h;
vector<int> sol;

double min_effort = 1e9;

void back(int sum, int pos, int effort) {
    if (pos == N) {
        if (sum == H) {
            if (effort < min_effort) {
                min_effort = effort;
                sol = h;
            }
        }
        return;
    }
    for (int i = 1; i <= M; i++) {
        if (sum + i <= H) {
            h[pos] = i;
            back(sum + i, pos + 1, effort + (double)(i + p) / 2);
        }
    }
}

int main() {
    fin >> H >> N >> M >> p;
    h.resize(N);
    back(0, 0, 0);
    fout << fixed << setprecision(2) << min_effort << '\n';
    for (int i = 0; i < N; i++) {
        fout << sol[i] << ' ';
    }
    return 0;
}
```
