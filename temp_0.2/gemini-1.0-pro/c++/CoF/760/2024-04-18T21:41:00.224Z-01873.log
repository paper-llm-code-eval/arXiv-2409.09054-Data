      up = true;
    } else {
      if (up) {
        special++;
        fuel += stations[i].first - stations[i - 1].first;
      }
      up = false;
    }
  }

  out << people << endl;
  out << fuel << endl;
  out << special << endl;

  return 0;
}
```
