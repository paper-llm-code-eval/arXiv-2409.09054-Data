#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream in("telecabina.in");
  ofstream out("telecabina.out");

  int n;
  in >> n;

  vector<pair<int, int>> stations(n);
  for (int i = 0; i < n; i++) {
    in >> stations[i].first >> stations[i].second;
  }

  int people = 0;
  int fuel = 0;
  int special = 0;
  bool up = true;

  for (int i = 0; i < n - 1; i++) {
    int alt = stations[i + 1].first - stations[i].first;
    int people_in = stations[i].second;

    if (up) {
      fuel += alt * 3;
      people += people_in;
    } else {
      fuel -= alt;
    }

    if (alt < 0) {
      up = false;
      special++;
    }
  }

  out << people << endl;
  out << fuel << endl;
  out << special << endl;

  return 0;
}
```
