#include <fstream>
#include <vector>
#include <algorithm>
#include <queue>

using namespace std;

const int MAXN = 255;
const int MAXM = 32384;
const int MAXK = 255;

struct Edge {
    int u, v, w;
    Edge(int u, int v, int w) : u(u), v(v), w(w) {}
};

vector<Edge> edges;
vector<int> adj[MAXN];
int dist[MAXN][MAXN];
int par[MAXN][MAXN];
int vis[MAXN];
int n, m, k;

void read_input() {
    ifstream fin("urgenta.in");
    fin >> n >> m >> k;
    for (int i = 0; i < m; i++) {
        int u, v, w;
        fin >> u >> v >> w;
        edges.push_back(Edge(u, v, w));
        adj[u].push_back(i);
        adj[v].push_back(i);
    }
    fin.close();
}

void write_output(int gravmax, int c, vector<Edge> &destroyed) {
    ofstream fout("urgenta.out");
    fout << gravmax << endl;
    fout << c << endl;
    for (Edge e : destroyed) {
        fout << e.u << ' ' << e.v << endl;
    }
    fout.close();
}

void bfs(int s) {
    queue<int> q;
    q.push(s);
    vis[s] = 1;
    while (!q.empty()) {
        int u = q.front();
        q.pop();
        for (int i : adj[u]) {
            int v = edges[i].u + edges[i].v - u;
            if (!vis[v]) {
                q.push(v);
                vis[v] = 1;
                dist[s][v] = dist[s][u] + 1;
                par[s][v] = i;
            }
        }
    }
}

int main() {
    read_input();
    for (int i = 1; i <= n; i++) {
        bfs(i);
    }
    int gravmax = 0;
    vector<Edge> destroyed;
    for (int i = 1; i <= n; i++) {
        for (int j = i + 1; j <= n; j++) {
            if (dist[i][j] == 0) {
                continue;
            }
            int u = i, v = j;
            while (u != v) {
                if (dist[i][u] > dist[i][v]) {
                    destroyed.push_back(edges[par[i][u]]);
                    gravmax += edges[par[i][u]].w;
                    u = edges[par[i][u]].u + edges[par[i][u]].v - u;
                } else {
                    destroyed.push_back(edges[par[j][v]]);
                    gravmax += edges[par[j][v]].w;
                    v = edges[par[j][v]].u + edges[par[j][v]].v - v;
                }
            }
        }
    }
    write_output(gravmax, destroyed.size(), destroyed);
    return 0;
}
```
