#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream in("urgenta.in");
ofstream out("urgenta.out");

const int NMAX = 255;
const int MMAX = 32768;
const int INF = 0x3f3f3f3f;

int n, m, k;
int t[NMAX][NMAX];
int p[MMAX];
int viz[NMAX];
int sol[MMAX];

vector<int> g[NMAX];
vector<int> ord;

