    viz[nod] = 1;
    ord.push_back(nod);
    for (int vecin : g[nod]) {
        if (!viz[vecin]) {
            dfs(vecin);
        }
    }
}

int main() {
    in >> n >> m >> k;
    for (int i = 1; i <= m; ++i) {
        int a, b, c;
        in >> a >> b >> c;
        g[a].push_back(b);
        g[b].push_back(a);
        p[i] = c;
    }

    for (int i = 1; i <= n; ++i) {
        if (!viz[i]) {
            dfs(i);
        }
    }

    reverse(ord.begin(), ord.end());

    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < n; ++j) {
            t[i][j] = INF;
        }
    }

    for (int i = 0; i < n; ++i) {
        t[i][i] = 0;
    }

    for (int i = 0; i < m; ++i) {
        int a = ord[i];
        int b = ord[i + 1];
        t[a][b] = t[b][a] = p[i + 1];
    }

    for (int k = 0; k < n; ++k) {
        for (int i = 0; i < n; ++i) {
            for (int j = 0; j < n; ++j) {
                t[i][j] = min(t[i][j], t[i][k] + t[k][j]);
            }
        }
    }

    int gravmax = -1;
    int nr_sol = -1;
    for (int i = 0; i < n; ++i) {
        for (int j = i + 1; j < n; ++j) {
            if (t[i][j] != INF) {
                int grav = 0;
                int nr = 0;
                for (int k = 0; k < m; ++k) {
                    int a = ord[k];
                    int b = ord[k + 1];
                    if (t[i][j] == t[i][a] + p[k + 1] + t[b][j]) {
                        sol[nr++] = k + 1;
                        grav += p[k + 1];
                    }
                }
                if (nr > nr_sol || (nr == nr_sol && grav > gravmax)) {
                    nr_sol = nr;
                    gravmax = grav;
                    for (int l = 0; l < nr; ++l) {
                        swap(ord[sol[l]], ord[sol[l + 1]]);
                    }
                }
            }
        }
    }

    out << gravmax << '\n';
    out << nr_sol << '\n';
    for (int i = 0; i < nr_sol; ++i) {
        out << ord[sol[i]] << ' ' << ord[sol[i + 1]] << '\n';
    }

    return 0;
}
