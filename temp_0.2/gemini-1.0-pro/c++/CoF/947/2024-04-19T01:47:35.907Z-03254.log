#include <fstream>
#include <vector>

using namespace std;

const int MAX_N = 1e9;
const int MAX_NR = 50000;
const int MAX_L = 20000;
const int MAX_POS = 5000;

int main() {
  ifstream in("strips.in");
  ofstream out("strips.out");

  int c;
  in >> c;

  int n, nr, l;
  in >> n >> nr >> l;

  vector<int> strips[2];
  strips[0].resize(nr);
  strips[1].resize(nr);

  int points[2] = {0, 0};
  int max_len[2] = {0, 0};

  int pos;
  while (in >> pos) {
    int player = pos % 2;
    int strip_idx = pos / 2;

    if (pos + l - 1 >= n) {
      points[player]++;
      continue;
    }

    bool valid = true;
    for (int i = 0; i < l; i++) {
      if (strips[1 - player][i + pos] != 0) {
        valid = false;
        break;
      }
    }

    if (valid) {
      for (int i = 0; i < l; i++) {
        strips[player][i + pos] = 1;
      }
      max_len[player] = max(max_len[player], l);
    } else {
      points[player]++;
    }
  }

  if (c == 1) {
    out << points[0] << " " << points[1] << endl;
  } else {
    out << max_len[0] << " " << max_len[1] << endl;
  }

  return 0;
}
