#include <fstream>
#include <vector>

using namespace std;

ifstream fin("nr.in");
ofstream fout("nr.out");

const int MAXN = 2e9;

int main() {
    int n, k, p;
    fin >> n >> k >> p;

    vector<int> v(MAXN);
    v[1] = 1;
    v[2] = 2;
    v[3] = 3;
    v[4] = 5;
    v[5] = 8;
    int i = 6;
    int j = 3;
    while (i <= p) {
        v[i] = v[j];
        i++;
        j++;
        v[i] = v[j];
        i++;
        j++;
        v[i] = v[j];
        i++;
        j++;
        v[i] = v[j] + v[j - 1];
        i++;
        j++;
        v[i] = v[j] + v[j - 1] + v[j - 2];
        i++;
        j++;
        v[i] = v[j];
        i++;
        j++;
        v[i] = v[j];
        i++;
        j++;
        v[i] = v[j] + v[j - 1];
        i++;
        j++;
    }

    int sum = 0;
    for (int i = 1; i <= n; i++) {
        if (v[i] % 2 == 1 && v[i] != 1) {
            sum += v[i];
        }
    }

    int cnt = 0;
    for (int i = 1; i <= n; i++) {
        int aux = v[i];
        while (aux > 0) {
            if (aux % 10 == k) {
                cnt++;
            }
            aux /= 10;
        }
    }

    fout << sum << '\n';
    fout << cnt << '\n';
    fout << v[p] << '\n';

    return 0;
}
