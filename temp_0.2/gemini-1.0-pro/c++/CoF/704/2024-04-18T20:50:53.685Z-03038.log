#include <fstream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
  ifstream in("cuburi.in");
  ofstream out("cuburi.out");
  int n;
  in >> n;
  vector<int> v(n);
  for (int i = 0; i < n; i++) in >> v[i];
  int fr[11] = {0};
  for (int i = 0; i < n; i++) fr[v[i]]++;
  int mx = 0, mx_color = 0;
  for (int i = 1; i <= 10; i++) {
    if (fr[i] > mx) {
      mx = fr[i];
      mx_color = i;
    }
  }
  int cnt = 0;
  for (int i = 1; i <= 10; i++) if (fr[i] > 0) cnt++;
  out << cnt << '\n' << mx_color << '\n';
  int last = -1, st = -1, dr = -1, mx_len = 0;
  for (int i = 0; i < n; i++) {
    if (v[i] == mx_color) {
      if (last == -1) {
        st = i;
        last = v[i];
      } else if (last != v[i]) {
        if (i - st > mx_len) {
          mx_len = i - st;
          dr = i - 1;
        }
        st = i;
        last = v[i];
      }
    }
  }
  if (i - st > mx_len) {
    mx_len = i - st;
    dr = i - 1;
  }
  out << mx_len << '\n';
  for (int i = st; i <= dr; i++) out << i + 1 << ' ';
  out << '\n';
  return 0;
}
