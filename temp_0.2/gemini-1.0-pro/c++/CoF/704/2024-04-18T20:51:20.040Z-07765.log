#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("cuburi.in");
    ofstream out("cuburi.out");
    int n;
    in >> n;
    vector<int> v(n);
    for (int i = 0; i < n; i++) {
        in >> v[i];
    }
    int cnt = 0;
    for (int i = 0; i < n; i++) {
        if (i == 0 || v[i] != v[i - 1]) {
            cnt++;
        }
    }
    out << cnt << '\n';
    int maxCnt = 0;
    int maxColor = 0;
    for (int i = 1; i <= 10; i++) {
        int cnt = 0;
        for (int j = 0; j < n; j++) {
            if (v[j] == i) {
                cnt++;
            }
        }
        if (cnt > maxCnt) {
            maxCnt = cnt;
            maxColor = i;
        }
    }
    out << maxColor << '\n';
    int last = 0;
    int maxLen = 0;
    int start = 0;
    for (int i = 0; i < n; i++) {
        if (v[i] != v[last]) {
            if (i - last > maxLen) {
                maxLen = i - last;
                start = last;
            }
            last = i;
        }
    }
    if (n - last > maxLen) {
        maxLen = n - last;
        start = last;
    }
    out << maxLen << '\n';
    for (int i = start; i < start + maxLen; i++) {
        out << i + 1 << ' ';
    }
    out << '\n';
    return 0;
}
