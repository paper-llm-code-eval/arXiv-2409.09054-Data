#include <fstream>
#include <vector>
#include <algorithm>
#include <cmath>

using namespace std;

struct Point {
    int x, y;
    Point() : x(0), y(0) {}
    Point(int x, int y) : x(x), y(y) {}
    Point operator+(const Point& other) const { return Point(x + other.x, y + other.y); }
    Point operator-(const Point& other) const { return Point(x - other.x, y - other.y); }
    int dot(const Point& other) const { return x * other.x + y * other.y; }
    int cross(const Point& other) const { return x * other.y - y * other.x; }
    double dist(const Point& other) const { return sqrt((x - other.x) * (x - other.x) + (y - other.y) * (y - other.y)); }
};

struct Segment {
    Point p1, p2;
    Segment() : p1(Point()), p2(Point()) {}
    Segment(const Point& p1, const Point& p2) : p1(p1), p2(p2) {}
    double dist(const Point& p) const {
        Point v1 = p2 - p1, v2 = p - p1;
        double dot = v1.dot(v2);
        if (dot <= 0) return p.dist(p1);
        if (v1.dot(v1) <= dot) return p.dist(p2);
        double t = dot / v1.dot(v1);
        Point proj = p1 + v1 * t;
        return p.dist(proj);
    }
};

struct Pole {
    Point p;
    int d;
    Pole() : p(Point()), d(0) {}
    Pole(const Point& p, int d) : p(p), d(d) {}
};

double area(const vector<Point>& points) {
    double sum = 0;
    for (int i = 0; i < points.size(); i++) {
        sum += points[i].cross(points[(i + 1) % points.size()]);
    }
    return abs(sum) / 2.0;
}

double max_area(vector<Pole>& poles) {
    int n = poles.size();
    vector<Segment> segments;
    for (int i = 0; i < n; i++) {
        segments.push_back(Segment(poles[i].p, poles[(i + 1) % n].p));
    }
    double max_area = 0;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            if (i == j) continue;
            Point v = poles[j].p - poles[i].p;
            if (v.dist(Point(0, 0)) > poles[i].d) continue;
            vector<Point> points;
            for (int k = 0; k < n; k++) {
                if (k == i || k == j) continue;
                double dist = segments[k].dist(poles[i].p + v);
                if (dist <= poles[k].d) {
                    points.push_back(segments[k].p1);
                    points.push_back(segments[k].p2);
                }
            }
            max_area = max(max_area, area(points));
        }
    }
    return max_area;
}

int main() {
    ifstream fin("mosia.in");
    ofstream fout("mosia.out");
    int n;
    fin >> n;
    vector<Pole> poles;
    for (int i = 0; i < n; i++) {
        int x, y, d;
        fin >> x >> y >> d;
        poles.push_back(Pole(Point(x, y), d));
    }
    fout << fixed << setprecision(4) << max_area(poles) << endl;
    return 0;
}
