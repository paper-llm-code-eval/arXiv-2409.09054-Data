#include <fstream>
#include <iostream>
#include <sstream>
#include <string>
#include <vector>

using namespace std;

int main() {
  ifstream in("ecuatii.in");
  ofstream out("ecuatii.out");

  int n;
  in >> n;
  in.ignore();

  for (int i = 0; i < n; i++) {
    string line;
    getline(in, line);

    vector<int> coefficients;
    int constant = 0;
    bool is_x = false;
    int sign = 1;

    for (char c : line) {
      if (c == '+') {
        sign = 1;
      } else if (c == '-') {
        sign = -1;
      } else if (c == 'x') {
        is_x = true;
        coefficients.push_back(sign);
      } else {
        int num = 0;
        while (isdigit(c)) {
          num = num * 10 + (c - '0');
          c = in.get();
        }
        in.unget();
        if (is_x) {
          coefficients.push_back(sign * num);
        } else {
          constant += sign * num;
        }
        is_x = false;
      }
    }

    int x_coefficient = 0;
    for (int c : coefficients) {
      x_coefficient += c;
    }

    if (x_coefficient == 0) {
      if (constant == 0) {
        out << "infinit" << endl;
      } else {
        out << "imposibil" << endl;
      }
    } else {
      double x = (double)-constant / x_coefficient;
      out << fixed << setprecision(4) << x << endl;
    }
  }

  in.close();
  out.close();

  return 0;
}
