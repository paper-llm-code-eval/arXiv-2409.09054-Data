#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream fin("concurs.in");
  ofstream fout("concurs.out");
  int h, n;
  fin >> h >> n;
  vector<int> premii(n);
  for (int i = 0; i < n; i++) { fin >> premii[i]; }
  bool premiat = false;
  int oras = 0, scoala = 0;
  for (int i = 0; i < n; i++) {
