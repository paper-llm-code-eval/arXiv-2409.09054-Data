    out << cnt << endl;
  } else {
    for (int i = 0; i < p; i++) {
      int cnt = 0;
      for (int j = 0; j < s; j++) {
        if (disk[i][j] == 1) cnt++;
      }
      if (cnt == 0) {
        out << 0 << " ";
        continue;
      }
      vector<int> pos;
      for (int j = 0; j < s; j++) {
        if (disk[i][j] == 1) pos.push_back(j);
