#include <fstream>
#include <vector>

using namespace std;

ifstream fin("maxd.in");
ofstream fout("maxd.out");

const int MAXN = 1e9;
vector<int> nrdiv(MAXN + 1, 0);

int main() {
  int a, b;
  fin >> a >> b;

  for (int i = 1; i <= MAXN; i++) {
    for (int j = i; j <= MAXN; j += i) {
      nrdiv[j]++;
    }
  }

  int maxdiv = 0, minnr = MAXN, cnt = 0;
  for (int i = a; i <= b; i++) {
    if (nrdiv[i] > maxdiv) {
      maxdiv = nrdiv[i];
      minnr = i;
      cnt = 1;
    } else if (nrdiv[i] == maxdiv) {
      cnt++;
    }
  }

  fout << minnr << " " << maxdiv << " " << cnt << "\n";

  return 0;
}
```
