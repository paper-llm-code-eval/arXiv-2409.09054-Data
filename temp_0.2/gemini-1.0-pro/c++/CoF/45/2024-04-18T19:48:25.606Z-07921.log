#include <fstream>
#include <vector>
#include <algorithm>
#include <numeric>
#include <functional>
#include <array>
#include <cassert>

namespace {
    const int MOD = 30011;
    const int MAX_N = 100;
    const int MAX_K = 100;

    std::vector<std::vector<int>> adj;
    std::vector<int> dp[MAX_N][MAX_K];
    std::vector<int> dp_sum[MAX_N];

    void dfs(int node, int parent) {
        dp[node][0][0] = 1;
        for (int k = 1; k <= K; ++k) {
            for (int i = 0; i < k; ++i) {
                dp[node][k][i] = dp[node][k - 1][i];
            }
            for (int i = k; i <= K; ++i) {
                dp[node][k][i] = (dp[node][k - 1][i] + dp[node][k - 1][i - 1]) % MOD;
            }
        }
        for (int child : adj[node]) {
            if (child == parent) {
                continue;
            }
            dfs(child, node);
            for (int k = 0; k <= K; ++k) {
                for (int i = 0; i <= K; ++i) {
                    dp_sum[node][k] = (dp_sum[node][k] + dp[child][k][i]) % MOD;
                }
            }
        }
        for (int k = 0; k <= K; ++k) {
            for (int i = 0; i <= K; ++i) {
                dp[node][k][i] = (dp[node][k][i] + dp_sum[node][k]) % MOD;
            }
        }
    }
}

int main() {
    std::ifstream in("iepuri.in");
    std::ofstream out("iepuri.out");

    int N, K;
    in >> N >> K;

    adj.resize(N + 1);
    for (int i = 0; i < N - 1; ++i) {
        int a, b;
        in >> a >> b;
        adj[a].push_back(b);
        adj[b].push_back(a);
    }

    dfs(1, 1);

    out << dp[1][K][K] << '\n';

    return 0;
}
