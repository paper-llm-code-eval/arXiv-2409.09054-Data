#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MOD = 30011;

int n, k;
vector<vector<int>> adj;
vector<int> dp;

int dfs(int node, int parent) {
    dp[node] = 1;
    for (int child : adj[node]) {
        if (child == parent) continue;
        dp[node] = (dp[node] * dfs(child, node)) % MOD;
    }
