    dp.resize(n + 1);
    for (int i = 1; i < n; i++) {
        int a, b;
        fin >> a >> b;
        adj[a].push_back(b);
        adj[b].push_back(a);
    }
    fout << dfs(1, 1) << '\n';
    return 0;
}
