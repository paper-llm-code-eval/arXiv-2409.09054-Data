#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MOD = 30011;

int n, k;
vector<vector<int>> adj;
vector<int> dp;

int dfs(int node, int parent) {
    dp[node] = 1;
    for (int child : adj[node]) {
        if (child == parent) continue;
        dp[node] = (dp[node] * (dfs(child, node) + 1)) % MOD;
    }
    return dp[node];
}

int main() {
    ifstream in("iepuri.in");
    ofstream out("iepuri.out");

    in >> n >> k;
    adj.resize(n + 1);
    dp.resize(n + 1);

    for (int i = 1; i < n; i++) {
        int a, b;
        in >> a >> b;
        adj[a].push_back(b);
        adj[b].push_back(a);
    }

    dfs(1, 1);

    out << dp[1] << '\n';

    return 0;
}
```
