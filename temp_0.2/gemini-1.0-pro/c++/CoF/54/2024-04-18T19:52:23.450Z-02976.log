    ofstream out("lanterna.out");

    int n, k;
    in >> n >> k;

    vector<bool> bases(n + 1);
    for (int i = 1; i <= n; i++) {
        int x;
        in >> x;
        bases[i] = x;
    }

    vector<vector<Edge>> graph(n + 1);
    int m;
    in >> m;
    for (int i = 0; i < m; i++) {
        int a, b, t, w;
        in >> a >> b >> t >> w;
        graph[a].push_back({b, t, w});
        graph[b].push_back({a, t, w});
    }

    priority_queue<State> pq;
    pq.push({1, 1, 0, 0});

    vector<vector<int>> dist(n + 1, vector<int>(k + 1, numeric_limits<int>::max()));
    dist[1][1] = 0;

    while (!pq.empty()) {
        State state = pq.top();
        pq.pop();

        if (state.node == n) {
            out << state.time << " " << state.lantern << endl;
            return 0;
        }

        for (Edge edge : graph[state.node]) {
            if (state.watts >= edge.watts) {
                int newTime = state.time + edge.time;
                int newWatts = state.watts - edge.watts;
                if (newTime < dist[edge.to][state.lantern]) {
                    dist[edge.to][state.lantern] = newTime;
                    pq.push({edge.to, state.lantern, newTime, newWatts});
                }
            }

            if (bases[edge.to]) {
                int newTime = state.time + edge.time;
                int newWatts = k;
                if (newTime < dist[edge.to][1]) {
                    dist[edge.to][1] = newTime;
                    pq.push({edge.to, 1, newTime, newWatts});
                }
            }
        }
    }

    out << -1 << endl;
    return 0;
}
