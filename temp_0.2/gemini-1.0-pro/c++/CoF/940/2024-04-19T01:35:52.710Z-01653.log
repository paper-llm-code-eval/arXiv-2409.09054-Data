#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("tort.in");
ofstream fout("tort.out");

const int MAXN = 200000;
const int MOD = 1e9 + 7;

int n;
int a[MAXN + 5];
int dp[MAXN + 5][400005];

int main() {
    fin >> n;
    for (int i = 1; i <= n; i++) {
        fin >> a[i];
    }

    dp[n + 1][0] = 1;
    for (int i = n; i >= 1; i--) {
        for (int sum = 0; sum <= 400000; sum++) {
            dp[i][sum] = (dp[i + 1][sum] + dp[i + 1][sum + a[i]]) % MOD;
        }
    }

    int sum = 0;
    for (int i = 1; i <= n; i++) {
        sum += a[i];
    }

    fout << dp[1][sum / 2] << '\n';

    return 0;
}
```
