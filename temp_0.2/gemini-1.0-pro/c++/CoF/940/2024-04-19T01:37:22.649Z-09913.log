#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAXN = 200000;
const int MAXA = 400000;

int main() {
    ifstream fin("tort.in");
    ofstream fout("tort.out");

    int n;
    fin >> n;

    vector<int> a(n);
    for (int i = 0; i < n; i++) {
        fin >> a[i];
    }

    vector<int> freq(MAXA + 1, 0);
    for (int i = 0; i < n; i++) {
        freq[a[i]]++;
    }

    int sum = 0;
    for (int i = 0; i < MAXA + 1; i++) {
        sum += freq[i] * i;
    }

