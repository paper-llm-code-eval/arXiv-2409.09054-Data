#include <fstream>
#include <queue>
#include <vector>

using namespace std;

ifstream fin("rj.in");
ofstream fout("rj.out");

const int dx[] = {-1, -1, -1, 0, 0, 1, 1, 1};
const int dy[] = {-1, 0, 1, -1, 1, -1, 0, 1};

int n, m;
vector<vector<char>> grid;
vector<vector<int>> dist;

struct Point {
  int x, y, dist;
  Point(int x, int y, int dist) : x(x), y(y), dist(dist) {}
};

bool valid(int x, int y) {
  return x >= 1 && x <= n && y >= 1 && y <= m && grid[x][y] != 'X';
}

int bfs(int sx, int sy) {
  queue<Point> q;
  q.push(Point(sx, sy, 0));
  dist[sx][sy] = 0;

  while (!q.empty()) {
    Point p = q.front();
    q.pop();

    for (int i = 0; i < 8; i++) {
      int nx = p.x + dx[i];
      int ny = p.y + dy[i];
      if (valid(nx, ny) && dist[nx][ny] == -1) {
        dist[nx][ny] = p.dist + 1;
        q.push(Point(nx, ny, p.dist + 1));
      }
    }
  }

  int min_dist = 1e9;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      if (grid[i][j] != 'X' && dist[i][j] != -1) {
        min_dist = min(min_dist, dist[i][j]);
      }
    }
  }

  return min_dist;
}

int main() {
  fin >> n >> m;
  grid.resize(n + 1, vector<char>(m + 1));
  dist.resize(n + 1, vector<int>(m + 1, -1));

  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      fin >> grid[i][j];
    }
  }

  int min_time = 1e9;
  int x, y;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      if (grid[i][j] == 'R') {
        int time = bfs(i, j);
        if (time < min_time) {
          min_time = time;
          x = i;
          y = j;
        }
      }
    }
  }

  fout << x << " " << y << " " << min_time << "\n";

  return 0;
}
