#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("poarta.in");
    ofstream out("poarta.out");
    int cerinta, n, p;
    in >> cerinta >> n >> p;
    vector<int> dp(p + 1, 1e9);
    dp[0] = 0;
    for (int i = 1; i <= p; i++) {
        if (i - 1 >= 0) dp[i] = min(dp[i], dp[i - 1] + 1);
        if (i % 2 == 0 && i / 2 >= 0) dp[i] = min(dp[i], dp[i / 2] + 2);
    }
    if (cerinta == 1) out << dp[p] << endl;
    else {
        int ans = 0;
        while (p > 0) {
            if (p % 2 == 0 && dp[p] == dp[p / 2] + 2) {
                ans += 2;
                p /= 2;
            } else {
                ans += 1;
                p -= 1;
            }
        }
        out << ans << endl;
    }
    return 0;
}
