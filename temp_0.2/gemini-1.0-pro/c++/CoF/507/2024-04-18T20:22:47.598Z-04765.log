#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("partitura.in");
ofstream fout("partitura.out");

const int MAXN = 300000;
const int MAXL = 18;
const int MAXY = 10000;

int n, x[MAXN], y[MAXN], dp[MAXN][MAXL + 1];
vector<int> v[MAXL + 1];

int main() {
    fin >> n;
    for (int i = 0; i < n; i++) {
        fin >> x[i] >> y[i];
        v[x[i]].push_back(y[i]);
    }
    for (int i = 1; i <= MAXL; i++) {
        sort(v[i].begin(), v[i].end());
        for (int j = 0; j < v[i].size(); j++) {
            dp[j][i] = v[i][j] * v[i][j];
            if (j > 0) dp[j][i] = max(dp[j][i], dp[j - 1][i]);
        }
        for (int j = v[i].size() - 1; j >= 0; j--) {
            dp[j][i] = max(dp[j][i], dp[j + 1][i]);
        }
    }
    int ans = 0;
    for (int i = 0; i < n; i++) {
        ans += dp[lower_bound(v[x[i]].begin(), v[x[i]].end(), y[i]) - v[x[i]].begin()][x[i]];
    }
    fout << ans << '\n';
    return 0;
}
