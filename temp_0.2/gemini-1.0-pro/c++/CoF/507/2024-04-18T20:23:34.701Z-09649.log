#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream fin("partitura.in");
    ofstream fout("partitura.out");

    int n;
    fin >> n;
    vector<pair<int, int>> notes(n);
    for (int i = 0; i < n; i++) {
        fin >> notes[i].first >> notes[i].second;
    }

    sort(notes.begin(), notes.end());

    int score = 0;
    int current_duration = 0;
    int current_score = 0;
    for (int i = 0; i < n; i++) {
        int duration = notes[i].first;
        int height = notes[i].second;

        if (current_duration + duration <= 1) {
            current_duration += duration;
            current_score += height * height;
        } else {
            score += current_score;
            current_duration = duration;
            current_score = height * height;
        }
    }
    score += current_score;

    fout << score << endl;

    return 0;
}
```
