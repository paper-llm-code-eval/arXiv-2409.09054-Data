#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAXN = 300000;
const int MAXY = 10000;

int n, x[MAXN], y[MAXN], dp[MAXN][20][2];
vector<int> v[MAXN];

int main() {
    ifstream fin("partitura.in");
    ofstream fout("partitura.out");

    fin >> n;
    for (int i = 0; i < n; i++) {
        fin >> x[i] >> y[i];
        v[x[i]].push_back(i);
    }

    for (int i = 1; i <= 18; i++) {
        for (int j = 0; j < v[i].size(); j++) {
            int idx = v[i][j];
            dp[idx][i][0] = y[idx] * y[idx];
            dp[idx][i][1] = y[idx];
        }
    }

    for (int i = 17; i >= 1; i--) {
        for (int j = 0; j < v[i].size(); j++) {
            int idx = v[i][j];
            for (int k = i + 1; k <= 18; k++) {
                dp[idx][i][0] = max(dp[idx][i][0], dp[idx][k][0] + dp[idx][i + k][0]);
                dp[idx][i][1] = max(dp[idx][i][1], dp[idx][k][1] + dp[idx][i + k][1]);
            }
        }
    }

    int ans = 0;
    for (int i = 1; i <= 18; i++) {
        for (int j = 0; j < v[i].size(); j++) {
            int idx = v[i][j];
            ans = max(ans, dp[idx][i][0]);
        }
    }

    fout << ans << '\n';
    return 0;
}
