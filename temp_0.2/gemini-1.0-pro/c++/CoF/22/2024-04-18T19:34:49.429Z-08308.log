#include <fstream>
#include <vector>

using namespace std;

const int MOD = 1e9 + 7;
const int MAXN = 100;

vector<int> adj[MAXN + 1];
int dp[MAXN + 1][10001];

int dfs(int node, int parent, int dist) {
  if (dp[node][dist] != -1) {
    return dp[node][dist];
  }
  dp[node][dist] = 0;
  if (dist == 0) {
    dp[node][dist] = 1;
  }
  for (int u : adj[node]) {
    if (u != parent) {
      dp[node][dist] = (dp[node][dist] + dfs(u, node, dist - 1)) % MOD;
    }
  }
  return dp[node][dist];
}

int main() {
  ifstream in("tairos.in");
  ofstream out("tairos.out");
  int n, d;
  in >> n >> d;
  for (int i = 1; i < n; i++) {
    int a, b;
    in >> a >> b;
    adj[a].push_back(b);
    adj[b].push_back(a);
  }
  memset(dp, -1, sizeof(dp));
  out << dfs(1, 1, d) << endl;
  return 0;
}
```
