        dist[u] = dist[p] + 1;
        cnt[dist[u]]++;
        for (int v : adj[u]) {
            if (v != p) {
                dfs(v, u);
            }
        }
    };
    dfs(1, 1);

    int ans = 0;
    for (int i = 1; i <= d; i++) {
        ans = (ans + cnt[i]) % MOD;
    }

    out << ans << '\n';

    return 0;
}
