using namespace std;

const int MOD = 1e9 + 7;

int main() {
    ifstream in("tairos.in");
    ofstream out("tairos.out");

    int n, d;
    in >> n >> d;
    vector<vector<int>> adj(n + 1);
    for (int i = 1; i < n; i++) {
        int u, v;
        in >> u >> v;
        adj[u].push_back(v);
        adj[v].push_back(u);
    }

    vector<int> dist(n + 1, -1);
    vector<int> cnt(n + 1, 0);
