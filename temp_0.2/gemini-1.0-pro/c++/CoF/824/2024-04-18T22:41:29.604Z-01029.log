#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream in("triunghi.in");
  ofstream out("triunghi.out");
  int n;
  in >> n;
  vector<vector<long long>> triunghi(n + 1, vector<long long>(n + 1, 0));
  for (int i = 1; i <= n; i++) {
    int p, v;
    in >> p >> v;
    triunghi[i][p] = v;
  }
  for (int i = n - 1; i >= 1; i--) {
    for (int j = 1; j <= n - i; j++) {
      triunghi[i][j] = triunghi[i + 1][j] + triunghi[i + 1][j + 1];
    }
  }
  for (int i = 1; i <= n; i++) {
    out << triunghi[1][i] << " ";
  }
  return 0;
}
```
