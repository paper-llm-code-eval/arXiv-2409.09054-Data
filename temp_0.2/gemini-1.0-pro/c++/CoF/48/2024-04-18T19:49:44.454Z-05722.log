#include <fstream>
#include <vector>
#include <queue>

using namespace std;

ifstream fin("cezar.in");
ofstream fout("cezar.out");

const int NMAX = 10005;
const int INF = 1e9;

int n, k;
vector<int> graph[NMAX];
int dist[NMAX];
int parent[NMAX];

void dijkstra(int start) {
    for (int i = 1; i <= n; i++) {
        dist[i] = INF;
    }
    dist[start] = 0;
    priority_queue<pair<int, int>> pq;
    pq.push({0, start});
    while (!pq.empty()) {
        int node = pq.top().second;
        int cost = -pq.top().first;
        pq.pop();
        if (cost > dist[node]) {
            continue;
        }
        for (int neighbor : graph[node]) {
            if (dist[neighbor] > cost + 1) {
                dist[neighbor] = cost + 1;
                parent[neighbor] = node;
                pq.push({-dist[neighbor], neighbor});
            }
        }
    }
}

int main() {
    fin >> n >> k;
    for (int i = 1; i < n; i++) {
        int a, b;
        fin >> a >> b;
        graph[a].push_back(b);
        graph[b].push_back(a);
    }
    int min_cost = INF;
    int best_node = -1;
    for (int i = 1; i <= n; i++) {
        dijkstra(i);
