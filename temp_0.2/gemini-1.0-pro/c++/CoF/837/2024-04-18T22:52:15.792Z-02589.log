        for (int j = 0; j < v[i].size(); j++) {
            for (int k = i + 1; k <= 1000; k++) {
                if (v[k].size() == 0) continue;
                int pos = lower_bound(v[k].begin(), v[k].end(), -v[i][j]) - v[k].begin();
                if (pos < v[k].size() && v[k][pos] == -v[i][j]) nrd++;
            }
