                int pos = lower_bound(v[k].begin(), v[k].end(), v[i][j] + v[k][j]) - v[k].begin();
                if (pos < v[k].size() && v[k][pos] == v[i][j] + v[k][j]) nrd++;
            }
        }
    }
    fout << nrd << '\n';
    int xp = 0, cnt = 0;
