#include <fstream>

using namespace std;

int main() {
  ifstream fin("furnica.in");
  ofstream fout("furnica.out");
  int n, k;
  fin >> n >> k;
  int a[n][n], b[9] = {0}, max = 0, cnt = 0;
  for (int i = 0; i < n; i++)
    for (int j = 0; j < n; j++) {
      a[i][j] = (i + j) % 6;
      if (i == 0 && j == 0) a[i][j] = 0;
    }
  int x = 0, y = 0;
  for (int i = 0; i < k; i++) {
    int nr;
    fin >> nr;
    if (nr == 1) {
      x--;
      y--;
    } else if (nr == 2) {
      y--;
    } else if (nr == 3) {
      x++;
      y--;
    } else if (nr == 4) {
      x--;
    } else if (nr == 6) {
      x++;
    } else if (nr == 7) {
      x--;
      y++;
    } else if (nr == 8) {
      y++;
    }
    b[nr]++;
    if (b[nr] > max) {
      max = b[nr];
      cnt = 1;
    } else if (b[nr] == max) cnt++;
  }
  int sum = 0;
  for (int i = 0; i < n; i++)
    for (int j = 0; j < n; j++) sum += a[i][j];
  fout << sum << " " << cnt;
  return 0;
}
