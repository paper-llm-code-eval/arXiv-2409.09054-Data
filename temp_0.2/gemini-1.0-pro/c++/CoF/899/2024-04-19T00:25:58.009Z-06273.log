#include <fstream>
#include <vector>
#include <algorithm>
#include <cstring>

using namespace std;

const int MAXN = 50005;
const int MAXL = 18;

int n, p;
int a[MAXN];
int dp[MAXN][MAXL][2];
int b[MAXN];
int c[MAXN];
int l[MAXN];
int sol[MAXN];
int sol_len;
int sol_last;

int main() {
    ifstream fin("pal.in");
    ofstream fout("pal.out");

    fin >> p >> n;
    for (int i = 1; i <= n; ++i) {
        fin >> a[i];
    }

    for (int i = 1; i <= n; ++i) {
        int x = a[i];
        int len = 0;
        while (x) {
            b[i][++len] = x % 10;
            x /= 10;
        }
        l[i] = len;
        for (int j = 1; j <= len; ++j) {
            c[i][j] = c[i][j - 1] * 10 + b[i][j];
        }
    }

    for (int i = 1; i <= n; ++i) {
        for (int j = 0; j <= l[i]; ++j) {
            dp[i][j][0] = dp[i][j][1] = -1;
        }
    }

    for (int i = 1; i <= n; ++i) {
        dp[i][0][0] = dp[i][0][1] = 0;
    }

    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= l[i]; ++j) {
            if (b[i][j] > 0) {
                dp[i][j][0] = max(dp[i][j][0], dp[i - 1][j - 1][0] + 1);
            }
            if (b[i][j] < 9) {
                dp[i][j][0] = max(dp[i][j][0], dp[i - 1][j - 1][1] + 1);
            }
            if (b[i][j] > 0) {
                dp[i][j][1] = max(dp[i][j][1], dp[i - 1][j - 1][0] + 1);
            }
            if (b[i][j] < 9) {
                dp[i][j][1] = max(dp[i][j][1], dp[i - 1][j - 1][1] + 1);
            }
        }
    }

    int max_len = 0;
    int max_i = 0;
    int max_j = 0;
    int max_type = 0;

    for (int i = 1; i <= n; ++i) {
        for (int j = 0; j <= l[i]; ++j) {
            if (dp[i][j][0] > max_len) {
                max_len = dp[i][j][0];
                max_i = i;
                max_j = j;
                max_type = 0;
            }
            if (dp[i][j][1] > max_len) {
                max_len = dp[i][j][1];
                max_i = i;
                max_j = j;
                max_type = 1;
            }
        }
    }

    if (p == 1) {
        for (int i = max_i; i >= 1; --i) {
            if (max_type == 0) {
                fout << c[i][max_j] << " ";
                max_j--;
            } else {
                fout << c[i][max_j + 1] << " ";
                max_j++;
            }
        }
        fout << "\n";
    } else if (p == 2) {
        fout << max_len << "\n";
    } else {
        sol_len = max_len;
        sol_last = c[max_i][max_j];
        int i = max_i;
        int j = max_j;
        int type = max_type;
        while (i > 0) {
            sol[sol_len--] = c[i][j];
            if (type == 0) {
                j--;
            } else {
                j++;
            }
            if (j == 0 || j == l[i] + 1) {
                i--;
                type = 1 - type;
                j = l[i] + 1 - j;
            }
        }
        for (int i = 1; i <= sol_len; ++i) {
            fout << sol[i] << " ";
        }
        fout << "\n";
    }

    return 0;
}
