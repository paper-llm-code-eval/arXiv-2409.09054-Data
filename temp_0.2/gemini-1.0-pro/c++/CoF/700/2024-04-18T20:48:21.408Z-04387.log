#include <fstream>
#include <vector>
#include <queue>
#include <tuple>

using namespace std;

int main() {
  ifstream in("mouse.in");
  ofstream out("mouse.out");
  int m, n;
  in >> m >> n;
  vector<vector<int>> grid(m, vector<int>(n));
  for (int i = 0; i < m; i++) {
    for (int j = 0; j < n; j++) {
      in >> grid[i][j];
    }
  }
  vector<vector<int>> dp(m, vector<int>(n, -1));
  vector<vector<pair<int, int>>> path(m, vector<pair<int, int>>(n));
  dp[0][0] = grid[0][0];
  priority_queue<tuple<int, int, int>> pq;
  pq.push(make_tuple(dp[0][0], 0, 0));
  while (!pq.empty()) {
    int dist, x, y;
    tie(dist, x, y) = pq.top();
    pq.pop();
    if (x == m - 1 && y == n - 1) {
      out << dp[x][y] << " " << dist << endl;
      vector<pair<int, int>> ans;
      while (x != 0 || y != 0) {
        ans.push_back(make_pair(x + 1, y + 1));
        x = path[x][y].first;
        y = path[x][y].second;
      }
      ans.push_back(make_pair(1, 1));
      reverse(ans.begin(), ans.end());
      for (auto p : ans) {
        out << p.first << " " << p.second << endl;
      }
      return 0;
    }
    if (x + 1 < m && dp[x + 1][y] < dist + grid[x + 1][y]) {
      dp[x + 1][y] = dist + grid[x + 1][y];
      path[x + 1][y] = make_pair(x, y);
      pq.push(make_tuple(dp[x + 1][y], x + 1, y));
    }
    if (y + 1 < n && dp[x][y + 1] < dist + grid[x][y + 1]) {
      dp[x][y + 1] = dist + grid[x][y + 1];
      path[x][y + 1] = make_pair(x, y);
      pq.push(make_tuple(dp[x][y + 1], x, y + 1));
    }
  }
  return 0;
}
