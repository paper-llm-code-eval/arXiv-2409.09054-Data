#include <fstream>
#include <vector>
#include <queue>

using namespace std;

ifstream fin("mouse.in");
ofstream fout("mouse.out");

const int MAXN = 100;
int m, n;
int a[MAXN][MAXN];
int d[MAXN][MAXN];
int dx[] = {0, 1, 0, -1};
int dy[] = {1, 0, -1, 0};

struct Node {
  int x, y, dist;
  Node(int x, int y, int dist) : x(x), y(y), dist(dist) {}
};

bool operator<(const Node &a, const Node &b) {
  return a.dist > b.dist;
}

int main() {
  fin >> m >> n;
  for (int i = 1; i <= m; i++) {
    for (int j = 1; j <= n; j++) {
      fin >> a[i][j];
    }
  }

  priority_queue<Node> q;
  q.push(Node(1, 1, a[1][1]));
  d[1][1] = a[1][1];

  while (!q.empty()) {
    Node u = q.top();
    q.pop();
    for (int i = 0; i < 4; i++) {
      int x = u.x + dx[i];
      int y = u.y + dy[i];
      if (x >= 1 && x <= m && y >= 1 && y <= n && d[x][y] < d[u.x][u.y] + a[x][y]) {
        d[x][y] = d[u.x][u.y] + a[x][y];
        q.push(Node(x, y, d[x][y]));
      }
    }
  }

  fout << d[m][n] << '\n';

  int x = m, y = n;
  vector<pair<int, int>> path;
  while (x != 1 || y != 1) {
    for (int i = 0; i < 4; i++) {
      int nx = x + dx[i];
      int ny = y + dy[i];
      if (nx >= 1 && nx <= m && ny >= 1 && ny <= n && d[nx][ny] == d[x][y] - a[x][y]) {
        path.push_back({x, y});
        x = nx;
        y = ny;
        break;
      }
    }
  }
  path.push_back({1, 1});

  for (int i = path.size() - 1; i >= 0; i--) {
    fout << path[i].first << ' ' << path[i].second << '\n';
  }

  return 0;
}
