int main() {
  ifstream in("zar.in");
  ofstream out("zar.out");

  int n, k;
  in >> n >> k;
  vector<int> zar(3);
  in >> zar[0] >> zar[1] >> zar[2];

  k %= 4;
  for (int i = 0; i < k; i++) {
    int aux = zar[0];
    zar[0] = 7 - zar[2];
    zar[2] = 7 - zar[1];
    zar[1] = 7 - aux;
  }

  out << zar[0] << ' ' << zar[1] << ' ' << zar[2] << '\n';

  return 0;
}
```
