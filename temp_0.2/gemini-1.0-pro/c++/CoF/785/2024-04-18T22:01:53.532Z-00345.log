#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream fin("inimioare.in");
  ofstream fout("inimioare.out");

  int n;
  fin >> n;

  vector<vector<int>> v(n, vector<int>(4));
  vector<vector<int>> w(n, vector<int>(4));

  for (int i = 0; i < n; i++) {
    for (int j = 0; j < 4; j++) {
      fin >> v[i][j];
    }
  }

  for (int i = 0; i < n; i++) {
    for (int j = 0; j < 4; j++) {
      fin >> w[i][j];
    }
  }

  sort(v.begin(), v.end());
  sort(w.begin(), w.end());

  int max_num = 0;

  for (int i = 0; i < n; i++) {
    for (int j = 0; j < n; j++) {
      int num = 0;
      num += max(v[i][0], w[j][0]) * 1000;
      num += max(v[i][1], w[j][1]) * 100;
      num += max(v[i][2], w[j][2]) * 10;
      num += max(v[i][3], w[j][3]);
      max_num = max(max_num, num);
    }
  }

  fout << max_num << endl;

  return 0;
}
