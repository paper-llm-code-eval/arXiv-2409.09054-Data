#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream in("vecini.in");
  ofstream out("vecini.out");
  int m, n, k;
  in >> m >> n >> k;
  vector<vector<int>> a(m + 2, vector<int>(n + 2));
  vector<vector<int>> b(m + 2, vector<int>(n + 2));
  for (int i = 1; i <= k; ++i) {
    int x, y;
    in >> x >> y;
    a[x][y] = 1;
  }
  int mx = 0, cnt = 0;
  for (int i = 1; i <= m; ++i) {
    for (int j = 1; j <= n; ++j) {
      b[i][j] = a[i - 1][j - 1] + a[i - 1][j] + a[i - 1][j + 1] + a[i][j - 1] + a[i][j + 1] + a[i + 1][j - 1] + a[i + 1][j] + a[i + 1][j + 1];
      if (b[i][j] > mx) {
        mx = b[i][j];
        cnt = 1;
      } else if (b[i][j] == mx) {
        ++cnt;
      }
    }
  }
  out << mx << ' ' << cnt << '\n';
  return 0;
}
