    int n;
    fin >> n;

    vector<string> comparisons(n);
    for (int i = 0; i < n; i++) {
        fin >> comparisons[i];
    }

    int num_less_than = 0;
    vector<int> results(n);
    for (int i = 0; i < n; i++) {
        string& comparison = comparisons[i];

        int pos = comparison.find('<');
        if (pos != string::npos) {
            num_less_than++;
        }

        int num1 = 0, num2 = 0;
        int j = 0;
        while (j < pos) {
            int num = 0;
            while (j < pos && isdigit(comparison[j])) {
                num = num * 10 + comparison[j] - '0';
                j++;
            }
            if (comparison[j] == 'm') {
                num *= 1000;
            } else if (comparison[j] == 's') {
                num *= 100;
            } else if (comparison[j] == 'z') {
                num *= 10;
            }
            num1 += num;
            j++;
        }

        while (j < comparison.size()) {
            int num = 0;
            while (j < comparison.size() && isdigit(comparison[j])) {
                num = num * 10 + comparison[j] - '0';
                j++;
            }
            if (comparison[j] == 'm') {
                num *= 1000;
            } else if (comparison[j] == 's') {
                num *= 100;
            } else if (comparison[j] == 'z') {
                num *= 10;
            }
            num2 += num;
            j++;
        }

        results[i] = (num1 > num2);
    }

    fout << num_less_than << endl;
    for (int i = 0; i < n; i++) {
        fout << results[i] << endl;
    }

    return 0;
}
