#include <fstream>
#include <iostream>
#include <string>
#include <vector>

using namespace std;

int main() {
  ifstream in("text.in");
  ofstream out("text.out");
  int max_line_length;
  in >> max_line_length;
  string line;
  vector<string> lines;
  while (getline(in, line)) {
    lines.push_back(line);
  }
  for (string& line : lines) {
    int line_length = line.length();
    int num_spaces = max_line_length - line_length;
    int num_words = 0;
    for (int i = 0; i < line_length; i++) {
      if (line[i] == ' ') {
        num_words++;
      }
    }
    int num_spaces_per_word = num_spaces / num_words;
    int num_extra_spaces = num_spaces % num_words;
    string new_line;
    int start = 0;
    for (int i = 0; i < line_length; i++) {
      if (line[i] == ' ') {
        new_line += line.substr(start, i - start);
        for (int j = 0; j < num_spaces_per_word; j++) {
          new_line += ' ';
        }
        if (num_extra_spaces > 0) {
          new_line += ' ';
          num_extra_spaces--;
        }
        start = i + 1;
      }
    }
    new_line += line.substr(start);
    out << new_line << endl;
  }
  return 0;
}
