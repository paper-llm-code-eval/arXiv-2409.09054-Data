#include <fstream>
#include <vector>
#include <queue>
#include <utility>
#include <algorithm>

using namespace std;

const int MAX_N = 20;
const int MAX_M = 20;
const int MAX_I = 15;
const int MAX_MOVES = 100;

int n, m, I;
int grid[MAX_N][MAX_M];
int moves[MAX_MOVES][4];
int move_count;

bool valid(int x, int y) {
    return x >= 1 && x <= n && y >= 1 && y <= m && grid[x][y] == 0;
}

void print_move(int x1, int y1, int x2, int y2) {
    moves[move_count][0] = x1;
    moves[move_count][1] = y1;
    moves[move_count][2] = x2;
    moves[move_count][3] = y2;
    move_count++;
}

bool bfs(int x1, int y1, int x2, int y2) {
    queue<pair<int, int>> q;
    q.push({x1, y1});
    grid[x1][y1] = 1;

    while (!q.empty()) {
        int x = q.front().first;
        int y = q.front().second;
        q.pop();

        if (x == x2 && y == y2) {
            return true;
        }

        if (valid(x - 1, y)) {
            q.push({x - 1, y});
            grid[x - 1][y] = 1;
        }
        if (valid(x + 1, y)) {
            q.push({x + 1, y});
            grid[x + 1][y] = 1;
        }
        if (valid(x, y - 1)) {
            q.push({x, y - 1});
            grid[x][y - 1] = 1;
        }
        if (valid(x, y + 1)) {
            q.push({x, y + 1});
            grid[x][y + 1] = 1;
        }
    }

    return false;
}

int main() {
    ifstream fin("immortal.in");
    ofstream fout("immortal.out");

    fin >> n >> m >> I;

    for (int i = 0; i < I; i++) {
        int x, y;
        fin >> x >> y;
        grid[x][y] = 1;
    }

    while (I > 1) {
        for (int i = 1; i <= n; i++) {
            for (int j = 1; j <= m; j++) {
                if (grid[i][j] == 1) {
                    for (int k = 1; k <= n; k++) {
                        for (int l = 1; l <= m; l++) {
                            if (grid[k][l] == 1 && (i != k || j != l)) {
                                if (bfs(i, j, k, l)) {
                                    print_move(i, j, k, l);
                                    grid[k][l] = 0;
                                    I--;
                                    break;
                                }
                            }
                        }
                    }
                }
            }
        }
    }

    for (int i = 0; i < move_count; i++) {
        fout << moves[i][0] << " " << moves[i][1] << " " << moves[i][2] << " " << moves[i][3] << endl;
    }

    return 0;
}
