int n, m, I;
int grid[MAX_N][MAX_M];
int moves[MAX_MOVES][4];
int move_count;

bool valid(int x, int y) {
    return x >= 1 && x <= n && y >= 1 && y <= m;
}

void print_move(int x1, int y1, int x2, int y2) {
    moves[move_count][0] = x1;
    moves[move_count][1] = y1;
    moves[move_count][2] = x2;
    moves[move_count][3] = y2;
    move_count++;
}

bool bfs(int x1, int y1, int x2, int y2) {
    queue<pair<int, int>> q;
    q.push({x1, y1});
    grid[x1][y1] = 1;

    while (!q.empty()) {
        int x = q.front().first;
        int y = q.front().second;
        q.pop();

        if (x == x2 && y == y2) {
            return true;
        }

        if (valid(x - 1, y) && grid[x - 1][y] == 0) {
            q.push({x - 1, y});
            grid[x - 1][y] = 1;
        }
        if (valid(x + 1, y) && grid[x + 1][y] == 0) {
            q.push({x + 1, y});
            grid[x + 1][y] = 1;
        }
        if (valid(x, y - 1) && grid[x][y - 1] == 0) {
            q.push({x, y - 1});
            grid[x][y - 1] = 1;
        }
        if (valid(x, y + 1) && grid[x][y + 1] == 0) {
            q.push({x, y + 1});
            grid[x][y + 1] = 1;
        }
    }

    return false;
}

