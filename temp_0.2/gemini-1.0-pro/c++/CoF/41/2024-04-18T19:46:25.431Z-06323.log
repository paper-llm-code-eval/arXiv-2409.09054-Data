            if (a[x][y] != 0 && a[x + dx[j]][y + dy[j]] != 0) {
                q.push(make_pair(a[x][y], j));
            }
        }
    }
    for (int j = 1; j < i; ++j) {
        for (int k = 0; k < 4; ++k) {
            out << sol[j][k] << ' ';
        }
        out << '\n';
    }
    return 0;
}
