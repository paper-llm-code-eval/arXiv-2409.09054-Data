#include <fstream>
#include <vector>
#include <queue>
#include <utility>

using namespace std;

ifstream in("immortal.in");
ofstream out("immortal.out");

const int MAXN = 20;
const int MAXM = 20;
const int MAXI = 15;
const int dx[] = {-2, 2, 0, 0};
const int dy[] = {0, 0, -2, 2};

int n, m, i, x, y, a[MAXN][MAXM], b[MAXN][MAXM], sol[MAXI][4];

int main() {
    in >> n >> m >> i;
    for (int j = 1; j <= i; ++j) {
        in >> x >> y;
        a[x][y] = j;
    }
    queue<pair<int, int>> q;
    for (int j = 1; j <= i; ++j) {
        for (int k = 0; k < 4; ++k) {
            x = j / m + 1;
            y = j % m + 1;
            if (a[x][y] == j && a[x + dx[k]][y + dy[k]] != 0) {
                q.push(make_pair(j, k));
            }
        }
    }
    while (!q.empty()) {
        x = q.front().first;
        y = q.front().second;
        q.pop();
        sol[++b[x][y]][0] = x;
        sol[b[x][y]][1] = y;
        sol[b[x][y]][2] = x + dx[y];
        sol[b[x][y]][3] = y + dy[y];
        a[x + dx[y]][y + dy[y]] = 0;
        for (int j = 0; j < 4; ++j) {
            x = sol[b[x][y]][2] + dx[j];
            y = sol[b[x][y]][3] + dy[j];
            if (a[x][y] != 0 && a[x + dx[j]][y + dy[j]] != 0) {
                q.push(make_pair(a[x][y], j));
            }
        }
    }
    for (int j = 1; j < i; ++j) {
        for (int k = 0; k < 4; ++k) {
            out << sol[j][k] << ' ';
        }
        out << '\n';
    }
    return 0;
}
