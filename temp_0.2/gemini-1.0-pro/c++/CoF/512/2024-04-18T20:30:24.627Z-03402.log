                x /= 10;
            }
            while (y > 0) {
                digits.push_back(y % 10);
                y /= 10;
            }
            sort(digits.begin(), digits.end(), greater<int>());
            int num = 0;
            for (int j = 0; j < digits.size(); j++) {
                num = num * 10 + digits[j];
            }
            ans = max(ans, num);
        }
        fout << ans << endl;
    } else if (C == 3) {
        int ans = 0;
        for (int i = 0; i <= n - k; i++) {
            vector<int> digits;
