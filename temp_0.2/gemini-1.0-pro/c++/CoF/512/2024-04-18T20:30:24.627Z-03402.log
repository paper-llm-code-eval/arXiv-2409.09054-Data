                int x = a[j];
                while (x > 0) {
                    digits.push_back(x % 10);
                    x /= 10;
                }
            }
            sort(digits.begin(), digits.end(), greater<int>());
            int num = 0;
            for (int j = 0; j < digits.size(); j++) {
                num = num * 10 + digits[j];
            }
            ans = max(ans, num);
        }
        fout << ans << endl;
    }

    return 0;
}
