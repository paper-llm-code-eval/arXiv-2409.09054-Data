#include <fstream>
#include <vector>

using namespace std;

ifstream fin("clepsidru.in");
ofstream fout("clepsidru.out");

const int MAX_N = 1000;
const int MAX_K = 1000;
const int MAX_B = 1000000000;

int n, b, k;
int s[MAX_K], p[MAX_K];
vector<vector<int>> sus(MAX_N + 1), jos(MAX_N + 1);

void read_input() {
  fin >> n >> b;
  fin >> k;
  for (int i = 0; i < k; i++) {
    fin >> s[i] >> p[i];
  }
}

void solve_first_experiment() {
  fout << n * b << '\n';
}

void solve_second_experiment() {
  for (int i = 0; i <= n; i++) {
    sus[i].resize(k + 1, 0);
    jos[i].resize(k + 1, 0);
  }

  sus[1][0] = b;
  for (int i = 0; i < k; i++) {
    int t = s[i];
    int pos = p[i];
    int s_sus = sus[pos][i];
    int s_jos = jos[pos][i];
    sus[pos][i + 1] = s_sus - min(s_sus, t);
    jos[pos][i + 1] = s_jos + min(s_sus, t);
    sus[3 - pos][i + 1] = s_sus + min(s_jos, t);
    jos[3 - pos][i + 1] = s_jos - min(s_jos, t);
  }

  for (int i = 1; i <= n; i++) {
    fout << sus[i][k] << ' ' << jos[i][k] << '\n';
  }
}

int main() {
  read_input();
  solve_first_experiment();
  solve_second_experiment();
  return 0;
}
