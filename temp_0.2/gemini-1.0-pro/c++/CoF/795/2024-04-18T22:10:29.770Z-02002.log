#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
  ifstream in("sir.in");
  ofstream out("sir.out");
  int k, x, a, b;
  in >> k >> x >> a >> b;
  vector<int> sir;
  sir.push_back(1);
  while (sir.back() < x) {
    int nr = sir.back();
    int p = 1;
    while (nr / p > 0) {
      int c = nr / p % 10;
      nr = nr % p * 10 + c + 1;
      p *= 10;
    }
    sir.push_back(nr);
  }
  int sum = 0;
  for (int i = 0; i < sir.size(); i++) {
    if (sir[i] <= x && sir[i] >= 100000000 / 10) {
      sum += sir[i] % 10;
    }
  }
  out << sum % 10 << endl;
  int pos = -1;
  for (int i = 0; i < sir.size(); i++) {
    if (sir[i] == x) {
      pos = i;
    }
  }
  out << sir[pos + 1] << endl;
  int cnt = 0;
  for (int i = 0; i < sir.size(); i++) {
    int nr = sir[i];
    int ok = 1;
    while (nr > 0) {
      if (nr % 10 == b) {
        ok = 0;
      }
      nr /= 10;
    }
    if (ok && sir[i] / 100000000 == a) {
      cnt++;
    }
  }
  out << cnt << endl;
  return 0;
}
