            }
        }
    }

    for (int i = 0; i < k; i++) {
        int x1 = l1[i], y1 = c1[i], x2 = l2[i], y2 = c2[i];
        if (x1 > x2) {
            swap(x1, x2);
            swap(y1, y2);
        }
        if (p[i] == -1) {
            swap(x1, x2);
            swap(y1, y2);
        }
        int dx = x2 - x1, dy = y2 - y1;
