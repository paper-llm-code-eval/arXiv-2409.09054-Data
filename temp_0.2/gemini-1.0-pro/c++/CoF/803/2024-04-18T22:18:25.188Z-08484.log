#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("expozitie.in");
ofstream fout("expozitie.out");

const int MOD = 1e9 + 7;
const int MAXN = 505;

int n, d, k;
vector<int> dp[MAXN][MAXN];

int main() {
    fin >> n >> d >> k;
    dp[0][0] = {1};
    for (int i = 1; i <= n; i++) {
        for (int j = 0; j <= d; j++) {
            for (int l = 0; l <= k; l++) {
                dp[i][j + l] = (dp[i][j + l] + dp[i - 1][j]) % MOD;
                if (j) {
                    dp[i][j + l] = (dp[i][j + l] + dp[i - 1][j - 1]) % MOD;
                }
            }
        }
    }
    fout << dp[n][d * k] << '\n';
    return 0;
}
