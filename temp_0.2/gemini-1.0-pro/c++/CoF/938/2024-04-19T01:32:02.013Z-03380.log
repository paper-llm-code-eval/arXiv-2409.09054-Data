#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>

using namespace std;

const int MAXN = 1000;
const int MAXM = 1000;
const int INF = 1e9;

int n, m;
int labirint[MAXN][MAXM];
int d[MAXN][MAXM];
int dx[] = {-1, 0, 1, 0};
int dy[] = {0, 1, 0, -1};

int main() {
    ifstream fin("labirint.in");
    ofstream fout("labirint.out");

    fin >> n >> m;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            fin >> labirint[i][j];
        }
    }

    queue<pair<int, int>> q;
    q.push({1, 1});
    d[1][1] = 0;

    while (!q.empty()) {
        int x = q.front().first;
        int y = q.front().second;
        q.pop();

        for (int i = 0; i < 4; i++) {
            int nx = x + dx[i];
            int ny = y + dy[i];

            if (nx >= 1 && nx <= n && ny >= 1 && ny <= m && labirint[nx][ny] == 0 && d[nx][ny] == INF) {
                d[nx][ny] = d[x][y] + 1;
                q.push({nx, ny});
            }
        }
    }

    int d0 = d[n][m];

    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= m; j++) {
            if (d[i][j] < d0) {
                fout << 1;
            } else {
                fout << 0;
            }
        }
        fout << '\n';
    }

    return 0;
}
