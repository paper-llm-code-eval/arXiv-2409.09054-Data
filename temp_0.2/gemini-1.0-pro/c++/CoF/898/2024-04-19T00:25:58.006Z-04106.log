#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("cruce.in");
    ofstream out("cruce.out");
    int n, k;
    in >> n >> k;
    vector<vector<int>> a(n, vector<int>(n));
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            in >> a[i][j];
        }
    }
    int max_sum = -1e9;
    int max_i = -1, max_j = -1;
    for (int i = 0; i <= n - k; i++) {
        for (int j = 0; j <= n - k; j++) {
            int sum = 0;
            for (int x = i; x < i + k; x++) {
                for (int y = j; y < j + k; y++) {
                    sum += a[x][y];
                }
            }
            if (sum > max_sum) {
                max_sum = sum;
                max_i = i;
                max_j = j;
            }
        }
    }
    out << max_sum << ' ' << max_i + 1 << ' ' << max_j + 1 << '\n';
    return 0;
}
```
