#include <fstream>
#include <vector>
#include <algorithm>
#include <cstring>

using namespace std;

const int MAX_N = 150;
const int MAX_M = 30;
const int MAX_K = 5;
const int MOD = 2000000000;

int n, m, k;
char s[MAX_N][MAX_M];
int dp[MAX_N][MAX_K + 1];
int last[MAX_N];
vector<int> adj[MAX_N];

int main() {
    ifstream fin("lant.in");
    ofstream fout("lant.out");

    fin >> k;
    while (fin.getline(s[n], MAX_M)) {
        n++;
    }
    fin.close();

    for (int i = 0; i < n; i++) {
        m = strlen(s[i]);
        for (int j = 0; j < n; j++) {
            if (i == j) {
                continue;
            }
            int dist = 0;
            for (int l = 0; l < m && l < strlen(s[j]); l++) {
                if (s[i][l] != s[j][l]) {
                    dist++;
                }
            }
            if (dist <= k) {
                adj[i].push_back(j);
            }
        }
    }

    memset(dp, -1, sizeof(dp));
    dp[0][0] = 1;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j <= k; j++) {
            if (dp[i][j] == -1) {
                continue;
            }
            for (int u : adj[i]) {
                if (last[u] < i) {
                    dp[u][j + 1] = (dp[u][j + 1] + dp[i][j]) % MOD;
                    last[u] = i;
                }
            }
        }
    }

    int ans = 0;
    for (int i = 0; i <= k; i++) {
        ans = (ans + dp[n - 1][i]) % MOD;
    }

    fout << ans << '\n';
    fout.close();

    return 0;
}
