#include <fstream>
#include <vector>
#include <algorithm>
#include <cstring>
#include <climits>

using namespace std;

const int MAXN = 150;
const int MAXL = 30;
const int MAXK = 5;

int n, k;
char text[MAXN][MAXL];
int dp[MAXN][1 << MAXK];
int last[MAXN][26];
vector<int> adj[MAXN];

int main() {
    ifstream in("lant.in");
    ofstream out("lant.out");

    in >> k;
    int i, j, l;
    while (in >> text[n]) {
        l = strlen(text[n]);
        for (i = 0; i < l; i++) {
            text[n][i] -= 'a';
        }
        n++;
    }

    for (i = 0; i < n; i++) {
        for (j = 0; j < 26; j++) {
            last[i][j] = -1;
        }
    }

    for (i = n - 1; i >= 0; i--) {
        for (j = 0; j < 26; j++) {
            last[i][j] = last[i + 1][j];
        }
        last[i][text[i][0]] = i;
    }

    for (i = 0; i < n; i++) {
        for (j = 0; j < (1 << k); j++) {
            dp[i][j] = INT_MAX;
        }
    }

    dp[0][0] = 0;

    for (i = 0; i < n; i++) {
        for (j = 0; j < (1 << k); j++) {
            if (dp[i][j] == INT_MAX) {
                continue;
            }
            for (l = 0; l < 26; l++) {
                int next = last[i + 1][l];
                if (next == -1) {
                    continue;
                }
                int cost = 0;
                if ((j & (1 << (k - 1))) == 0) {
                    cost++;
                }
                if (text[i][0] != l) {
                    cost++;
                }
                if (dp[next][j | (1 << (k - 1))] > dp[i][j] + cost) {
                    dp[next][j | (1 << (k - 1))] = dp[i][j] + cost;
                    adj[i].push_back(next);
                }
            }
        }
    }

    int ans = 0;

    for (i = 0; i < n; i++) {
        if (dp[i][(1 << k) - 1] != INT_MAX) {
            ans++;
        }
    }

    out << ans << '\n';

    return 0;
}
