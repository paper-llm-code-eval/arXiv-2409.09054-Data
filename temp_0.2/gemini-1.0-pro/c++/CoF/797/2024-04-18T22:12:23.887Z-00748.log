                }
            }
        }

        extrase[i] = bile[min_idx];
        bile.erase(bile.begin() + min_idx);
    }

    sort(extrase.begin(), extrase.end());

    for (int i = 0; i < 6; i++) {
        out << extrase[i] << " ";
    }

    return 0;
}
