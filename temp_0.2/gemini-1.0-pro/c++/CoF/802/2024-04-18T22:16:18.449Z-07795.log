        }

        for (int j = 1; j <= n; j++) {
            for (int l = 0; l <= k; l++) {
                dp[j][l][0] = dp[j][l][1] = 0;
            }
        }

        for (int j = 1; j <= n; j++) {
            if (a[j] == 1) {
                continue;
            }

            if (!prim[a[j]]) {
                dp[j][1][0] = dp[j][1][1] = 1;
            } else if (a[j] % p == 0) {
                dp[j][1][0] = dp[j - 1][0][0] + 1;
                dp[j][1][1] = dp[j - 1][0][1] + 1;
            }
        }

        for (int j = 2; j <= n; j++) {
            for (int l = 1; l <= k; l++) {
                if (a[j] == 1) {
                    continue;
                }

                if (!prim[a[j]]) {
                    dp[j][l][0] = max(dp[j - 1][l][0], dp[j - 1][l - 1][1] + 1);
                    dp[j][l][1] = max(dp[j - 1][l][1], dp[j - 1][l - 1][0] + 1);
                } else if (a[j] % p == 0) {
                    dp[j][l][0] = max(dp[j - 1][l][0], dp[j - 1][l - 1][1] + 1);
                    dp[j][l][1] = max(dp[j - 1][l][1], dp[j - 1][l - 1][0] + 1);
                }
            }
        }

        int mx = 0;
        for (int j = 1; j <= n; j++) {
            mx = max(mx, dp[j][k][1]);
        }

        fout << mx << '\n';
        for (int j = 1; j <= n; j++) {
            if (dp[j][k][1] == mx) {
                ans.push_back({j, j});
            }
        }

        for (int j = n; j >= 1; j--) {
            if (dp[j][k][0] == mx) {
                ans.back().second = j;
            }
        }

        sort(ans.begin(), ans.end());
        for (auto& it : ans) {
            fout << it.first << ' ' << it.second << '\n';
        }
        ans.clear();
    }

    return 0;
}
