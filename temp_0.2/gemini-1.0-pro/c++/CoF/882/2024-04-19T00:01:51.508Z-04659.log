#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("fermier.in");
    ofstream out("fermier.out");

    int n, c;
    in >> n >> c;

    vector<int> d(n + 1), q(n + 1);
    for (int i = 0; i <= n; i++) {
        in >> d[i];
    }
    for (int i = 1; i <= n; i++) {
        in >> q[i];
    }

    int dist = 0, curr = c, pos = 1;
    while (pos <= n) {
        int dist_to_next = d[pos];
        int dist_to_depo = d[0] + d[n];
        if (dist_to_next <= dist_to_depo) {
            dist += dist_to_next;
            curr -= q[pos];
            pos++;
        } else {
            dist += dist_to_depo;
            curr = c - q[pos];
            pos++;
        }
        if (curr < 0) {
            dist += d[0] + d[n];
            curr = c;
        }
    }

    out << dist << endl;

    return 0;
}
```
