#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("fermier.in");
    ofstream out("fermier.out");

    int n, c;
    in >> n >> c;

    vector<int> d(n + 1), q(n + 1);
    for (int i = 0; i < n; i++) in >> d[i];
    d[n] = d[0];
    for (int i = 0; i < n; i++) in >> q[i];

    int dist = 0, rem = 0;
    for (int i = 0; i < n; i++) {
        int dist1 = d[i] + d[i + 1];
        int dist2 = d[i] + d[n] + d[0] + d[i + 1];
        int dist_min = min(dist1, dist2);
        if (rem >= q[i]) {
            rem -= q[i];
        } else {
            int k = (q[i] - rem + c - 1) / c;
            dist += k * dist_min;
            rem = c - (q[i] - rem) % c;
        }
    }

    out << dist << endl;

    return 0;
}
```
