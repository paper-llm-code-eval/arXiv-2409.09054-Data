#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

const int MAXN = 100;
int n, c, d[MAXN + 1], q[MAXN + 1];
vector<int> sol;

int main() {
    ifstream fin("fermier.in");
    ofstream fout("fermier.out");
    fin >> n >> c;
    for (int i = 0; i <= n; i++) fin >> d[i];
    for (int i = 1; i <= n; i++) fin >> q[i];
    int pos = 1, dist = 0, rem = c;
    while (pos <= n) {
        int dist1 = d[pos], dist2 = d[n - pos + 1];
        if (dist1 > dist2) swap(dist1, dist2), pos = n - pos + 1;
        if (rem >= q[pos]) {
            rem -= q[pos];
            dist += dist1;
            sol.push_back(pos);
            pos++;
        } else {
            dist += dist2;
            rem = c - q[pos];
            pos = n - pos + 1;
        }
    }
    dist += d[0];
    fout << dist << '\n';
    for (int x : sol) fout << x << ' ';
    return 0;
}
