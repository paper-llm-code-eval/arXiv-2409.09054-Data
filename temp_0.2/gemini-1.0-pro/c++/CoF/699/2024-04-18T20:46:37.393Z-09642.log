#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>

using namespace std;

const int NMAX = 250;
const int INF = 1e9;

int n, dist[NMAX][NMAX];
vector<pair<int, int>> pos[NMAX][NMAX];
vector<pair<int, int>> start, finish;

int di[] = {-1, 0, 1, 0};
int dj[] = {0, 1, 0, -1};

int main() {
    ifstream fin("poarta.in");
    ofstream fout("poarta.out");

    fin >> n;
    start.resize(n);
    finish.resize(n);
    for (int i = 0; i < n; ++i) {
        int x1, y1, x2, y2;
        fin >> x1 >> y1 >> x2 >> y2;
        start[i] = {x1, y1};
        finish[i] = {x2, y2};
    }

    for (int i = 1; i <= 250; ++i) {
        for (int j = 1; j <= 250; ++j) {
            dist[i][j] = INF;
        }
    }

    queue<pair<int, int>> q;
    for (int i = 0; i < n; ++i) {
        q.push(start[i]);
        dist[start[i].first][start[i].second] = 0;
    }

    while (!q.empty()) {
        int x = q.front().first;
        int y = q.front().second;
        q.pop();

        for (int k = 0; k < 4; ++k) {
            int nx = x + di[k];
            int ny = y + dj[k];
            if (nx < 1 || nx > 250 || ny < 1 || ny > 250) {
                continue;
            }
            if (dist[nx][ny] > dist[x][y] + 1) {
                dist[nx][ny] = dist[x][y] + 1;
                q.push({nx, ny});
            }
        }
    }

    int ans = INF;
    for (int i = 0; i < n; ++i) {
        ans = min(ans, dist[finish[i].first][finish[i].second]);
    }

    if (ans == INF) {
        fout << -1 << '\n';
    } else {
        fout << ans << '\n';
    }

    return 0;
}
