#include <fstream>
#include <vector>
#include <queue>

using namespace std;

const int MAX_N = 250;
const int MAX_M = 250;
const int INF = 1e9;

int n, m;
int dist[MAX_N][MAX_M];
bool visited[MAX_N][MAX_M];
vector<pair<int, int>> adj[MAX_N][MAX_M];

void bfs(int x, int y) {
  queue<pair<int, int>> q;
  q.push({x, y});
  visited[x][y] = true;
  dist[x][y] = 0;

  while (!q.empty()) {
    int x = q.front().first;
    int y = q.front().second;
    q.pop();

    for (auto& [nx, ny] : adj[x][y]) {
      if (!visited[nx][ny]) {
        visited[nx][ny] = true;
        dist[nx][ny] = dist[x][y] + 1;
        q.push({nx, ny});
      }
    }
  }
}

int main() {
  ifstream fin("poarta.in");
  ofstream fout("poarta.out");

  fin >> n >> m;

  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      int x, y;
      fin >> x >> y;
      adj[x][y].push_back({i, j});
    }
  }

  int min_dist = INF;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= m; j++) {
      if (!visited[i][j]) {
        bfs(i, j);
        min_dist = min(min_dist, dist[n][m]);
      }
    }
  }

  fout << min_dist << '\n';

  return 0;
}
```
