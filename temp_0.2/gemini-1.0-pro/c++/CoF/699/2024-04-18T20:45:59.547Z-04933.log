#include <vector>
#include <queue>
#include <utility>

using namespace std;

const int MAX_N = 250;

int main() {
  ifstream fin("poarta.in");
  ofstream fout("poarta.out");

  int n;
  fin >> n;

  vector<pair<int, int>> start(n + 1);
  vector<pair<int, int>> end(n + 1);
  for (int i = 1; i <= n; ++i) {
    fin >> start[i].first >> start[i].second >> end[i].first >> end[i].second;
  }

  vector<vector<int>> dist(MAX_N + 1, vector<int>(MAX_N + 1, -1));
  queue<pair<int, int>> q;
  for (int i = 1; i <= n; ++i) {
    if (start[i] == end[i]) {
      dist[start[i].first][start[i].second] = 0;
      q.push(start[i]);
    }
  }

  while (!q.empty()) {
    int x = q.front().first;
    int y = q.front().second;
    q.pop();

    if (x + 1 <= MAX_N && dist[x + 1][y] == -1) {
      dist[x + 1][y] = dist[x][y] + 1;
      q.push(make_pair(x + 1, y));
    }
    if (x - 1 >= 1 && dist[x - 1][y] == -1) {
      dist[x - 1][y] = dist[x][y] + 1;
      q.push(make_pair(x - 1, y));
    }
    if (y + 1 <= MAX_N && dist[x][y + 1] == -1) {
      dist[x][y + 1] = dist[x][y] + 1;
      q.push(make_pair(x, y + 1));
    }
    if (y - 1 >= 1 && dist[x][y - 1] == -1) {
      dist[x][y - 1] = dist[x][y] + 1;
      q.push(make_pair(x, y - 1));
    }
  }

  int ans = 0;
  for (int i = 1; i <= n; ++i) {
    if (dist[end[i].first][end[i].second] == -1) {
      ans = -1;
      break;
    }
    ans += dist[end[i].first][end[i].second];
  }

  fout << ans << '\n';
  return 0;
}
