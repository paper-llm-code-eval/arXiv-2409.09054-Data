#include <fstream>
#include <vector>
#include <algorithm>
#include <numeric>

using namespace std;

const int MOD = 1e9 + 7;
const int MAXN = 16;

int n;
vector<int> p;
vector<vector<int>> adj;
vector<bool> vis;
vector<int> ans;
int cnt;

void dfs(int u, int prv) {
    vis[u] = true;
    for (int v : adj[u]) {
        if (v == prv) continue;
        if (!vis[v]) {
            p.push_back(v);
            dfs(v, u);
            p.pop_back();
        } else {
            vector<int> t;
            t.push_back(u);
            t.push_back(v);
            while (p.back() != v) {
                t.push_back(p.back());
                p.pop_back();
            }
            t.push_back(p.back());
            p.pop_back();
            reverse(t.begin(), t.end());
            int code = 1;
            for (int i = 0; i < t.size(); i++) {
                for (int j = i + 1; j < t.size(); j++) {
                    code = (1LL * code * (137LL * min(t[i], t[j]) + max(t[i], t[j]))) % MOD;
                }
            }
            ans.push_back(code);
            cnt++;
        }
    }
    vis[u] = false;
}

int main() {
    ifstream fin("triang.in");
    ofstream fout("triang.out");
    fin >> n;
    adj.resize(n + 1);
    vis.resize(n + 1);
    for (int i = 1; i < n; i++) {
        int u, v;
        fin >> u >> v;
        adj[u].push_back(v);
        adj[v].push_back(u);
    }
    dfs(1, 1);
    sort(ans.begin(), ans.end());
    fout << cnt << '\n';
    for (int x : ans) {
        fout << x << '\n';
    }
    return 0;
}
