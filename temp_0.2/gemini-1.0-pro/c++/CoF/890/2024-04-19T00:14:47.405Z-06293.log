#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream fin("cufar.in");
  ofstream fout("cufar.out");

  int p, n;
  fin >> p >> n;

  if (p == 1) {
    int nr, k;
    fin >> nr >> k;

    int div = 0;
    for (int i = 2; i * i <= nr; i++) {
      if (nr % i == 0) {
        div++;
        if (div == k) {
          fout << i << endl;
          return 0;
        }
        while (nr % i == 0) nr /= i;
      }
    }
    if (nr > 1) div++;
    if (div < k)
      fout << -1 << endl;
    else
      fout << nr << endl;
  } else {
    vector<int> v(n);
    for (int i = 0; i < n; i++) {
      int nr, k;
      fin >> nr >> k;
      v[i] = nr;
    }

    vector<int> div(1000001, 0);
    for (int i = 0; i < n; i++) {
      int nr = v[i];
      int d = 0;
      for (int j = 2; j * j <= nr; j++) {
        if (nr % j == 0) {
          d++;
          if (d == k) {
            div[j]++;
            break;
          }
          while (nr % j == 0) nr /= j;
        }
      }
      if (nr > 1) d++;
      if (d >= k) div[nr]++;
    }

    int sum = 0;
    for (int i = 0; i < 1000001; i++) {
      while (div[i] > 0) {
        sum += i;
        div[i]--;
      }
    }

    vector<int> cifre;
    while (sum > 0) {
      cifre.push_back(sum % 10);
      sum /= 10;
    }

    for (int i = cifre.size() - 1; i >= 0; i--) fout << cifre[i];
    fout << endl;
  }

  return 0;
}
