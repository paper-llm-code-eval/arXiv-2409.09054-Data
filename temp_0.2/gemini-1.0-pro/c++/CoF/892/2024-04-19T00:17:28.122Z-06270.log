int main() {
    ifstream fin("tnia.in");
    ofstream fout("tnia.out");

    int n, m, q;
    fin >> n >> m;
    vector<int> h(n);
    for (int i = 0; i < n; i++) {
        fin >> h[i];
    }
    fin >> q;

    vector<int> pref(m + 1);
    for (int i = 0; i < n; i++) {
        pref[h[i]]++;
    }
    for (int i = 1; i <= m; i++) {
        pref[i] += pref[i - 1];
    }

    while (q--) {
        int a, b, c, d;
        fin >> a >> b >> c >> d;
        fout << pref[d] - pref[b - 1] << '\n';
    }

    return 0;
}
```
