#include <fstream>
#include <vector>
#include <algorithm>
#include <numeric>

using namespace std;

ifstream in("logic.in");
ofstream out("logic.out");

const int MAXN = 8;
const int MOD = 666013;

int n, c, k;
vector<vector<int>> circuit;
vector<vector<int>> dp;

int get_value(const vector<int>& bits) {
    int value = 0;
    for (int i = 0; i < n; ++i) {
        int op = circuit[i][0];
        int a = bits[circuit[i][1]];
        int b = bits[circuit[i][2]];
        if (op == '&') {
            value = (value & a) & b;
        } else {
            value = (value | a) | b;
        }
    }
    return value;
}

int count_bits(const vector<int>& bits) {
    int count = 0;
    for (int i = 0; i < n; ++i) {
        int op = circuit[i][0];
        int a = bits[circuit[i][1]];
        int b = bits[circuit[i][2]];
        if (op == '&') {
            count += (a & b);
        } else {
            count += (a | b);
        }
    }
    return count;
}

int main() {
    in >> c >> n;
    circuit.resize(n);
    for (int i = 0; i < n; ++i) {
        string line;
        in >> line;
        for (char c : line) {
            circuit[i].push_back(c == '&' ? 0 : 1);
        }
    }
    if (c == 1) {
        in >> k;
        for (int i = 0; i < k; ++i) {
            string line;
            in >> line;
            vector<int> bits;
            for (char c : line) {
                bits.push_back(c - '0');
            }
            out << get_value(bits) << '\n';
        }
    } else {
        int value;
        in >> value;
        dp.resize(n + 1, vector<int>(1 << n, -1));
        function<int(int, int)> dfs = [&](int level, int mask) {
            if (level == n) {
                return mask == value;
            }
            int& res = dp[level][mask];
            if (res != -1) {
                return res;
            }
            res = 0;
            int op = circuit[level][0];
            int a = mask & (1 << circuit[level][1]);
            int b = mask & (1 << circuit[level][2]);
            if (op == '&') {
                res = (dfs(level + 1, mask) + dfs(level + 1, mask | a) + dfs(level + 1, mask | b) + dfs(level + 1, mask | a | b)) % MOD;
            } else {
                res = (dfs(level + 1, mask) + dfs(level + 1, mask | a) + dfs(level + 1, mask | b) - dfs(level + 1, mask | a | b)) % MOD;
            }
            return res;
        };
        out << dfs(0, 0) << '\n';
    }
    return 0;
}
