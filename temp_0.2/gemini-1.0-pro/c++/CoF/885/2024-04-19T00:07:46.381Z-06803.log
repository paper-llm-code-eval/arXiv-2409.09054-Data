#include <fstream>
#include <vector>
using namespace std;

ifstream in("tablou.in");
ofstream out("tablou.out");

int main() {
  int p;
  in >> p;
  int n, k, z;
  in >> n >> (p == 1 ? k : z);
  vector<int> l(n + 1, 1), c(n + 1, 1);
  while (k--) {
    char op;
    int nr;
    in >> op >> nr;
    if (op == 'L')
      l[nr] *= -1;
    else
      c[nr] *= -1;
  }
  if (p == 1) {
    int ans = 0;
    for (int i = 1; i <= n; i++)
      for (int j = 1; j <= n; j++) ans += l[i] * c[j];
    out << ans << '\n';
  } else {
    int ans = 0;
    for (int i = 1; i <= n; i++)
      for (int j = 1; j <= n; j++) ans += (l[i] * c[j] == -1);
    if (ans == z)
      out << 0 << '\n';
    else {
      ans = n * n - ans;
      if (ans % 2 == z % 2)
        out << ans / 2 << '\n';
      else
        out << 0 << '\n';
    }
  }
  return 0;
}
