    soldiers[sorted_id].push_back(i + 1);
  }

  vector<vector<int>> platoons;
  for (auto& [sorted_id, soldiers_ids] : soldiers) {
    platoons.push_back(soldiers_ids);
  }

  sort(platoons.begin(), platoons.end(), [](const vector<int>& a, const vector<int>& b) {
    return a.size() > b.size();
  });

  out << platoons.size() << endl;
  out << platoons[0].size() << endl;
  int max_platoon_count = 0;
  for (const auto& platoon : platoons) {
