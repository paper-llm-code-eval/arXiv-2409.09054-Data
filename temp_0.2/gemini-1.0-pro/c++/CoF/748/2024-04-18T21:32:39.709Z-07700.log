  in >> n;

  unordered_map<int, vector<int>> soldiers;
  for (int i = 0; i < n; i++) {
    int id;
    in >> id;

    vector<int> digits;
    while (id > 0) {
      digits.push_back(id % 10);
      id /= 10;
    }
