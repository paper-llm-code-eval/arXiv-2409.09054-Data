#include <fstream>
#include <vector>
#include <queue>
#include <algorithm>

using namespace std;

const int MAXN = 1005;
const int MAXK = 150005;
const int INF = 1e9;

int n, k;
int T1, T2, S1, S2, S3, S4, R1, R2, R3, R4;
int dist[MAXN][MAXN];
int dist_obs[MAXN][MAXN];
vector<pair<int, int>> obstacles;

void read_input() {
    ifstream fin("ai.in");
    fin >> n;
    fin >> T1 >> T2 >> S1 >> S2 >> S3 >> S4 >> R1 >> R2 >> R3 >> R4;
    fin >> k;
    for (int i = 1; i <= k; i++) {
        int x, y;
        fin >> x >> y;
        obstacles.push_back({x, y});
    }
    fin.close();
}

void write_output(int max_wall_length, int min_time) {
    ofstream fout("ai.out");
    fout << max_wall_length << '\n';
    fout << min_time << '\n';
    fout.close();
}

void bfs(int x, int y, int dist_arr[MAXN][MAXN]) {
    queue<pair<int, int>> q;
    q.push({x, y});
    dist_arr[x][y] = 0;
    while (!q.empty()) {
        int cx = q.front().first;
        int cy = q.front().second;
        q.pop();
        if (cx > 1 && dist_arr[cx - 1][cy] == -1) {
            dist_arr[cx - 1][cy] = dist_arr[cx][cy] + 1;
            q.push({cx - 1, cy});
        }
        if (cx < n && dist_arr[cx + 1][cy] == -1) {
            dist_arr[cx + 1][cy] = dist_arr[cx][cy] + 1;
            q.push({cx + 1, cy});
        }
        if (cy > 1 && dist_arr[cx][cy - 1] == -1) {
            dist_arr[cx][cy - 1] = dist_arr[cx][cy] + 1;
            q.push({cx, cy - 1});
        }
        if (cy < n && dist_arr[cx][cy + 1] == -1) {
            dist_arr[cx][cy + 1] = dist_arr[cx][cy] + 1;
            q.push({cx, cy + 1});
        }
    }
}

int main() {
    read_input();

    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n; j++) {
            dist[i][j] = -1;
            dist_obs[i][j] = -1;
        }
    }

    bfs(T1, T2, dist);

    for (auto& obstacle : obstacles) {
        dist_obs[obstacle.first][obstacle.second] = 0;
    }

    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n; j++) {
            if (dist_obs[i][j] == -1) {
                bfs(i, j, dist_obs);
            }
        }
    }

    int max_wall_length = 0;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n; j++) {
            if (dist_obs[i][j] != -1) {
                int wall_length = 0;
                if (dist_obs[i][j] == dist_obs[i - 1][j]) {
                    wall_length++;
                }
                if (dist_obs[i][j] == dist_obs[i + 1][j]) {
                    wall_length++;
                }
                if (dist_obs[i][j] == dist_obs[i][j - 1]) {
                    wall_length++;
                }
                if (dist_obs[i][j] == dist_obs[i][j + 1]) {
                    wall_length++;
                }
                max_wall_length = max(max_wall_length, wall_length);
            }
        }
    }

    int min_time = INF;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n; j++) {
            if (dist[i][j] != -1) {
                int time_to_target = dist[i][j];
                int time_to_source1 = dist_obs[i][j] + dist[S1][S2];
                int time_to_source2 = dist_obs[i][j] + dist[S3][S4];
                if (time_to_source1 != -1 && time_to_source2 != -1) {
                    min_time = min(min_time, max(time_to_target, max(time_to_source1, time_to_source2)));
                }
            }
        }
    }

    write_output(max_wall_length, min_time);

    return 0;
}
