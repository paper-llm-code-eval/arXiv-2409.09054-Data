#include <fstream>
#include <vector>
#include <queue>

using namespace std;

const int NMAX = 1005;

int n, k;
int t1, t2, s1, s2, s3, s4, r1, r2, r3, r4;
int dist[NMAX][NMAX];
vector<pair<int, int>> obstacles;

int dx[] = {-1, 0, 1, 0};
int dy[] = {0, 1, 0, -1};

bool inside(int x, int y) {
    return 1 <= x && x <= n && 1 <= y && y <= n;
}

int main() {
    ifstream fin("ai.in");
    ofstream fout("ai.out");

    fin >> n;
    fin >> t1 >> t2 >> s1 >> s2 >> s3 >> s4 >> r1 >> r2 >> r3 >> r4;
    fin >> k;
    for (int i = 1; i <= k; ++i) {
        int x, y;
        fin >> x >> y;
        obstacles.push_back({x, y});
    }

    queue<pair<int, int>> q;
    q.push({t1, t2});
    dist[t1][t2] = 0;
    while (!q.empty()) {
        int x = q.front().first;
        int y = q.front().second;
        q.pop();
        for (int i = 0; i < 4; ++i) {
            int nx = x + dx[i];
            int ny = y + dy[i];
            if (inside(nx, ny) && dist[nx][ny] == -1) {
                bool ok = true;
                for (auto& obstacle : obstacles) {
                    if (obstacle.first == nx && obstacle.second == ny) {
                        ok = false;
                        break;
                    }
                }
                if (ok) {
                    dist[nx][ny] = dist[x][y] + 1;
                    q.push({nx, ny});
                }
            }
        }
    }

    int max_wall = 0;
    for (int i = 1; i <= n; ++i) {
        int cnt_h = 0;
        int cnt_v = 0;
        for (int j = 1; j <= n; ++j) {
            if (dist[i][j] != -1) {
                ++cnt_h;
            }
            if (dist[j][i] != -1) {
                ++cnt_v;
            }
        }
        max_wall = max({max_wall, cnt_h, cnt_v});
    }

    int min_time = -1;
    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= n; ++j) {
            if (dist[i][j] != -1) {
                int time1 = dist[i][j];
                int time2 = dist[t1][t2] - dist[i][j];
                if (min_time == -1 || min_time > time1 + time2) {
                    min_time = time1 + time2;
                }
            }
        }
    }

    fout << max_wall << '\n';
    fout << min_time << '\n';

    return 0;
}
```
