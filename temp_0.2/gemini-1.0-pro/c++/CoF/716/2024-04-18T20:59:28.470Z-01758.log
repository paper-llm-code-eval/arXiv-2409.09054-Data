#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream fin("gardul.in");
  ofstream fout("gardul.out");
  int n, p, q;
  fin >> n >> p >> q;
  vector<bool> vopsit(n + 1, false);
  int nevopsit = 0, rosu = 0, albastru = 0, violet = 0;
  for (int i = p; i <= n; i += p) {
    if (!vopsit[i]) {
      vopsit[i] = true;
      rosu++;
    }
  }
  for (int i = q; i <= n; i += q) {
    if (!vopsit[i]) {
      vopsit[i] = true;
      albastru++;
    }
  }
  for (int i = 1; i <= n; i++) {
    if (!vopsit[i]) {
      nevopsit++;
    } else if (vopsit[i] && vopsit[i - p] && vopsit[i - q]) {
      violet++;
    }
  }
  fout << nevopsit << endl;
  fout << rosu << endl;
  fout << albastru << endl;
  fout << violet << endl;
  return 0;
}
