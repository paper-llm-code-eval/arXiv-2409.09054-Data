#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream in("gardul.in");
  ofstream out("gardul.out");

  int n, p, q;
  in >> n >> p >> q;

  vector<bool> painted(n + 1, false);

  for (int i = p; i <= n; i += p) {
    painted[i] = true;
  }

  for (int i = q; i <= n; i += q) {
    painted[i] = true;
  }

  int unpainted = 0, red = 0, blue = 0, violet = 0;

  for (int i = 1; i <= n; i++) {
    if (!painted[i]) {
      unpainted++;
    } else if (i % p == 0 && i % q == 0) {
      violet++;
    } else if (i % p == 0) {
      red++;
    } else {
      blue++;
    }
  }

  out << unpainted << endl;
  out << red << endl;
  out << blue << endl;
  out << violet << endl;

  return 0;
}
```
