#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

ifstream fin("turnuri.in");
ofstream fout("turnuri.out");

const int NMAX = 100005;
int n, v[NMAX], st[NMAX], dr[NMAX], ans[NMAX];
vector<int> ord;

int main() {
    fin >> n;
    for (int i = 1; i <= n; ++i) {
        fin >> v[i];
        ord.push_back(i);
    }
    sort(ord.begin(), ord.end(), [&](int a, int b) { return v[a] < v[b]; });
    for (int i = 1; i <= n; ++i) {
        int j = i - 1;
        while (j >= 1 && v[ord[j]] >= v[ord[i]]) {
            j = st[ord[j]];
        }
        st[ord[i]] = ord[j];
    }
    for (int i = n; i >= 1; --i) {
        int j = i + 1;
        while (j <= n && v[ord[j]] >= v[ord[i]]) {
            j = dr[ord[j]];
        }
        dr[ord[i]] = ord[j];
    }
    for (int i = 1; i <= n; ++i) {
        ans[i] = dr[i] - st[i] - 1;
    }
    for (int i = 1; i <= n; ++i) {
        fout << ans[i] << '\n';
    }
    return 0;
}
