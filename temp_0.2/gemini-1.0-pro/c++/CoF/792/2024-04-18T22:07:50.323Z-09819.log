      cantitate_curenta = cantitate_curenta * 10 + (c - '0');
    } else {
      ingredient_curent += c;
      if (paranteze_deschise == 0 && !isdigit(c)) {
        ingredient_curent += ' ';
      }
    }
  }
  if (paranteze_deschise == 0) {
    ingrediente[ingredient_curent] += cantitate_curenta;
    ingrediente_ordonate.push_back({ingrediente[ingredient_curent], ingredient_curent});
  }
  out << timp_total << endl;
  sort(ingrediente_ordonate.begin(), ingrediente_ordonate.end());
  for (auto &ingrediente_ordonata : ingrediente_ordonate) {
    out << ingrediente_ordonata.second << ' ' << ingrediente_ordonata.first << endl;
  }
  return 0;
}
