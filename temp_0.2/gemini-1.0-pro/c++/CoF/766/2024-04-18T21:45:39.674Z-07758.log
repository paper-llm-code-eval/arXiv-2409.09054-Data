#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream fin("afise.in");
    ofstream fout("afise.out");

    int L, n, k;
    fin >> L >> n >> k;

    vector<int> afise(n);
    for (int i = 0; i < n; i++) {
        fin >> afise[i];
    }

    sort(afise.begin(), afise.end());

    int min_lungime = L;
    int num_panouri = 0;

    int st = 0, dr = 0;
    while (st < n) {
        while (dr < n && afise[dr] - afise[st] <= L) {
            dr++;
        }

        if (dr - st > k) {
            st++;
        } else {
            min_lungime = min(min_lungime, afise[dr - 1] - afise[st] + 1);
            num_panouri = dr - st;
            st++;
        }
    }

    fout << min_lungime << " " << num_panouri << "\n";

    return 0;
}
```
