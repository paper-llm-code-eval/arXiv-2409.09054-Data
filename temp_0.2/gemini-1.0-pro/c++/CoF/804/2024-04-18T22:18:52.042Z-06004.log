#include <fstream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    ifstream in("text.in");
    ofstream out("text.out");

    vector<string> words;
    string word;
    while (in >> word) {
        words.push_back(word);
    }
    in.close();

    int n = words.size();
    vector<int> dp(n, 1);
    for (int i = 1; i < n; i++) {
        for (int j = 0; j < i; j++) {
            if (words[i][0] == words[j][words[j].size() - 1]) {
                dp[i] = max(dp[i], dp[j] + 1);
            }
        }
    }

    int max_len = 0;
    int last_idx = -1;
    for (int i = 0; i < n; i++) {
        if (dp[i] > max_len) {
            max_len = dp[i];
            last_idx = i;
        }
    }

    out << n - max_len << endl;
    out << n << endl;

    while (last_idx != -1) {
        out << words[last_idx] << endl;
        for (int i = last_idx - 1; i >= 0; i--) {
            if (words[last_idx][0] == words[i][words[i].size() - 1]) {
                last_idx = i;
                break;
            }
        }
    }

    out.close();
    return 0;
}
