#include <fstream>
#include <vector>
#include <cmath>
#include <algorithm>
using namespace std;

int main() {
    ifstream in("expresie.in");
    ofstream out("expresie.out");
    int m, n;
    in >> m >> n;
    vector<int> v(n);
    for (int i = 0; i < n; i++) {
        in >> v[i];
    }
    long long prod = 1;
    for (int i = 0; i < n; i++) {
        prod *= v[i];
    }
    double root = pow(prod, 1.0 / m);
    if (floor(root) != root) {
        out << 0 << endl;
        return 0;
    }
    out << 1 << endl;
    int num = root;
    vector<pair<int, int>> factors;
    for (int i = 2; i * i <= num; i++) {
        int exp = 0;
        while (num % i == 0) {
            num /= i;
            exp++;
        }
        if (exp > 0) {
            factors.push_back({i, exp});
        }
    }
    if (num > 1) {
        factors.push_back({num, 1});
    }
    sort(factors.begin(), factors.end());
    for (auto &factor : factors) {
        out << factor.first << " " << factor.second << endl;
    }
    return 0;
}
