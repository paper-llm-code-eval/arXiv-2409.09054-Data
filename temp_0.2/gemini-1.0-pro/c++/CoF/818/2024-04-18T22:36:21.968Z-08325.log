#include <fstream>
#include <vector>

using namespace std;

int main() {
  ifstream in("roata.in");
  ofstream out("roata.out");

  int n, p;
  in >> n >> p;

  vector<int> c(p);
  for (int i = 0; i < p; i++) {
    in >> c[i];
  }

  int sum = 0;
  for (int i = 0; i < p; i++) {
    sum += c[i];
  }

  out << sum << endl;

  vector<int> order(p);
  for (int i = 0; i < p; i++) {
    order[i] = i + 1;
  }

  int pos = 0;
  for (int i = 0; i < p; i++) {
    pos = (pos + c[i]) % n;
    out << order[pos] << " ";
    order.erase(order.begin() + pos);
    pos--;
  }

  out << endl << pos + 1 << endl;

  return 0;
}
```
