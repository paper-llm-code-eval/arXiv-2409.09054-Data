        long long ans4 = 0;
        for (int i = 1; i <= n; i++) {
            for (int j = 1; j <= i; j++) {
                int node = (i - 1) * i / 2 + j;
                if (in[node] >= ans2)
                    ans4 += c[node];
            }
        }
        fout << in[1] << ' ' << ans4 << '\n';
    }
    return 0;
}
