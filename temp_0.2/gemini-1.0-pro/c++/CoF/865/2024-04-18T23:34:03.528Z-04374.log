                g[(i - 2) * (i - 1) / 2 + j / 2].push_back(node);
            } else {
                g[node].push_back((i - 2) * (i - 1) / 2 + (j + 1) / 2);
                g[(i - 2) * (i - 1) / 2 + (j + 1) / 2].push_back(node);
            }
        }
    }
    dfs1(1, 0);
    dfs2(1, 0, 0);
    bfs();
    if (v == 1)
        fout << ans2 / n + 1 << '\n';
