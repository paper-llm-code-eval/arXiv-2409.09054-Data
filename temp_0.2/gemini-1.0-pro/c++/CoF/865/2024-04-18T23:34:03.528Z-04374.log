        ans2 = in[node];
    }
    for (int next : g[node]) {
        if (next == parent)
            continue;
        dfs2(next, node, sum_node);
    }
}

void bfs() {
    queue<int> q;
    q.push(1);
    while (!q.empty()) {
        int node = q.front();
        q.pop();
        for (int next : g[node]) {
            if (in[next] == -1) {
                q.push(next);
                in[next] = in[node] + 1;
            }
        }
    }
}

