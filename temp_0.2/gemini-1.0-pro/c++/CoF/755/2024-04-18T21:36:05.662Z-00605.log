#include <fstream>
#include <vector>
#include <algorithm>
#include <queue>

using namespace std;

int main() {
  ifstream fin("elfi.in");
  ofstream fout("elfi.out");

  int n;
  fin >> n;

  vector<pair<int, int>> spiridusi(n);
  for (int i = 0; i < n; i++) {
    fin >> spiridusi[i].first >> spiridusi[i].second;
  }

  sort(spiridusi.begin(), spiridusi.end());

  priority_queue<int, vector<int>, greater<int>> pq;
  int min_robinete = 0;
  for (int i = 0; i < n; i++) {
    while (!pq.empty() && pq.top() <= spiridusi[i].first) {
      pq.pop();
    }
    pq.push(spiridusi[i].first + spiridusi[i].second);
    min_robinete = max(min_robinete, (int)pq.size());
  }

  fout << min_robinete << endl;

  return 0;
}
```
