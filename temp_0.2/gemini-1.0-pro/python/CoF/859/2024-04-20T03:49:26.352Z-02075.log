import sys

fin = open("lasere.in", "r")
fout = open("lasere.out", "w")

c = int(fin.readline())
n, m = map(int, fin.readline().split())
mat = [list(map(int, fin.readline().split())) for _ in range(n)]
lasere = [list(map(int, fin.readline().split())) for _ in range(m)]

dirc = [(-1, 0), (0, 1), (1, 0), (0, -1)]

for i, j, d in lasere:
    i -= 1
    j -= 1
    mat[i][j] -= 1
    for dx, dy in dirc:
        x, y = i + dx, j + dy
        while 0 <= x < n and 0 <= y < n:
            mat[x][y] -= 1
            x += dx
            y += dy

if c == 1:
    cnt = 0
    for i in range(n):
        for j in range(n):
            if all(mat[x][y] >= mat[i][j] for x in range(max(0, i - 1), min(n, i + 2)) for y in range(max(0, j - 1), min(n, j + 2))):
                cnt += 1
    fout.write(str(cnt) + "\n")
else:
    cnt = 0
    for i in range(n):
        for j in range(n):
            if mat[i][j] < mat[i - 1][j] or mat[i][j] < mat[i][j - 1]:
                continue
            l = j
            while l >= 0 and mat[i][l] >= mat[i][j]:
                l -= 1
            r = j
            while r < n and mat[i][r] >= mat[i][j]:
                r += 1
            if r - l >= 2:
                cnt += 1
    fout.write(str(cnt) + "\n")
```
