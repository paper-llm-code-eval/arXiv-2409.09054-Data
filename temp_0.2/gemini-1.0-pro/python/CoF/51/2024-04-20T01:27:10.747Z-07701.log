import sys

def read_input():
    with open('lant.in', 'r') as f:
        k = int(f.readline())
        text = f.read()
    return k, text

def write_output(result):
    with open('lant.out', 'w') as f:
        f.write(str(result))

def get_words(text):
    words = []
    word = ''
    for char in text:
        if char.isalpha():
            word += char
        elif word:
            words.append(word)
            word = ''
    if word:
        words.append(word)
    return words

def get_similarities(words):
    similarities = {}
    for i in range(len(words)):
        for j in range(i + 1, len(words)):
            similarities[(words[i], words[j])] = get_similarity(words[i], words[j])
    return similarities

def get_similarity(word1, word2):
    m, n = len(word1), len(word2)
    dp = [[0] * (n + 1) for _ in range(m + 1)]
    for i in range(1, m + 1):
        dp[i][0] = i
    for j in range(1, n + 1):
        dp[0][j] = j
    for i in range(1, m + 1):
        for j in range(1, n + 1):
            if word1[i - 1] == word2[j - 1]:
                dp[i][j] = dp[i - 1][j - 1]
            else:
                dp[i][j] = min(dp[i - 1][j], dp[i][j - 1], dp[i - 1][j - 1]) + 1
    return dp[m][n]

def get_chains(words, similarities, k):
    chains = []
    for i in range(len(words)):
        chain = [words[i]]
        for j in range(i + 1, len(words)):
            if similarities[(words[i], words[j])] <= k:
                chain.append(words[j])
        if len(chain) > 1:
            chains.append(chain)
    return chains

def count_chains(chains):
    count = 0
    for chain in chains:
        count += len(chain) - 1
    return count

def main():
    k, text = read_input()
    words = get_words(text)
    similarities = get_similarities(words)
    chains = get_chains(words, similarities, k)
    result = count_chains(chains)
    write_output(result)

if __name__ == '__main__':
    main()
```
