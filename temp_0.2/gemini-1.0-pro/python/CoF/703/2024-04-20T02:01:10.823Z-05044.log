with open("valori-panta.in", "r") as f:
    n = int(f.readline())
    v = list(map(int, f.readline().split()))

nr_valori_panta = 0
max_valoare_panta = -1
poz_max_valoare_panta = []
min_valoare_panta = 10 ** 9
poz_min_valoare_panta = []

for i in range(n):
    cifre = []
    x = v[i]
    while x > 0:
        cifre.append(x % 10)
        x //= 10
    cifre.reverse()

    if cifre == sorted(cifre):
        nr_valori_panta += 1
        if v[i] > max_valoare_panta:
            max_valoare_panta = v[i]
            poz_max_valoare_panta = [i + 1]
        elif v[i] == max_valoare_panta:
            poz_max_valoare_panta.append(i + 1)
        if v[i] < min_valoare_panta:
            min_valoare_panta = v[i]
            poz_min_valoare_panta = [i + 1]
        elif v[i] == min_valoare_panta:
            poz_min_valoare_panta.append(i + 1)

with open("valori-panta.out", "w") as f:
    f.write(str(nr_valori_panta) + "\n")
    if nr_valori_panta > 0:
        f.write(str(max_valoare_panta) + " " + " ".join(map(str, poz_max_valoare_panta)) + "\n")
        f.write(str(min_valoare_panta) + " " + " ".join(map(str, poz_min_valoare_panta)) + "\n")
    else:
        f.write("NU EXISTA")
```
