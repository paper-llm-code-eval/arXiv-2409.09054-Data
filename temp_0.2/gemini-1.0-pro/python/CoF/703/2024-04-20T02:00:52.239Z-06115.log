with open("valori-panta.in", "r") as f:
    n = int(f.readline())
    v = list(map(int, f.readline().split()))

def este_panta(x):
    x = str(x)
    return x == ''.join(sorted(x))

nr_pante = 0
max_panta = -1
poz_max = -1
min_panta = 1000000000
poz_min = -1

for i in range(n):
