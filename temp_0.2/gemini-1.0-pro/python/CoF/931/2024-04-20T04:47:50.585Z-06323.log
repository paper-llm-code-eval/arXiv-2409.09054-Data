                f.write(str(i + 1))
            break
elif c == 2:
    ans = []
    for i in range(n):
        if a[i] > max(a[:i]):
            ans.append(i + 1)
    with open('sir.out', 'w') as f:
        f.write(' '.join(map(str, ans)))
else:
    mx = max(a)
    ans = 0
    for i in range(n):
        if a[i] < mx:
            ans += mx - a[i]
    with open('sir.out', 'w') as f:
        f.write(str(ans))
