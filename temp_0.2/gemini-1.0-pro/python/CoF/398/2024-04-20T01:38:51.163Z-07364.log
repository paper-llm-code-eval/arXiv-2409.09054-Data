    return 1 + dfs(i+1, j, c) + dfs(i-1, j, c) + dfs(i, j+1, c) + dfs(i, j-1, c)

def count_islands():
    nr = ng = nb = 0
    for i in range(n):
        for j in range(m):
            if grid[i][j] == 1:
                nr += dfs(i, j, 1)
            elif grid[i][j] == 2:
                ng += dfs(i, j, 2)
            elif grid[i][j] == 3:
                nb += dfs(i, j, 3)
    return nr, ng, nb

def find_bridge():
    min_length = float('inf')
    for i in range(n):
        for j in range(m):
            if grid[i][j] == 0:
                for k in range(m):
                    if grid[i][k] == 1:
                        length = dfs(i, j, 0) + dfs(i, k, 0) - 1
                        if length > 0 and length < min_length:
                            min_length = length
                for k in range(n):
                    if grid[k][j] == 1:
                        length = dfs(i, j, 0) + dfs(k, j, 0) - 1
                        if length > 0 and length < min_length:
                            min_length = length
                if grid[i][j] == 1:
                    length = dfs(i, j, 0)
                    if length > 0 and length < min_length:
                        min_length = length
    return min_length if min_length != float('inf') else 0

nr, ng, nb = count_islands()
lg = find_bridge()
with open('insule.out', 'w') as f:
    f.write(f'{nr} {ng} {nb} {lg}')
