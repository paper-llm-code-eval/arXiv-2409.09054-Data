with open('arrows.in') as fin, open('arrows.out', 'w') as fout:
    cerinta = int(fin.readline())
    n, m = map(int, fin.readline().split())
    tabla = [list(map(int, fin.readline().split())) for _ in range(n)]
    lin, col = map(int, fin.readline().split())
    lin -= 1
    col -= 1

    viz = [[False] * m for _ in range(n)]
    viz[lin][col] = True
    puncte = 0
    celule_distincte = set()
    while True:
        celule_distincte.add((lin, col))
        puncte += 1
        if tabla[lin][col] == 1:
            col += 1
        elif tabla[lin][col] == 2:
            lin -= 1
        elif tabla[lin][col] == 3:
            col -= 1
        elif tabla[lin][col] == 4:
            lin += 1
        if viz[lin][col]:
            break
        viz[lin][col] = True

    if cerinta == 1:
        fout.write(str(puncte))
    elif cerinta == 2:
        fout.write(str(sum(sum(not viz[i][j] for j in range(m)) for i in range(n))))
    else:
        fout.write(str(max(puncte * 1000, max(puncte for viz_i, viz_j in zip(viz, viz[1:]) for viz_ii, viz_jj in zip(viz_i, viz_j[1:]) if viz_ii != viz_jj))))
```
