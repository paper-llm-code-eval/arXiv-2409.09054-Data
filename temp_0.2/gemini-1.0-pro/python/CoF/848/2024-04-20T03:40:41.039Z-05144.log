with open('arrows.in') as f:
    req, n, m = map(int, f.readline().split())
    board = [list(map(int, f.readline().split())) for _ in range(n)]
    start_row, start_col = map(int, f.readline().split())

visited = [[False] * m for _ in range(n)]
directions = [(0, 1), (1, 0), (0, -1), (-1, 0)]

def dfs(row, col, score):
    if row < 0 or row >= n or col < 0 or col >= m or visited[row][col]:
        return score
    visited[row][col] = True
    return dfs(*[row + directions[board[row][col] - 1][i] for i in range(2)], score + 1)

def find_favorable_cells():
    count = 0
    for i in range(n):
        for j in range(m):
            if not visited[i][j]:
                score = dfs(i, j, 0)
                if score > 0:
                    count += 1
    return count

def find_max_score():
    max_score = 0
    for i in range(n):
        for j in range(m):
            if not visited[i][j]:
                max_score = max(max_score, dfs(i, j, 0))
    return max_score * 1000

if req == 1:
    print(dfs(start_row - 1, start_col - 1, 0))
elif req == 2:
    print(find_favorable_cells())
else:
    print(find_max_score())
