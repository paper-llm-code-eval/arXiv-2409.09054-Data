import sys
from collections import deque

def read_input():
    p = int(input())
    n, m, T = map(int, input().split())
    L, C, k, S = map(int, input().split())
    food = [tuple(map(int, input().split())) for _ in range(T)]
    codes = [list(map(int, input().split())) for _ in range(n)]
    return p, n, m, T, L, C, k, S, food, codes

def is_accessible(code, k, S):
    return (code & (1 << (S - 1))) == (k & (1 << (S - 1)))

def bfs(n, m, L, C, food, codes, S, k):
    visited = [[False] * m for _ in range(n)]
    visited[L - 1][C - 1] = True
    queue = deque([(L - 1, C - 1, 0)])
    min_time = sys.maxsize
    num_food = 0
    while queue:
        x, y, time = queue.popleft()
        if (x, y) in food:
            if time < min_time:
                min_time = time
                num_food = 1
            elif time == min_time:
                num_food += 1
        if x > 0 and not visited[x - 1][y] and is_accessible(codes[x - 1][y], k, S):
            visited[x - 1][y] = True
            queue.append((x - 1, y, time + 1))
        if x < n - 1 and not visited[x + 1][y] and is_accessible(codes[x + 1][y], k, S):
            visited[x + 1][y] = True
            queue.append((x + 1, y, time + 1))
        if y > 0 and not visited[x][y - 1] and is_accessible(codes[x][y - 1], k, S):
            visited[x][y - 1] = True
            queue.append((x, y - 1, time + 1))
        if y < m - 1 and not visited[x][y + 1] and is_accessible(codes[x][y + 1], k, S):
            visited[x][y + 1] = True
            queue.append((x, y + 1, time + 1))
    return min_time, num_food

def main():
    p, n, m, T, L, C, k, S, food, codes = read_input()
    if p == 1:
        cnt = 0
        for i in range(n):
            for j in range(m):
                if i != L - 1 or j != C - 1:
                    if is_accessible(codes[i][j], k, S):
                        cnt += 1
        print(cnt)
    else:
        min_time, num_food = bfs(n, m, L - 1, C - 1, food, codes, S, k)
        print(min_time, num_food)

if __name__ == "__main__":
    main()
