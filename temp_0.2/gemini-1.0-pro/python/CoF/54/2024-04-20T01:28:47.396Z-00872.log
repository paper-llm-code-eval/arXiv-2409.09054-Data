import heapq
import sys

fin = open("lanterna.in", "r")
fout = open("lanterna.out", "w")

n, k = map(int, fin.readline().split())
is_base = [int(x) for x in fin.readline().split()]
m = int(fin.readline())
edges = []
for _ in range(m):
    a, b, t, w = map(int, fin.readline().split())
    edges.append((a, b, t, w))
    edges.append((b, a, t, w))

# Dijkstra's algorithm with priority queue
dist = [float('inf')] * (n + 1)
dist[1] = 0
pq = [(0, 1, k)]  # (distance, node, lantern type)
while pq:
    d, node, lantern = heapq.heappop(pq)
    if node == n:
        fout.write(f"{d} {lantern}\n")
        sys.exit(0)
    if d > dist[node]:
        continue
    for neighbor, t, w in edges:
        if neighbor == node:
            continue
        if lantern >= w and d + t < dist[neighbor]:
            dist[neighbor] = d + t
            heapq.heappush(pq, (d + t, neighbor, lantern))
        elif is_base[neighbor] and d + t < dist[neighbor]:
            for l in range(lantern, k + 1):
                dist[neighbor] = d + t
                heapq.heappush(pq, (d + t, neighbor, l))

fout.write("-1 -1\n")
```
