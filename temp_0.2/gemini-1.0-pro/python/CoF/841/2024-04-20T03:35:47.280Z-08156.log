import sys

def read_input():
    p = int(sys.stdin.readline())
    n, k, d = map(int, sys.stdin.readline().split())
    balls = [tuple(map(int, sys.stdin.readline().split())) for _ in range(k)]
    m = int(sys.stdin.readline())
    queries = [tuple(map(int, sys.stdin.readline().split())) for _ in range(m)]
    return p, n, k, d, balls, m, queries

def solve_part1(n, d):
    return (n - d // 2) ** 2

def solve_part2(n, d, balls, queries):
    answers = []
    for x, y in queries:
        count = 0
        for ball_x, ball_y in balls:
            if (x - ball_x) ** 2 + (y - ball_y) ** 2 <= (d // 2) ** 2:
                count += 1
        answers.append(count)
    return answers

def write_output(p, n1, n2):
    if p == 1:
        sys.stdout.write(str(n1) + "\n")
    else:
        for answer in n2:
            sys.stdout.write(str(answer) + "\n")

if __name__ == "__main__":
    p, n, k, d, balls, m, queries = read_input()
    if p == 1:
        n1 = solve_part1(n, d)
        write_output(p, n1, None)
    else:
        n2 = solve_part2(n, d, balls, queries)
        write_output(p, None, n2)
