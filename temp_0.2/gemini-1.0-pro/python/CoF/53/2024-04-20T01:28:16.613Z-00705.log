import sys
from math import sqrt

def dist(p1, p2):
    return sqrt((p1[0] - p2[0]) ** 2 + (p1[1] - p2[1]) ** 2)

def area(points):
    n = len(points)
    area = 0
    for i in range(n):
        area += points[i][0] * points[(i + 1) % n][1] - points[i][1] * points[(i + 1) % n][0]
    return abs(area) / 2

def main():
    with open('mosia.in') as f:
        n = int(f.readline())
        points = []
        for _ in range(n):
            x, y, d = map(int, f.readline().split())
            points.append((x, y, d))

    max_area = 0
    for i in range(n):
        for j in range(n):
            if i == j:
                continue
            x1, y1, d1 = points[i]
            x2, y2, d2 = points[j]
            dx = x2 - x1
            dy = y2 - y1
            if dx == 0:
                if dy > 0:
                    y1 += min(d1, dy)
                else:
                    y1 -= min(d1, -dy)
            elif dy == 0:
                if dx > 0:
                    x1 += min(d1, dx)
                else:
                    x1 -= min(d1, -dx)
            else:
                k = dy / dx
                if dx > 0:
                    if k > 0:
                        x1 += min(d1, dx)
                        y1 += min(d1, dy)
                    else:
                        x1 += min(d1, dx)
                        y1 -= min(d1, -dy)
                else:
                    if k > 0:
                        x1 -= min(d1, -dx)
                        y1 += min(d1, dy)
                    else:
                        x1 -= min(d1, -dx)
                        y1 -= min(d1, -dy)
            points[i] = (x1, y1, d1)
            max_area = max(max_area, area(points))

    with open('mosia.out', 'w') as f:
        f.write(f'{max_area:.4f}')

if __name__ == '__main__':
    main()
```
