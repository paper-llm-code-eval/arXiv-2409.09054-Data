import sys

# Read input
n = int(sys.stdin.readline())
points = []
for _ in range(n):
    x, y, d = map(int, sys.stdin.readline().split())
    points.append((x, y, d))

# Calculate the convex hull of the initial points
convex_hull = []
for point in points:
    while len(convex_hull) >= 2 and (
        (convex_hull[-1][0] - convex_hull[-2][0]) * (point[1] - convex_hull[-2][1])
        - (convex_hull[-1][1] - convex_hull[-2][1]) * (point[0] - convex_hull[-2][0])
    ) < 0:
        convex_hull.pop()
    convex_hull.append(point)

# Calculate the area of the convex hull
area = 0
for i in range(len(convex_hull)):
    area += convex_hull[i][0] * convex_hull[(i + 1) % len(convex_hull)][1]
    area -= convex_hull[i][1] * convex_hull[(i + 1) % len(convex_hull)][0]
area = abs(area) / 2

# Calculate the maximum area that can be added to the convex hull by moving each point
max_area = 0
for point in points:
    for i in range(len(convex_hull)):
        # Calculate the area that can be added by moving the point to the line segment between convex_hull[i] and convex_hull[i+1]
        x1, y1 = convex_hull[i]
        x2, y2 = convex_hull[(i + 1) % len(convex_hull)]
        dx = x2 - x1
        dy = y2 - y1
        t = ((point[0] - x1) * dx + (point[1] - y1) * dy) / (dx * dx + dy * dy)
        if t >= 0 and t <= 1:
            x3 = x1 + t * dx
            y3 = y1 + t * dy
            area_added = abs((x3 - point[0]) * (y2 - y1) - (x2 - x1) * (y3 - point[1])) / 2
            max_area = max(max_area, area_added)

# Output the maximum area
print(f"{max_area:.4f}")
