import sys

def check_groups(n, k, gender, groups):
    # Check if all students are in a group
    students_in_groups = set()
    for group in groups:
        students_in_groups.update(group)
    if len(students_in_groups) != n:
        return False

    # Check if the number of students in each group differs by at most 1
    group_sizes = [len(group) for group in groups]
    if max(group_sizes) - min(group_sizes) > 1:
        return False

    # Check if the number of boys and girls in each group differs by at most 1
    for group in groups:
        num_boys = sum(gender[student - 1] == 'b' for student in group)
        num_girls = sum(gender[student - 1] == 'f' for student in group)
        if abs(num_boys - num_girls) > 1:
            return False

    return True

def main():
    with open('grupe.in', 'r') as f_in, open('grupe.out', 'w') as f_out:
        n, k = map(int, f_in.readline().split())
        gender = f_in.readline()
        groups = []
        for _ in range(k):
            group_size, *group = map(int, f_in.readline().split())
            groups.append(group)

        if check_groups(n, k, gender, groups):
            f_out.write('DA\n')
        else:
            f_out.write('NU\n')

        for group in groups:
            num_boys = sum(gender[student - 1] == 'b' for student in group)
            num_girls = sum(gender[student - 1] == 'f' for student in group)
            f_out.write(f'{num_boys} {num_girls}\n')

if __name__ == '__main__':
    main()
