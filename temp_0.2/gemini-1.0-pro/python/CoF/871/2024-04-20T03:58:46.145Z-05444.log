                coada.append((x, y - 1))
            if teren[x][y + 1] == 0:
                coada.append((x, y + 1))
            # Check if the mole has reached the starting point
            if (x, y) == (0, pozitie):
                # If the mole has reached the starting point, stop digging and close the gallery
                break
    max_celule_sapate = max(max_celule_sapate, celule_sapate)
    max_celule_galerie = max(max_celule_galerie, celule_galerie)

if cerinta == 1:
    with open('galerie.out', 'w') as f:
        f.write(str(max_celule_sapate))
else:
    with open('galerie.out', 'w') as f:
        f.write(str(max_celule_galerie))
