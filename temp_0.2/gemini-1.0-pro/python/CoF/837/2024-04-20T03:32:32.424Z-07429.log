import sys

def main():
    with open('puncte.in', 'r') as fin, open('puncte.out', 'w') as fout:
        n = int(fin.readline())
        points = []
        for _ in range(n):
            x, y = map(int, fin.readline().split())
            points.append((x, y))

        # Punctul a)
        max_points = 0
        for x in range(-100, 101):
            count = 0
            for point in points:
                if point[0] == x or point[1] == x:
                    count += 1
            max_points = max(max_points, count)

        # Punctul b)
        lines = set()
        for point1 in points:
            for point2 in points:
                if point1 != point2:
                    if point1[0] == point2[0] or point1[1] == point2[1] or point1[0] + point1[1] == point2[0] + point2[1]:
                        lines.add((point1, point2))

        # Punctul c)
        points.sort(key=lambda point: (point[0], point[1]))
        while len(points) > 1:
            points = points[3:]

        fout.write(f'{max_points}\n')
        fout.write(f'{len(lines)}\n')
        fout.write(f'{points[0][0]}\n')

if __name__ == '__main__':
    main()
