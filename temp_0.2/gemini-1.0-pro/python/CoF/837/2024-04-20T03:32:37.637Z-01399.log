import sys

# Read input data
N = int(sys.stdin.readline())
points = []
for _ in range(N):
    x, y = map(int, sys.stdin.readline().split())
    points.append((x, y))

# Find the maximum number of points on a line
max_points = 0
for i in range(N):
    x1, y1 = points[i]
    cnt = 1
    for j in range(i+1, N):
        x2, y2 = points[j]
        if x1 == x2 or y1 == y2 or x1 + y1 == x2 + y2 or x1 - y1 == x2 - y2:
            cnt += 1
    max_points = max(max_points, cnt)

# Find the number of lines passing through at least two points
lines = set()
for i in range(N):
    x1, y1 = points[i]
    for j in range(i+1, N):
        x2, y2 = points[j]
        if x1 == x2 or y1 == y2 or x1 + y1 == x2 + y2 or x1 - y1 == x2 - y2:
            lines.add((x1, y1, x2, y2))

# Find the x-coordinate of the last point
last_point = None
cnt = 0
for i in range(N):
    x, y = points[i]
    if last_point is None or x > last_point:
        cnt += 1
        if cnt % 3 == 0:
            last_point = x

# Write output data
sys.stdout.write(str(max_points) + '\n')
sys.stdout.write(str(len(lines)) + '\n')
sys.stdout.write(str(last_point) + '\n')
```
