import sys
import heapq

with open('startrek.in', 'r') as f:
    n, t = map(int, f.readline().split())
    d = list(map(int, f.readline().split()))
    v = list(map(int, f.readline().split()))

d_v = [(d[i] / v[i], i) for i in range(n)]
heapq.heapify(d_v)

ans = 0
time = 0
while d_v:
    dist, i = heapq.heappop(d_v)
    if time + dist <= t:
        ans += 1
        time += dist + t
    else:
        break

with open('startrek.out', 'w') as f:
    f.write(str(ans))
```
