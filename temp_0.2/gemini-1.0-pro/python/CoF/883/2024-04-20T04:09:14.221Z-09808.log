import sys

with open('cursuri.in') as f:
    C = int(f.readline())
    N, K = map(int, f.readline().split())
    intervals = [list(map(int, f.readline().split())) for _ in range(N)]

intervals.sort(key=lambda x: x[1])

if C == 1:
    ans = 0
    used = [False] * K
    for a, b in intervals:
        for i in range(K):
            if not used[i] and a >= intervals[i][1]:
                used[i] = True
                ans += 1
                break
    with open('cursuri.out', 'w') as f:
        f.write(str(ans))

elif C == 2:
    ans = intervals[-1][1] - intervals[0][0]
    while True:
        used = [False] * K
        for a, b in intervals:
            for i in range(K):
                if not used[i] and a + ans <= intervals[i][1]:
                    used[i] = True
                    break
        if all(used):
            break
        ans -= 1
    with open('cursuri.out', 'w') as f:
        f.write(str(ans))
```
