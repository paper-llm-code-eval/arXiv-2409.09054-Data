import sys

def main():
    with open('elfi.in', 'r') as f_in, open('elfi.out', 'w') as f_out:
        n = int(f_in.readline())
        times = [list(map(int, f_in.readline().split())) for _ in range(n)]
        times.sort(key=lambda x: x[0] + x[1])
        max_time = times[-1][0] + times[-1][1]
        min_taps = 0
        cur_time = 0
        for time in times:
            if cur_time <= time[0]:
                min_taps += 1
                cur_time = time[0] + time[1]
            else:
                cur_time = min(cur_time, time[0] + time[1])
        f_out.write(str(min_taps))

if __name__ == '__main__':
    main()
```
