                        # Check if the line actually passes through all 3 circles
                        if is_on_line(x1, y1, x2, y2, x3, y3):
                            p += 1

def is_on_line(x1, y1, x2, y2, x3, y3):
    """
    Check if the point (x3, y3) is on the line defined by the points (x1, y1) and (x2, y2).
    """
    return abs((x2 - x1) * (y3 - y1) - (x3 - x1) * (y2 - y1)) < 1e-9

g.write(f"{m} {q} {p}")
g.close()
