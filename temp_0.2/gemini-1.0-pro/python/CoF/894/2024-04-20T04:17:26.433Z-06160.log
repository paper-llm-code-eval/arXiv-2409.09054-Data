def is_palindrome(n):
    return str(n) == str(n)[::-1]

def get_reversed(n):
    rev = 0
    while n > 0:
        rev = rev * 10 + n % 10
        n //= 10
    return rev

def get_n_th_removed(n):
    removed = 0
    i = 1
    while removed < n:
        if i % 10 == 0 or not is_palindrome(i):
            removed += 1
            i = get_reversed(i)
        i += 1
    return i - 1

def get_n_th_occurrences(x, n):
    occurrences = [0, 0, 0]
    while x > 0:
        if x % 10 == 0:
            occurrences[0] += 1
        elif not is_palindrome(x):
            occurrences[1] += 1
        else:
            occurrences[2] += 1
        x //= 10
        n -= 1
        if n == 0:
            break
    return occurrences

def get_n_k_digit_numbers(k):
    count = 0
    i = 1
    while i <= 2000000000:
        if len(str(i)) == k:
            count += 1
        i = get_reversed(i)
        i += 1
    return count

with open('numere.in', 'r') as f:
    c = int(f.readline())
    if c == 1:
        n = int(f.readline())
        result = get_n_th_removed(n)
        with open('numere.out', 'w') as out:
            out.write(str(result))
    elif c == 2:
        x = int(f.readline())
        occurrences = get_n_th_occurrences(x, 3)
        with open('numere.out', 'w') as out:
            out.write(' '.join(map(str, occurrences)))
    elif c == 3:
        k = int(f.readline())
        result = get_n_k_digit_numbers(k)
        with open('numere.out', 'w') as out:
            out.write(str(result))
```
