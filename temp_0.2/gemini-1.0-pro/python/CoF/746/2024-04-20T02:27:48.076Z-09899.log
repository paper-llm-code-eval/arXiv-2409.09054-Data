import sys

def main():
    with open('scara.in') as f:
        h, n, m, p = map(int, f.readline().split())
    
    dp = [[float('inf')] * (m + 1) for _ in range(n + 1)]
    dp[0][0] = 0
    
    for i in range(1, n + 1):
        for j in range(1, m + 1):
            for k in range(1, j + 1):
                if i - k >= 0:
                    dp[i][j] = min(dp[i][j], dp[i - k][j - k] + (h * k + p * (k > 1)) / k)
    
    effort = dp[n][m]
    
    with open('scara.out', 'w') as f:
        f.write(f'{effort:.2f}\n')
        
        i = n
        j = m
        stairs = []
        while i > 0 and j > 0:
            for k in range(j, 0, -1):
                if i - k >= 0 and dp[i][j] == dp[i - k][j - k] + (h * k + p * (k > 1)) / k:
                    stairs.append(h * k // k)
                    i -= k
                    j -= k
                    break
        
        f.write(' '.join(map(str, stairs)))

if __name__ == '__main__':
    main()
```
