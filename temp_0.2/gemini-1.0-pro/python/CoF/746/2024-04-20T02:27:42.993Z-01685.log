            effort = (height + next_height) / 2 + p

            # Check if this is the step that leads to the minimum effort
            if effort + min_effort(i + 1, j - 1, next_height, steps) == min_effort:
                heights.append(next_height)
                i += 1
                j -= 1
                height = next_height
                steps -= next_height
                break

    # Write the output to the file
    with open('scara.out', 'w') as f:
        f.write(f'{min_effort:.2f}\n')
        f.write(' '.join(map(str, heights)))

if __name__ == '__main__':
    main()
