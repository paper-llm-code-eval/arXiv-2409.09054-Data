import sys

def main():
    with open('scara.in', 'r') as f:
        h, n, m, p = map(int, f.readline().split())

    # Initialize the memoization table
    memo = [[[[-1] * (m + 1) for _ in range(n + 1)] for _ in range(h + 1)] for _ in range(n + 1)]

    # Define the recursive function to calculate the minimum effort
    def min_effort(i, j, height, steps):
        # Base case: we have reached the top of the stairs
        if i == n:
            return 0

        # Base case: we have used all the steps
        if j == 0:
            return sys.maxsize

        # Check if the subproblem has already been solved
        if memo[i][j][height][steps] != -1:
            return memo[i][j][height][steps]

        # Try all possible heights for the next step
        min_effort_so_far = sys.maxsize
        for next_height in range(1, m + 1):
            # Calculate the effort for the next step
            effort = (height + next_height) / 2 + p

            # Recursively calculate the minimum effort for the remaining steps
            min_effort_so_far = min(min_effort_so_far, effort + min_effort(i + 1, j - 1, next_height, steps))

        # Store the minimum effort for the subproblem
        memo[i][j][height][steps] = min_effort_so_far

        return min_effort_so_far

    # Find the minimum effort and the corresponding sequence of heights
    min_effort = min_effort(0, n, 0, h)
    heights = []

    i = 0
    j = n
    height = 0
    steps = h
    while i != n:
        # Try all possible heights for the next step
        for next_height in range(1, m + 1):
            # Calculate the effort for the next step
            effort = (height + next_height) / 2 + p

            # Check if this is the step that leads to the minimum effort
            if effort + min_effort(i + 1, j - 1, next_height, steps) == min_effort:
                heights.append(next_height)
                i += 1
                j -= 1
                height = next_height
                steps -= next_height
                break

    # Write the output to the file
    with open('scara.out', 'w') as f:
        f.write(f'{min_effort:.2f}\n')
        f.write(' '.join(map(str, heights)))

if __name__ == '__main__':
    main()
