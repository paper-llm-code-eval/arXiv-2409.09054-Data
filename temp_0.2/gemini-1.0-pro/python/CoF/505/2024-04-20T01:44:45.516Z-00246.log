import sys

mod = 10 ** 9 + 9

def main():
    with open('fotbal.in') as fin, open('fotbal.out', 'w') as fout:
        n, k = map(int, fin.readline().split())
        players = []
        for _ in range(n):
            start, end, foot = map(int, fin.readline().split())
            players.append((start, end, foot))
        players.sort()

        # dp[i][j][l][r] = numarul de moduri de a alege j jucatori din primii i jucatori,
        # astfel incat sa fie l jucatori stangaci si r jucatori dreptaci
        dp = [[[[0 for _ in range(k + 1)] for _ in range(k + 1)] for _ in range(n + 1)] for _ in range(2)]

        dp[0][0][0][0] = 1
        for i in range(n):
            for j in range(k + 1):
                for l in range(k + 1):
                    for r in range(k + 1):
                        # nu alegem jucatorul curent
                        dp[i + 1][j][l][r] = dp[i][j][l][r]

                        # alegem jucatorul curent
                        if j < k and (l > 0 or players[i][2] == 1) and (r > 0 or players[i][2] == 0):
                            if players[i][2] == 0:
                                dp[i + 1][j + 1][l + 1][r] = (dp[i + 1][j + 1][l + 1][r] + dp[i][j][l][r]) % mod
                            else:
                                dp[i + 1][j + 1][l][r + 1] = (dp[i + 1][j + 1][l][r + 1] + dp[i][j][l][r]) % mod

        fout.write(str(dp[n][k][k][k]))

if __name__ == '__main__':
    main()
