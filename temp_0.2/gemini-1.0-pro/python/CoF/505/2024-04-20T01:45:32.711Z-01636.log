import sys

mod = 10**9 + 9

def add(a, b):
    return (a + b) % mod

def mul(a, b):
    return (a * b) % mod

def binpow(a, b):
    if b == 0:
        return 1
    if b == 1:
        return a
    if b % 2 == 0:
        return binpow(mul(a, a), b // 2)
    return mul(binpow(a, b - 1), a)

n, k = map(int, sys.stdin.readline().split())
intervals = []
for _ in range(n):
    start, end, foot = map(int, sys.stdin.readline().split())
    intervals.append((start, end, foot))

intervals.sort(key=lambda x: x[1])

left = 0
right = 0
l_count = 0
r_count = 0
ans = 0

for start, end, foot in intervals:
    if foot == 0:
        l_count += 1
    else:
        r_count += 1
    while l_count >= k or r_count >= k:
        if intervals[left][2] == 0:
            l_count -= 1
        else:
            r_count -= 1
        left += 1
    if l_count + r_count >= k:
        ans = add(ans, mul(binpow(2, l_count), binpow(2, r_count)))

print(ans)
```
