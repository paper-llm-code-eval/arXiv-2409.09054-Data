import sys

def read_input():
    n = int(sys.stdin.readline())
    costs = [int(x) for x in sys.stdin.readline().split()]
    return n, costs

def solve(n, costs):
    # dp[i][j] = minimum cost to reach level i, ending in room j
    dp = [[float('inf') for _ in range(n+1)] for _ in range(n+1)]
    dp[1][1] = costs[0]
    
    # Iterate over levels
    for i in range(2, n+1):
        # Iterate over rooms in the level
        for j in range(1, i+1):
            # Try all possible previous rooms
            for k in range(1, j+1):
                # Check if the previous room is valid
                if i-k >= 1 and j-k >= 1:
                    dp[i][j] = min(dp[i][j], dp[i-k][j-k] + costs[i-1])
    
    # Find the minimum cost to reach the last level
    min_cost = float('inf')
    for i in range(1, n+1):
        min_cost = min(min_cost, dp[n][i])
    
    # Reconstruct the path
    path = []
    i = n
    j = 1
    while i > 1:
        # Find the previous room that minimizes the cost
        min_cost = float('inf')
        for k in range(1, j+1):
            if i-k >= 1 and j-k >= 1 and dp[i][j] == dp[i-k][j-k] + costs[i-1]:
                min_cost = dp[i-k][j-k]
                prev_i = i-k
                prev_j = j-k
        
        # Add the previous room to the path
        path.append(i)
        
        # Update i and j
        i = prev_i
        j = prev_j
    
    # Reverse the path
    path.reverse()
    
    return min_cost, path

def write_output(min_cost, path):
    sys.stdout.write(f'{len(path)} {min_cost}\n')
    sys.stdout.write(' '.join(map(str, path)))

if __name__ == '__main__':
    n, costs = read_input()
    min_cost, path = solve(n, costs)
    write_output(min_cost, path)
