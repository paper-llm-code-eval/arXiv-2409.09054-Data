elif V == 2:
    mine.sort()
    min_move = -1
    for i in range(M):
        for j in range(i + 1, M):
            x1, y1 = mine[i]
            x2, y2 = mine[j]
            if x1 == x2 or y1 == y2:
                move = max(abs(x1 - x2), abs(y1 - y2)) - 1
                if min_move == -1 or move < min_move:
                    min_move = move
    with open('deminare.out', 'w') as f:
        f.write(str(min_move))
