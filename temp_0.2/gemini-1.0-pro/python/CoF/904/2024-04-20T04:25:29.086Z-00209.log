with open('deminare.in', 'r') as f:
    V = int(f.readline())
    L, C = map(int, f.readline().split())
    M = int(f.readline())
    mine = [tuple(map(int, f.readline().split())) for _ in range(M)]

if V == 1:
    max_mine = 0
    max_line = []
    for i in range(1, L + 1):
        cnt = 0
        for x, y in mine:
            if x == i:
                cnt += 1
        if cnt > max_mine:
            max_mine = cnt
            max_line = [i]
        elif cnt == max_mine:
            max_line.append(i)
    with open('deminare.out', 'w') as f:
