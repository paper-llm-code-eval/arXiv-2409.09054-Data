with open('ciocolata.in', 'r') as f:
    c, n = map(int, f.readline().split())
    lst = list(map(int, f.readline().split()))

if c == 1:
    cnt = {}
    for x in lst:
        if x not in cnt:
            cnt[x] = 0
        cnt[x] += 1
    mx = 0
    ans = 1000000000
    for x in cnt:
        if cnt[x] > mx:
            mx = cnt[x]
            ans = x
        elif cnt[x] == mx:
            ans = min(ans, x)
    with open('ciocolata.out', 'w') as f:
        f.write(str(ans))

else:
    lst.sort()
    sum1 = 0
    sum2 = 0
    ans = 1000000000
    for i in range(n):
        if sum1 <= sum2:
            sum1 += lst[i]
        else:
            sum2 += lst[n - i - 1]
        ans = min(ans, abs(sum1 - sum2))
    with open('ciocolata.out', 'w') as f:
        f.write(str(ans))
