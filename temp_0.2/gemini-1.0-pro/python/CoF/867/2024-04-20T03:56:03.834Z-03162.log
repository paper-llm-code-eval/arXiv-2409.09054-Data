import sys

def is_palindrome(n):
    s = str(n)
    return s == s[::-1]

def is_almost_palindrome(n):
    s = str(n)
    return sum(1 for i in range(len(s) // 2) if s[i] != s[len(s) - i - 1]) <= 1

def next_palindrome(n):
    s = str(n)
    i = (len(s) - 1) // 2
    while i >= 0 and s[i] == s[len(s) - i - 1]:
        i -= 1
    if i < 0:
        s = '1' + '0' * (len(s) - 1) + '1'
    else:
        s = s[:i] + str(int(s[i]) + 1) + s[i + 1:len(s) - i - 1] + str(int(s[i]) + 1) + s[len(s) - i:]
    return int(s)

with open('palindrom.in', 'r') as f_in, open('palindrom.out', 'w') as f_out:
    C = int(f_in.readline())
    n = int(f_in.readline())
    numbers = list(map(int, f_in.readline().split()))
    if C == 1:
        f_out.write(str(sum(is_palindrome(n) for n in numbers)))
    elif C == 2:
        f_out.write(str(sum(is_almost_palindrome(n) for n in numbers)))
    elif C == 3:
        f_out.write(' '.join(map(str, [next_palindrome(n) for n in numbers])))
```
