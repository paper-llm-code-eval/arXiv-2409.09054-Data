import sys

def is_palindrome(n):
    s = str(n)
    return s == s[::-1]

def is_almost_palindrome(n):
    s = str(n)
    return sum(c1 != c2 for c1, c2 in zip(s, s[::-1])) == 1

def associated_palindrome(n):
    s = str(n)
    i = len(s) // 2
    while i >= 0 and s[i] == s[~i]:
        i -= 1
    if i < 0:
        return int(s[::2] + s[1::2][::-1])
    s = s[:i] + str(int(s[i]) + 1) + s[i+1:]
    return int(s[::2] + s[1::2][::-1])

with open('palindrom.in') as f_in, open('palindrom.out', 'w') as f_out:
    c = int(f_in.readline())
    n = int(f_in.readline())
    numbers = list(map(int, f_in.readline().split()))
    if c == 1:
        f_out.write(str(sum(is_palindrome(n) for n in numbers)))
    elif c == 2:
        f_out.write(str(sum(is_almost_palindrome(n) for n in numbers)))
    else:
        f_out.write(' '.join(map(str, (associated_palindrome(n) for n in numbers))))
```
