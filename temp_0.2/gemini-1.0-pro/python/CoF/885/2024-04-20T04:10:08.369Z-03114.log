import sys

def read_input():
    p = int(sys.stdin.readline())
    if p == 1:
        n, k = map(int, sys.stdin.readline().split())
        operations = [sys.stdin.readline().split() for _ in range(k)]
    else:
        n, z = map(int, sys.stdin.readline().split())
    return p, n, k, operations, z

def solve_part1(n, k, operations):
    grid = [[1 for _ in range(n)] for _ in range(n)]
    for op, nr in operations:
        if op == 'L':
            for i in range(n):
                grid[nr - 1][i] *= -1
        else:
            for i in range(n):
                grid[i][nr - 1] *= -1
    return sum(sum(row) for row in grid)

def solve_part2(n, z):
    if z % 2 == 1:
        return 0
    z //= 2
    if z > n * n // 2:
        return 0
    return z

def write_output(result):
    sys.stdout.write(str(result) + '\n')

if __name__ == '__main__':
    p, n, k, operations, z = read_input()
    if p == 1:
        result = solve_part1(n, k, operations)
    else:
        result = solve_part2(n, z)
    write_output(result)
