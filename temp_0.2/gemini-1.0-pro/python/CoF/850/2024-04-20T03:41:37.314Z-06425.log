with open('ferma.in', 'r') as fin, open('ferma.out', 'w') as fout:
    v = int(fin.readline())
    m, n = map(int, fin.readline().split())
    grid = [list(fin.readline().strip()) for _ in range(m)]
    
    def dfs(x, y, color):
        if x < 0 or x >= m or y < 0 or y >= n or grid[x][y] != color:
            return 0
        grid[x][y] = '.'
        return 1 + dfs(x + 1, y, color) + dfs(x - 1, y, color) + dfs(x, y + 1, color) + dfs(x, y - 1, color)
    
    max_area = 0
    if v == 1:
        for i in range(m):
            for j in range(n):
                max_area = max(max_area, dfs(i, j, grid[i][j]))
        fout.write(str(max_area))
    else:
        max_area = 0
        for i in range(m):
            for j in range(n):
                for color in 'abcdefghijklmnopqrstuvwxyz':
                    if color != grid[i][j]:
                        grid[i][j] = color
                        area = dfs(i, j, color)
                        if area > max_area:
                            max_area = area
                            x, y, new_color = i + 1, j + 1, color
                        grid[i][j] = '.'
        fout.write(f'{x} {y}\n{new_color}')
