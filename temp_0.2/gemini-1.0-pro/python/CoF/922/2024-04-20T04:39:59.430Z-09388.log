with open("furnica.in") as f:
    c, n = map(int, f.readline().split())
    if c == 1 or c == 2:
        h = [list(map(int, f.readline().split())) for _ in range(n)]
    else:
        n, t = map(int, f.readline().split())
        h = [list(map(int, f.readline().split())) for _ in range(n)]
    
    if c == 1:
        t = 0
        for i in range(n):
            if i % 2 == 0:
                t += h[i][0] * 3
            else:
                t += h[i][1] * 5
                t += h[i][0] * 3
        t += h[n - 1][1] * 2
        with open("furnica.out", "w") as g:
            g.write(str(t))
    elif c == 2:
        mx = 0
        for i in range(n):
            if i % 2 == 0:
                mx = max(mx, h[i][0])
            else:
                mx = max(mx, h[i][1])
        with open("furnica.out", "w") as g:
            g.write(str(mx))
    else:
        i = 0
        while t > 0:
            if i % 2 == 0:
                if t >= h[i][0] * 3:
                    t -= h[i][0] * 3
                    i += 1
                else:
                    break
            else:
                if t >= h[i][1] * 5:
                    t -= h[i][1] * 5
                    i += 1
                else:
                    break
        with open("furnica.out", "w") as g:
            g.write(str(i + 1))
