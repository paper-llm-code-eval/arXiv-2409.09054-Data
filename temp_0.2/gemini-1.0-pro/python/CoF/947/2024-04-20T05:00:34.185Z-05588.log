import sys

with open("strips.in", "r") as f:
    c = int(f.readline())
    n, nr, l = map(int, f.readline().split())
    positions = [int(line) for line in f]

# Initialize the game state
red_zones = []
green_zones = []
red_penalties = 0
green_penalties = 0
red_bands = nr
green_bands = nr

# Process the positions
for position in positions:
    # Check if the position is valid for the current player
    if position + l <= n:
        # Check if the position overlaps with any existing zones
        overlaps_red = False
        overlaps_green = False
        for zone in red_zones:
            if zone[0] <= position < zone[1]:
                overlaps_red = True
                break
        for zone in green_zones:
            if zone[0] <= position < zone[1]:
                overlaps_green = True
                break

        # If the position is valid, place a band and update the game state
        if not overlaps_red:
            red_zones.append([position, position + l])
            red_bands -= 1
        else:
            red_penalties += 1

        if not overlaps_green:
            green_zones.append([position, position + l])
            green_bands -= 1
        else:
            green_penalties += 1

    # If the position is invalid, penalize the current player
    else:
        if c == 1:
            if red_bands > 0:
                red_penalties += 1
                red_bands -= 1
            else:
                green_penalties += 1
                green_bands -= 1
        else:
            break

# Calculate the maximum zone lengths
max_red_zone = 0
max_green_zone = 0
for zone in red_zones:
    max_red_zone = max(max_red_zone, zone[1] - zone[0])
for zone in green_zones:
    max_green_zone = max(max_green_zone, zone[1] - zone[0])

# Write the output
with open("strips.out", "w") as f:
    if c == 1:
        f.write(f"{red_penalties} {green_penalties}")
    else:
        f.write(f"{max_red_zone} {max_green_zone}")
```
