        if debt[1] not in firms:
            firms[debt[1]] = [0, 0]
        firms[debt[0]][0] += debt[2]
        firms[debt[1]][1] += debt[2]
    return firms

def write_output(c, result):
    with open('datorii.out', 'w') as f:
        if c == 1:
            f.write(str(result))
        else:
            for firm, (sd, sp) in sorted(result.items()):
                f.write(f'{firm} {sd} {sp}\n')

if __name__ == '__main__':
    c, debts = read_input()
    if c == 1:
        result = count_distinct_firms(debts)
    else:
        result = calculate_financial_situation(debts)
    write_output(c, result)
