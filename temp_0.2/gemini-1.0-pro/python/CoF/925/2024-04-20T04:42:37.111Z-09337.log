import sys

def read_input():
    with open('datorii.in', 'r') as f:
        c = int(f.readline())
        d = int(f.readline())
        debts = []
        for _ in range(d):
            debt = f.readline().strip().split(' > ')
            debts.append((debt[0], debt[1], int(debt[2])))
    return c, debts

def count_distinct_firms(debts):
    firms = set()
    for debt in debts:
        firms.add(debt[0])
        firms.add(debt[1])
    return len(firms)

def calculate_financial_situation(debts):
