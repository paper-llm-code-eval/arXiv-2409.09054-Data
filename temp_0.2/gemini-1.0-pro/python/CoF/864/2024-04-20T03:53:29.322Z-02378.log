with open('cifre.in') as f:
    v = int(f.readline())
    n = int(f.readline())

if v == 1:
    segmente = 0
    while n > 0:
        if n % 10 in [0, 2, 3, 5, 6, 7, 8, 9]:
            segmente += 7
        elif n % 10 == 4:
            segmente += 4
        elif n % 10 == 1:
            segmente += 2
        n //= 10
    with open('cifre.out', 'w') as f:
        f.write(str(segmente))
else:
