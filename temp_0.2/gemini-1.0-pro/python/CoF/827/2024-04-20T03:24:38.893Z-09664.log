with open('compresie.in') as f:
    s = f.readline().strip()

n = int(s[1])
nr = 0
m = [[' ' for _ in range(n)] for _ in range(n)]
i, j = 0, 0
for c in s[2:]:
    if c == '*':
        nr += 1
        i, j = 0, 0
    elif c.isdigit():
        for _ in range(int(c)):
            m[i][j] = s[2]
            j += 1
            if j == n:
                i += 1
                j = 0
    else:
        m[i][j] = c
        j += 1
        if j == n:
            i += 1
            j = 0

with open('compresie.out', 'w') as f:
    f.write(f'{nr}\n')
    for row in m:
        f.write(''.join(row) + '\n')
```
