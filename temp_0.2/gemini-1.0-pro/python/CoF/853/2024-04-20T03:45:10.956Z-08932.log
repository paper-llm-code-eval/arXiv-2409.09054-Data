import sys

def main():
    with open('defrag.in', 'r') as f:
        V = int(f.readline())
        P, S = map(int, f.readline().split())
        C = int(f.readline())
        clusters = [[0] * (S + 1) for _ in range(P + 1)]
        for _ in range(C):
            p, s = map(int, f.readline().split())
            clusters[p][s] = 1

    if V == 1:
        free_tracks = 0
        for p in range(1, P + 1):
            if all(clusters[p][s] == 0 for s in range(1, S + 1)):
                free_tracks += 1
        with open('defrag.out', 'w') as f:
            f.write(str(free_tracks))
    else:
        moves = [0] * (P + 1)
        for p in range(1, P + 1):
            occupied = [s for s in range(1, S + 1) if clusters[p][s] == 1]
            if not occupied:
                continue
            start = occupied[0]
            end = occupied[-1]
            moves[p] = min(start - 1, S - end)
            for s in range(start + 1, end + 1):
                if clusters[p][s] == 0:
                    moves[p] += 1
        with open('defrag.out', 'w') as f:
            f.write(' '.join(map(str, moves[1:])))

if __name__ == '__main__':
    main()
