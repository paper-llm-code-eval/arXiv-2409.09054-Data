import sys

def is_cool(a, k):
    if len(set(a)) != k:
        return False
    a.sort()
    for i in range(1, k):
        if a[i] - a[i - 1] != 1:
            return False
    return True

def main():
    with open('cool.in', 'r') as f_in, open('cool.out', 'w') as f_out:
        p = int(f_in.readline())
        n, k = map(int, f_in.readline().split())
        a = list(map(int, f_in.readline().split()))
        if p == 1:
            if is_cool(a[:k], k):
                f_out.write(str(max(a[:k])) + '\n')
            else:
                f_out.write(str(len(set(a[:k]))) + '\n')
        elif p == 2:
            max_len = 0
            cnt = 0
            for i in range(n - k + 1):
                if is_cool(a[i:i + k], k):
                    max_len = max(max_len, k)
                    cnt += 1
            f_out.write(str(max_len) + '\n')
            f_out.write(str(cnt) + '\n')

if __name__ == '__main__':
    main()
```
