import sys

def is_cool(arr, k):
    if len(set(arr[:k])) != k:
        return False
    arr.sort()
    for i in range(1, k):
        if arr[i] != arr[i - 1] + 1:
            return False
    return True

def main():
    with open('cool.in', 'r') as f:
        p = int(f.readline())
        n, k = map(int, f.readline().split())
        arr = list(map(int, f.readline().split()))

    if p == 1:
        if is_cool(arr, k):
            print(max(arr[:k]))
        else:
            print(len(set(arr[:k])))
    elif p == 2:
        max_len = 0
        cnt = 0
        for i in range(n - k + 1):
            if is_cool(arr[i:i + k], k):
                max_len = k
                cnt += 1
        print(max_len)
        print(cnt)

if __name__ == '__main__':
    main()
```
