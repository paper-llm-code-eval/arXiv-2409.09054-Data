import sys
import heapq

def dijkstra(graph, start, end):
    dist = [float('inf')] * (len(graph) + 1)
    dist[start] = 0
    pq = [(0, start)]
    while pq:
        d, u = heapq.heappop(pq)
        if u == end:
            return d
        for v, w in graph[u]:
            if d + w < dist[v]:
                dist[v] = d + w
                heapq.heappush(pq, (d + w, v))
    return -1

with open('ubuntzei.in', 'r') as f:
    n, m = map(int, f.readline().split())
    k, *friends = map(int, f.readline().split())
    graph = [[] for _ in range(n + 1)]
    for _ in range(m):
        a, b, w = map(int, f.readline().split())
        graph[a].append((b, w))
        graph[b].append((a, w))

    ans = float('inf')
    for friend in friends:
        ans = min(ans, dijkstra(graph, 1, friend) + dijkstra(graph, friend, n))

    with open('ubuntzei.out', 'w') as f:
        f.write(str(ans))
