import sys

def is_palindrome(n):
    return str(n) == str(n)[::-1]

def min_digits_to_palindrome(n):
    n = str(n)
    l = len(n)
    for i in range(l // 2):
        if n[i] != n[l - i - 1]:
            return l - i - 1
    return 0

def max_consecutive_palindromes(n, s):
    n = str(n)
    l = len(n)
    dp = [[0] * (s + 1) for _ in range(l + 1)]
    for i in range(l - 1, -1, -1):
        for j in range(s + 1):
            if is_palindrome(int(n[i:])):
                dp[i][j] = max(dp[i][j], dp[i + 1][j] + 1)
            if j > 0:
                dp[i][j] = max(dp[i][j], dp[i + 1][j - 1])
    return dp[0][s]

with open('palindrom.in', 'r') as f_in, open('palindrom.out', 'w') as f_out:
    c = int(f_in.readline())
    n = int(f_in.readline())
    a = [int(f_in.readline()) for _ in range(n)]
    if c == 1:
        ans = sum(min_digits_to_palindrome(x) for x in a)
        f_out.write(str(ans) + '\n')
    else:
        s = int(f_in.readline())
        ans = max(max_consecutive_palindromes(x, s) for x in a)
        f_out.write(str(ans) + '\n')
```
