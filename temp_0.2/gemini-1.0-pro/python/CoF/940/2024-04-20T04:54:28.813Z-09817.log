import sys

def main():
    with open('tort.in', 'r') as f_in, open('tort.out', 'w') as f_out:
        n = int(f_in.readline())
        a = list(map(int, f_in.readline().split()))
        a.sort()
        ans = 0
        pairs = set()
        for i in range(n):
            for j in range(i + 1, n):
                if a[j] - a[i] != a[j - 1]:
                    break
                pairs.add((i, j))
        ans = len(pairs)
        if a[n - 1] - a[0] == a[n - 2]:
