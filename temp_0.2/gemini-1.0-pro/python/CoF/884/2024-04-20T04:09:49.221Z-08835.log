        if all(board[i][j] == board[i][0] for j in range(N)):
            return board[i][0]
        if all(board[j][i] == board[0][i] for j in range(N)):
            return board[0][i]
    if all(board[i][i] == board[0][0] for i in range(N)):
        return board[0][0]
    if all(board[i][N - i - 1] == board[0][N - 1] for i in range(N)):
        return board[0][N - 1]
    if all(board[i][j] == 0 for i in range(N) for j in range(N)):
        return 0
    return -1

def solve_1(N, K, rounds):
    score = [0, 0]
    for round in rounds:
        board = [[0 for _ in range(N)] for _ in range(N)]
        player = 1
        for cell in round:
            i = (cell - 1) // N
            j = (cell - 1) % N
            board[i][j] = player
            winner = check_win(board, N)
            if winner != -1:
                score[player - 1] += 1
                break
            player = 3 - player
    return score

def solve_2(N, K, rounds):
    max_moves = 0
    for round in rounds:
        board = [[0 for _ in range(N)] for _ in range(N)]
        player = 1
        moves = 0
        for cell in round:
            moves += 1
            i = (cell - 1) // N
            j = (cell - 1) % N
            board[i][j] = player
            winner = check_win(board, N)
            if winner != -1:
                max_moves = max(max_moves, moves)
                break
            player = 3 - player
    return max_moves

def main():
    C, N, K, rounds = read_input()
    if C == 1:
        score = solve_1(N, K, rounds)
        sys.stdout.write(' '.join(map(str, score)))
    elif C == 2:
        max_moves = solve_2(N, K, rounds)
        sys.stdout.write(str(max_moves))

if __name__ == '__main__':
    main()
