            i = (cell - 1) // N
            j = (cell - 1) % N
            board[i][j] = player
            winner = check_win(board, N)
            if winner != -1:
                max_moves = max(max_moves, moves)
                break
            player = 3 - player
    return max_moves

def main():
    C, N, K, rounds = read_input()
    if C == 1:
        score = solve_1(N, K, rounds)
        with open('joc.out', 'w') as f:
            f.write(' '.join(map(str, score)))
    elif C == 2:
        max_moves = solve_2(N, K, rounds)
        with open('joc.out', 'w') as f:
            f.write(str(max_moves))

if __name__ == '__main__':
    main()
