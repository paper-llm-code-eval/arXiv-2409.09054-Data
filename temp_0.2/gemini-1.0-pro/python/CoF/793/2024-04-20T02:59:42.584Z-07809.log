import sys

with open('livada.in', 'r') as f:
    m, n, p = map(int, f.readline().split())
    grid = [list(map(int, f.readline().split())) for _ in range(m)]

# Count the number of rows with a majority element
row_counts = [0 for _ in range(m)]
for row in grid:
    counts = [0 for _ in range(p + 1)]
    for el in row:
        counts[el] += 1
    for i in range(1, p + 1):
        if counts[i] > n // 2:
            row_counts[row.index(i)] += 1

# Find the longest consecutive sequence of the same element
max_consecutive = 0
for row in grid:
    consecutive = 1
    for i in range(1, n):
        if row[i] == row[i - 1]:
            consecutive += 1
        else:
            max_consecutive = max(max_consecutive, consecutive)
            consecutive = 1
    max_consecutive = max(max_consecutive, consecutive)

# Write the output to the file
with open('livada.out', 'w') as f:
    f.write(str(sum(row_counts)) + '\n')
    f.write(str(max_consecutive))
