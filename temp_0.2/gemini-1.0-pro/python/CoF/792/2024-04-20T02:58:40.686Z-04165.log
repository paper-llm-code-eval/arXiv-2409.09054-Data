with open('reteta.in', 'r') as f_in, open('reteta.out', 'w') as f_out:
    reteta = f_in.readline().strip()
    timp_total = 0
    ingrediente = {}
    paranteze = 0
    ingrediente_curente = []
    timp_curent = 0

    for caracter in reteta:
        if caracter == '(':
            paranteze += 1
        elif caracter == ')':
            paranteze -= 1
            timp_total += timp_curent
            for ingredient in ingrediente_curente:
                if ingredient in ingrediente:
                    ingrediente[ingredient] += ingrediente_curente[ingredient]
                else:
                    ingrediente[ingredient] = ingrediente_curente[ingredient]
            ingrediente_curente = []
            timp_curent = 0
        elif caracter.isalpha():
            ingredient = ''
            while caracter.isalpha():
                ingredient += caracter
                caracter = reteta[reteta.index(caracter) + 1]
            ingrediente_curente.append(ingredient)
        elif caracter.isdigit():
            timp = ''
            while caracter.isdigit():
                timp += caracter
                caracter = reteta[reteta.index(caracter) + 1]
            timp_curent = int(timp)

    ingrediente_sortate = sorted(ingrediente.items())
    f_out.write(str(timp_total) + '\n')
    for ingredient, cantitate in ingrediente_sortate:
        f_out.write(ingredient + ' ' + str(cantitate) + '\n')
