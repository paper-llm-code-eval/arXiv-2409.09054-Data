        ingrediente[ingredient] = ingrediente.get(ingredient, 0) + int(cantitate)

with open('reteta.out', 'w') as f:
    f.write(str(timp_total) + '\n')
    for ingredient, cantitate in sorted(ingrediente.items()):
        f.write(f'{ingredient} {cantitate}\n')
