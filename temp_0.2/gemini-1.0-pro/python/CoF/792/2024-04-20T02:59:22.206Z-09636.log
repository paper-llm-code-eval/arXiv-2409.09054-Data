with open('reteta.in', 'r') as f:
    reteta = f.readline()

timp_total = 0
ingrediente = {}

while reteta:
    paranteza = reteta.find('(')
    if paranteza != -1:
        timp, reteta = int(reteta[paranteza + 1]), reteta[paranteza + 3:]
        timp_total += timp
        ingrediente_paranteza = {}
        while reteta and reteta[0] != ')':
            ingredient, cantitate, reteta = reteta.split(' ', 2)
            ingrediente_paranteza[ingredient] = ingrediente_paranteza.get(ingredient, 0) + int(cantitate)
        reteta = reteta[1:]
