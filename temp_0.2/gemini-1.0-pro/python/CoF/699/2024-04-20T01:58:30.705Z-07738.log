    echipaje_poz[echipaj[0]].append(echipaj[1])
    echipaje_poz[echipaj[2]].append(echipaj[3])

viz = [False] * 251
dist = [0] * 251
q = []

for i in range(1, 251):
    if len(echipaje_poz[i]) % 2 == 1:
        q.append(i)
        viz[i] = True

while q:
    nod = q.pop(0)
    for vecin in echipaje_poz[nod]:
        if not viz[vecin]:
            viz[vecin] = True
            dist[vecin] = dist[nod] + 1
            q.append(vecin)

ans = 0
for d in dist[1:]:
    if d > 0:
        ans += d

with open('poarta.out', 'w') as f:
    f.write(str(ans))
```
