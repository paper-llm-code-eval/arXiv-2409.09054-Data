with open('poarta.in') as f:
    p = int(f.readline())
    echipaje = []
    for _ in range(p):
        echipaje.append(tuple(map(int, f.readline().split())))

def dist(a, b):
    return abs(a[0] - b[0]) + abs(a[1] - b[1])

def bfs(echipaje):
    viz = [[False for _ in range(251)] for _ in range(251)]
    dist = [[0 for _ in range(251)] for _ in range(251)]
    q = []
    for echipaj in echipaje:
        viz[echipaj[0]][echipaj[1]] = True
        q.append(echipaj)
    while q:
        x, y = q.pop(0)
        for dx, dy in [(1, 0), (-1, 0), (0, 1), (0, -1)]:
            nx, ny = x + dx, y + dy
            if 1 <= nx <= 250 and 1 <= ny <= 250 and not viz[nx][ny]:
                viz[nx][ny] = True
                dist[nx][ny] = dist[x][y] + 1
                q.append((nx, ny))
    return max(max(row) for row in dist)

print(bfs(echipaje))
```
