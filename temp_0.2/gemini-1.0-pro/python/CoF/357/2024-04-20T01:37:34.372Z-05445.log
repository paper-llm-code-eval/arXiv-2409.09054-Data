        output = evaluate_level(circuit, input, 0)
        outputs.append(output)
    return outputs


def evaluate_level(circuit, input, level):
    if level == n - 1:
        return evaluate_gate(circuit[level], input[0], input[1])
    else:
        left = evaluate_level(circuit, input[0:2], level + 1)
        right = evaluate_level(circuit, input[2:4], level + 1)
        return evaluate_gate(circuit[level], left, right)


def evaluate_gate(gate, left, right):
    if gate == '&':
        return left & right
    else:
        return left | right


def count_inputs(circuit, output):
    count = 0
    for i in range(2 ** n):
        input = bin(i)[2:].zfill(n)
        if evaluate_circuit(circuit, [input]) == output:
            count += 1
    return count


with open('logic.in', 'r') as f:
