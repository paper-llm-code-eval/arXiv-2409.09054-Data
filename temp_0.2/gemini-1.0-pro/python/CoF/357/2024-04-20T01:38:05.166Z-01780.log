def read_circuit(f):
    circuit = []
    for _ in range(n):
        circuit.append(f.readline().strip())
    return circuit

def read_inputs(f):
    inputs = []
    for _ in range(k):
        inputs.append(f.readline().strip())
    return inputs

def evaluate_circuit(circuit, inputs):
    values = [int(bit) for bit in inputs[0]]
    for level in range(1, n + 1):
        new_values = []
        for i in range(0, len(values), 2):
            if circuit[level - 1][i // 2] == '&':
                new_values.append(values[i] & values[i + 1])
            else:
                new_values.append(values[i] | values[i + 1])
        values = new_values
    return values[0]

def count_inputs(circuit, target):
    count = 0
    for i in range(2 ** n):
        inputs = [int(bit) for bit in bin(i)[2:].zfill(n)]
        if evaluate_circuit(circuit, inputs) == target:
            count += 1
    return count

with open('logic.in', 'r') as f, open('logic.out', 'w') as g:
    c = int(f.readline())
    n = int(f.readline())
    circuit = read_circuit(f)
    if c == 1:
        k = int(f.readline())
        inputs = read_inputs(f)
        for input in inputs:
            g.write(str(evaluate_circuit(circuit, input)) + '\n')
    else:
        target = int(f.readline())
        g.write(str(count_inputs(circuit, target) % 666013) + '\n')
```
