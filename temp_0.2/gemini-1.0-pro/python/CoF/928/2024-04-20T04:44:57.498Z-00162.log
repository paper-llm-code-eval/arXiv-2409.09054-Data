def decompress(s):
    stack = []
    decompressed = ""
    transformations = 0
    i = 0
    while i < len(s):
        if s[i].isdigit():
            num = 0
            while s[i].isdigit():
                num = num * 10 + int(s[i])
                i += 1
            i -= 1
            stack.append((num, ""))
        elif s[i] == "(":
            stack.append(("", ""))
        elif s[i] == ")":
            num, prev = stack.pop()
            decompressed += prev * num
            transformations += 1
        elif s[i] == "[":
            stack.append(("", ""))
        elif s[i] == "]":
            prev, decompressed_part = stack.pop()
            decompressed += prev + decompressed_part + prev[::-1]
            transformations += 1
        elif s[i] == "*":
            prev, decompressed_part = stack.pop()
            decompressed += prev + decompressed_part[::-1]
            transformations += 1
        else:
            decompressed += s[i]
        i += 1
    return transformations, decompressed

with open("arh.in", "r") as f:
    s = f.readline().strip()

transformations, decompressed = decompress(s)

with open("arh.out", "w") as f:
    f.write(str(transformations) + "\n")
    f.write(decompressed)
