import sys

def dfs(node):
    global time
    global dp
    global graph
    global indegree
    global earliest
    global latest

    if dp[node] != -1:
        return dp[node]

    dp[node] = durations[node]
    earliest[node] = time
    time += durations[node]
    latest[node] = time - 1

    for neighbor in graph[node]:
        if indegree[neighbor] == 0:
            indegree[neighbor] = 1
            dfs(neighbor)

        dp[node] = max(dp[node], durations[node] + dp[neighbor])
        latest[node] = min(latest[node], time - 1 - dp[neighbor])

    return dp[node]

sys.stdin = open("pm.in", "r")
sys.stdout = open("pm.out", "w")

n = int(input())
durations = list(map(int, input().split()))
graph = [[] for _ in range(n + 1)]
indegree = [0] * (n + 1)
earliest = [0] * (n + 1)
latest = [0] * (n + 1)

for i in range(1, n + 1):
    m = int(input())
    dependencies = list(map(int, input().split()))
    for dependency in dependencies:
        graph[dependency].append(i)
        indegree[i] += 1

time = 0
dp = [-1] * (n + 1)
dfs(1)

print(dp[1])
for i in range(1, n + 1):
    print(earliest[i], latest[i])
