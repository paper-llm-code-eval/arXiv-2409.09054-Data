import sys
import heapq

def dijkstra(n, p, k, d, c, g):
    dist = [float('inf')] * (p + 1)
    dist[1] = 0
    heap = [(0, 1)]
    while heap:
        cost, node = heapq.heappop(heap)
        if node == p:
            return cost
        if cost > dist[node]:
            continue
        for u in g[node]:
            if dist[node] + d[u] < dist[u]:
                dist[u] = dist[node] + d[u]
                heapq.heappush(heap, (dist[u], u))
    return -1

if __name__ == '__main__':
    with open('zmeu.in', 'r') as f:
        n, p, k = map(int, f.readline().split())
        d = [0] + [int(x) for x in f.readline().split()]
        c = [0] + [int(x) for x in f.readline().split()]
        g = [[] for _ in range(p + 1)]
        for _ in range(k):
            a, b = map(int, f.readline().split())
            g[a].append(b)
    if n - sum(c) < 0:
        with open('zmeu.out', 'w') as f:
            f.write('-1')
    else:
        with open('zmeu.out', 'w') as f:
            f.write(str(dijkstra(n, p, k, d, c, g)))
