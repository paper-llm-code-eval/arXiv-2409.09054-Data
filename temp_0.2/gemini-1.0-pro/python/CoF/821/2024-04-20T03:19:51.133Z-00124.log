with open('cifru.in', 'r') as f:
    n = int(f.readline())
    discuri = [int(f.readline()) for _ in range(n)]

max_cifra = max(discuri)
min_cifra = min(discuri)
mutari = 0
for disc in discuri:
    if disc != max_cifra:
        mutari += max_cifra - disc

cifra_minima = max_cifra - mutari
combinatii = 1
for disc in discuri:
    if disc == max_cifra:
        combinatii *= disc

with open('cifru.out', 'w') as f:
    f.write(str(max_cifra) + '\n')
    f.write(str(mutari) + '\n')
    f.write(str(cifra_minima) + '\n')
    f.write(str(combinatii) + '\n')
```
