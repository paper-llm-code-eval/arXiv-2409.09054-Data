import sys

sys.setrecursionlimit(100000)

N, D = map(int, input().split())
edges = [[] for _ in range(N+1)]
for _ in range(N-1):
    a, b = map(int, input().split())
    edges[a].append(b)
    edges[b].append(a)

dp = [[-1] * (D+1) for _ in range(N+1)]

def dfs(node, parent, dist):
    if dp[node][dist] != -1:
        return dp[node][dist]
    dp[node][dist] = 1
    for u in edges[node]:
        if u != parent:
            dp[node][dist] += dfs(u, node, dist-1)
    return dp[node][dist]

print(dfs(1, 1, D) % 1000000007)
