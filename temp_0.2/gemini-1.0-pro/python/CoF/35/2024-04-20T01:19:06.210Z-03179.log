    return comb(n - 1, k - 1) + comb(n - 1, k)

def write_output(n, perfect_bipermutations, minimum_moves, c):
    sys.stdout.write(f"{perfect_bipermutations} {minimum_moves}\n")
    for i in range(n):
        sys.stdout.write(f"{c[i]} ")
    sys.stdout.write("\n")
    for i in range(n):
        sys.stdout.write(f"{c[n + i]} ")
    sys.stdout.write("\n")

if __name__ == "__main__":
    n, a, b = read_input()
    if n is not None:
        perfect_bipermutations = count_perfect_bipermutations(n, a, b)
        minimum_moves = count_minimum_moves(n, a, b)
        c = find_perfect_bipermutation(n, a, b)
        write_output(n, perfect_bipermutations, minimum_moves, c)
    else:
        sys.exit(0)
