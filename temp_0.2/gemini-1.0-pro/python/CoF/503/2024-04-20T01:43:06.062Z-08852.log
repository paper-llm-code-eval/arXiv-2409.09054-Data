with open('bingo.in', 'r') as fin, open('bingo.out', 'w') as fout:
    T = int(fin.readline())
    for _ in range(T):
        S = fin.readline().strip()
        N = len(S)
        cnt = [0] * 5
        for c in S:
            if c == 'b':
                cnt[0] += 1
            elif c == 'i':
                cnt[1] += 1
            elif c == 'n':
                cnt[2] += 1
            elif c == 'g':
                cnt[3] += 1
            elif c == 'o':
                cnt[4] += 1
        ans = N
        for perm in permutations(S):
            P = ''.join(perm)
