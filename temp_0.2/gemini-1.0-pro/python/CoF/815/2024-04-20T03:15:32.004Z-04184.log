import sys
import queue

def read_input():
    n = int(input())
    t = [int(x) for x in input().split()]
    k = int(input())
    obstacles = []
    for _ in range(k):
        obstacles.append([int(x) for x in input().split()])
    return n, t, obstacles

def get_walls(obstacles):
    walls = []
    for obstacle in obstacles:
        if obstacle[0] == obstacle[2]:
            walls.append([obstacle[1], obstacle[3]])
        else:
            walls.append([obstacle[0], obstacle[1]])
    return walls

def get_max_wall_length(walls):
    max_length = 0
    for wall in walls:
        length = wall[1] - wall[0] + 1
        if length > max_length:
            max_length = length
    return max_length

def get_min_time(n, t, obstacles):
    walls = get_walls(obstacles)
    max_wall_length = get_max_wall_length(walls)
    time = 0
    if max_wall_length % 2 == 0:
        time += max_wall_length // 2
    else:
        time += max_wall_length // 2 + 1
    time += abs(t[0] - t[2]) + abs(t[1] - t[3])
    return time

def main():
    n, t, obstacles = read_input()
    max_wall_length = get_max_wall_length(obstacles)
    min_time = get_min_time(n, t, obstacles)
    with open('ai.out', 'w') as f:
        f.write(str(max_wall_length) + '\n')
        f.write(str(min_time) + '\n')

if __name__ == '__main__':
    main()
