with open('litere.in') as f:
    n = int(f.readline())
    word = f.readline().strip()
    m = int(f.readline())
    codes = list(map(int, f.readline().split()))
    code_map = {chr(ord('A') + i): code for i, code in enumerate(codes)}

vowels = {'A', 'E', 'I', 'O', 'U'}
k = [0] * n
for i in range(n):
    k[i] = code_map[word[i]]
    if i > 0 and (word[i] in vowels) ^ (word[i - 1] in vowels):
        k[i] += k[i - 1]

with open('litere.out', 'w') as f:
    f.write(f'{k[n - 1]}\n')
    f.write(f'{k[0]}\n')
    f.write(f'{sum(k)}')
```
