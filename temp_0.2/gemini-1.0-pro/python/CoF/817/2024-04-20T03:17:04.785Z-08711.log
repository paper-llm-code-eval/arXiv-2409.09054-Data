with open('elicop.in') as f:
    m, n = map(int, f.readline().split())
    a = [[int(x) for x in f.readline().split()] for _ in range(m)]
    k = int(f.readline())
    elicop = [(tuple(map(int, f.readline().split()[:4])), int(f.readline().split()[4])) for _ in range(k)]

def get_tri(l1, c1, l2, c2, p):
    if p == 1:
        return [(i, j) for i in range(l1, l2+1) for j in range(c1, c2+1) if i-l1 == j-c1]
    else:
        return [(i, j) for i in range(l1, l2+1) for j in range(c1, c2+1) if i-l1 == c2-j]

def check(tri):
    return sum(a[i][j] for i, j in tri) > len(tri) / 2

N1 = 0
N2 = 0
e = []
for l1, c1, l2, c2, p in elicop:
    tri = get_tri(l1, c1, l2, c2, p)
    if not check(tri):
        N1 += 1
    else:
        N2 += 1
        e.append(elicop.index((l1, c1, l2, c2, p)) + 1)

with open('elicop.out', 'w') as f:
    f.write(f'{N1}\n')
    f.write(f'{N2} {" ".join(map(str, e))}')
```
