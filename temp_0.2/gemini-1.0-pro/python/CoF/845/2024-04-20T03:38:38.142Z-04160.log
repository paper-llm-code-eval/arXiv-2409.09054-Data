with open('munte.in') as f:
    n = int(f.readline())
    x = [int(i) for i in f.readline().split()]

def munte(x):
    m = []
    for i in range(1, len(x) - 1):
        if x[i - 1] < x[i] > x[i + 1]:
            m.append(i)
    return m

def main(x):
    nr_m = 0
    nr_t = 0
    while True:
        m = munte(x)
        if not m:
            break
        nr_m += len(m)
        nr_t += 1
        x = [x[i] for i in range(len(x)) if i not in m]
    return nr_m, nr_t, len(x)

nr_m, nr_t, nr_f = main(x)
with open('munte.out', 'w') as f:
    f.write(f'{nr_m}\n')
    f.write(f'{nr_t}\n')
    f.write(f'{nr_f}\n')
```
