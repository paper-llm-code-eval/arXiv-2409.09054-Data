import sys

with open('clepsidru.in', 'r') as f:
    n, b = map(int, f.readline().split())
    k = int(f.readline())
    states = [list(map(int, f.readline().split())) for _ in range(k)]

# First experiment
time = (n - 1) * b

# Second experiment
sand = [b] + [0] * (n - 1)
for s, p in states:
    if p == 1:
        sand[0] -= s
        sand[1] += s
    else:
        sand[n - 1] -= s
        sand[n - 2] += s

with open('clepsidru.out', 'w') as f:
    f.write(str(time) + '\n')
    for i in range(n):
        f.write(f'{sand[i]} {sand[i + 1]}\n')
```
