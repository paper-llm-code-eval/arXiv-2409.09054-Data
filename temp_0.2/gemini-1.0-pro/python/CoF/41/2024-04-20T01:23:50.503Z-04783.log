def read_input():
    with open('immortal.in', 'r') as f:
        n, m, I = map(int, f.readline().split())
        positions = [tuple(map(int, f.readline().split())) for _ in range(I)]
    return n, m, I, positions


def write_output(battles):
    with open('immortal.out', 'w') as f:
        for battle in battles:
            f.write(' '.join(map(str, battle)) + '\n')


def is_valid_move(n, m, position):
    x, y = position
    return 1 <= x <= n and 1 <= y <= m


def get_valid_moves(n, m, position):
    x, y = position
    moves = [(x - 2, y), (x + 2, y), (x, y - 2), (x, y + 2)]
    return [move for move in moves if is_valid_move(n, m, move)]


def get_next_move(n, m, positions):
    for position in positions:
        moves = get_valid_moves(n, m, position)
        for move in moves:
            if move in positions:
                return position, move
    return None


def simulate_battle(n, m, positions):
    battles = []
    while len(positions) > 1:
        next_move = get_next_move(n, m, positions)
        if next_move is None:
            return None
        start, end = next_move
        positions.remove(start)
        positions.remove(end)
        battles.append(start + end)
    return battles


def main():
    n, m, I, positions = read_input()
    battles = simulate_battle(n, m, positions)
    if battles is None:
        print('No solution')
    else:
        write_output(battles)


if __name__ == '__main__':
    main()
