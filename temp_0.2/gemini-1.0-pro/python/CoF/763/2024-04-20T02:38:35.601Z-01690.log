with open("furnica.in", "r") as f:
    n, k = map(int, f.readline().split())
    path = list(map(int, f.readline().split()))

board = [[(i + j) % 6 for j in range(n)] for i in range(n)]

ant_pos = (0, 0)
crumbs = 0
max_crumbs = 0
max_crumbs_count = 0
crumbs_count = {}

for direction in path:
    if direction == 1:
        ant_pos = (ant_pos[0] - 1, ant_pos[1] - 1)
    elif direction == 2:
        ant_pos = (ant_pos[0] - 1, ant_pos[1])
    elif direction == 3:
        ant_pos = (ant_pos[0] - 1, ant_pos[1] + 1)
    elif direction == 4:
        ant_pos = (ant_pos[0], ant_pos[1] + 1)
    elif direction == 5:
        ant_pos = (ant_pos[0] + 1, ant_pos[1] + 1)
    elif direction == 6:
        ant_pos = (ant_pos[0] + 1, ant_pos[1])
    elif direction == 7:
        ant_pos = (ant_pos[0] + 1, ant_pos[1] - 1)
    elif direction == 8:
        ant_pos = (ant_pos[0], ant_pos[1] - 1)

    crumbs += board[ant_pos[0]][ant_pos[1]]
    board[ant_pos[0]][ant_pos[1]] = 0

    if ant_pos not in crumbs_count:
        crumbs_count[ant_pos] = 1
    else:
        crumbs_count[ant_pos] += 1

    if crumbs_count[ant_pos] > max_crumbs_count:
        max_crumbs_count = crumbs_count[ant_pos]
        max_crumbs = ant_pos

with open("furnica.out", "w") as f:
    f.write(f"{crumbs} {max_crumbs_count}")
