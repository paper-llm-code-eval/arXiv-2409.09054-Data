with open("expresie.in", "r") as f:
    expresie = f.readline().strip()

numere = 0
valoare = 0
paranteze = []
secventa = []
for caracter in expresie:
    if caracter.isdigit():
        numere += 1
        secventa.append(int(caracter))
    elif caracter == "(":
        paranteze.append("(")
    elif caracter == ")":
        valoare += max(secventa)
        secventa = []
        paranteze.pop()
    elif caracter == "[":
        paranteze.append("[")
    elif caracter == "]":
        secventa.sort()
        valoare += secventa[len(secventa) // 2]
        secventa = []
        paranteze.pop()
    elif caracter == ",":
        if paranteze[-1] == "(":
            valoare += sum(secventa)
            secventa = []
        elif paranteze[-1] == "[":
            secventa.sort()
            valoare += secventa[len(secventa) // 2]
            secventa = []

with open("expresie.out", "w") as f:
    f.write(f"{numere}\n{valoare}")
```
