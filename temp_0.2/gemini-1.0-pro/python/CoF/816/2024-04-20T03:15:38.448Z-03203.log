with open('expresie.in', 'r') as f:
    expresie = f.readline().strip()

numere = 0
valoare = 0
paranteze = []
numar_curent = 0
semn = 1

for caracter in expresie:
    if caracter.isdigit():
        numar_curent = numar_curent * 10 + int(caracter)
    elif caracter == ',':
        valoare += numar_curent * semn
        numar_curent = 0
        numere += 1
    elif caracter == '(':
        paranteze.append((valoare, numere, semn))
        valoare = 0
        numere = 0
        semn = 1
    elif caracter == ')':
        valoare_paranteza, numere_paranteza, semn_paranteza = paranteze.pop()
        valoare += valoare_paranteza * semn_paranteza
        numere += numere_paranteza
    elif caracter == '[':
        paranteze.append((valoare, numere, semn))
        valoare = 0
        numere = 0
        semn = 1
    elif caracter == ']':
        valoare_paranteza, numere_paranteza, semn_paranteza = paranteze.pop()
        valoare += valoare_paranteza * semn_paranteza
        numere += numere_paranteza
        valoare = valoare_paranteza + numar_curent * semn
        numere = numere_paranteza + 1
    elif caracter == '-':
        semn = -1

valoare += numar_curent * semn

with open('expresie.out', 'w') as f:
    f.write(f'{numere}\n{valoare}')
